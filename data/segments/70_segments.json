[
  {
    "id": 2,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 6.22,
    "end": 11.5,
    "text": "سَأَلَ سَائِلٌ بِعَذَابٍ وَاقِعٍ"
  },
  {
    "id": 3,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 12.47,
    "end": 15.92,
    "text": "لِلْكَافِرِينَ لَيْسَ لَهُ دَافِعٌ"
  },
  {
    "id": 4,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 16.93,
    "end": 19.41,
    "text": "وَمِنَ اللَّهِ ذِي الْمَعَارِجِ"
  },
  {
    "id": 5,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 20.51,
    "end": 30.56,
    "text": "تِعْرُجُ الْمَلَائِكَةُ وَالرُّوحُ إِلَيْهِ فِي يَوْمٍ كَانَ مِقْدَارُهُ خَمْسِينَ أَلْفَ سَنَةٍ"
  },
  {
    "id": 6,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 31.61,
    "end": 34.99,
    "text": "فَاصْبِرْ صَبْرًا جَمِيلًا"
  },
  {
    "id": 7,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 35.72,
    "end": 39.38,
    "text": "إِنَّهُمْ يَرَوْنَهُ بَعِيدًا"
  },
  {
    "id": 8,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 40.16,
    "end": 42.28,
    "text": "وَنَرَاهُ قَرِيبًا"
  },
  {
    "id": 9,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 43.33,
    "end": 47.19,
    "text": "يَوْمَ تَكُونُ السَّمَاءُ كَالْمُهْلِ"
  },
  {
    "id": 10,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 47.93,
    "end": 50.82,
    "text": "وَتَكُونُ الْجِبَالُ كَالْعِهْنِ"
  },
  {
    "id": 11,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 51.72,
    "end": 55.27,
    "text": "وَلَا يَسْأَلُ حَمِيمٌ حَمِيمًا"
  },
  {
    "id": 12,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 56.43,
    "end": 58.09,
    "text": "يُبَصَّرُونَهُمْ"
  },
  {
    "id": 13,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 58.88,
    "end": 66.47,
    "text": "يَوَدُّ الْمُجْرِمُ لَوْ يَفْتَدِي مِنْ عَذَابِ يَوْمِئِذٍ بِبَنِيهِ"
  },
  {
    "id": 14,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 67.22,
    "end": 70.47,
    "text": "وَصَاحِبَتِهِ وَأَخِيهِ"
  },
  {
    "id": 15,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 71.33,
    "end": 75.06,
    "text": "وَفَصِيلَتِهِ الَّتِي تُؤْوِيهِ"
  },
  {
    "id": 16,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 75.78,
    "end": 82.95,
    "text": "وَمَنْ فِي الْأَرْضِ جَمِيعًا ثُمَّ يُنْجِيهِ"
  },
  {
    "id": 17,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 83.82,
    "end": 84.6,
    "text": "كَلَّا"
  },
  {
    "id": 18,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 85.27,
    "end": 87.23,
    "text": "إِنَّهَا لَغَى"
  },
  {
    "id": 19,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 87.93,
    "end": 90.36,
    "text": "نَزَّاعَةً لِلشَّوَى"
  },
  {
    "id": 20,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 91.03,
    "end": 94.24,
    "text": "وَيَدْعُو مَنْ أَدْبَرَ وَتَوَلَّى"
  },
  {
    "id": 21,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 95.01,
    "end": 96.87,
    "text": "وَجَمَعَ فَأَوْعَى"
  },
  {
    "id": 22,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 97.79,
    "end": 101.98,
    "text": "إِنَّ الْإِنْسَانَ خُلِقَ هَلُوعًا"
  },
  {
    "id": 23,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 102.69,
    "end": 105.86,
    "text": "إِذَا مَسَّهُ الشَّرُّ جَزُوعًا"
  },
  {
    "id": 24,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 106.71,
    "end": 110.15,
    "text": "وَإِذَا مَسَّهُ الْخَيْرُ مَنُوعًا"
  },
  {
    "id": 25,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 111.06,
    "end": 113.58,
    "text": "إِلَّا الْمُصَلِّينَ"
  },
  {
    "id": 26,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 114.33,
    "end": 120.66,
    "text": "الَّذِينَ هُمْ عَلَى صَلَاتِهِمْ دَائِمُونَ"
  },
  {
    "id": 27,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 121.73,
    "end": 128.0,
    "text": "وَالَّذِينَ فِي أَمْوَالِهِمْ حَقٌّ مَعْلُومٌ"
  },
  {
    "id": 28,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 128.74,
    "end": 132.95,
    "text": "لِلسَّائِلِ وَالْمَحْرُومِ"
  },
  {
    "id": 29,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 133.81,
    "end": 138.42,
    "text": "وَالَّذِينَ يُصَدِّقُونَ بِيَوْمِ الدِّينِ"
  },
  {
    "id": 30,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 139.22,
    "end": 145.36,
    "text": "وَالَّذِينَ هُمْ مِنْ عَذَابِ رَبِّهِمْ مُشْفِقُونَ"
  },
  {
    "id": 31,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 146.09,
    "end": 150.89,
    "text": "إِنَّ عَذَابَ رَبِّهِمْ غَيْرُ مَأْمُونٍ"
  },
  {
    "id": 32,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 152.1,
    "end": 156.8,
    "text": "وَالَّذِينَ هُمْ لِفُرُوجِهِمْ آفِضُونَ"
  },
  {
    "id": 33,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 157.51,
    "end": 167.69,
    "text": "إِلَّا عَلَى أَزْوَاجِهِمْ أَوْ مَا مَلَكَتْ أَيْمَانُهُمْ فَإِنَّهُمْ غَيْرُ مَلُومِينَ"
  },
  {
    "id": 34,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 168.91,
    "end": 176.92,
    "text": "فَمَنِ ابْتَغَى وَرَاءَ ذَلِكَ فَأُولَئِكَ هُمُ الْعَادُونَ"
  },
  {
    "id": 35,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 177.73,
    "end": 183.54,
    "text": "وَالَّذِينَ هُمْ لِأَمَانَاتِهِمْ وَعَهْدِهِمْ رَاعُونَ"
  },
  {
    "id": 36,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 184.53,
    "end": 191.52,
    "text": "وَالَّذِينَ هُمْ بِشَهَادَاتِهِمْ قَائِمُونَ"
  },
  {
    "id": 37,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 192.6,
    "end": 197.95,
    "text": "وَالَّذِينَ هُمْ عَلَى صَلَاتِهِمْ يُحَافِظُونَ"
  },
  {
    "id": 38,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 198.62,
    "end": 205.18,
    "text": "أُولَئِكَ فِي جَنَّاتٍ مُكْرَمُونَ"
  },
  {
    "id": 39,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 206.43,
    "end": 211.59,
    "text": "فَمَالِ الَّذِينَ كَفَرُوا قِبَلَكَ مُهْطِعِينَ"
  },
  {
    "id": 40,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 212.35,
    "end": 216.71,
    "text": "عَنِ الْيَمِينِ وَعَنِ الشِّمَالِ عِزِينَ"
  },
  {
    "id": 41,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 217.66,
    "end": 225.13,
    "text": "أَيَطْمَعُ كُلُّ امْرِئٍ مِنْهُمْ أَنْ يُدْخَلَ جَنَّةَ نَعِيمٍ"
  },
  {
    "id": 42,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 225.91,
    "end": 226.66,
    "text": "كَلَّا"
  },
  {
    "id": 43,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 227.34,
    "end": 233.36,
    "text": "إِنَّا خَلَقْنَاهُمْ مِمَّا يَعْلَمُونَ"
  },
  {
    "id": 44,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 234.53,
    "end": 242.82,
    "text": "فَلَا أُقْسِمُ بِرَبِّ الْمَشَارِقِ وَالْمَغَارِبِ إِنَّا لَقَادِرُونَ"
  },
  {
    "id": 45,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 243.79,
    "end": 252.11,
    "text": "عَلَى أَنْ نُبَدِّلَ خَيْرًا مِنْهُمْ وَمَا نَحْنُ بِمَسْبُوقِينَ"
  },
  {
    "id": 46,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 253.03,
    "end": 261.49,
    "text": "فَذَرْهُمْ يَخُوضُوا وَيَلْعَبُوا حَتَّى يُلَاقُوا يَوْمَهُمُ الَّذِي يُوعَدُونَ"
  },
  {
    "id": 47,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 262.85,
    "end": 273.01,
    "text": "يَوْمَ يَخْرُجُونَ مِنَ الْأَجْدَاثِ سِرَاعًا كَأَنَّهُمْ إِلَى نُصُبٍ يُوفِضُونَ"
  },
  {
    "id": 48,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 273.77,
    "end": 278.31,
    "text": "خَاشِعَةً أَبْصَارُهُمْ تَرْهَقُهُمْ ذِلَّةٌ"
  },
  {
    "id": 49,
    "sura_id": 70,
    "recitation_uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6",
    "start": 279.36,
    "end": 285.01,
    "text": "ذَلِكَ الْيَوْمُ الَّذِي كَانُوا يُوعَدُونَ"
  }
]