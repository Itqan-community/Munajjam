[
  {
    "id": 2,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 6.22,
    "end": 8.91,
    "text": "حم"
  },
  {
    "id": 3,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 10.0,
    "end": 16.24,
    "text": "تَنْزِيلُ الْكِتَابِ مِنَ اللَّهِ الْعَزِيزِ الْحَكِيمِ"
  },
  {
    "id": 4,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 17.77,
    "end": 24.85,
    "text": "إِنَّ فِي السَّمَاوَاتِ وَالْأَرْضِ لَآيَاتٍ لِلْمُؤْمِنِينَ"
  },
  {
    "id": 5,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 26.33,
    "end": 37.08,
    "text": "وَفِي خَلْقِكُمْ وَمَا يَبُثُّ مِنْ دَابَّةٍ آيَاتٌ لِقَوْمٍ يُوقِنُونَ"
  },
  {
    "id": 6,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 38.25,
    "end": 50.81,
    "text": "وَاخْتِلَافِ اللَّيْلِ وَالنَّهَارِ وَمَا أَنْزَلَ اللَّهُ مِنَ السَّمَاءِ مِنْ رِزْقٍ فَأَحْيَا بِهِ"
  },
  {
    "id": 7,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 52.42,
    "end": 62.84,
    "text": "فَأَحْيَا بِهِ الْأَرْضَ بَعْدَ مَوْتِهَا وَتَصْرِيفِ الرِّيَاحِ آيَاتٌ لِقَوْمٍ يَعْقِلُونَ"
  },
  {
    "id": 8,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 64.15,
    "end": 68.88,
    "text": "تِلْكَ آيَاتُ اللَّهِ نَتْلُوهَا عَلَيْكَ بِالْحَقَيمِ"
  },
  {
    "id": 9,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 69.36,
    "end": 69.46,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 10,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 70.31,
    "end": 77.24,
    "text": "فَبِأَيِّ حَدِيثٍ بَعْدَ اللَّهِ وَآيَاتِهِ يُؤْمِنُونَ"
  },
  {
    "id": 11,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 78.27,
    "end": 82.56,
    "text": "وَيْلٌ لِكُلِّ أَفَّاكٍ أَثِيمٍ"
  },
  {
    "id": 12,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 83.43,
    "end": 93.18,
    "text": "يَسْمَعُ آيَاتِ اللَّهِ تُتْلَى عَلَيْهِ ثُمَّ يُصِرُّ مُسْتَكْبِرًا كَأَنْ لَمْ يَسْمَعْهَا"
  },
  {
    "id": 13,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 94.06,
    "end": 98.1,
    "text": "فَبَشِّرْهُ بِعَذَابٍ أَلِيمٍ"
  },
  {
    "id": 14,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 99.24,
    "end": 105.08,
    "text": "وَإِذَا عَلِمَ مِنْ آيَاتِنَا شَيْئًا اتَّخَذَهَا هُزُوًا"
  },
  {
    "id": 15,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 106.01,
    "end": 112.19,
    "text": "أُولَئِكَ لَهُمْ عَذَابٌ مُهِينٌ"
  },
  {
    "id": 16,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 113.56,
    "end": 117.95,
    "text": "مِنْ وَرَائِهِمْ جَهَنَّمَ"
  },
  {
    "id": 17,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 118.86,
    "end": 129.96,
    "text": "وَلَا يُغْنِي عَنْهُمْ مَا كَسَبُوا شَيْئًا وَلَا مَا اتَّخَذُوا مِنْ دُونِ اللَّهِ أَوْلِيَاءَ"
  },
  {
    "id": 18,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 131.08,
    "end": 134.44,
    "text": "وَلَهُمْ عَذَابٌ عَظِيمٌ"
  },
  {
    "id": 19,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 135.64,
    "end": 137.1,
    "text": "هَذَا هُدًى"
  },
  {
    "id": 20,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 138.12,
    "end": 146.93,
    "text": "وَالَّذِينَ كَفَرُوا بِآيَاتِ رَبِّهِمْ لَهُمْ عَذَابٌ مِنْ رِجْزٍ أَلِيمٌ"
  },
  {
    "id": 21,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 148.23,
    "end": 158.65,
    "text": "اللَّهُ الَّذِي سَخَّرَ لَكُمُ الْبَحْرَ لِتَجْرِيَ الْفُلْكُ فِيهِ بِأَمْرِهِ وَلِتَبْتَغُوا مِنْ فَضْلِهِ"
  },
  {
    "id": 22,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 159.78,
    "end": 166.81,
    "text": "وَلِتَبْتَغُوا مِنْ فَضْلِهِ وَلَعَلَّكُمْ تَشْكُرُونَ"
  },
  {
    "id": 23,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 167.91,
    "end": 175.37,
    "text": "وَسَخَّرَ لَكُمْ مَا فِي السَّمَاوَاتِ وَمَا فِي الْأَرْضِ جَمِيعًا مِنْهُ"
  },
  {
    "id": 24,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 176.2,
    "end": 184.04,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِقَوْمٍ يَتَفَكَّرُونَ"
  },
  {
    "id": 25,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 185.33,
    "end": 198.59,
    "text": "قُلْ لِلَّذِينَ آمَنُوا يَغْفِرُوا لِلَّذِينَ لَا يَرْجُونَ أَيَّامَ اللَّهِ لِيَجْزِيَ قَوْمًا بِمَا كَانُوا يَكْسِبُونَ"
  },
  {
    "id": 26,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 199.93,
    "end": 203.38,
    "text": "مَنْ عَمِلَ صَالِحًا فَلِنَفْسِهِ"
  },
  {
    "id": 27,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 204.26,
    "end": 207.82,
    "text": "وَمَنْ أَسَاءَ فَعَلَيْهَا"
  },
  {
    "id": 28,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 208.8,
    "end": 213.23,
    "text": "ثُمَّ إِلَى رَبِّكُمْ تُرْجَعُونَ"
  },
  {
    "id": 29,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 214.4,
    "end": 227.21,
    "text": "وَلَقَدْ آتَيْنَا بَنِي إِسْرَائِيلَ الْكِتَابَ وَالْحُكْمَ وَالنُّبُوَّةَ وَرَزَقْنَاهُمْ مِنَ الطَّيِّبَاتِ"
  },
  {
    "id": 30,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 228.43,
    "end": 236.53,
    "text": "وَرَزَقْنَاهُمْ مِنَ الطَّيِّبَاتِ وَفَضَّلْنَاهُمْ عَلَى الْعَالَمِينَ"
  },
  {
    "id": 31,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 237.71,
    "end": 242.37,
    "text": "وَآتَيْنَاهُمْ بَيِّنَاتٍ مِنَ الْأَمْرِ"
  },
  {
    "id": 32,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 243.55,
    "end": 252.68,
    "text": "فَمَا اخْتَلَفُوا إِلَّا مِنْ بَعْدِ مَا جَاءَهُمُ الْعِلْمُ بَغْيًا بَيْنَهُمْ"
  },
  {
    "id": 33,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 253.8,
    "end": 263.55,
    "text": "إِنَّ رَبَّكَ يَقْضِي بَيْنَهُمْ يَوْمَ الْقِيَامَةِ فِيمَا كَانُوا فِيهِ يَخْتَلِفُونَ"
  },
  {
    "id": 34,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 265.11,
    "end": 271.64,
    "text": "ثُمَّ جَعَلْنَاكَ عَلَى شَرِيعَةٍ مِنَ الْأَمْرِ فَاتَّبِعْهَا"
  },
  {
    "id": 35,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 272.65,
    "end": 280.36,
    "text": "فَاتَّبِعْهَا وَلَا تَتَّبِعْ أَهْوَاءَ الَّذِينَ لَا يَعْلَمُونَ"
  },
  {
    "id": 36,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 281.34,
    "end": 287.49,
    "text": "إِنَّهُمْ لَنْ يُغْنُوا عَنْكَ مِنَ اللَّهِ شَيْئًا"
  },
  {
    "id": 37,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 288.58,
    "end": 294.43,
    "text": "وَإِنَّ الظَّالِمِينَ بَعْضُهُمْ أَوْلِيَاءُ بَعْضٍ"
  },
  {
    "id": 38,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 295.46,
    "end": 299.77,
    "text": "وَاللَّهُ وَلِيُّ الْمُتَّقِينَ"
  },
  {
    "id": 39,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 300.81,
    "end": 310.9,
    "text": "هَذَا بَصَائِرُ لِلنَّاسِ وَهُدًى وَرَحْمَةٌ لِقَوْمٍ يُوقِنُونَ"
  },
  {
    "id": 40,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 312.2,
    "end": 327.31,
    "text": "أَمْ حَسِبَ الَّذِينَ اجْتَرَحُوا السَّيِّئَاتِ أَنْ نَجْعَلَهُمْ كَالَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ سَوَاءً مَحْيَاهُمْ وَمَمَاتُهُمْ"
  },
  {
    "id": 41,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 328.41,
    "end": 332.32,
    "text": "يَا أَمَا يَحْكُمُونَ"
  },
  {
    "id": 42,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 333.73,
    "end": 346.28,
    "text": "وَخَلَقَ اللَّهُ السَّمَاوَاتِ وَالْأَرْضَ بِالْحَقِّ وَلِتُجْزَى كُلُّ نَفْسٍ بِمَا كَسَبَتْ وَهُمْ لَا يُظْلَمُونَ"
  },
  {
    "id": 43,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 347.58,
    "end": 354.63,
    "text": "أَفَرَأَيْتَ مَنِ اتَّخَذَ إِلَهَهُ هَوَاهُ وَأَضَلَّهُ اللَّهُ عَلَى عِلْمٍ"
  },
  {
    "id": 44,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 355.82,
    "end": 372.38,
    "text": "وَأَضَلَّهُ اللَّهُ عَلَى عِلْمٍ وَخَتَمَ عَلَى سَمْعِهِ وَقَلْبِهِ وَجَعَلَ عَلَى بَصَرِهِ غِشَاوَةً فَمَنْ يَهْدِيهِ مِنْ بَعْدِ اللَّهِ"
  },
  {
    "id": 45,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 373.54,
    "end": 376.31,
    "text": "أَفَلَا تَذَكَّرُونَ"
  },
  {
    "id": 46,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 377.52,
    "end": 387.19,
    "text": "وَقَالُوا مَا هِيَ إِلَّا حَيَاتُنَا الدُّنْيَا نَمُوتُ وَنَحْيَا وَمَا يُهْلِكُنَا إِلَّا الدَّهْرُ"
  },
  {
    "id": 47,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 388.25,
    "end": 391.65,
    "text": "وَمَا لَهُمْ بِذَلِكَ مِنْ عِلْمٍ"
  },
  {
    "id": 48,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 392.57,
    "end": 396.56,
    "text": "إِنْ هُمْ إِلَّا يَظُنُّونَ"
  },
  {
    "id": 49,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 397.79,
    "end": 411.74,
    "text": "وَإِذَا تُتْلَى عَلَيْهِمْ آيَاتُنَا بَيِّنَاتٍ مَا كَانَ حُجَّتَهُمْ إِلَّا أَنْ قَالُوا ائْتُوا بِآبَائِنَا"
  },
  {
    "id": 50,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 413.22,
    "end": 426.75,
    "text": "مَا كَانَ حُجَّتَهُمْ إِلَّا أَنْ قَالُوا ائْتُوا بِآبَائِنَا إِنْ كُنْتُمْ صَادِقِينَ"
  },
  {
    "id": 51,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 427.94,
    "end": 434.73,
    "text": "قُلِ اللَّهُ يُحْيِيكُمْ ثُمَّ يُمِيتُكُمْ ثُمَّ يَجْمَعُكُمْ"
  },
  {
    "id": 52,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 435.82,
    "end": 447.44,
    "text": "ثُمَّ يَجْمَعُكُمْ إِلَى يَوْمِ الْقِيَامَةِ لَا رَيْبَ فِيهِ وَلَكِنَّ أَكْثَرَ النَّاسِ لَا يَعْلَمُونَ"
  },
  {
    "id": 53,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 448.72,
    "end": 452.65,
    "text": "وَلِلَّهِ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 54,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 453.69,
    "end": 460.47,
    "text": "وَيَوْمَ تَقُومُ السَّاعَةُ يَوْمَئِذٍ يَخْسَرُ الْمُبْطِلُونَ"
  },
  {
    "id": 55,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 461.62,
    "end": 465.95,
    "text": "وَتَرَى كُلَّ أُمَّةٍ جَاثِيَةً"
  },
  {
    "id": 56,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 467.08,
    "end": 478.4,
    "text": "كُلُّ أُمَّةٍ تُدْعَى إِلَى كِتَابِهَا الْيَوْمَ تُجْزَوْنَ مَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 57,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 479.63,
    "end": 485.14,
    "text": "هَذَا كِتَابُنَا يَنْطِقُ عَلَيْكُمْ بِالْحَقِّ"
  },
  {
    "id": 58,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 485.64,
    "end": 485.72,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 59,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 486.54,
    "end": 494.81,
    "text": "إِنَّا كُنَّا نَسْتَنْسِخُ مَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 60,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 496.25,
    "end": 505.01,
    "text": "فَأَمَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ فَيُدْخِلُهُمْ رَبُّهُمْ فِي رَحْمَتِهِ"
  },
  {
    "id": 61,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 505.98,
    "end": 509.89,
    "text": "ذَلِكَ هُوَ الْفَوْزُ الْمُبِينُ"
  },
  {
    "id": 62,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 511.17,
    "end": 518.86,
    "text": "وَأَمَّا الَّذِينَ كَفَرُوا أَفَلَمْ تَكُنْ آيَاتِي تُتْلَى عَلَيْكُمْ"
  },
  {
    "id": 63,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 520.06,
    "end": 531.07,
    "text": "أَفَلَمْ تَكُنْ آيَاتِي تُتْلَى عَلَيْكُمْ فَاسْتَكْبَرْتُمْ وَكُنْتُمْ قَوْمًا مُجْرِمِينَ"
  },
  {
    "id": 64,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 532.47,
    "end": 536.26,
    "text": "وَإِذَا قِيلَ إِنَّ وَعْدَ اللَّهِ"
  },
  {
    "id": 65,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 536.59,
    "end": 543.67,
    "text": "وَالسَّاعَةُ لَا رَيْبَ فِيهَا قُلْتُمْ مَا نَدْرِي مَا السَّاعَةُ"
  },
  {
    "id": 66,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 545.2,
    "end": 557.18,
    "text": "قُلْتُمْ مَا نَدْرِي مَا السَّاعَةُ إِنْ نَظُنُّ إِلَّا ظَنًّا وَمَا نَحْنُ بِمُسْتَيْقِنِينَ"
  },
  {
    "id": 67,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 558.68,
    "end": 568.48,
    "text": "وَبَدَا لَهُمْ سَيِّئَاتُ مَا عَمِلُوا وَحَاقَ بِهِمْ مَا كَانُوا بِهِ يَسْتَهْزِئُونَ"
  },
  {
    "id": 68,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 569.75,
    "end": 580.88,
    "text": "وَقِيلَ الْيَوْمَ نَنْسَاكُمْ كَمَا نَسِيتُمْ لِقَاءَ يَوْمِكُمْ هَذَا وَمَأْوَاكُمُ النَّارُ"
  },
  {
    "id": 69,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 582.0,
    "end": 589.23,
    "text": "وَمَأْوَاكُمُ النَّارُ وَمَا لَكُمْ مِنْ نَاصِرِينَ"
  },
  {
    "id": 70,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 590.05,
    "end": 600.32,
    "text": "ذَلِكُمْ بِأَنَّكُمُ اتَّخَذْتُمْ آيَاتِ اللَّهِ هُزُوًا وَغَرَّتْكُمُ الْحَيَاةُ الدُّنْيَا"
  },
  {
    "id": 71,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 601.32,
    "end": 607.83,
    "text": "فَالْيَوْمَ لَا يُخْرَجُونَ مِنْهَا وَلَا هُمْ يُسْتَعْتَبُونَ"
  },
  {
    "id": 72,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 609.0,
    "end": 617.26,
    "text": "بَلِ اللَّهِ الْحَمْدُ رَبِّ السَّمَاوَاتِ وَرَبِّ الْأَرْضِ رَبِّ الْعَالَمِينَ"
  },
  {
    "id": 73,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 618.41,
    "end": 624.25,
    "text": "وَلَهُ الْكِبْرِيَاءُ فِي السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 74,
    "sura_id": 45,
    "recitation_uuid": "92b54e26-47d8-40cc-a435-18d99bf57c26",
    "start": 625.14,
    "end": 628.8,
    "text": "وَهُوَ الْعَزِيزُ الْحَكِيمُ"
  }
]