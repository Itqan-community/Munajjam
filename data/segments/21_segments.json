[
  {
    "id": 2,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 5.59,
    "end": 12.83,
    "text": "اقْتَرَبَ لِلنَّاسِ حِسَابُهُمْ وَهُمْ فِي غَفْلَةٍ مُعْرِضُونَ"
  },
  {
    "id": 3,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 13.81,
    "end": 23.99,
    "text": "فَمَا يَأْتِيهِمْ مِنْ ذِكْرٍ مِنْ رَبِّهِمْ مُحْدَثٍ إِلَّا اسْتَمَعُوهُ وَهُمْ يَلْعَبُونَ"
  },
  {
    "id": 4,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 24.99,
    "end": 27.75,
    "text": "لَاهِيَةً قُلُوبُهُمْ"
  },
  {
    "id": 5,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 28.73,
    "end": 36.96,
    "text": "وَأَسَرُّوا النَّجْوَى الَّذِينَ ظَلَمُوا هَلْ هَذَا إِلَّا بَشَرٌ مِثْلُكُمْ"
  },
  {
    "id": 6,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 37.97,
    "end": 43.33,
    "text": "أَفَتَأْتُونَ السِّحْرَ وَأَنْتُمْ تُبْصِرُونَ"
  },
  {
    "id": 7,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 44.34,
    "end": 50.08,
    "text": "قَالَ رَبِّي يَعْلَمُ الْقَوْلَ فِي السَّمَاءِ وَالْأَرْضِ"
  },
  {
    "id": 8,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 51.04,
    "end": 54.34,
    "text": "وَهُوَ السَّمِيعُ الْعَلِيمُ"
  },
  {
    "id": 9,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 55.29,
    "end": 65.04,
    "text": "بَلْ قَالُوا أَضْغَاثُ أَحْلَامٍ بَلِ افْتَرَاهُ بَلْ هُوَ شَاعِرٌ فَلْيَأْتِنَا بِآيَةٍ"
  },
  {
    "id": 10,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 66.24,
    "end": 73.16,
    "text": "فَلْيَأْتِنَا بِآيَةٍ كَمَا أُرْسِلَ الْأَوَّلُونَ"
  },
  {
    "id": 11,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 74.06,
    "end": 80.48,
    "text": "وَمَا آمَنَتْ قَبْلَهُمْ مِنْ قَرْيَةٍ أَهْلَكْنَاهَا"
  },
  {
    "id": 12,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 81.48,
    "end": 84.06,
    "text": "أَفَهُمْ يُؤْمِنُونَ"
  },
  {
    "id": 13,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 85.02,
    "end": 92.72,
    "text": "وَمَا أَرْسَلْنَا قَبْلَكَ إِلَّا رِجَالًا نُوحِي إِلَيْهِمْ"
  },
  {
    "id": 14,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 93.89,
    "end": 101.86,
    "text": "يَسْأَلُوا أَهْلَ الذِّكْرِ إِنْ كُنْتُمْ لَا تَعْلَمُونَ"
  },
  {
    "id": 15,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 102.81,
    "end": 111.41,
    "text": "وَمَا جَعَلْنَاهُمْ جَسَدًا لَا يَأْكُلُونَ الطَّعَامَ وَمَا كَانُوا خَالِدِينَ"
  },
  {
    "id": 16,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 112.78,
    "end": 125.0,
    "text": "ثُمَّ صَدَقْنَاهُمُ الْوَعْدَ فَأَنْجَيْنَاهُمْ وَمَنْ نَشَاءُ وَأَهْلَكْنَا الْمُسْرِفِينَ"
  },
  {
    "id": 17,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 126.48,
    "end": 133.11,
    "text": "لَقَدْ أَنْزَلْنَا إِلَيْكُمْ كِتَابًا فِيهِ ذِكْرُكُمْ"
  },
  {
    "id": 18,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 134.09,
    "end": 136.68,
    "text": "أَفَلَا تَعْقِلُونَ"
  },
  {
    "id": 19,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 137.73,
    "end": 149.27,
    "text": "وَكَمْ قَصَمْنَا مِنْ قَرْيَةٍ كَانَتْ ظَالِمَةً وَأَنْشَأْنَا بَعْدَهَا قَوْمًا آخَرِينَ"
  },
  {
    "id": 20,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 150.42,
    "end": 159.39,
    "text": "فَلَمَّا أَحَسُّوا بَأْسَنَا إِذَا هُمْ مِنْهَا يَرْكُضُونَ"
  },
  {
    "id": 21,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 160.61,
    "end": 171.88,
    "text": "لَا تَرْكُضُوا وَارْجِعُوا إِلَى مَا أُتْرِفْتُمْ فِيهِ وَمَسَاكِنِكُمْ لَعَلَّكُمْ تُسْأَلُونَ"
  },
  {
    "id": 22,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 172.78,
    "end": 179.79,
    "text": "قَالُوا يَا وَيْلَنَا إِنَّا كُنَّا ظَالِمِينَ"
  },
  {
    "id": 23,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 180.71,
    "end": 188.82,
    "text": "فَمَا زَالَتْ تِلْكَ دَعْوَاهُمْ حَتَّى جَعَلْنَاهُمْ حَصِيدًا خَامِدِينَ"
  },
  {
    "id": 24,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 189.88,
    "end": 197.13,
    "text": "وَمَا خَلَقْنَا السَّمَاءَ وَالْأَرْضَ وَمَا بَيْنَهُمَا لَاعِبِينَ"
  },
  {
    "id": 25,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 198.14,
    "end": 211.26,
    "text": "لَوْ أَرَدْنَا أَنْ نَتَّخِذَ لَهْوًا لَاتَّخَذْنَاهُ مِنْ لَدُنَّا إِنْ كُنَّا فَاعِلِينَ"
  },
  {
    "id": 26,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 212.53,
    "end": 219.16,
    "text": "بَلْ نَقْذِفُ بِالْحَقِّ عَلَى الْبَاطِلِ فَيَدْمَغُهُ فَإِذَا هُوَ زَاهِقٌ"
  },
  {
    "id": 27,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 220.19,
    "end": 224.7,
    "text": "وَلَكُمُ الْوَيْلُ مِمَّا تَصِفُونَ"
  },
  {
    "id": 28,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 225.57,
    "end": 229.32,
    "text": "وَلَهُ مَنْ فِي السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 29,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 230.16,
    "end": 236.6,
    "text": "وَمَنْ عِنْدَهُ لَا يَسْتَكْبِرُونَ عَنْ عِبَادَتِهِ وَلَا يَسْتَكْبِرُونَ"
  },
  {
    "id": 30,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 236.92,
    "end": 238.22,
    "text": "ذِرُونَ"
  },
  {
    "id": 31,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 239.19,
    "end": 244.99,
    "text": "يُسَبِّحُونَ اللَّيْلَ وَالنَّهَارَ لَا يَفْتُرُونَ"
  },
  {
    "id": 32,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 246.11,
    "end": 253.51,
    "text": "أَمِ اتَّخَذُوا آلِهَةً مِنَ الْأَرْضِ هُمْ يُنْشِرُونَ"
  },
  {
    "id": 33,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 254.44,
    "end": 260.82,
    "text": "لَوْ كَانَ فِيهِمَا آلِهَةٌ إِلَّا اللَّهُ لَفَسَدَتَا"
  },
  {
    "id": 34,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 261.78,
    "end": 267.8,
    "text": "فَسُبْحَانَ اللَّهِ رَبِّ الْعَرْشِ عَمَّا يَصِفُونَ"
  },
  {
    "id": 35,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 268.69,
    "end": 274.0,
    "text": "لَا يُسْأَلُ عَمَّا يَفْعَلُ وَهُمْ يُسْأَلُونَ"
  },
  {
    "id": 36,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 275.08,
    "end": 279.48,
    "text": "أَمِ اتَّخَذُوا مِنْ دُونِهِ آلِهَةً"
  },
  {
    "id": 37,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 280.35,
    "end": 282.82,
    "text": "قُلْ هَاتُوا بُرْهَانَكُمْ"
  },
  {
    "id": 38,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 283.86,
    "end": 288.7,
    "text": "هَذَا ذِكْرُ مَنْ مَعِيَ وَذِكْرُ مَنْ قَبْلِي"
  },
  {
    "id": 39,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 289.6,
    "end": 292.85,
    "text": "بَلْ أَكْثَرُهُمْ لَا يَعْلَمُونَ"
  },
  {
    "id": 40,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 293.27,
    "end": 293.29,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 41,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 294.12,
    "end": 297.07,
    "text": "لَهُمْ مُعْرِضُونَ"
  },
  {
    "id": 42,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 298.12,
    "end": 313.69,
    "text": "وَمَا أَرْسَلْنَا مِنْ قَبْلِكَ مِنْ رَسُولٍ إِلَّا نُوحِي إِلَيْهِ أَنَّهُ لَا إِلَهَ إِلَّا أَنَا فَاعْبُدُونِ"
  },
  {
    "id": 43,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 314.68,
    "end": 318.02,
    "text": "وَقَالُوا اتَّخَذَ الرَّحْمَنُ وَلَدًا"
  },
  {
    "id": 44,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 318.79,
    "end": 319.75,
    "text": "سُبْحَانَهُ"
  },
  {
    "id": 45,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 320.55,
    "end": 324.18,
    "text": "بَلْ عِبَادٌ مُكْرَمُونَ"
  },
  {
    "id": 46,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 325.04,
    "end": 331.77,
    "text": "لَا يَسْبِقُونَهُ بِالْقَوْلِ وَهُمْ بِأَمْرِهِ يَعْمَلُونَ"
  },
  {
    "id": 47,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 332.79,
    "end": 345.71,
    "text": "يَعْلَمُ مَا بَيْنَ أَيْدِيهِمْ وَمَا خَلْفَهُمْ وَلَا يَشْفَعُونَ إِلَّا لِمَنِ ارْتَضَى وَهُمْ مِنْ خَشْيَتِهِ مُشْفِقُونَ"
  },
  {
    "id": 48,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 347.1,
    "end": 357.75,
    "text": "وَمَنْ يَقُلْ مِنْهُمْ إِنِّي إِلَهٌ مِنْ دُونِهِ فَذَلِكَ نَجْزِيهِ جَهَنَّمَ"
  },
  {
    "id": 49,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 358.59,
    "end": 362.07,
    "text": "كَذَلِكَ نَجْزِي الظَّالِمِينَ"
  },
  {
    "id": 50,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 363.25,
    "end": 373.66,
    "text": "أَوَلَمْ يَرَ الَّذِينَ كَفَرُوا أَنَّ السَّمَاوَاتِ وَالْأَرْضَ كَانَتَا رَتْقًا فَفَتَقْنَاهُمَا"
  },
  {
    "id": 51,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 374.79,
    "end": 379.39,
    "text": "وَجَعَلْنَا مِنَ الْمَاءِ كُلَّ شَيْءٍ حَيًّا"
  },
  {
    "id": 52,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 380.33,
    "end": 382.92,
    "text": "أَفَلَا يُؤْمِنُونَ"
  },
  {
    "id": 53,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 383.9,
    "end": 396.44,
    "text": "وَجَعَلْنَا فِي الْأَرْضِ رَوَاسِيَ أَنْ تَمِيدَ بِهِمْ وَجَعَلْنَا فِيهَا فِجَاجًا سُبُلًا لَعَلَّهُمْ يَهْتَدُونَ"
  },
  {
    "id": 54,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 397.56,
    "end": 401.4,
    "text": "وَجَعَلْنَا السَّمَاءَ سَقْفًا"
  },
  {
    "id": 55,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 401.77,
    "end": 402.54,
    "text": "مُوضًى"
  },
  {
    "id": 56,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 403.33,
    "end": 407.31,
    "text": "وَهُمْ عَنْ آيَاتِهَا مُعْرِضُونَ"
  },
  {
    "id": 57,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 408.47,
    "end": 414.35,
    "text": "وَهُوَ الَّذِي خَلَقَ اللَّيْلَ وَالنَّهَارَ وَالشَّمْسَ وَالْقَمَرَ"
  },
  {
    "id": 58,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 415.46,
    "end": 420.13,
    "text": "كُلٌّ فِي فَلَكٍ يَسْبَحُونَ"
  },
  {
    "id": 59,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 421.21,
    "end": 426.39,
    "text": "وَمَا جَعَلْنَا لِبَشَرٍ مِنْ قَبْلِكَ الْخُلْدَ"
  },
  {
    "id": 60,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 427.56,
    "end": 432.0,
    "text": "أَفَإِنْ مِتَّ فَهُمُ الْخَالِدُونَ"
  },
  {
    "id": 61,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 432.94,
    "end": 437.79,
    "text": "كُلُّ نَفْسٍ ذَائِقَةُ الْمَوْتِ"
  },
  {
    "id": 62,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 438.54,
    "end": 442.88,
    "text": "وَنَبْلُوكُمْ بِالشَّرِّ وَالْخَيْرِ"
  },
  {
    "id": 63,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 443.88,
    "end": 446.72,
    "text": "وَإِلَيْنَا تُرْجَعُونَ"
  },
  {
    "id": 64,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 447.85,
    "end": 458.98,
    "text": "وَإِذَا رَآكَ الَّذِينَ كَفَرُوا إِنْ يَتَّخِذُونَكَ إِلَّا هُزُوًا أَهَذَا الَّذِي يَذْكُرُ آلِهَتَكُمْ"
  },
  {
    "id": 65,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 460.01,
    "end": 468.69,
    "text": "وَمَا هَذَا الَّذِي يَذْكُرُ آلِهَتَكُمْ وَهُمْ بِذِكْرِ الرَّحْمَنِ هُمْ كَافِرُونَ"
  },
  {
    "id": 66,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 469.61,
    "end": 472.45,
    "text": "خُلِقَ الْإِنْسَانُ مِنْ عَجَلٍ"
  },
  {
    "id": 67,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 473.27,
    "end": 478.28,
    "text": "لَأُرِيكُمْ آيَاتِي فَلَا تَسْتَعْجِلُونِ"
  },
  {
    "id": 68,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 479.2,
    "end": 486.53,
    "text": "وَيَقُولُونَ مَتَى هَذَا الْوَعْدُ إِنْ كُنْتُمْ صَادِقِينَ"
  },
  {
    "id": 69,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 487.61,
    "end": 501.51,
    "text": "لَوْ يَعْلَمُ الَّذِينَ كَفَرُوا حِينَ لَا يَكُفُّونَ عَنْ وُجُوهِهِمُ النَّارَ وَلَا عَنْ ظُهُورِهِمْ وَلَا هُمْ يُنْصَرُونَ"
  },
  {
    "id": 70,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 503.01,
    "end": 510.79,
    "text": "بَلْ تَأْتِيهِمْ بَغْتَةً فَتَبْهَتُهُمْ فَلَا يَسْتَطِيعُونَ رَدَّهَا"
  },
  {
    "id": 71,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 511.89,
    "end": 518.33,
    "text": "فَلَا يَسْتَطِيعُونَ رَدَّهَا وَلَا هُمْ يُنْظَرُونَ"
  },
  {
    "id": 72,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 519.67,
    "end": 532.11,
    "text": "وَلَقَدِ اسْتُهْزِئَ بِرُسُلٍ مِنْ قَبْلِكَ فَحَاقَ بِالَّذِينَ سَخِرُوا مِنْهُمْ مَا كَانُوا بِهِ يَسْتَهْزِئُونَ"
  },
  {
    "id": 73,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 533.42,
    "end": 540.58,
    "text": "قُلْ مَنْ يَكْلَؤُكُمْ بِاللَّيْلِ وَالنَّهَارِ مِنَ الرَّحْمَنِ"
  },
  {
    "id": 74,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 541.52,
    "end": 546.82,
    "text": "بَلْ هُمْ عَنْ ذِكْرِ رَبِّهِمْ مُعْرِضُونَ"
  },
  {
    "id": 75,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 547.83,
    "end": 553.71,
    "text": "أَمْ لَهُمْ آلِهَةٌ تَمْنَعُهُمْ مِنْ دُونِنَا"
  },
  {
    "id": 76,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 554.56,
    "end": 562.92,
    "text": "لَا يَسْتَطِيعُونَ نَصْرَ أَنْفُسِهِمْ وَلَا هُمْ مِنَّا يُصْحَبُونَ"
  },
  {
    "id": 77,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 564.13,
    "end": 573.64,
    "text": "بَلْ مَتَّعْنَا هَؤُلَاءِ وَآبَاءَهُمْ حَتَّى طَالَ عَلَيْهِمُ الْعُمُرُ"
  },
  {
    "id": 78,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 574.64,
    "end": 582.06,
    "text": "أَفَلَا يَرَوْنَ أَنَّا نَأْتِي الْأَرْضَ نَنْقُصُهَا مِنْ أَطْرَافِهَا"
  },
  {
    "id": 79,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 582.89,
    "end": 585.67,
    "text": "فَهُمُ الْغَالِبُونَ"
  },
  {
    "id": 80,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 586.49,
    "end": 592.12,
    "text": "قُلْ إِنَّمَا أُنْذِرُكُمْ بِالْوَحْيِ"
  },
  {
    "id": 81,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 593.2,
    "end": 600.47,
    "text": "وَلَا يَسْمَعُ الصُّمُّ الدُّعَاءَ إِذَا مَا يُنْذَرُونَ"
  },
  {
    "id": 82,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 601.54,
    "end": 609.88,
    "text": "وَلَئِنْ مَسَّتْهُمْ نَفْحَةٌ مِنْ عَذَابِ رَبِّكَ لَيَقُولُنَّ يَا وَيْلَنَا"
  },
  {
    "id": 83,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 610.94,
    "end": 618.26,
    "text": "لَيَقُولُنَّ يَا وَيْلَنَا إِنَّا كُنَّا ظَالِمِينَ"
  },
  {
    "id": 84,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 619.33,
    "end": 627.75,
    "text": "وَنَضَعُ الْمَوَازِينَ الْقِسْطَ لِيَوْمِ الْقِيَامَةِ فَلَا تُظْلَمُ نَفْسٌ شَيْئًا"
  },
  {
    "id": 85,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 628.74,
    "end": 635.5,
    "text": "وَإِنْ كَانَ مِثْقَالَ حَبَّةٍ مِنْ خَرْدَلٍ أَتَيْنَا بِهَا"
  },
  {
    "id": 86,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 636.43,
    "end": 639.83,
    "text": "وَكَفَى بِنَا حَاسِبِينَ"
  },
  {
    "id": 87,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 640.81,
    "end": 651.32,
    "text": "وَلَقَدْ آتَيْنَا مُوسَى وَهَارُونَ الْفُرْقَانَ وَضِيَاءً وَذِكْرًا لِلْمُتَّقِينَ"
  },
  {
    "id": 88,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 652.23,
    "end": 660.99,
    "text": "الَّذِينَ يَخْشَوْنَ رَبَّهُمْ بِالْغَيْبِ وَهُمْ مِنَ السَّاعَةِ مُشْفِقُونَ"
  },
  {
    "id": 89,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 661.94,
    "end": 667.33,
    "text": "وَهَذَا ذِكْرٌ مُبَارَكٌ أَنْزَلْنَا"
  },
  {
    "id": 90,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 668.26,
    "end": 673.74,
    "text": "أَفَأَنْتُمْ لَهُ مُنْكِرُونَ"
  },
  {
    "id": 91,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 675.56,
    "end": 686.14,
    "text": "وَلَقَدْ آتَيْنَا إِبْرَاهِيمَ رُشْدَهُ مِنْ قَبْلُ وَكُنَّا بِهِ عَالِمِينَ"
  },
  {
    "id": 92,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 687.49,
    "end": 698.68,
    "text": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِ مَا هَذِهِ التَّمَاثِيلُ الَّتِي أَنْتُمْ لَهَا عَاكِفُونَ"
  },
  {
    "id": 93,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 699.85,
    "end": 706.74,
    "text": "قَالُوا وَجَدْنَا آبَاءَنَا لَهَا عَابِدِينَ"
  },
  {
    "id": 94,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 708.03,
    "end": 717.76,
    "text": "قَالَ لَقَدْ كُنْتُمْ أَنْتُمْ وَآبَاؤُكُمْ فِي ضَلَالٍ مُبِينٍ"
  },
  {
    "id": 95,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 719.1,
    "end": 726.78,
    "text": "قَالُوا أَجِئْتَنَا بِالْحَقِّ أَمْ أَنْتَ مِنَ اللَّاعِبِينَ"
  },
  {
    "id": 96,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 727.79,
    "end": 740.11,
    "text": "قَالَ بَلْ رَبُّكُمْ رَبُّ السَّمَاوَاتِ وَالْأَرْضِ الَّذِي فَطَرَهُنَّ وَأَنَا عَلَى ذَلِكُمْ مِنَ الشَّاهِدِينَ"
  },
  {
    "id": 97,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 741.53,
    "end": 750.83,
    "text": "وَتَاللَّهِ لَأَكِيدَنَّ أَصْنَامَكُمْ بَعْدَ أَنْ تُوَلُّوا مُدْبِرِينَ"
  },
  {
    "id": 98,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 752.07,
    "end": 760.25,
    "text": "فَجَعَلَهُمْ جُذَاذًا إِلَّا كَبِيرًا لَهُمْ لَعَلَّهُمْ إِلَيْهِ يَرْجِعُونَ"
  },
  {
    "id": 99,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 761.38,
    "end": 770.95,
    "text": "قَالُوا مَنْ فَعَلَ هَذَا بِآلِهَتِنَا إِنَّهُ لَمِنَ الظَّالِمِينَ"
  },
  {
    "id": 100,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 772.09,
    "end": 779.99,
    "text": "قَالُوا سَمِعْنَا فَتًى يَذْكُرُهُمْ يُقَالُ لَهُ إِبْرَاهِيمُ"
  },
  {
    "id": 101,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 781.03,
    "end": 789.55,
    "text": "قَالُوا فَأْتُوا بِهِ عَلَى أَعْيُنِ النَّاسِ لَعَلَّهُمْ يَشْهَدُونَ"
  },
  {
    "id": 102,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 790.64,
    "end": 799.35,
    "text": "قَالُوا أَأَنْتَ فَعَلْتَ هَذَا بِآلِهَتِنَا يَا إِبْرَاهِيمُ"
  },
  {
    "id": 103,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 800.38,
    "end": 809.68,
    "text": "قَالَ بَلْ فَعَلَهُ كَبِيرُهُمْ هَذَا فَاسْأَلُوهُمْ إِنْ كَانُوا يَنْطِقُونَ"
  },
  {
    "id": 104,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 810.99,
    "end": 822.22,
    "text": "فَرَجَعُوا إِلَى أَنْفُسِهِمْ فَقَالُوا إِنَّكُمْ أَنْتُمُ الظَّالِمُونَ"
  },
  {
    "id": 105,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 823.6,
    "end": 834.17,
    "text": "ثُمَّ نُكِسُوا عَلَى رُءُوسِهِمْ لَقَدْ عَلِمْتَ مَا هَؤُلَاءِ يَنْطِقُونَ"
  },
  {
    "id": 106,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 835.59,
    "end": 845.08,
    "text": "قَالَ أَفَتَعْبُدُونَ مِنْ دُونِ اللَّهِ مَا لَا يَنْفَعُكُمْ شَيْئًا وَلَا يَضُرُّكُمْ"
  },
  {
    "id": 107,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 846.16,
    "end": 852.24,
    "text": "أُفٍّ لَكُمْ وَلِمَا تَعْبُدُونَ مِنْ دُونِ اللَّهِ"
  },
  {
    "id": 108,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 853.24,
    "end": 856.38,
    "text": "أَفَلَا تَعْقِلُونَ"
  },
  {
    "id": 109,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 857.42,
    "end": 867.25,
    "text": "قَالُوا حَرِّقُوهُ وَانْصُرُوا آلِهَتَكُمْ إِنْ كُنْتُمْ فَاعِلِينَ"
  },
  {
    "id": 110,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 868.27,
    "end": 876.62,
    "text": "قُلْنَا يَا نَارُ كُونِي بَرْدًا وَسَلَامًا عَلَى إِبْرَاهِيمَ"
  },
  {
    "id": 111,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 877.86,
    "end": 884.34,
    "text": "وَأَرَادُوا بِهِ كَيْدًا فَجَعَلْنَاهُمُ الْأَخْسَرِينَ"
  },
  {
    "id": 112,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 885.46,
    "end": 893.78,
    "text": "وَنَجَّيْنَاهُ وَلُوطًا إِلَى الْأَرْضِ الَّتِي بَارَكْنَا فِيهَا لِلْعَالَمِينَ"
  },
  {
    "id": 113,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 894.49,
    "end": 899.84,
    "text": "وَوَهَبْنَا لَهُ إِسْحَاقَ وَيَعْقُوبَ نَافِلَةً"
  },
  {
    "id": 114,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 900.63,
    "end": 904.9,
    "text": "وَكُلًّا جَعَلْنَا صَالِحِينَ"
  },
  {
    "id": 115,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 905.87,
    "end": 916.67,
    "text": "وَجَعَلْنَاهُمْ أَئِمَّةً يَهْدُونَ بِأَمْرِنَا وَأَوْحَيْنَا إِلَيْهِمْ فِعْلًا خَيْرًا"
  },
  {
    "id": 116,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 917.78,
    "end": 928.48,
    "text": "وَأَوْحَيْنَا إِلَيْهِمْ فِعْلَ الْخَيْرَاتِ وَإِقَامَ الصَّلَاةِ وَإِيتَاءَ الزَّكَاةِ"
  },
  {
    "id": 117,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 929.5,
    "end": 933.12,
    "text": "وَكَانُوا لَنَا عَابِدِينَ"
  },
  {
    "id": 118,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 934.38,
    "end": 945.63,
    "text": "وَلُوطًا آتَيْنَاهُ حُكْمًا وَعِلْمًا وَنَجَّيْنَاهُ مِنَ الْقَرْيَةِ الَّتِي كَانَتْ تَعْمَلُ الْخَبَائِثَ"
  },
  {
    "id": 119,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 946.5,
    "end": 952.58,
    "text": "إِنَّهُمْ كَانُوا قَوْمَ سَوْءٍ فَاسِقِينَ"
  },
  {
    "id": 120,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 953.55,
    "end": 956.62,
    "text": "وَأَدْخَلْنَاهُ فِي رَحْمَتِنَا"
  },
  {
    "id": 121,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 957.49,
    "end": 961.54,
    "text": "إِنَّهُ مِنَ الصَّالِحِينَ"
  },
  {
    "id": 122,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 962.63,
    "end": 973.5,
    "text": "وَنُوحًا إِذْ نَادَى مِنْ قَبْلُ فَاسْتَجَبْنَا لَهُ فَنَجَّيْنَاهُ وَأَهْلَهُ مِنَ الْكَرْبِ الْعَظِيمِ"
  },
  {
    "id": 123,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 974.69,
    "end": 980.54,
    "text": "وَنَصَرْنَاهُ مِنَ الْقَوْمِ الَّذِينَ كَذَّبُوا بِآيَاتِنَا"
  },
  {
    "id": 124,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 981.46,
    "end": 989.1,
    "text": "إِنَّهُمْ كَانُوا قَوْمَ سَوْءٍ فَأَغْرَقْنَاهُمْ أَجْمَعِينَ"
  },
  {
    "id": 125,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 990.49,
    "end": 1002.83,
    "text": "وَدَاوُودَ وَسُلَيْمَانَ إِذْ يَحْكُمَانِ فِي الْحَرْثِ إِذْ نَفَشَتْ فِيهِ غَنَمُ الْقَوْمِ وَكُنَّا لِحُكْمِهِمْ شَاهِدِينَ"
  },
  {
    "id": 126,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1004.0,
    "end": 1007.43,
    "text": "فَفَهَّمْنَاهَا سُلَيْمَانِ"
  },
  {
    "id": 127,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1008.16,
    "end": 1012.23,
    "text": "وَكُلًّا آتَيْنَا حُكْمًا وَعِلْمًا"
  },
  {
    "id": 128,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1013.24,
    "end": 1019.5,
    "text": "وَسَخَّرْنَا مَعَ دَاوُودَ الْجِبَالَ يُسَبِّحْنَ وَالطَّيْرَ"
  },
  {
    "id": 129,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1020.34,
    "end": 1023.53,
    "text": "وَكُنَّا فَاعِلِينَ"
  },
  {
    "id": 130,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1024.85,
    "end": 1031.88,
    "text": "وَعَلَّمْنَاهُ صَنْعَةَ لَبُوسٍ لَكُمْ لِتُحْصِنَكُمْ مِنْ بَأْسِكُمْ"
  },
  {
    "id": 131,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1032.87,
    "end": 1036.57,
    "text": "فَهَلْ أَنْتُمْ شَاكِرُونَ"
  },
  {
    "id": 132,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1037.7,
    "end": 1047.73,
    "text": "وَلِسُلَيْمَانَ الرِّيحَ عَاصِفَةً تَجْرِي بِأَمْرِهِ إِلَى الْأَرْضِ الَّتِي بَارَكْنَا فِيهَا"
  },
  {
    "id": 133,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1048.68,
    "end": 1053.82,
    "text": "وَكُنَّا بِكُلِّ شَيْءٍ عَالِمِينَ"
  },
  {
    "id": 134,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1054.82,
    "end": 1062.76,
    "text": "وَمِنَ الشَّيَاطِينِ مَنْ يَغُوصُونَ لَهُ وَيَعْمَلُونَ عَمَلًا دُونَ ذَلِكَ"
  },
  {
    "id": 135,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1063.71,
    "end": 1067.57,
    "text": "وَكُنَّا لَهُمْ حَافِظِينَ"
  },
  {
    "id": 136,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1068.81,
    "end": 1079.77,
    "text": "وَأَيُّوبَ إِذْ نَادَى رَبَّهُ أَنِّي مَسَّنِيَ الضُّرُّ وَأَنْتَ أَرْحَمُ الرَّاحِمِينَ"
  },
  {
    "id": 137,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1081.17,
    "end": 1086.78,
    "text": "فَاسْتَجَبْنَا لَهُ فَكَشَفْنَا مَا بِهِ مِنْ ضُرٍّ"
  },
  {
    "id": 138,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1087.98,
    "end": 1098.87,
    "text": "وَآتَيْنَاهُ أَهْلَهُ وَمِثْلَهُمْ مَعَهُمْ رَحْمَةً مِنْ عِنْدِنَا وَذِكْرَى لِلْعَابِدِينَ"
  },
  {
    "id": 139,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1100.0,
    "end": 1103.4,
    "text": "وَإِسْمَاعِيلَ وَإِدْرِيسَ وَذَا الْكِفْلِ"
  },
  {
    "id": 140,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1104.36,
    "end": 1108.09,
    "text": "كُلٌّ مِنَ الصَّابِرِينَ"
  },
  {
    "id": 141,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1108.77,
    "end": 1112.02,
    "text": "وَأَدْخَلْنَاهُمْ فِي رَحْمَتِنَا"
  },
  {
    "id": 142,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1112.83,
    "end": 1117.09,
    "text": "إِنَّهُمْ مِنَ الصَّالِحِينَ"
  },
  {
    "id": 143,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1118.18,
    "end": 1128.81,
    "text": "وَذَا النُّونِ إِذْ ذَهَبَ مُغَاضِبًا فَظَنَّ أَنْ لَنْ نَقْدِرَ عَلَيْهِ فَنَادَى فِي الظُّلُمَاتِ"
  },
  {
    "id": 144,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1129.64,
    "end": 1143.08,
    "text": "فَنَادَى فِي الظُّلُمَاتِ أَنْ لَا إِلَهَ إِلَّا أَنْتَ سُبْحَانَكَ إِنِّي كُنْتُ مِنَ الظَّالِمِينَ"
  },
  {
    "id": 145,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1144.23,
    "end": 1148.99,
    "text": "فَاسْتَجَبْنَا لَهُ وَنَجَّيْنَاهُ مِنَ الْغَمِّ"
  },
  {
    "id": 146,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1149.83,
    "end": 1154.23,
    "text": "وَكَذَلِكَ نُنْجِي الْمُؤْمِنِينَ"
  },
  {
    "id": 147,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1155.43,
    "end": 1166.51,
    "text": "وَزَكَرِيَّا إِذْ نَادَى رَبَّهُ رَبِّ لَا تَذَرْنِي فَرْدًا وَأَنْتَ خَيْرُ الْوَارِثِينَ"
  },
  {
    "id": 148,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1167.66,
    "end": 1174.48,
    "text": "فَاسْتَجَبْنَا لَهُ وَوَهَبْنَا لَهُ يَحْيَى وَأَصْلَحْنَا لَهُ زَوْجَهُ"
  },
  {
    "id": 149,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1175.82,
    "end": 1184.24,
    "text": "إِنَّهُمْ كَانُوا يُسَارِعُونَ فِي الْخَيْرَاتِ وَيَدْعُونَنَا رَغَبًا وَرَهَبًا"
  },
  {
    "id": 150,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1185.17,
    "end": 1188.88,
    "text": "وَكَانُوا لَنَا خَاشِعِينَ"
  },
  {
    "id": 151,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1190.11,
    "end": 1203.86,
    "text": "وَالَّتِي أَحْصَنَتْ فَرْجَهَا فَنَفَخْنَا فِيهَا مِنْ رُوحِنَا وَجَعَلْنَاهَا وَابْنَهَا آيَةً لِلْعَالَمِينَ"
  },
  {
    "id": 152,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1205.04,
    "end": 1215.72,
    "text": "إِنَّ هَذِهِ أُمَّتُكُمْ أُمَّةً وَاحِدَةً وَأَنَا رَبُّكُمْ فَاعْبُدُونِ"
  },
  {
    "id": 153,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1216.81,
    "end": 1220.94,
    "text": "وَتَقَطَّعُوا أَمْرَهُمْ بَيْنَهُمْ"
  },
  {
    "id": 154,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1221.76,
    "end": 1225.36,
    "text": "كُلٌّ إِلَيْنَا رَاجِعُونَ"
  },
  {
    "id": 155,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1226.47,
    "end": 1233.91,
    "text": "فَمَنْ يَعْمَلْ مِنَ الصَّالِحَاتِ وَهُوَ مُؤْمِنٌ فَلَا كُفْرَانَ لِسَعْيِهِ"
  },
  {
    "id": 156,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1235.12,
    "end": 1241.42,
    "text": "فَلَا كُفْرَانَ لِسَعْيِهِ وَإِنَّا لَهُ كَاتِبُونَ"
  },
  {
    "id": 157,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1242.56,
    "end": 1250.7,
    "text": "وَحَرَامٌ عَلَى قَرْيَةٍ أَهْلَكْنَاهَا أَنَّهُمْ لَا يَرْجِعُونَ"
  },
  {
    "id": 158,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1251.85,
    "end": 1262.9,
    "text": "حَتَّى إِذَا فُتِحَتْ يَأْجُوجُ وَمَأْجُوجُ وَهُمْ مِنْ كُلِّ حَدَبٍ يَنْسِلُونَ"
  },
  {
    "id": 159,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1264.45,
    "end": 1272.0,
    "text": "وَاقْتَرَبَ الْوَعْدُ الْحَقُّ فَإِذَا هِيَ شَاخِصَةٌ أَبْصَارُ الَّذِينَ كَفَرُوا"
  },
  {
    "id": 160,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1273.38,
    "end": 1288.15,
    "text": "فَإِذَا هِيَ شَاخِصَةٌ أَبْصَارُ الَّذِينَ كَفَرُوا يَا وَيْلَنَا قَدْ كُنَّا فِي غَفْلَةٍ مِنْ هَذَا بَلْ كُنَّا ظَالِمِينَ"
  },
  {
    "id": 161,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1289.1,
    "end": 1300.32,
    "text": "إِنَّكُمْ وَمَا تَعْبُدُونَ مِنْ دُونِ اللَّهِ حَصَبُ جَهَنَّمَ أَنْتُمْ لَهَا وَارِدُونَ"
  },
  {
    "id": 162,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1301.57,
    "end": 1308.53,
    "text": "لَوْ كَانَ هَؤُلَاءِ آلِهَةً مَا وَرَدُوهَا"
  },
  {
    "id": 163,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1309.47,
    "end": 1313.61,
    "text": "وَكُلٌّ فِيهَا خَالِدُونَ"
  },
  {
    "id": 164,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1314.57,
    "end": 1321.41,
    "text": "لَهُمْ فِيهَا زَفِيرٌ وَهُمْ فِيهَا لَا يَسْمَعُونَ"
  },
  {
    "id": 165,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1322.43,
    "end": 1333.47,
    "text": "إِنَّ الَّذِينَ سَبَقَتْ لَهُمْ مِنَّا الْحُسْنَى أُولَئِكَ عَنْهَا مُبْعَدُونَ"
  },
  {
    "id": 166,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1334.64,
    "end": 1337.34,
    "text": "لَا يَسْمَعُونَ حَسِيسَهَا"
  },
  {
    "id": 167,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1338.47,
    "end": 1343.78,
    "text": "وَهُمْ فِي مَا اشْتَهَتْ أَنْفُسُهُمْ خَالِدُونَ"
  },
  {
    "id": 168,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1344.8,
    "end": 1352.01,
    "text": "لَا يَحْزُنُهُمُ الْفَزَعُ الْأَكْبَرُ وَتَتَلَقَّاهُمُ الْمَلَائِكَةُ"
  },
  {
    "id": 169,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1353.11,
    "end": 1362.99,
    "text": "وَتَتَلَقَّاهُمُ الْمَلَائِكَةُ هَذَا يَوْمُكُمُ الَّذِي كُنْتُمْ تُوعَدُونَ"
  },
  {
    "id": 170,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1364.16,
    "end": 1369.43,
    "text": "يَوْمَ نَطْوِي السَّمَاءَ كَطَيِّ السِّجِلِّ لِلْكُتُبِ"
  },
  {
    "id": 171,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1370.44,
    "end": 1375.5,
    "text": "كَمَا بَدَأْنَا أَوَّلَ خَلْقٍ نُعِيدُهُ"
  },
  {
    "id": 172,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1376.51,
    "end": 1378.15,
    "text": "وَعْدًا عَلَيْنَا"
  },
  {
    "id": 173,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1379.13,
    "end": 1383.49,
    "text": "إِنَّا كُنَّا سَاعِلِينَ"
  },
  {
    "id": 174,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1384.75,
    "end": 1395.11,
    "text": "وَلَقَدْ كَتَبْنَا فِي الزَّبُورِ مِنْ بَعْدِ الذِّكْرِ أَنَّ الْأَرْضَ يَرِثُهَا عِبَادِيَ الصَّالِحُونَ"
  },
  {
    "id": 175,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1396.38,
    "end": 1402.16,
    "text": "إِنَّ فِي هَذَا لَبَلَاغًا لِقَوْمٍ عَابِدِينَ"
  },
  {
    "id": 176,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1403.06,
    "end": 1409.92,
    "text": "وَمَا أَرْسَلْنَاكَ إِلَّا رَحْمَةً لِلْعَالَمِينَ"
  },
  {
    "id": 177,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1411.28,
    "end": 1420.47,
    "text": "قُلْ إِنَّمَا يُوحَى إِلَيَّ أَنَّمَا إِلَهُكُمْ إِلَهٌ وَاحِدٌ"
  },
  {
    "id": 178,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1421.58,
    "end": 1425.76,
    "text": "فَهَلْ أَنْتُمْ مُسْلِمُونَ"
  },
  {
    "id": 179,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1426.81,
    "end": 1433.58,
    "text": "فَإِنْ تَوَلَّوْا فَقُلْ آذَنْتُكُمْ عَلَى سَوَاءٍ"
  },
  {
    "id": 180,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1434.68,
    "end": 1442.4,
    "text": "وَإِنْ أَدْرِي أَقَرِيبٌ أَمْ بَعِيدٌ مَا تُوعَدُونَ"
  },
  {
    "id": 181,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1443.57,
    "end": 1450.68,
    "text": "إِنَّهُ يَعْلَمُ الْجَهْرَ مِنَ الْقَوْلِ وَيَعْلَمُ مَا تَكْتُمُونَ"
  },
  {
    "id": 182,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1451.86,
    "end": 1458.84,
    "text": "وَإِنْ أَدْرِي لَعَلَّهُ فِتْنَةٌ لَكُمْ وَمَتَاعٌ إِلَى حِينٍ"
  },
  {
    "id": 183,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1459.98,
    "end": 1462.82,
    "text": "قَالَ رَبِّ احْكُمْ بِالْحَبِّ"
  },
  {
    "id": 184,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1463.3,
    "end": 1463.32,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 185,
    "sura_id": 21,
    "recitation_uuid": "87390656-e4cf-472c-b2c1-91c42d7db68b",
    "start": 1464.16,
    "end": 1470.77,
    "text": "وَرَبُّنَا الرَّحْمَنُ الْمُسْتَعَانُ عَلَى مَا تَصِفُونَ"
  }
]