[
  {
    "id": 2,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 5.54,
    "end": 9.45,
    "text": "يَا أَيُّهَا الْمُزَّمِّلُ"
  },
  {
    "id": 3,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 10.44,
    "end": 13.17,
    "text": "قُومِ اللَّيْلَ إِلَّا قَلِيلًا"
  },
  {
    "id": 4,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 14.3,
    "end": 18.85,
    "text": "نِصْفَهُ أَوِ انْقُصْ مِنْهُ قَلِيلًا"
  },
  {
    "id": 5,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 19.74,
    "end": 24.53,
    "text": "أَوْ زِدْ عَلَيْهِ وَرَتِّلِ الْقُرْآنَ تَرْتِيلًا"
  },
  {
    "id": 6,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 25.71,
    "end": 30.73,
    "text": "إِنَّا سَنُلْقِي عَلَيْكَ قَوْلًا ثَقِيلًا"
  },
  {
    "id": 7,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 31.75,
    "end": 38.31,
    "text": "إِنَّ نَاشِئَةَ اللَّيْلِ هِيَ أَشَدُّ وَطْئًا وَأَقْوَمُ قِيلًا"
  },
  {
    "id": 8,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 39.65,
    "end": 44.54,
    "text": "إِنَّ لَكَ فِي النَّهَارِ سَبْحًا طَوِيلًا"
  },
  {
    "id": 9,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 45.8,
    "end": 50.78,
    "text": "وَاذْكُرِ اسْمَ رَبِّكَ وَتَبَتَّلْ إِلَيْهِ تَبْتِيلًا"
  },
  {
    "id": 10,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 52.3,
    "end": 60.69,
    "text": "رَبُّ الْمَشْرِقِ وَالْمَغْرِبِ لَا إِلَهَ إِلَّا هُوَ فَاتَّخِذْهُ وَكِيلًا"
  },
  {
    "id": 11,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 61.88,
    "end": 68.12,
    "text": "وَاصْبِرْ عَلَى مَا يَقُولُونَ وَاهْجُرْهُمْ هَجْرًا جَمِيلًا"
  },
  {
    "id": 12,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 69.23,
    "end": 75.8,
    "text": "وَذَرْنِي وَالْمُكَذِّبِينَ أُولِي النَّعْمَةِ وَمَهِّلْهُمْ قَلِيلًا"
  },
  {
    "id": 13,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 76.94,
    "end": 82.64,
    "text": "إِنَّ لَدَيْنَا أَنْكَالًا وَجَحِيمًا"
  },
  {
    "id": 14,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 83.61,
    "end": 89.0,
    "text": "وَطَعَامًا ذَا غُصَّةٍ وَعَذَابًا أَلِيمًا"
  },
  {
    "id": 15,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 90.18,
    "end": 98.11,
    "text": "يَوْمَ تَرْجُفُ الْأَرْضُ وَالْجِبَالُ وَكَانَتِ الْجِبَالُ كَثِيبًا مَهِيلًا"
  },
  {
    "id": 16,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 99.24,
    "end": 113.15,
    "text": "إِنَّا أَرْسَلْنَا إِلَيْكُمْ رَسُولًا شَاهِدًا عَلَيْكُمْ كَمَا أَرْسَلْنَا إِلَى فِرْعَوْنَ رَسُولًا"
  },
  {
    "id": 17,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 114.23,
    "end": 120.36,
    "text": "فَعَصَى فِرْعَوْنُ الرَّسُولَ فَأَخَذْنَاهُ أَخْذًا وَبِيلًا"
  },
  {
    "id": 18,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 121.51,
    "end": 128.91,
    "text": "فَكَيْفَ تَتَّقُونَ إِنْ كَفَرْتُمْ يَوْمًا يَجْعَلُ الْوِلْدَانَ شِيبًا"
  },
  {
    "id": 19,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 129.98,
    "end": 134.05,
    "text": "السَّمَاءُ مُنْفَطِرٌ بِهِ"
  },
  {
    "id": 20,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 134.79,
    "end": 137.55,
    "text": "يَا نَوَعْدُهُ مَفْعُولًا"
  },
  {
    "id": 21,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 138.42,
    "end": 141.14,
    "text": "إِنَّ هَذِهِ تَذْكِرَةٌ"
  },
  {
    "id": 22,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 141.89,
    "end": 147.52,
    "text": "فَمَنْ شَاءَ اتَّخَذَ إِلَى رَبِّهِ سَبِيلًا"
  },
  {
    "id": 23,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 148.57,
    "end": 162.77,
    "text": "إِنَّ رَبَّكَ يَعْلَمُ أَنَّكَ تَقُومُ أَدْنَى مِنْ سُلُسَ إِلَّيْنِ وَنِصْفَهُ وَسُلُسَهُ وَطَائِفَةٌ مِنَ الَّذِينَ مَعَ سَلِيلٌ"
  },
  {
    "id": 24,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 164.14,
    "end": 168.7,
    "text": "وَاللَّهُ يُقَدِّرُ اللَّيْلَ وَالنَّهَارَ"
  },
  {
    "id": 25,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 169.78,
    "end": 174.11,
    "text": "عَلِمَ أَنْ لَنْ تُحْصُوهُ فَتَابَ عَلَيْكُمْ"
  },
  {
    "id": 26,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 174.85,
    "end": 179.08,
    "text": "اقْرَءُوا مَا تَيَسَّرَ مِنَ الْقُرْآنِ"
  },
  {
    "id": 27,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 179.92,
    "end": 196.02,
    "text": "عَلِمَ أَنْ سَيَكُونُ مِنْكُمْ مَرْضَى وَآخَرُونَ يَضْرِبُونَ فِي الْأَرْضِ يَبْتَغُونَ مِنْ فَضْلِ اللَّهِ وَآخَرُونَ يُقَاتِلُونَ فِي سَبِيلِ اللَّهِ"
  },
  {
    "id": 28,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 197.22,
    "end": 199.76,
    "text": "اقْرَءُوا مَا تَيَسَّرَ مِنْهُ"
  },
  {
    "id": 29,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 200.68,
    "end": 207.69,
    "text": "وَأَقِيمُوا الصَّلَاةَ وَآتُوا الزَّكَاةَ وَأَقْرِضُوا اللَّهَ قَرْضًا حَسَنًا"
  },
  {
    "id": 30,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 208.65,
    "end": 220.27,
    "text": "وَمَا تُقَدِّمُوا لِأَنْفُسِكُمْ مِنْ خَيْرٍ تَجِدُوهُ عِنْدَ اللَّهِ هُوَ خَيْرًا وَأَعْظَمَ أَجْرًا"
  },
  {
    "id": 31,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 221.37,
    "end": 223.91,
    "text": "وَاسْتَغْفِرُوا اللَّهَ"
  },
  {
    "id": 32,
    "sura_id": 73,
    "recitation_uuid": "e256085a-94fc-42cd-a2d6-9f0363a195e0",
    "start": 224.56,
    "end": 229.16,
    "text": "إِنَّ اللَّهَ غَفُورٌ رَحِيمٌ"
  }
]