[
  {
    "id": 2,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 6.55,
    "end": 9.3,
    "text": "حم"
  },
  {
    "id": 3,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 10.25,
    "end": 16.07,
    "text": "تَنْزِيلُ الْكِتَابِ مِنَ اللَّهِ الْعَزِيزِ الْعَلِيمِ"
  },
  {
    "id": 4,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 17.27,
    "end": 24.45,
    "text": "غَافِرِ الذَّنْبِ وَقَابِلِ التَّوْبِ شَدِيدِ الْعِقَابِ ذِي الطَّوْلِ"
  },
  {
    "id": 5,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 25.39,
    "end": 28.89,
    "text": "لَا إِلَهَ إِلَّا هُوَ"
  },
  {
    "id": 6,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 29.67,
    "end": 32.21,
    "text": "إِلَيْهِ الْمَصِيرُ"
  },
  {
    "id": 7,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 33.34,
    "end": 44.74,
    "text": "لَا يُجَادِلُ فِي آيَاتِ اللَّهِ إِلَّا الَّذِينَ كَفَرُوا فَلَا يَغْرُرْكَ تَقَلُّبُهُمْ فِي الْبِلَادِ"
  },
  {
    "id": 8,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 45.83,
    "end": 52.19,
    "text": "كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍ وَالْأَحْزَابُ مِنْ بَعْدِهِمْ"
  },
  {
    "id": 9,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 53.22,
    "end": 59.92,
    "text": "وَهَمَّتْ كُلُّ أُمَّةٍ بِرَسُولِهِمْ لِيَأْخُذُوهُ"
  },
  {
    "id": 10,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 61.0,
    "end": 66.91,
    "text": "وَجَادَلُوا بِالْبَاطِلِ لِيُدْحِضُوا بِهِ الْحَقَّ فَأَخَذْتُهُمْ"
  },
  {
    "id": 11,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 67.84,
    "end": 70.99,
    "text": "فَكَيْفَ كَانَ عِقَابِ"
  },
  {
    "id": 12,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 72.05,
    "end": 82.47,
    "text": "وَكَذَلِكَ حَقَّتْ كَلِمَتُ رَبِّكَ عَلَى الَّذِينَ كَفَرُوا أَنَّهُمْ أَصْحَابُ النَّارِ"
  },
  {
    "id": 13,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 83.72,
    "end": 97.05,
    "text": "الَّذِينَ يَحْمِلُونَ الْعَرْشَ وَمَنْ حَوْلَهُ يُسَبِّحُونَ بِحَمْدِ رَبِّهِمْ وَيُؤْمِنُونَ بِهِ وَيَسْتَغْفِرُونَ لِلَّذِينَ آمَنُوا"
  },
  {
    "id": 14,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 98.35,
    "end": 107.03,
    "text": "رَبَّنَا وَسِعْتَ كُلَّ شَيْءٍ رَحْمَةً وَعِلْمًا فَاغْفِرْ لِلَّذِينَ تَابُوا"
  },
  {
    "id": 15,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 108.14,
    "end": 116.26,
    "text": "يَغْفِرْ لِلَّذِينَ تَابُوا وَاتَّبَعُوا سَبِيلَكَ وَقِهِمْ عَذَابَ الْجَحِيمِ"
  },
  {
    "id": 16,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 117.45,
    "end": 131.36,
    "text": "رَبَّنَا وَأَدْخِلْهُمْ جَنَّاتِ عَدْنٍ الَّتِي وَعَدْتَهُمْ وَمَنْ صَلَحَ مِنْ آبَائِهِمْ وَأَزْوَاجِهِمْ وَذُرِّيَّاتِهِمْ"
  },
  {
    "id": 17,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 132.53,
    "end": 137.47,
    "text": "إِنَّكَ أَنْتَ الْعَزِيزُ الْحَكِيمُ"
  },
  {
    "id": 18,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 138.36,
    "end": 141.26,
    "text": "وَقِهِمُ السَّيِّئَاتِ"
  },
  {
    "id": 19,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 142.13,
    "end": 148.08,
    "text": "وَمَنْ تَقِسَّيِّ آتِيَهُمَئِذٍ فَقَدْ رَحِمْتَهُ"
  },
  {
    "id": 20,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 149.01,
    "end": 153.07,
    "text": "وَذَلِكَ هُوَ الْفَوْزُ الْعَظِيمُ"
  },
  {
    "id": 21,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 154.41,
    "end": 168.77,
    "text": "إِنَّ الَّذِينَ كَفَرُوا يُنَادَوْنَ لَمَقْتُ اللَّهِ أَكْبَرُ مِنْ مَقْتِكُمْ أَنْفُسَكُمْ إِذْ تُدْعَوْنَ إِلَى الْإِيمَانِ فَتَكْفُرُونَ"
  },
  {
    "id": 22,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 169.88,
    "end": 179.48,
    "text": "قَالُوا رَبَّنَا أَمَتَّنَا اثْنَتَيْنِ وَأَحْيَيْتَنَا اثْنَتَيْنِ فَاعْتَرَفْنَا بِذُنُوبِنَا"
  },
  {
    "id": 23,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 180.54,
    "end": 188.77,
    "text": "فَاعْتَرَفْنَا بِذُنُوبِنَا فَهَلْ إِلَى خُرُوجٍ مِنْ سَبِيلٍ"
  },
  {
    "id": 24,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 189.73,
    "end": 200.67,
    "text": "ذَلِكُمْ بِأَنَّهُ إِذَا دُعِيَ اللَّهُ وَحْدَهُ كَفَرْتُمْ وَإِنْ يُشْرَكْ بِهِ تُؤْمِنُونَ"
  },
  {
    "id": 25,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 201.49,
    "end": 206.02,
    "text": "بَلْ حُكْمُ لِلَّهِ الْعَلِيِّ الْكَبِيرِ"
  },
  {
    "id": 26,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 207.11,
    "end": 215.56,
    "text": "هُوَ الَّذِي يُرِيكُمْ آيَاتِهِ وَيُنَزِّلُ لَكُمْ مِنَ السَّمَاءِ رِزْقًا"
  },
  {
    "id": 27,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 216.65,
    "end": 221.73,
    "text": "وَمَا يَتَذَكَّرُ إِلَّا مَنْ يُنِيبُ"
  },
  {
    "id": 28,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 222.75,
    "end": 229.42,
    "text": "فَادْعُوا اللَّهَ مُخْلِصِينَ لَهُ الدِّينَ وَلَوْ كَرِهَ الْكَافِرُونَ"
  },
  {
    "id": 29,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 230.34,
    "end": 244.63,
    "text": "وَفِي عُدَّرَ جَاثِدُ الْعَرْشِ يُلْقِي الرُّوحَ مِنْ أَمْرِهِ عَلَى مَنْ يَشَاءُ مِنْ عِبَادِهِ لِيُنْذِرَ يَوْمَ التَّلَاقِ"
  },
  {
    "id": 30,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 245.78,
    "end": 248.93,
    "text": "يَوْمَ هُمْ بَارِزُونَ"
  },
  {
    "id": 31,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 249.69,
    "end": 253.99,
    "text": "لَا يَخْفَى عَلَى اللَّهِ مِنْهُمْ شَيْءٌ"
  },
  {
    "id": 32,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 254.86,
    "end": 257.94,
    "text": "لِمَنِ الْمُلْكُ الْيَوْمِ"
  },
  {
    "id": 33,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 258.5,
    "end": 262.37,
    "text": "اللَّهِ الْوَاحِدِ الْقَهَّارِ"
  },
  {
    "id": 34,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 263.53,
    "end": 268.39,
    "text": "فَالْيَوْمَ تُجْزَى كُلُّ نَفْسٍ بِمَا كَسَبَ"
  },
  {
    "id": 35,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 269.29,
    "end": 271.86,
    "text": "لَا ظُلْمَ الْيَوْمِ"
  },
  {
    "id": 36,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 272.55,
    "end": 277.08,
    "text": "إِنَّ اللَّهَ سَرِيعٌ حِسَابٌ"
  },
  {
    "id": 37,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 278.5,
    "end": 287.08,
    "text": "وَأَنْذِرْهُمْ يَوْمَ الْآزِفَةِ إِذِ الْقُلُوبُ لَدَى الْحَنَاجِرِ كَاظِمِينَ"
  },
  {
    "id": 38,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 287.77,
    "end": 294.66,
    "text": "مَا لِلظَّالِمِينَ مِنْ حَمِيمٍ وَلَا شَفِيعٍ يُطَاعُ"
  },
  {
    "id": 39,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 295.83,
    "end": 301.9,
    "text": "يَعْلَمُ خَائِنَةَ الْأَعْيُنِ وَمَا تُخْفِي الصُّدُورُ"
  },
  {
    "id": 40,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 302.76,
    "end": 305.04,
    "text": "وَاللَّهُ يَقْضِي بِالْحَقِّ"
  },
  {
    "id": 41,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 305.49,
    "end": 305.59,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 42,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 306.57,
    "end": 313.19,
    "text": "وَالَّذِينَ يَدْعُونَ مِنْ دُونِهِ لَا يَقْضُونَ بِشَيْءٍ"
  },
  {
    "id": 43,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 314.03,
    "end": 319.06,
    "text": "إِنَّ اللَّهَ هُوَ السَّمِيعُ الْبَصِيرُ"
  },
  {
    "id": 44,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 320.17,
    "end": 330.15,
    "text": "أَوَلَمْ يَسِيرُوا فِي الْأَرْضِ فَيَنْظُرُوا كَيْفَ كَانَ عَاقِبَةُ الَّذِينَ كَانُوا مِنْ قَبْلِهِمْ"
  },
  {
    "id": 45,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 331.16,
    "end": 341.45,
    "text": "وَكَانُوا هُمْ أَشَدَّ مِنْهُمْ قُوَّةً وَآثَارًا فِي الْأَرْضِ فَأَخَذَهُمُ اللَّهُ بِذُنُوبِهِمْ"
  },
  {
    "id": 46,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 342.46,
    "end": 351.55,
    "text": "فَأَخَذَهُمُ اللَّهُ بِذُنُوبِهِمْ وَمَا كَانَ لَهُمْ مِنَ اللَّهِ مِنْ وَاقٍ"
  },
  {
    "id": 47,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 352.79,
    "end": 364.07,
    "text": "ذَلِكَ بِأَنَّهُ كَانَتْ تَأْتِيهِمْ رُسُلُهُمْ بِالْبَيِّنَاتِ فَكَفَرُوا فَأَخَذَهُمُ اللَّهُ"
  },
  {
    "id": 48,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 365.06,
    "end": 371.39,
    "text": "إِنَّهُ قَوِيٌّ شَدِيدُ الْعِقَابِ"
  },
  {
    "id": 49,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 373.05,
    "end": 380.85,
    "text": "وَلَقَدْ أَرْسَلْنَا مُوسَى بِآيَاتِنَا وَسُلْطَانٍ مُبِينٍ"
  },
  {
    "id": 50,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 382.03,
    "end": 390.64,
    "text": "إِلَى فِرْعَوْنَ وَهَامَانَ وَقَارُونَ فَقَالُوا سَاحِرٌ كَذَّابٌ"
  },
  {
    "id": 51,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 392.03,
    "end": 408.47,
    "text": "فَلَمَّا جَاءَهُمْ بِالْحَقِّ مِنْ عِنْدِنَا قَالُوا اقْتُلُوا أَبْنَاءَ الَّذِينَ آمَنُوا مَعَهُ وَاسْتَحْيُوا نِسَاءَهُمْ"
  },
  {
    "id": 52,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 409.7,
    "end": 415.11,
    "text": "وَمَا كَيْدُ الْكَافِرِينَ إِلَّا فِي ضَلَالٍ"
  },
  {
    "id": 53,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 416.41,
    "end": 423.35,
    "text": "وَقَالَ فِرْعَوْنُ ذَرُونِي أَقْتُلْ مُوسَى وَلْيَدْعُ رَبَّهُ"
  },
  {
    "id": 54,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 424.47,
    "end": 435.43,
    "text": "إِنِّي أَخَافُ أَنْ يُبَدِّلَ دِينَكُمْ أَوْ أَنْ يُظْهِرَ فِي الْأَرْضِ الْفَسَادَ"
  },
  {
    "id": 55,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 436.82,
    "end": 450.5,
    "text": "وَقَالَ مُوسَى إِنِّي عُذْتُ بِرَبِّي وَرَبِّكُمْ مِنْ كُلِّ مُتَكَبِّرٍ لَا يُؤْمِنُ بِيَوْمِ الْحِسَابِ"
  },
  {
    "id": 56,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 451.85,
    "end": 466.37,
    "text": "وَقَالَ رَجُلٌ مُؤْمِنٌ مِنْ آلِ فِرْعَوْنَ يَكْتُمُ إِيمَانَهُ أَتَقْتُلُونَ رَجُلًا أَنْ يَقُولَ رَبِّيَ اللَّهُ"
  },
  {
    "id": 57,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 467.68,
    "end": 479.25,
    "text": "أَتَقْتُلُونَ رَجُلًا أَنْ يَقُولَ رَبِّيَ اللَّهُ وَقَدْ جَاءَكُمْ بِالْبَيِّنَاتِ مِنْ رَبِّكُمْ"
  },
  {
    "id": 58,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 480.35,
    "end": 484.84,
    "text": "وَإِنْ يَكُ كَاذِبًا فَعَلَيْهِ كَذِبُهُ"
  },
  {
    "id": 59,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 485.8,
    "end": 492.57,
    "text": "وَإِنْ يَكُ صَادِقًا يُصِبْكُمْ بَعْضُ الَّذِي يَعِدُكُمْ"
  },
  {
    "id": 60,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 493.46,
    "end": 500.7,
    "text": "إِنَّ اللَّهَ لَا يَهْدِي مَنْ هُوَ مُسْرِفٌ كَذَّابٌ"
  },
  {
    "id": 61,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 502.44,
    "end": 516.22,
    "text": "يَا قَوْمِ لَكُمُ الْمُلْكُ الْيَوْمَ ظَاهِرِينَ فِي الْأَرْضِ فَمَنْ يَنْصُرُنَا مِنْ بَأْسِ اللَّهِ إِنْ جَاءَنَا"
  },
  {
    "id": 62,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 517.67,
    "end": 530.19,
    "text": "قَالَ فِرْعَوْنُ مَا أُرِيكُمْ إِلَّا مَا أَرَى وَمَا أَهْدِيكُمْ إِلَّا سَبِيلَ الرَّشَادِ"
  },
  {
    "id": 63,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 531.59,
    "end": 542.15,
    "text": "وَقَالَ الَّذِي آمَنَ يَا قَوْمِ إِنِّي أَخَافُ عَلَيْكُمْ مِثْلَ يَوْمِ الْأَحْزَابِ"
  },
  {
    "id": 64,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 543.18,
    "end": 551.91,
    "text": "مِثْلَ دَأْبِ قَوْمِ نُوحٍ وَعَادٍ وَثَمُودَ وَالَّذِينَ مِنْ بَعْدِهِمْ"
  },
  {
    "id": 65,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 552.76,
    "end": 557.62,
    "text": "وَمَا اللَّهُ يُرِيدُ ظُلْمًا لِلْعِبَادِ"
  },
  {
    "id": 66,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 558.99,
    "end": 566.06,
    "text": "وَيَا قَوْمِ إِنِّي أَخَافُ عَلَيْكُمْ يَوْمَ التَّنَادِ"
  },
  {
    "id": 67,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 567.09,
    "end": 574.23,
    "text": "يَوْمَ تُوَلُّونَ مُدْبِرِينَ مَا لَكُمْ مِنَ اللَّهِ مِنْ عَاصِمٍ"
  },
  {
    "id": 68,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 575.09,
    "end": 580.86,
    "text": "وَمَنْ يُضْلِلِ اللَّهُ فَمَا لَهُ مِنْ هَادٍ"
  },
  {
    "id": 69,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 582.64,
    "end": 596.35,
    "text": "وَلَقَدْ جَاءَكُمْ يُوسُفُ مِنْ قَبْلُ بِالْبَيِّنَاتِ فَمَا زِلْتُمْ فِي شَكٍّ مِمَّا جَاءَكُمْ بِهِ"
  },
  {
    "id": 70,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 597.52,
    "end": 606.09,
    "text": "حَتَّى إِذَا هَلَكَ قُلْتُمْ لَنْ يَبْعَثَ اللَّهُ مِنْ بَعْدِهِ رَسُولًا"
  },
  {
    "id": 71,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 607.05,
    "end": 613.85,
    "text": "كَذَلِكَ يُضِلُّ اللَّهُ مَنْ هُوَ مُسْرِفٌ مُرْتَابٌ"
  },
  {
    "id": 72,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 615.2,
    "end": 623.34,
    "text": "الَّذِينَ يُجَادِلُونَ فِي آيَاتِ اللَّهِ بِغَيْرِ سُلْطَانٍ أَتَاهُمْ"
  },
  {
    "id": 73,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 624.22,
    "end": 630.55,
    "text": "كَبُرَ مَقْتًا عِنْدَ اللَّهِ وَعِنْدَ الَّذِينَ آمَنُوا"
  },
  {
    "id": 74,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 631.58,
    "end": 639.8,
    "text": "كَذَلِكَ يَطْبَعُ اللَّهُ عَلَى كُلِّ قَلْبِ مُتَكَبِّرٍ جَبَّارٍ"
  },
  {
    "id": 75,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 641.42,
    "end": 650.33,
    "text": "وَقَالَ فِرْعَوْنُ يَا هَامَانُ ابْنِ لِي صَرْحًا لَعَلِّي أَبْلُغُ الْأَسْبَابَ"
  },
  {
    "id": 76,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 651.48,
    "end": 661.92,
    "text": "أَسْبَابَ السَّمَاوَاتِ فَأَطَّلِعَ إِلَى إِلَهِ مُوسَى وَإِنِّي لَأَظُنُّهُ كَاذِبًا"
  },
  {
    "id": 77,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 663.03,
    "end": 671.32,
    "text": "وَكَذَلِكَ زُيِّنَ لِفِرْعَوْنَ سُوءُ عَمَلِهِ وَصُدَّ عَنِ السَّبِيلِ"
  },
  {
    "id": 78,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 672.1,
    "end": 677.12,
    "text": "وَمَا كَيْدُ فِرْعَوْنَ إِلَّا فِي تَبَابٍ"
  },
  {
    "id": 79,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 678.59,
    "end": 687.42,
    "text": "وَقَالَ الَّذِي آمَنَ يَا قَوْمِ اتَّبِعُونِ أَهْدِكُمْ سَبِيلَ الرَّشَادِ"
  },
  {
    "id": 80,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 688.41,
    "end": 700.49,
    "text": "يَا قَوْمِ إِنَّمَا هَذِهِ الْحَيَاةُ الدُّنْيَا مَتَاعٌ وَإِنَّ الْآخِرَةَ هِيَ دَارُ الْقَرَارِ"
  },
  {
    "id": 81,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 701.76,
    "end": 708.55,
    "text": "مَنْ عَمِلَ سَيِّئَةً فَلَا يُجْزَى إِلَّا مِثْلَهَا"
  },
  {
    "id": 82,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 709.77,
    "end": 722.65,
    "text": "وَمَنْ عَمِلَ صَالِحًا مِنْ ذَكَرٍ أَوْ أُنْثَى وَهُوَ مُؤْمِنٌ فَأُولَئِكَ يَدْخُلُونَ الْجَنَّةَ"
  },
  {
    "id": 83,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 723.71,
    "end": 733.74,
    "text": "أُولَئِكَ يَدْخُلُونَ الْجَنَّةَ يُرْزَقُونَ فِيهَا بِغَيْرِ حِسَابٍ"
  },
  {
    "id": 84,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 735.19,
    "end": 745.48,
    "text": "وَيَا قَوْمِ مَا لِي أَدْعُوكُمْ إِلَى النَّجَاةِ وَتَدْعُونَنِي إِلَى النَّارِ"
  },
  {
    "id": 85,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 746.58,
    "end": 759.83,
    "text": "تَدْعُونَنِي لِأَكْفُرَ بِاللَّهِ وَأُشْرِكَ بِهِ مَا لَيْسَ لِي بِهِ عِلْمٌ وَأَنَا أَدْعُوكُمْ إِلَى الْعَزِيزِ الْغَفَّارِ"
  },
  {
    "id": 86,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 760.89,
    "end": 777.12,
    "text": "لَا جَرَمَ أَنَّمَا تَدْعُونَنِي إِلَيْهِ لَيْسَ لَهُ دَعْوَةٌ فِي الدُّنْيَا وَلَا فِي الْآخِرَةِ وَأَنَّ مَرَدَّنَا إِلَى اللَّهِ"
  },
  {
    "id": 87,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 778.43,
    "end": 789.12,
    "text": "وَأَنَّ مَرَدَّنَا إِلَى اللَّهِ وَأَنَّ الْمُسْرِفِينَ هُمْ أَصْحَابُ النَّارِ"
  },
  {
    "id": 88,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 790.2,
    "end": 794.52,
    "text": "فَسَتَذْكُرُونَ مَا أَقُولُ لَكُمْ"
  },
  {
    "id": 89,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 795.2,
    "end": 799.85,
    "text": "وَأُفَوِّضُ أَمْرِي إِلَى اللَّهِ"
  },
  {
    "id": 90,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 800.63,
    "end": 806.11,
    "text": "إِنَّ اللَّهَ بَصِيرٌ بِالْعِبَادِ"
  },
  {
    "id": 91,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 807.13,
    "end": 811.57,
    "text": "فَوَقَاهُ اللَّهُ سَيِّئَاتِ مَا مَكَرُوا"
  },
  {
    "id": 92,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 812.14,
    "end": 817.78,
    "text": "وَحَاقَ بِآلِ فِرْعَوْنَ سُوءُ الْعَذَابِ"
  },
  {
    "id": 93,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 818.82,
    "end": 824.96,
    "text": "النَّارُ يُعْرَضُونَ عَلَيْهَا غُدُوًّا وَعَشِيًّا"
  },
  {
    "id": 94,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 825.86,
    "end": 834.32,
    "text": "وَيَوْمَ تَقُومُ السَّاعَةُ أَدْخِلُوا آلَ فِرْعَوْنَ أَشَدَّ الْعَذَابِ"
  },
  {
    "id": 95,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 835.85,
    "end": 850.43,
    "text": "وَإِذْ يَتَحَاجُّونَ فِي النَّارِ فَيَقُولُ الضُّعَفَاءُ لِلَّذِينَ اسْتَكْبَرُوا إِنَّا كُنَّا لَكُمْ تَبَعًا"
  },
  {
    "id": 96,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 851.93,
    "end": 871.31,
    "text": "سَيَقُولُ الضُّعَفَاءُ لِلَّذِينَ اسْتَكْبَرُوا إِنَّا كُنَّا لَكُمْ تَبَعًا فَهَلْ أَنْتُمْ مُغْنُونَ عَنَّا نَصِيبًا مِنَ النَّارِ"
  },
  {
    "id": 97,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 872.82,
    "end": 885.18,
    "text": "قَالَ الَّذِينَ اسْتَكْبَرُوا إِنَّا كُلٌّ فِيهَا إِنَّ اللَّهَ قَدْ حَكَمَ بَيْنَ الْعِبَادِ"
  },
  {
    "id": 98,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 886.43,
    "end": 898.67,
    "text": "وَقَالَ الَّذِينَ فِي النَّارِ لِخَزَنَةِ جَهَنَّمَ ادْعُوا رَبَّكُمْ يُخَفِّفْ عَنَّا يَوْمًا مِنَ الْعَذَابِ"
  },
  {
    "id": 99,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 900.07,
    "end": 907.45,
    "text": "قَالُوا أَوَلَمْ تَكُ تَأْتِيكُمْ رُسُلُكُمْ بِالْبَيِّنَاتِ"
  },
  {
    "id": 100,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 908.33,
    "end": 909.79,
    "text": "قَالُوا بَلَى"
  },
  {
    "id": 101,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 910.64,
    "end": 912.32,
    "text": "قَالُوا فَادْعُوا"
  },
  {
    "id": 102,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 913.07,
    "end": 919.17,
    "text": "وَمَا دُعَاءُ الْكَافِرِينَ إِلَّا فِي ضَلَالٍ"
  },
  {
    "id": 103,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 920.4,
    "end": 931.45,
    "text": "إِنَّا لَنَنْصُرُ رُسُلَنَا وَالَّذِينَ آمَنُوا فِي الْحَيَاةِ الدُّنْيَا وَيَوْمَ يَقُومُ الْأَشْهَادُ"
  },
  {
    "id": 104,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 932.71,
    "end": 937.65,
    "text": "يَوْمَ لَا يَنْفَعُ الظَّالِمِينَ مَعْذِرَتُهُمْ"
  },
  {
    "id": 105,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 938.34,
    "end": 944.04,
    "text": "وَلَهُمُ اللِّعْنَةُ وَلَهُمْ سُوءُ الدَّارِ"
  },
  {
    "id": 106,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 945.23,
    "end": 955.67,
    "text": "وَلَقَدْ آتَيْنَا مُوسَى الْهُدَى وَأَوْرَثْنَا بَنِي إِسْرَائِيلَ الْكِتَابَ"
  },
  {
    "id": 107,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 956.59,
    "end": 961.6,
    "text": "هُدًى وَذِكْرَى لِأُولِي الْأَلْبَابِ"
  },
  {
    "id": 108,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 963.13,
    "end": 975.78,
    "text": "فَاصْبِرْ إِنَّ وَعْدَ اللَّهِ حَقٌّ وَاسْتَغْفِرْ لِذَنْبِكَ وَسَبِّحْ بِحَمْدِ رَبِّكَ بِالْعَشِيِّ وَالْإِبْكَارِ"
  },
  {
    "id": 109,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 977.24,
    "end": 994.23,
    "text": "إِنَّ الَّذِينَ يُجَادِلُونَ فِي آيَاتِ اللَّهِ بِغَيْرِ سُلْطَانٍ أَتَاهُمْ إِنْ فِي صُدُورِهِمْ إِلَّا كِبْرٌ مَا هُمْ بِبَالِغِيهِ"
  },
  {
    "id": 110,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 995.36,
    "end": 997.81,
    "text": "فَاسْتَعِذْ بِاللَّهِ"
  },
  {
    "id": 111,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 998.51,
    "end": 1003.12,
    "text": "إِنَّهُ هُوَ السَّمِيعُ الْبَصِيرُ"
  },
  {
    "id": 112,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1004.32,
    "end": 1016.08,
    "text": "لَخَلْقُ السَّمَاوَاتِ وَالْأَرْضِ أَكْبَرُ مِنْ خَلْقِ النَّاسِ وَلَكِنَّ أَكْثَرَ النَّاسِ لَا يَعْلَمُونَ"
  },
  {
    "id": 113,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1016.91,
    "end": 1026.96,
    "text": "وَمَا يَسْتَوِي الْأَعْمَى وَالْبَصِيرُ وَالَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ وَلَا الْمُسِيءُ"
  },
  {
    "id": 114,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1027.86,
    "end": 1032.01,
    "text": "وَلِيلًا مَا تَتَذَكَّرُونَ"
  },
  {
    "id": 115,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1033.14,
    "end": 1043.56,
    "text": "إِنَّ السَّاعَةَ لَآتِيَةٌ لَا رَيْبَ فِيهَا وَلَكِنَّ أَكْثَرَ النَّاسِ لَا يُؤْمِنُونَ"
  },
  {
    "id": 116,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1044.93,
    "end": 1049.8,
    "text": "وَقَالَ رَبُّكُمُ ادْعُونِي أَسْتَجِبْ لَكُمْ"
  },
  {
    "id": 117,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1050.82,
    "end": 1060.55,
    "text": "إِنَّ الَّذِينَ يَسْتَكْبِرُونَ عَنْ عِبَادَتِي سَيَدْخُلُونَ جَهَنَّمَ دَاخِرِينَ"
  },
  {
    "id": 118,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1061.89,
    "end": 1069.87,
    "text": "اللَّهُ الَّذِي جَعَلَ لَكُمُ اللَّيْلَ لِتَسْكُنُوا فِيهِ وَالنَّهَارَ مُبْصِرًا"
  },
  {
    "id": 119,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1071.08,
    "end": 1081.38,
    "text": "إِنَّ اللَّهَ لَذُو فَضْلٍ عَلَى النَّاسِ وَلَكِنَّ أَكْثَرَ النَّاسِ لَا يَشْكُرُونَ"
  },
  {
    "id": 120,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1082.77,
    "end": 1091.58,
    "text": "ذَلِكُمُ اللَّهُ رَبُّكُمْ خَالِقُ كُلِّ شَيْءٍ لَا إِلَهَ إِلَّا هُوَ"
  },
  {
    "id": 121,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1092.51,
    "end": 1095.83,
    "text": "فَأَنَّى تُؤْفَكُونَ"
  },
  {
    "id": 122,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1096.93,
    "end": 1104.13,
    "text": "كَذَلِكَ يُؤْفَكُ الَّذِينَ كَانُوا بِآيَاتِ اللَّهِ يَجْحَدُونَ"
  },
  {
    "id": 123,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1105.5,
    "end": 1121.95,
    "text": "اللَّهُ الَّذِي جَعَلَ لَكُمُ الْأَرْضَ قَرَارًا وَالسَّمَاءَ بِنَاءً وَصَوَّرَكُمْ فَأَحْسَنَ صُوَرَكُمْ وَرَزَقَكُمْ مِنَ الطَّيِّبَاتِ"
  },
  {
    "id": 124,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1123.22,
    "end": 1125.95,
    "text": "ذَلِكُمُ اللَّهُ رَبُّكُمْ"
  },
  {
    "id": 125,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1126.8,
    "end": 1131.53,
    "text": "فَتَبَارَكَ اللَّهُ رَبُّ الْعَالَمِينَ"
  },
  {
    "id": 126,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1132.75,
    "end": 1141.59,
    "text": "وَالْحَيُّ لَا إِلَهَ إِلَّا هُوَ فَادْعُوهُ مُخْلِصِينَ لَهُ الدِّينَ"
  },
  {
    "id": 127,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1142.49,
    "end": 1146.87,
    "text": "وَالْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ"
  },
  {
    "id": 128,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1148.12,
    "end": 1161.29,
    "text": "قُلْ إِنِّي نُهِيتُ أَنْ أَعْبُدَ الَّذِينَ تَدْعُونَ مِنْ دُونِ اللَّهِ لَمَّا جَاءَنِيَ الْبَيِّنَاتُ مِنْ رَبِّي"
  },
  {
    "id": 129,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1162.43,
    "end": 1172.98,
    "text": "فَلَمَّا جَاءَنِيَ الْبَيِّنَاتُ مِنْ رَبِّي وَأُمِرْتُ أَنْ أُسْلِمَ لِرَبِّ الْعَالَمِينَ"
  },
  {
    "id": 130,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1174.68,
    "end": 1185.5,
    "text": "هُوَ الَّذِي خَلَقَكُمْ مِنْ تُرَابٍ ثُمَّ مِنْ نُطْفَةٍ ثُمَّ مِنْ عَلَقَةٍ"
  },
  {
    "id": 131,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1186.82,
    "end": 1200.28,
    "text": "ثُمَّ مِنْ عَلَقَةٍ ثُمَّ يُخْرِجُكُمْ طِفْلًا ثُمَّ لِتَبْلُغُوا أَشُدَّكُمْ ثُمَّ لِتَكُونُوا شُيُوخًا"
  },
  {
    "id": 132,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1201.21,
    "end": 1206.05,
    "text": "وَمِنْكُمْ مَنْ يُتَوَفَّى مِنْ قَبْلِ"
  },
  {
    "id": 133,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1206.95,
    "end": 1215.22,
    "text": "وَلِتَبْلُو أَجَلًا مُسَمًّى وَلَعَلَّكُمْ تَعْقِلُونَ"
  },
  {
    "id": 134,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1216.51,
    "end": 1219.96,
    "text": "هُوَ الَّذِي يُحْيِي وَيُمِيتُ"
  },
  {
    "id": 135,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1220.9,
    "end": 1230.09,
    "text": "فَإِذَا قَضَى أَمْرًا فَإِنَّمَا يَقُولُ لَهُ كُنْ فَيَكُونُ"
  },
  {
    "id": 136,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1231.16,
    "end": 1241.12,
    "text": "أَلَمْ تَرَ إِلَى الَّذِينَ يُجَادِلُونَ فِي آيَاتِ اللَّهِ أَنَّى يُصْرَفُونَ"
  },
  {
    "id": 137,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1242.29,
    "end": 1249.67,
    "text": "الَّذِينَ كَذَّبُوا بِالْكِتَابِ وَبِمَا أَرْسَلْنَا بِهِ رُسُلَنَا"
  },
  {
    "id": 138,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1250.5,
    "end": 1253.06,
    "text": "فَسَوْفَ يَعْلَمُونَ"
  },
  {
    "id": 139,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1254.11,
    "end": 1261.31,
    "text": "إِذِ الْأَغْلَالُ فِي أَعْنَاقِهِمْ وَالسَّلَاسِلُ يُسْحَبُونَ"
  },
  {
    "id": 140,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1262.2,
    "end": 1267.64,
    "text": "فِي الْحَمِيمِ ثُمَّ فِي النَّارِ يُسْجَرُونَ"
  },
  {
    "id": 141,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1268.92,
    "end": 1275.51,
    "text": "ثُمَّ قِيلَ لَهُمْ أَيْنَ مَا كُنْتُمْ تُشْرِكُونَ"
  },
  {
    "id": 142,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1276.33,
    "end": 1279.12,
    "text": "مِنْ دُونِ اللَّهِ"
  },
  {
    "id": 143,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1280.11,
    "end": 1287.72,
    "text": "قَالُوا ضَلُّوا عَنَّا بَلْ لَمْ نَكُنْ نَدْعُو مِنْ قَبْلُ شَيْئًا"
  },
  {
    "id": 144,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1288.67,
    "end": 1293.14,
    "text": "كَذَلِكَ يُضِلُّ اللَّهُ الْكَافِرِينَ"
  },
  {
    "id": 145,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1294.56,
    "end": 1305.61,
    "text": "ذَلِكُمْ بِمَا كُنْتُمْ تَفْرَحُونَ فِي الْأَرْضِ بِغَيْرِ الْحَقِّ وَبِمَا كُنْتُمْ تَمْرَحُونَ"
  },
  {
    "id": 146,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1306.75,
    "end": 1312.68,
    "text": "ادْخُلُوا أَبْوَابَ جَهَنَّمَ خَالِدِينَ فِيهَا"
  },
  {
    "id": 147,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1313.55,
    "end": 1317.34,
    "text": "فَبِئْسَ مَثْوَى الْمُتَكَبِّرِينَ"
  },
  {
    "id": 148,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1318.56,
    "end": 1321.7,
    "text": "فَاصْبِرْ إِنَّ وَعْدَ اللَّهِ"
  },
  {
    "id": 149,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1322.11,
    "end": 1322.23,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 150,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1323.29,
    "end": 1333.87,
    "text": "فَإِمَّا نُرِيَنَّكَ بَعْضَ الَّذِي نَعِدُهُمْ أَوْ نَتَوَفَّيَنَّكَ فَإِلَيْنَا يُرْجَعُونَ"
  },
  {
    "id": 151,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1335.01,
    "end": 1347.13,
    "text": "وَلَقَدْ أَرْسَلْنَا رُسُلًا مِنْ قَبْلِكَ مِنْهُمْ مَنْ قَصَصْنَا عَلَيْكَ وَمِنْهُمْ مَنْ لَمْ نَقْصُصْ عَلَيْكَ"
  },
  {
    "id": 152,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1348.32,
    "end": 1356.03,
    "text": "وَمَا كَانَ لِرَسُولٍ أَنْ يَأْتِيَ بِآيَةٍ إِلَّا بِإِذْنِ اللَّهِ"
  },
  {
    "id": 153,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1357.06,
    "end": 1365.8,
    "text": "فَإِذَا جَاءَ أَمْرُ اللَّهِ قُضِيَ بِالْحَقِّ وَخَسِرَ هُنَالِكَ الْمُبْطِلُونَ"
  },
  {
    "id": 154,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1367.15,
    "end": 1375.83,
    "text": "اللَّهُ الَّذِي جَعَلَ لَكُمُ الْأَنْعَامَ لِتَرْكَبُوا مِنْهَا وَمِنْهَا تَأْكُلُونَ"
  },
  {
    "id": 155,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1376.98,
    "end": 1389.34,
    "text": "وَلَكُمْ فِيهَا مَنَافِعُ وَلِتَبْلُغُوا عَلَيْهَا حَاجَةً فِي صُدُورِكُمْ وَعَلَيْهَا وَعَلَى الْفُلْكِ تُحْمَلُونَ"
  },
  {
    "id": 156,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1390.52,
    "end": 1398.2,
    "text": "وَيُرِيكُمْ آيَاتِهِ فَأَيَّ آيَاتِ اللَّهِ تُنْكِرُونَ"
  },
  {
    "id": 157,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1399.34,
    "end": 1408.48,
    "text": "أَفَلَمْ يَسِيرُوا فِي الْأَرْضِ فَيَنْظُرُوا كَيْفَ كَانَ عَاقِبَةُ الَّذِينَ مِنْ قَبْلِهِمْ"
  },
  {
    "id": 158,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1409.6,
    "end": 1424.43,
    "text": "يَا نُوءَ كْثَرَ مِنْهُمْ وَأَشَدَّ قُوَّةً وَآثَارًا فِي الْأَرْضِ فَمَا أَغْنَى عَنْهُمْ مَا كَانُوا يَكْسِبُونَ"
  },
  {
    "id": 159,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1425.83,
    "end": 1442.31,
    "text": "فَلَمَّا جَاءَتْهُمْ رُسُلُهُمْ بِالْبَيِّنَاتِ فَرِحُوا بِمَا عِنْدَهُمْ مِنَ الْعِلْمِ وَحَاقَ بِهِمْ مَا كَانُوا بِهِ يَسْتَهْزِئُونَ"
  },
  {
    "id": 160,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1443.51,
    "end": 1456.53,
    "text": "فَلَمَّا رَأَوْا بَأْسَنَا قَالُوا آمَنَّا بِاللَّهِ وَحْدَهُ وَكَفَرْنَا بِمَا كُنَّا بِهِ مُشْرِكِينَ"
  },
  {
    "id": 161,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1457.65,
    "end": 1464.21,
    "text": "فَلَمْ يَكُ يَنْفَعُهُمْ إِيمَانُهُمْ لَمَّا رَأَوْا بَأْسَنَا"
  },
  {
    "id": 162,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1465.15,
    "end": 1470.33,
    "text": "جُنَّةَ اللَّهِ الَّتِي قَدْ خَلَتْ فِي عِبَادِهِ"
  },
  {
    "id": 163,
    "sura_id": 40,
    "recitation_uuid": "b0f2fec0-316b-4d5a-88ca-6947e836bad1",
    "start": 1471.35,
    "end": 1475.73,
    "text": "وَخَسِرَ هُنَالِكَ الْكَافِرُونَ"
  }
]