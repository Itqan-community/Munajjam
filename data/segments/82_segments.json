[
  {
    "id": 2,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 5.74,
    "end": 9.73,
    "text": "إِذَا السَّمَاءُ انْفَطَرَتْ"
  },
  {
    "id": 3,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 10.8,
    "end": 14.33,
    "text": "وَإِذَا الْكَوَاكِبُ انْتَثَرَتْ"
  },
  {
    "id": 4,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 15.27,
    "end": 17.7,
    "text": "وَإِذَا الْبِحَارُ فُجِّرَتْ"
  },
  {
    "id": 5,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 18.74,
    "end": 21.23,
    "text": "وَإِذَا الْقُبُورُ بُعْثِرَتْ"
  },
  {
    "id": 6,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 22.12,
    "end": 26.7,
    "text": "عَلِمَتْ نَفْسٌ مَا قَدَّمَتْ وَأَخَّرَ"
  },
  {
    "id": 7,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 27.86,
    "end": 35.85,
    "text": "يَا أَيُّهَا الْإِنْسَانُ مَا غَرَّكَ بِرَبِّكَ الْكَرِيمِ"
  },
  {
    "id": 8,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 36.91,
    "end": 40.79,
    "text": "الَّذِي خَلَقَكَ فَسَوَّاكَ فَعَدَلَ"
  },
  {
    "id": 9,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 41.76,
    "end": 47.58,
    "text": "فِي أَيِّ صُورَةٍ مَا شَاءَ رَكَّبَةٍ"
  },
  {
    "id": 10,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 48.62,
    "end": 52.79,
    "text": "كَلَّا بَلْ تُكَذِّبُونَ بِالدِّينِ"
  },
  {
    "id": 11,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 53.59,
    "end": 57.76,
    "text": "وَإِنَّ عَلَيْكُمْ لَحَافِظِينَ"
  },
  {
    "id": 12,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 58.55,
    "end": 61.72,
    "text": "كِرَامًا كَاتِبِينَ"
  },
  {
    "id": 13,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 62.56,
    "end": 66.05,
    "text": "يَعْلَمُونَ مَا تَفْعَلُونَ"
  },
  {
    "id": 14,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 67.11,
    "end": 71.24,
    "text": "إِنَّ الْأَبْرَارَ لَفِي نَعِيمٍ"
  },
  {
    "id": 15,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 72.26,
    "end": 76.67,
    "text": "وَإِنَّ الْفُجَّارَ لَفِي جَحِيمٍ"
  },
  {
    "id": 16,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 77.44,
    "end": 80.78,
    "text": "يَصْلَوْنَهَا يَوْمَ الدِّينِ"
  },
  {
    "id": 17,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 81.63,
    "end": 86.65,
    "text": "وَمَا هُمْ عَنْهَا بِغَائِبِينَ"
  },
  {
    "id": 18,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 87.5,
    "end": 91.98,
    "text": "وَمَا أَدْرَاكَ مَا يَوْمُ الدِّينِ"
  },
  {
    "id": 19,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 92.91,
    "end": 98.32,
    "text": "ثُمَّ مَا أَدْرَاكَ مَا يَوْمُ الدِّينِ"
  },
  {
    "id": 20,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 99.12,
    "end": 104.14,
    "text": "يَوْمَ لَا تَمْلِكُ نَفْسٌ لِنَفْسٍ شَيْئًا"
  },
  {
    "id": 21,
    "sura_id": 82,
    "recitation_uuid": "b7b10321-e6f0-406c-8c9c-5dbe4f176c9d",
    "start": 105.09,
    "end": 108.92,
    "text": "وَالْأَمْرُ يَوْمَئِذٍ لِلَّهِ"
  }
]