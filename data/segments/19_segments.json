[
  {
    "id": 2,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 6.13,
    "end": 16.06,
    "text": "يَا أَفْهَايَ عِيٌّ صَادٌ"
  },
  {
    "id": 3,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 17.14,
    "end": 21.54,
    "text": "ذِكْرُ رَحْمَتِ رَبِّكَ عَبْدَهُ زَكَرِيَّا"
  },
  {
    "id": 4,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 22.63,
    "end": 28.4,
    "text": "إِذْ نَادَى رَبَّهُ نِدَاءً خَفِيًّا"
  },
  {
    "id": 5,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 29.87,
    "end": 44.42,
    "text": "قَالَ رَبِّ إِنِّي وَهَنَ الْعَظْمُ مِنِّي وَاشْتَعَلَ الرَّأْسُ شَيْبًا وَلَمْ أَكُنْ بِدُعَائِكَ رَبِّ شَقِيًّا"
  },
  {
    "id": 6,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 45.63,
    "end": 59.38,
    "text": "وَإِنِّي خِفْتُ الْمَوَالِيَ مِنْ وَرَائِي وَكَانَتِ امْرَأَتِي عَاقِرًا فَهَبْ لِي مِنْ لَدُنْكَ وَلِيًّا"
  },
  {
    "id": 7,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 60.79,
    "end": 65.74,
    "text": "يَرِثُنِي وَيَرِثُ مِنْ آلِ يَعْقُوبَ"
  },
  {
    "id": 8,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 66.58,
    "end": 69.43,
    "text": "وَاجْعَلْهُ رَبِّ رَضِيًّا"
  },
  {
    "id": 9,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 70.64,
    "end": 82.88,
    "text": "يَا زَكَرِيَّا إِنَّا نُبَشِّرُكَ بِغُلَامٍ اسْمُهُ يَحْيَى لَمْ نَجْعَلْ لَهُ مِنْ قَبْلُ سَمِيًّا"
  },
  {
    "id": 10,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 84.2,
    "end": 96.37,
    "text": "قَالَ رَبِّ أَنَّى يَكُونُ لِي غُلَامٌ وَكَانَتِ امْرَأَتِي عَاقِرًا وَقَدْ بَلَغْتُ مِنَ الْكِبَرِ عِتِيًّا"
  },
  {
    "id": 11,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 97.69,
    "end": 109.0,
    "text": "قَالَ كَذَلِكَ قَالَ رَبُّكَ هُوَ عَلَيَّ هَيِّنٌ وَقَدْ خَلَقْتُكَ مِنْ قَبْلُ وَلَمْ تَكُ شَيْئًا"
  },
  {
    "id": 12,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 110.62,
    "end": 114.13,
    "text": "قَالَ رَبِّ اجْعَلْ لِي آيَةً"
  },
  {
    "id": 13,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 115.11,
    "end": 122.94,
    "text": "قَالَ آيَتُكَ أَلَّا تُكَلِّمَ النَّاسَ ثَلَاثَ لَيَالٍ سَوِيًّا"
  },
  {
    "id": 14,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 124.07,
    "end": 135.83,
    "text": "فَخَرَجَ عَلَى قَوْمِهِ مِنَ الْمِحْرَابِ فَأَوْحَى إِلَيْهِمْ أَنْ سَبِّحُوا بُكْرَةً وَعَشِيًّا"
  },
  {
    "id": 15,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 136.92,
    "end": 140.52,
    "text": "يَا يَحْيَى خُذِ الْكِتَابَ بِقُوَّةٍ"
  },
  {
    "id": 16,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 141.58,
    "end": 145.15,
    "text": "وَآتَيْنَاهُ الْحُكْمَ صَبِيًّا"
  },
  {
    "id": 17,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 146.14,
    "end": 151.34,
    "text": "وَحَنَانًا مِنْ لَدُنَّا وَزَكَاةً"
  },
  {
    "id": 18,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 152.18,
    "end": 154.09,
    "text": "وَكَانَ تَقِيًّا"
  },
  {
    "id": 19,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 155.14,
    "end": 161.97,
    "text": "وَبَرًّا بِوَالِدَيْهِ وَلَمْ يَكُنْ جَبَّارًا عَصِيًّا"
  },
  {
    "id": 20,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 163.25,
    "end": 170.91,
    "text": "وَسَلَامٌ عَلَيْهِ يَوْمَ وُلِدَ وَيَوْمَ يَمُوتُ وَيَوْمَ يُبْعَثُ حَيًّا"
  },
  {
    "id": 21,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 171.99,
    "end": 180.55,
    "text": "وَاذْكُرْ فِي الْكِتَابِ مَرْيَمَ إِذِ انْتَبَذَتْ مِنْ أَهْلِهَا مَكَانًا شَرْقِيًّا"
  },
  {
    "id": 22,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 181.65,
    "end": 194.51,
    "text": "فَاتَّخَذَتْ مِنْ دُونِهِمْ حِجَابًا فَأَرْسَلْنَا إِلَيْهَا رُوحَنَا فَتَمَثَّلَ لَهَا بَشَرًا سَوِيًّا"
  },
  {
    "id": 23,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 196.07,
    "end": 205.37,
    "text": "قَالَتْ إِنِّي أَعُوذُ بِالرَّحْمَنِ مِنْكَ إِنْ كُنْتَ تَقِيًّا"
  },
  {
    "id": 24,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 206.46,
    "end": 214.9,
    "text": "قَالَ إِنَّمَا أَنَا رَسُولُ رَبِّكِ لِأَهَبَ لَكِ غُلَامًا زَكِيًّا"
  },
  {
    "id": 25,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 216.07,
    "end": 225.78,
    "text": "قَالَتْ أَنَّى يَكُونُ لِي غُلَامٌ وَلَمْ يَمْسَسْنِي بَشَرٌ وَلَمْ أَكُ بَغِيًّا"
  },
  {
    "id": 26,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 226.64,
    "end": 231.75,
    "text": "قَالَ كَذَلِكِ قَالَ رَبُّكِ هُوَ عَلَيَّ هَيِّنٌ"
  },
  {
    "id": 27,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 232.69,
    "end": 240.24,
    "text": "وَلِنَجْعَلَهُ آيَةً لِلنَّاسِ وَرَحْمَةً مِنَّا"
  },
  {
    "id": 28,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 241.26,
    "end": 244.71,
    "text": "وَكَانَ أَمْرًا مَقْضِيًّا"
  },
  {
    "id": 29,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 245.87,
    "end": 251.96,
    "text": "فَحَمَلَتْهُ فَانْتَبَذَتْ بِهِ مَكَانًا قَصِيًّا"
  },
  {
    "id": 30,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 253.28,
    "end": 268.45,
    "text": "فَأَجَاءَهَا الْمَخَاضُ إِلَى جِذْعِ النَّخْلَةِ قَالَتْ يَا لَيْتَنِي مِتُّ قَبْلَ هَذَا وَكُنْتُ نَسْيًا مَنْسِيًّا"
  },
  {
    "id": 31,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 269.88,
    "end": 277.98,
    "text": "فَنَادَاهَا مِنْ تَحْتِهَا أَلَّا تَحْزَنِي قَدْ جَعَلَ رَبُّكِ"
  },
  {
    "id": 32,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 278.3,
    "end": 279.75,
    "text": "كِسَرِيًّا"
  },
  {
    "id": 33,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 281.21,
    "end": 290.49,
    "text": "وَهُزِّي إِلَيْكِ بِجِذْعِ النَّخْلَةِ تُسَاقِطْ عَلَيْكِ رُطَبًا جَنِيًّا"
  },
  {
    "id": 34,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 291.7,
    "end": 295.39,
    "text": "فَكُلِي وَاشْرَبِي وَقَرِّي عَيْنًا"
  },
  {
    "id": 35,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 296.34,
    "end": 312.11,
    "text": "فَإِمَّا تَرَيِنَّ مِنَ الْبَشَرِ أَحَدًا فَقُولِي إِنِّي نَذَرْتُ لِلرَّحْمَنِ صَوْمًا فَلَنْ أُكَلِّمَ الْيَوْمَ إِنْسِيًّا"
  },
  {
    "id": 36,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 313.62,
    "end": 316.83,
    "text": "فَأَتَتْ بِهِ قَوْمَهَا تَحْمِلُهُ"
  },
  {
    "id": 37,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 317.68,
    "end": 323.13,
    "text": "قَالُوا يَا مَرْيَمُ لَقَدْ جِئْتِ شَيْئًا فَرِيًّا"
  },
  {
    "id": 38,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 324.28,
    "end": 334.46,
    "text": "يَا أُخْتَ هَارُونَ مَا كَانَ أَبُوكِ امْرَأَ سَوْءٍ وَمَا كَانَتْ أُمُّكِ بَغِيًّا"
  },
  {
    "id": 39,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 335.47,
    "end": 338.03,
    "text": "فَأَشَارَتْ إِلَيْهِ"
  },
  {
    "id": 40,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 338.9,
    "end": 345.05,
    "text": "قَالُوا كَيْفَ نُكَلِّمُ مَنْ كَانَ فِي الْمَهْدِ صَبِيًّا"
  },
  {
    "id": 41,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 346.15,
    "end": 354.04,
    "text": "قَالَ إِنِّي عَبْدُ اللَّهِ آتَانِيَ الْكِتَابَ وَجَعَلَنِي نَبِيًّا"
  },
  {
    "id": 42,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 355.15,
    "end": 366.28,
    "text": "وَجَعَلَنِي مُبَارَكًا أَيْنَ مَا كُنْتُ وَأَوْصَانِي بِالصَّلَاةِ وَالزَّكَاةِ مَا دُمْتُ حِيًّا"
  },
  {
    "id": 43,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 367.47,
    "end": 375.39,
    "text": "وَبَرًّا بِوَالِدَتِي وَلَمْ يَجْعَلْنِي جَبَّارًا شَقِيًّا"
  },
  {
    "id": 44,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 376.31,
    "end": 379.25,
    "text": "وَالسَّلَامُ عَلَيَّ يَوْمَ"
  },
  {
    "id": 45,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 379.56,
    "end": 384.18,
    "text": "وَيَوْمَ أَمُوتُ وَيَوْمَ أُبْعَثُ حِيًّا"
  },
  {
    "id": 46,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 385.59,
    "end": 388.24,
    "text": "ذَلِكَ عِيسَى ابْنُ مَرْيَمَ"
  },
  {
    "id": 47,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 389.05,
    "end": 393.59,
    "text": "قَوْلَ الْحَقِّ الَّذِي فِيهِ يَمْتَرُونَ"
  },
  {
    "id": 48,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 394.55,
    "end": 399.83,
    "text": "وَمَا كَانَ لِلَّهِ أَنْ يَتَّخِذَ مِنْ وَلَدٍ"
  },
  {
    "id": 49,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 400.71,
    "end": 401.72,
    "text": "سُبْحَانَهُ"
  },
  {
    "id": 50,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 402.56,
    "end": 411.92,
    "text": "إِذَا قَضَى أَمْرًا فَإِنَّمَا يَقُولُ لَهُ كُنْ فَيَكُونُ"
  },
  {
    "id": 51,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 412.86,
    "end": 418.57,
    "text": "وَإِنَّ اللَّهَ رَبِّي وَرَبُّكُمْ فَاعْبُدُوهُ"
  },
  {
    "id": 52,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 419.37,
    "end": 423.8,
    "text": "هَذَا صِرَاطٌ مُسْتَقِيمٌ"
  },
  {
    "id": 53,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 424.73,
    "end": 428.59,
    "text": "فَاخْتَلَفَ الْأَحْزَابُ مِنْ بَيْنِهِمْ"
  },
  {
    "id": 54,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 429.48,
    "end": 436.27,
    "text": "فَوَيْلٌ لِلَّذِينَ كَفَرُوا مِنْ مَشْهَدِ يَوْمٍ عَظِيمٍ"
  },
  {
    "id": 55,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 437.41,
    "end": 448.75,
    "text": "أَسْمِعْ بِهِمْ وَأَبْصِرْ يَوْمَ يَأْتُونَنَا لَكِنِ الظَّالِمُونَ الْيَوْمَ فِي ضَلَالٍ مُبِينٍ"
  },
  {
    "id": 56,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 450.43,
    "end": 460.9,
    "text": "وَأَنْذِرْهُمْ يَوْمَ الْحَسْرَةِ إِذْ قُضِيَ الْأَمْرُ وَهُمْ فِي غَفْلَةٍ وَهُمْ لَا يُؤْمِنُونَ"
  },
  {
    "id": 57,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 461.85,
    "end": 469.8,
    "text": "إِنَّا نَحْنُ نَرِثُ الْأَرْضَ وَمَنْ عَلَيْهَا وَإِلَيْنَا يُرْجَعُونَ"
  },
  {
    "id": 58,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 471.21,
    "end": 475.07,
    "text": "وَاذْكُرْ فِي الْكِتَابِ إِبْرَاهِيمَ"
  },
  {
    "id": 59,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 475.9,
    "end": 480.47,
    "text": "إِنَّهُ كَانَ صِدِّيقًا نَبِيًّا"
  },
  {
    "id": 60,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 481.5,
    "end": 492.56,
    "text": "إِذْ قَالَ لِأَبِيهِ يَا أَبَتِ لِمَ تَعْبُدُ مَا لَا يَسْمَعُ وَلَا يُبْصِرُ وَلَا يُغْنِي عَنْكَ شَيْئًا"
  },
  {
    "id": 61,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 493.79,
    "end": 508.21,
    "text": "يَا أَبَتِ إِنِّي قَدْ جَاءَنِي مِنَ الْعِلْمِ مَا لَمْ يَأْتِكَ فَاتَّبِعْنِي أَهْدِكَ صِرَاطًا سَوِيًّا"
  },
  {
    "id": 62,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 509.67,
    "end": 514.35,
    "text": "يَا أَبَتِ لَا تَعْبُدِ الشَّيْطَانَ"
  },
  {
    "id": 63,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 515.27,
    "end": 520.75,
    "text": "إِنَّ الشَّيْطَانَ كَانَ لِلرَّحْمَنِ عَصِيًّا"
  },
  {
    "id": 64,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 521.96,
    "end": 535.03,
    "text": "يَا أَبَتِ إِنِّي أَخَافُ أَنْ يَمَسَّكَ عَذَابٌ مِنَ الرَّحْمَنِ فَتَكُونَ لِلشَّيْطَانِ وَلِيًّا"
  },
  {
    "id": 65,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 536.37,
    "end": 543.51,
    "text": "قَالَ أَرَاغِبٌ أَنْتَ عَنْ آلِهَتِي يَا إِبْرَاهِيمُ"
  },
  {
    "id": 66,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 544.68,
    "end": 548.79,
    "text": "لَئِنْ لَمْ تَنْتَهِ لَأَرْجُمَنَّهُ"
  },
  {
    "id": 67,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 549.61,
    "end": 551.84,
    "text": "وَاهْجُرْنِي مَلِيًّا"
  },
  {
    "id": 68,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 552.7,
    "end": 555.53,
    "text": "قَالَ سَلَامٌ عَلَيْهِ"
  },
  {
    "id": 69,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 556.45,
    "end": 559.08,
    "text": "سَأَسْتَغْفِرُ لَكَ رَبِّي"
  },
  {
    "id": 70,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 559.93,
    "end": 563.54,
    "text": "إِنَّهُ كَانَ بِي حَفِيًّا"
  },
  {
    "id": 71,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 564.71,
    "end": 579.64,
    "text": "وَأَعْتَزِلُكُمْ وَمَا تَدْعُونَ مِنْ دُونِ اللَّهِ وَأَدْعُو رَبِّي عَسَى أَلَّا أَكُونَ بِدُعَاءِ رَبِّي شَقِيًّا"
  },
  {
    "id": 72,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 580.63,
    "end": 591.63,
    "text": "فَلَمَّا اعْتَزَلَهُمْ وَمَا يَعْبُدُونَ مِنْ دُونِ اللَّهِ وَهَبْنَا لَهُ إِسْحَاقَ وَيَعْقُوبَ"
  },
  {
    "id": 73,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 592.59,
    "end": 595.99,
    "text": "وَكُلًّا جَعَلْنَا نَبِيًّا"
  },
  {
    "id": 74,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 597.15,
    "end": 605.41,
    "text": "وَوَهَبْنَا لَهُمْ مِنْ رَحْمَتِنَا وَجَعَلْنَا لَهُمْ لِسَانَ صِدْقٍ عَلِيًّا"
  },
  {
    "id": 75,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 606.71,
    "end": 617.3,
    "text": "وَاذْكُرْ فِي الْكِتَابِ مُوسَى إِنَّهُ كَانَ مُخْلَصًا وَكَانَ رَسُولًا نَبِيًّا"
  },
  {
    "id": 76,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 618.69,
    "end": 626.54,
    "text": "وَنَادَيْنَاهُ مِنْ جَانِبِ الطُّورِ الْأَيْمَنِ وَقَرَّبْنَاهُ نَجِيًّا"
  },
  {
    "id": 77,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 627.52,
    "end": 634.61,
    "text": "وَوَهَبْنَا لَهُ مِنْ رَحْمَتِنَا أَخَاهُ هَارُونَ نَبِيًّا"
  },
  {
    "id": 78,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 635.99,
    "end": 639.95,
    "text": "وَاذْكُرْ فِي الْكِتَابِ إِسْمَاعِيلَ"
  },
  {
    "id": 79,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 640.69,
    "end": 647.58,
    "text": "إِنَّهُ كَانَ صَادِقَ الْوَعْدِ وَكَانَ رَسُولًا نَبِيًّا"
  },
  {
    "id": 80,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 648.81,
    "end": 658.25,
    "text": "وَكَانَ يَأْمُرُ أَهْلَهُ بِالصَّلَاةِ وَالزَّكَاةِ وَكَانَ عِنْدَ رَبِّهِ مَرْضِيًّا"
  },
  {
    "id": 81,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 659.39,
    "end": 662.79,
    "text": "وَاذْكُرْ فِي الْكِتَابِ إِدْرِيسَ"
  },
  {
    "id": 82,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 663.52,
    "end": 668.18,
    "text": "إِنَّهُ كَانَ صِدِّيقًا نَبِيًّا"
  },
  {
    "id": 83,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 669.0,
    "end": 672.6,
    "text": "وَرَفَعْنَاهُ مَكَانًا عَلِيًّا"
  },
  {
    "id": 84,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 673.88,
    "end": 699.71,
    "text": "أُولَئِكَ الَّذِينَ أَنْعَمَ اللَّهُ عَلَيْهِمْ مِنَ النَّبِيِّينَ مِنْ ذُرِّيَّةِ آدَمَ وَمِمَّنْ حَمَلْنَا مَعَ نُوحٍ وَمِنْ ذُرِّيَّةِ إِبْرَاهِيمَ وَإِسْرَائِيلَ وَمِمَّنْ هَدَيْنَا وَاجْتَبَيْنَا"
  },
  {
    "id": 85,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 700.86,
    "end": 709.28,
    "text": "إِذَا تُتْلَى عَلَيْهِمْ آيَاتُ الرَّحْمَنِ خَرُّوا سُجَّدًا وَبُكِيًّا"
  },
  {
    "id": 86,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 710.47,
    "end": 718.19,
    "text": "فَخَلَفَ مِنْ بَعْدِهِمْ خَلْفٌ أَضَاعُوا الصَّلَاةَ وَاتَّبَعُوا الشَّهَوَاتِ"
  },
  {
    "id": 87,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 719.1,
    "end": 721.64,
    "text": "فَسَوْفَ يَلْقَوْنَ غَيًّا"
  },
  {
    "id": 88,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 722.8,
    "end": 735.23,
    "text": "إِلَّا مَنْ تَابَ وَآمَنَ وَعَمِلَ صَالِحًا فَأُولَئِكَ يَدْخُلُونَ الْجَنَّةَ وَلَا يُظْلَمُونَ شَيْئًا"
  },
  {
    "id": 89,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 736.47,
    "end": 744.1,
    "text": "جَنَّاتِ عَدْنٍ الَّتِي وَعَدَ الرَّحْمَنُ عِبَادَهُ بِالْغَيْبِ"
  },
  {
    "id": 90,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 744.98,
    "end": 748.12,
    "text": "إِنَّهُ كَانَ وَعْدُهُ"
  },
  {
    "id": 91,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 748.47,
    "end": 749.29,
    "text": "فِي يَا"
  },
  {
    "id": 92,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 750.2,
    "end": 755.38,
    "text": "لَا يَسْمَعُونَ فِيهَا لَغْوًا إِلَّا سَلَامًا"
  },
  {
    "id": 93,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 756.23,
    "end": 761.66,
    "text": "وَلَهُمْ رِزْقُهُمْ فِيهَا بُكْرَةً وَعَشِيًّا"
  },
  {
    "id": 94,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 762.99,
    "end": 770.86,
    "text": "تِلْكَ الْجَنَّةُ الَّتِي نُورِثُ مِنْ عِبَادِنَا مَنْ كَانَ تَقِيًّا"
  },
  {
    "id": 95,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 771.86,
    "end": 775.67,
    "text": "وَمَا نَتَنَزَّلُ إِلَّا بِأَمْرِ رَبِّكَ"
  },
  {
    "id": 96,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 776.6,
    "end": 782.87,
    "text": "لَهُ مَا بَيْنَ أَيْدِينَا وَمَا خَلْفَنَا وَمَا بَيْنَ ذَلِكَ"
  },
  {
    "id": 97,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 783.76,
    "end": 786.98,
    "text": "وَمَا كَانَ رَبُّكَ نَسِيًّا"
  },
  {
    "id": 98,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 788.11,
    "end": 796.03,
    "text": "رَبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا فَاعْبُدْهُ وَاصْطَبِرْ لِعِبَادَتِهِ"
  },
  {
    "id": 99,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 796.97,
    "end": 799.78,
    "text": "سَلْ تَعْلَمُ لَهُ سَمِيًّا"
  },
  {
    "id": 100,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 801.04,
    "end": 807.95,
    "text": "وَيَقُولُ الْإِنْسَانُ أَإِذَا مَا مِتُّ لَسَوْفَ أُخْرَجُ حَيًّا"
  },
  {
    "id": 101,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 808.99,
    "end": 818.37,
    "text": "أَوَلَا يَذْكُرُ الْإِنْسَانُ أَنَّا خَلَقْنَاهُ مِنْ قَبْلُ وَلَمْ يَكُ شَيْئًا"
  },
  {
    "id": 102,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 819.58,
    "end": 831.0,
    "text": "وَرَبِّكَ لَنَحْشُرَنَّهُمْ وَالشَّيَاطِينَ ثُمَّ لَنُحْضِرَنَّهُمْ حَوْلَ جَهَنَّمَ جِثِيًّا"
  },
  {
    "id": 103,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 832.22,
    "end": 842.37,
    "text": "ثُمَّ لَنَنْزِعَنَّ مِنْ كُلِّ شِيعَةٍ أَيُّهُمْ أَشَدُّ عَلَى الرَّحْمَنِ عِتِيًّا"
  },
  {
    "id": 104,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 843.43,
    "end": 850.11,
    "text": "ثُمَّ لَنَحْنُ أَعْلَمُ بِالَّذِينَ هُمْ أَوْلَى بِهَا صِلِيًّا"
  },
  {
    "id": 105,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 851.29,
    "end": 855.31,
    "text": "وَإِنْ مِنْكُمْ إِلَّا وَارِدُهَا"
  },
  {
    "id": 106,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 856.1,
    "end": 860.59,
    "text": "وَيَا نَعْلَى رَبِّكَ حَتْمًا مَقْضِيًّا"
  },
  {
    "id": 107,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 861.53,
    "end": 869.18,
    "text": "ثُمَّ نُنَجِّي الَّذِينَ اتَّقَوْا وَنَذَرُ الظَّالِمِينَ فِيهَا جِثِيًّا"
  },
  {
    "id": 108,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 870.37,
    "end": 887.46,
    "text": "وَإِذَا تُتْلَى عَلَيْهِمْ آيَاتُنَا بَيِّنَاتٍ قَالَ الَّذِينَ كَفَرُوا لِلَّذِينَ آمَنُوا أَيُّ الْفَرِيقَيْنِ خَيْرٌ مَقَامًا وَأَحْسَنُ نَدِيًّا"
  },
  {
    "id": 109,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 888.55,
    "end": 897.01,
    "text": "وَكَمْ أَهْلَكْنَا قَبْلَهُمْ مِنْ قَرْنٍ هُمْ أَحْسَنُ أَثَاثًا وَرِئْيًا"
  },
  {
    "id": 110,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 898.41,
    "end": 905.1,
    "text": "قُلْ مَنْ كَانَ فِي الضَّلَالَةِ فَلْيَمْدُدْ لَهُ الرَّحْمَنُ مَدًّا"
  },
  {
    "id": 111,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 906.16,
    "end": 916.72,
    "text": "حَتَّى إِذَا رَأَوْا مَا يُوعَدُونَ إِمَّا الْعَذَابَ وَإِمَّا السَّاعَةَ فَسَيَعْلَمُونَ"
  },
  {
    "id": 112,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 917.6,
    "end": 925.02,
    "text": "فَسَيَعْلَمُونَ مَنْ هُوَ شَرٌّ مَكَانًا وَأَضْعَفُ جُنْدًا"
  },
  {
    "id": 113,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 926.34,
    "end": 930.52,
    "text": "وَيَزِيدُ اللَّهُ الَّذِينَ اهْتَدَوْا هُدًى"
  },
  {
    "id": 114,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 931.43,
    "end": 940.22,
    "text": "وَالْبَاقِيَاتُ الصَّالِحَاتُ خَيْرٌ عِنْدَ رَبِّكَ ثَوَابًا وَخَيْرٌ مَرَدِينَ"
  },
  {
    "id": 115,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 940.56,
    "end": 940.98,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 116,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 942.4,
    "end": 950.96,
    "text": "أَفَرَأَيْتَ الَّذِي كَفَرَ بِآيَاتِنَا وَقَالَ لَأُوتَيَنَّ مَالًا وَوَلَدًا"
  },
  {
    "id": 117,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 951.97,
    "end": 958.03,
    "text": "أَطَّلَعَ الْغَيْبَ أَمِ اتَّخَذَ عِنْدَ الرَّحْمَنِ عَهْدًا"
  },
  {
    "id": 118,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 958.89,
    "end": 959.66,
    "text": "كَلَّا"
  },
  {
    "id": 119,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 960.42,
    "end": 966.48,
    "text": "سَنَكْتُبُ مَا يَقُولُ وَنَمُدُّ لَهُ مِنَ الْعَذَابِ مَدًّا"
  },
  {
    "id": 120,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 967.26,
    "end": 972.03,
    "text": "وَنَرِثُهُ مَا يَقُولُ وَيَأْتِينَا فَرْدًا"
  },
  {
    "id": 121,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 973.14,
    "end": 980.38,
    "text": "وَاتَّخَذُوا مِنْ دُونِ اللَّهِ آلِهَةً لِيَكُونُوا لَهُمْ عِزًّا"
  },
  {
    "id": 122,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 981.31,
    "end": 982.08,
    "text": "كَلَّا"
  },
  {
    "id": 123,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 982.75,
    "end": 988.32,
    "text": "سَيَكْفُرُونَ بِعِبَادَتِهِمْ وَيَكُونُونَ عَلَيْهِمْ"
  },
  {
    "id": 124,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 988.68,
    "end": 989.03,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 125,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 990.35,
    "end": 999.03,
    "text": "أَلَمْ تَرَ أَنَّا أَرْسَلْنَا الشَّيَاطِينَ عَلَى الْكَافِرِينَ تَؤُزُّهُمْ أَزًّا"
  },
  {
    "id": 126,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1000.01,
    "end": 1002.3,
    "text": "لَا تَعْجَلْ عَلَيْهِمْ"
  },
  {
    "id": 127,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1003.02,
    "end": 1006.72,
    "text": "إِنَّمَا نَعُدُّ لَهُمْ عَدًّا"
  },
  {
    "id": 128,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1007.54,
    "end": 1012.7,
    "text": "يَوْمَ نَحْشُرُ الْمُتَّقِينَ إِلَى الرَّحْمَنِ وَفْدًا"
  },
  {
    "id": 129,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1013.59,
    "end": 1018.85,
    "text": "وَنَسُوقُ الْمُجْرِمِينَ إِلَى جَهَنَّمَ وِرْدًا"
  },
  {
    "id": 130,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1019.81,
    "end": 1027.58,
    "text": "لَا يَمْلِكُونَ الشَّفَاعَةَ إِلَّا مَنِ اتَّخَذَ عِنْدَ الرَّحْمَنِ عَهْدًا"
  },
  {
    "id": 131,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1028.6,
    "end": 1032.09,
    "text": "وَقَالُوا اتَّخَذَ الرَّحْمَنُ وَلَدًا"
  },
  {
    "id": 132,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1032.98,
    "end": 1033.65,
    "text": "لَقَدْ جِئَتْ"
  },
  {
    "id": 133,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1034.0,
    "end": 1035.36,
    "text": "عُمْ شَيْئًا"
  },
  {
    "id": 134,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1035.67,
    "end": 1036.08,
    "text": "وَزَادَى"
  },
  {
    "id": 135,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1036.82,
    "end": 1046.45,
    "text": "تَكَادُ السَّمَاوَاتُ يَتَفَطَّرْنَ مِنْهُ وَتَنْشَقُّ الْأَرْضُ وَتَخِرُّ الْجِبَالُ هَدًّا"
  },
  {
    "id": 136,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1047.38,
    "end": 1051.22,
    "text": "أَنْ دَعَوْا لِلرَّحْمَنِ وَلَدًا"
  },
  {
    "id": 137,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1052.13,
    "end": 1056.32,
    "text": "وَمَا يَنْبَغِي لِلرَّحْمَنِ أَنْ يَدْعُلُونَ"
  },
  {
    "id": 138,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1056.63,
    "end": 1058.04,
    "text": "وَالدَّخِذَ وَلَدًا"
  },
  {
    "id": 139,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1059.08,
    "end": 1068.5,
    "text": "إِنْ كُلُّ مَنْ فِي السَّمَاوَاتِ وَالْأَرْضِ إِلَّا آتِي الرَّحْمَنِ عَبْدًا"
  },
  {
    "id": 140,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1069.56,
    "end": 1073.35,
    "text": "لَقَدْ أَحْصَاهُمْ وَعَدَّهُمْ عَدًّا"
  },
  {
    "id": 141,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1074.09,
    "end": 1079.1,
    "text": "وَكُلُّهُمْ آتِيهِ يَوْمَ الْقِيَامَةِ فَرْدًا"
  },
  {
    "id": 142,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1080.36,
    "end": 1088.87,
    "text": "إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ سَيَجْعَلُ لَهُمُ الرَّحْمَنُ وُدًّا"
  },
  {
    "id": 143,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1090.07,
    "end": 1097.52,
    "text": "فَإِنَّمَا يَسَّرْنَاهُ بِلِسَانِكَ لِتُبَشِّرَ بِهِ الْمُتَّقِينَ"
  },
  {
    "id": 144,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1098.45,
    "end": 1104.56,
    "text": "لِتُبَشِّرَ بِهِ الْمُتَّقِينَ وَتُنْذِرَ بِهِ قَوْمًا"
  },
  {
    "id": 145,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1104.9,
    "end": 1105.33,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 146,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1106.5,
    "end": 1114.73,
    "text": "وَكَمْ أَهْلَكْنَا قَبْلَهُمْ مِنْ قَرْنٍ هَلْ تُحِسُّ مِنْهُمْ مِنْ أَحَدٍ"
  },
  {
    "id": 147,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1115.73,
    "end": 1121.87,
    "text": "سَبَلْتُ حِسُّ مِنْهُمْ مِنْ أَحَدٍ أَوْ تَسْمَعُ لَهُمْ"
  },
  {
    "id": 148,
    "sura_id": 19,
    "recitation_uuid": "7c9a268f-8702-4590-a4cb-093fa3e7fd3e",
    "start": 1122.32,
    "end": 1122.84,
    "text": "وَالْمُؤْمِنِينَ"
  }
]