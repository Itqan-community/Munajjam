[
  {
    "id": 2,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 5.81,
    "end": 14.54,
    "text": "يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تُقَدِّمُوا بَيْنَ يَدَيِ اللَّهِ وَرَسُولِهِ"
  },
  {
    "id": 3,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 15.34,
    "end": 17.71,
    "text": "وَاتَّقُوا اللَّهَ"
  },
  {
    "id": 4,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 18.63,
    "end": 22.65,
    "text": "إِنَّ اللَّهَ سَمِيعٌ عَلِيمٌ"
  },
  {
    "id": 5,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 23.71,
    "end": 37.33,
    "text": "يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تَرْفَعُوا أَصْوَاتَكُمْ فَوْقَ صَوْتِ النَّبِيِّ وَلَا تَجْهَرُوا لَهُ بِالْقَوْلِ"
  },
  {
    "id": 6,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 38.47,
    "end": 50.52,
    "text": "وَلَا تَجْهَرُوا لَهُ بِالْقَوْلِ كَجَهْرِ بَعْضِكُمْ لِبَعْضٍ أَنْ تَحْبَطَ أَعْمَالُكُمْ وَأَنْتُمْ لَا تَشْعُرُونَ"
  },
  {
    "id": 7,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 51.94,
    "end": 64.53,
    "text": "إِنَّ الَّذِينَ يَوُضُّونَ أَصْوَاتَهُمْ عِنْدَ رَسُولِ اللَّهِ أُولَئِكَ الَّذِينَ امْتَحَنَ اللَّهُ قُلُوبَهُمْ رِئْوًا"
  },
  {
    "id": 8,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 65.73,
    "end": 70.58,
    "text": "لَهُمْ مَغْفِرَةٌ وَأَجْرٌ عَظِيمٌ"
  },
  {
    "id": 9,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 71.99,
    "end": 81.28,
    "text": "إِنَّ الَّذِينَ يُنَادُونَكَ مِنْ وَرَاءِ الْحُجُرَاتِ أَكْثَرُهُمْ لَا يَعْقِلُونَ"
  },
  {
    "id": 10,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 82.26,
    "end": 89.1,
    "text": "وَلَوْ أَنَّهُمْ صَبَرُوا حَتَّى تَخْرُجَ إِلَيْهِمْ لَكَانَ خَيْرًا لَهُمْ"
  },
  {
    "id": 11,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 90.22,
    "end": 93.77,
    "text": "وَاللَّهُ غَفُورٌ رَحِيمٌ"
  },
  {
    "id": 12,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 95.03,
    "end": 107.37,
    "text": "يَا أَيُّهَا الَّذِينَ آمَنُوا إِنْ جَاءَكُمْ فَاسِقٌ بِنَبَإٍ فَتَبَيَّنُونَ"
  },
  {
    "id": 13,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 108.49,
    "end": 120.25,
    "text": "فَتَبَيَّنُوا أَنْ تُصِيبُوا قَوْمًا بِجَهَالَةٍ فَتُصْبِحُوا عَلَى مَا فَعَلْتُمْ نَادِمِينَ"
  },
  {
    "id": 14,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 121.4,
    "end": 127.03,
    "text": "وَاعْلَمُوا أَنَّ فِيكُمْ رَسُولَ اللَّهِ"
  },
  {
    "id": 15,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 128.22,
    "end": 139.47,
    "text": "لَوْ يُطِيعُكُمْ فِي كَثِيرٍ مِنَ الْأَمْرِ لَعَنِتُّمْ وَلَكِنَّ اللَّهَ حَبَّبَ إِلَيْكُمُ الْإِيمَانُ"
  },
  {
    "id": 16,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 140.65,
    "end": 154.72,
    "text": "وَلَكِنَّ اللَّهَ حَبَّبَ إِلَيْكُمُ الْإِيمَانَ وَزَيَّنَهُ فِي قُلُوبِكُمْ وَكَرَّهَ إِلَيْكُمُ الْكُفْرَ وَالْفُسُوقَ وَالْعِصْيَانَ"
  },
  {
    "id": 17,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 155.87,
    "end": 160.39,
    "text": "أُولَئِكَ هُمُ الرَّاشِدُونَ"
  },
  {
    "id": 18,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 161.27,
    "end": 164.15,
    "text": "فَضْلًا مِنَ اللَّهِ وَنِعْمَةً"
  },
  {
    "id": 19,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 165.03,
    "end": 168.78,
    "text": "وَاللَّهُ عَلِيمٌ حَكِيمٌ"
  },
  {
    "id": 20,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 169.91,
    "end": 177.93,
    "text": "وَإِنْ طَائِفَتَانِ مِنَ الْمُؤْمِنِينَ اقْتَتَلُوا فَأَصْلِحُوا بَيْنَهُمَا"
  },
  {
    "id": 21,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 179.02,
    "end": 190.48,
    "text": "فَإِنْ بَغَتْ إِحْدَاهُمَا عَلَى الْأُخْرَى فَقَاتِلُوا الَّتِي تَبْغِي حَتَّى تَفِيءَ إِلَى أَمْرِ اللَّهِ"
  },
  {
    "id": 22,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 191.71,
    "end": 198.38,
    "text": "فَإِنْ فَاءَتْ فَأَصْلِحُوا بَيْنَهُمَا بِالْعَدْلِ وَأَقْسِطُونَ"
  },
  {
    "id": 23,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 199.29,
    "end": 204.06,
    "text": "إِنَّ اللَّهَ يُحِبُّ الْمُقْسِطِينَ"
  },
  {
    "id": 24,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 205.42,
    "end": 212.01,
    "text": "إِنَّمَا الْمُؤْمِنُونَ إِخْوَةٌ فَأَصْلِحُوا بَيْنَ أَخَوَيْكُمْ"
  },
  {
    "id": 25,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 212.81,
    "end": 217.24,
    "text": "وَاتَّقُوا اللَّهَ لَعَلَّكُمْ تُرْحَمُونَ"
  },
  {
    "id": 26,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 218.37,
    "end": 236.24,
    "text": "يَا أَيُّهَا الَّذِينَ آمَنُوا لَا يَسْخَرْ قَوْمٌ مِنْ قَوْمٍ عَسَى أَنْ يَكُونُوا خَيْرًا مِنْهُمْ وَلَا نِسَاءٌ مِنْ نِسَاءٍ"
  },
  {
    "id": 27,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 237.8,
    "end": 248.71,
    "text": "وَلَا نِسَاءٌ مِنْ نِسَاءٍ عَسَى أَنْ يَكُنَّ خَيْرًا مِنْهُنَّ"
  },
  {
    "id": 28,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 249.92,
    "end": 257.04,
    "text": "وَلَا تَلْمِزُوا أَنْفُسَكُمْ وَلَا تَنَابَزُوا بِالْأَلْقَابِ"
  },
  {
    "id": 29,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 257.84,
    "end": 262.16,
    "text": "بِئْسَ لِاسْمُ الْفُسُوقُ بَعْدَ الْإِيمَانِ"
  },
  {
    "id": 30,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 262.98,
    "end": 269.21,
    "text": "وَمَنْ لَمْ يَتُبْفَأُولَئِكَ هُمُ الظَّالِمُونَ"
  },
  {
    "id": 31,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 270.73,
    "end": 281.19,
    "text": "يَا أَيُّهَا الَّذِينَ آمَنُوا اجْتَنِبُوا كَثِيرًا مِنَ الظَّنِّ إِنَّ بَعْضَ الظَّنِّ"
  },
  {
    "id": 32,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 282.34,
    "end": 287.47,
    "text": "وَلَا تَجَسَّسُوا وَلَا يَغْتَبَّعْضُكُمْ بَعْضًا"
  },
  {
    "id": 33,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 288.49,
    "end": 296.89,
    "text": "أَيُحِبُّ أَحَدُكُمْ أَنْ يَأْكُلَ لَحْمَ أَخِيهِ مَيْتًا فَكَرِهْتُمُوهُ"
  },
  {
    "id": 34,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 297.62,
    "end": 299.74,
    "text": "وَاتَّقُوا اللَّهَ"
  },
  {
    "id": 35,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 300.64,
    "end": 305.08,
    "text": "إِنَّ اللَّهَ تَوَّابٌ رَحِيمٌ"
  },
  {
    "id": 36,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 306.4,
    "end": 322.1,
    "text": "يَا أَيُّهَا النَّاسُ إِنَّا خَلَقْنَاكُمْ مِنْ ذَكَرٍ وَأُنْثَى وَجَعَلْنَاكُمْ شُعُوبًا وَقَبَائِلَ لِتَعَارَفُونَ"
  },
  {
    "id": 37,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 323.38,
    "end": 328.15,
    "text": "إِنَّ أَكْرَمَكُمْ عِنْدَ اللَّهِ أَتْقَاكُمْ"
  },
  {
    "id": 38,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 329.01,
    "end": 332.85,
    "text": "إِنَّ اللَّهَ عَلِيمٌ خَبِيرٌ"
  },
  {
    "id": 39,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 333.81,
    "end": 337.07,
    "text": "قَالَتِ الْأَعْرَابُ آمَنَّا"
  },
  {
    "id": 40,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 338.05,
    "end": 348.1,
    "text": "قُلْ لَمْ تُؤْمِنُوا وَلَكِنْ قُولُوا أَسْلَمْنَا وَلَمَّا يَدْخُلِ الْإِيمَانُ فِي قُلُوبِكُمْ"
  },
  {
    "id": 41,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 349.26,
    "end": 357.06,
    "text": "وَإِنْ تُطِيعُوا اللَّهَ وَرَسُولَهُ لَا يَلِتْكُمْ مِنْ أَعْمَالِكُمْ شَيْئًا"
  },
  {
    "id": 42,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 357.95,
    "end": 362.07,
    "text": "إِنَّ اللَّهَ غَفُورٌ رَحِيمٌ"
  },
  {
    "id": 43,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 363.37,
    "end": 378.82,
    "text": "إِنَّمَا الْمُؤْمِنُونَ الَّذِينَ آمَنُوا بِاللَّهِ وَرَسُولِهِ ثُمَّ لَمْ يَرْتَابُوا وَجَاهَدُوا بِأَمْوَالِهِمْ وَأَنْفُسِهِمْ فِي سَبِيلِ اللَّهِ"
  },
  {
    "id": 44,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 379.9,
    "end": 384.41,
    "text": "أُولَئِكَ هُمُ الصَّادِقُونَ"
  },
  {
    "id": 45,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 385.72,
    "end": 394.18,
    "text": "قُلْ أَتُعَلِّمُونَ اللَّهَ بِدِينِكُمْ وَاللَّهُ يَعْلَمُ مَا فِي السَّمَاوَاتِ وَمَا فِي الْأَرْضِ"
  },
  {
    "id": 46,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 395.04,
    "end": 399.01,
    "text": "وَاللَّهُ بِكُلِّ شَيْءٍ عَلِيمٌ"
  },
  {
    "id": 47,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 400.21,
    "end": 403.57,
    "text": "يَمُنُّونَ عَلَيْكَ أَنْ أَسْلَمُونَ"
  },
  {
    "id": 48,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 404.36,
    "end": 408.36,
    "text": "قُلْ لَا تَمُنُّوا عَلَيَّ إِسْلَامَكُمْ"
  },
  {
    "id": 49,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 409.36,
    "end": 419.37,
    "text": "بَلِ اللَّهُ يَمُنُّ عَلَيْكُمْ أَنْ هَدَاكُمْ لِلْإِيمَانِ إِنْ كُنْتُمْ صَادِقِينَ"
  },
  {
    "id": 50,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 420.47,
    "end": 425.51,
    "text": "إِنَّ اللَّهَ يَعْلَمُ غَيْبَ السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 51,
    "sura_id": 49,
    "recitation_uuid": "d53661c1-cde4-4a38-89b6-dc5b6bbbf60a",
    "start": 426.57,
    "end": 431.87,
    "text": "وَاللَّهُ بَصِيرٌ بِمَا تَعْمَلُونَ"
  }
]