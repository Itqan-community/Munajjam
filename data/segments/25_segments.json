[
  {
    "id": 2,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 5.76,
    "end": 14.41,
    "text": "تَبَارَكَ الَّذِي نَزَّلَ الْفُرْقَانَ عَلَى عَبْدِهِ لِيَكُونَ لِلْعَالَمِينَ نَذِيرًا"
  },
  {
    "id": 3,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 15.8,
    "end": 27.16,
    "text": "الَّذِي لَهُ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ وَلَمْ يَتَّخِذْ وَلَدًا وَلَمْ يَكُنْ لَهُ شَرِيكٌ فِي الْمُلْكِرِينَ"
  },
  {
    "id": 4,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 28.72,
    "end": 38.33,
    "text": "وَلَمْ يَكُنْ لَهُ شَرِيكٌ فِي الْمُلْكِ وَخَلَقَ كُلَّ شَيْءٍ فَقَدَّرَهُ تَقْدِيرًا"
  },
  {
    "id": 5,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 39.51,
    "end": 48.98,
    "text": "وَاتَّخَذُوا مِنْ دُونِهِ آلِهَةً لَا يَخْلُقُونَ شَيْئًا وَهُمْ يُخْلَقُونَ"
  },
  {
    "id": 6,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 50.47,
    "end": 68.48,
    "text": "لَا يَخْلُقُونَ شَيْئًا وَهُمْ يُخْلَقُونَ وَلَا يَمْلِكُونَ لِأَنْفُسِهِمْ ضَرًّا وَلَا نَفْعًا وَلَا يَمْلِكُونَ مَوْتًا وَلَا حَيَاةً وَلَا نُشُورًا"
  },
  {
    "id": 7,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 70.22,
    "end": 82.55,
    "text": "وَقَالَ الَّذِينَ كَفَرُوا إِنْ هَذَا إِلَّا إِفْكٌ افْتَرَاهُ وَأَعَانَهُ عَلَيْهِ قَوْمٌ آخَرُونَ"
  },
  {
    "id": 8,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 83.44,
    "end": 88.06,
    "text": "لَقَدْ جَاءُوا ظُلْمًا وَزُورًا"
  },
  {
    "id": 9,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 89.75,
    "end": 99.86,
    "text": "وَقَالُوا أَسَاطِيرُ الْأَوَّلِينَ اكْتَتَبَهَا فَهِيَ تُمْلَى عَلَيْهِ بُكْرَةً وَأَصِيلًا"
  },
  {
    "id": 10,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 101.36,
    "end": 107.57,
    "text": "قُلْ أَنْزَلَهُ الَّذِي يَعْلَمُ السِّرَّ فِي السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 11,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 108.47,
    "end": 112.46,
    "text": "إِنَّهُ كَانَ غَفُورًا رَحِيمًا"
  },
  {
    "id": 12,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 114.05,
    "end": 128.73,
    "text": "وَقَالُوا مَالِ هَذَا الرَّسُولِ يَأْكُلُ الطَّعَامَ وَيَمْشِي فِي الْأَسْوَاقِ لَوْلَا أُنْزِلَ إِلَيْهِ مَلَكٌ فَيَكُونَ مَعَهُ نَذِيرًا"
  },
  {
    "id": 13,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 130.17,
    "end": 137.43,
    "text": "لَوْ يُلْقَى إِلَيْهِ كَنْزٌ أَوْ تَكُونُ لَهُ جَنَّةٌ"
  },
  {
    "id": 14,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 137.76,
    "end": 138.86,
    "text": "كُلُوا مِنْهَا"
  },
  {
    "id": 15,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 140.0,
    "end": 146.2,
    "text": "وَقَالَ الظَّالِمُونَ إِنْ تَتَّبِعُونَ إِلَّا رَجُلًا"
  },
  {
    "id": 16,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 146.53,
    "end": 147.28,
    "text": "وَأُمُورًا"
  },
  {
    "id": 17,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 148.64,
    "end": 156.9,
    "text": "انْظُرْ كَيْفَ ضَرَبُوا لَكَ الْأَمْثَالَ فَضَلُّوا فَلَا يَسْتَطِيعُونَ سَبِيلًا"
  },
  {
    "id": 18,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 158.29,
    "end": 169.04,
    "text": "تَبَارَكَ الَّذِي إِنْ شَاءَ جَعَلَ لَكَ خَيْرًا مِنْ ذَلِكَ جَنًّا"
  },
  {
    "id": 19,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 169.9,
    "end": 177.99,
    "text": "جَنَّاتٍ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ وَيَجْعَلْ لَكَ قُصُورًا"
  },
  {
    "id": 20,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 178.87,
    "end": 181.09,
    "text": "بَلْ كَذَّبُوا بِالسَّاعَةِ"
  },
  {
    "id": 21,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 181.88,
    "end": 187.07,
    "text": "وَأَعْتَدْنَا لِمَنْ كَذَّبَ بِالسَّاعَةِ سَعِيرًا"
  },
  {
    "id": 22,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 188.44,
    "end": 197.89,
    "text": "إِذَا رَأَتْهُمْ مِنْ مَكَانٍ بَعِيدٍ سَمِعُوا لَهَا تَغَيُّظًا وَزَفِيرًا"
  },
  {
    "id": 23,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 199.21,
    "end": 209.19,
    "text": "وَإِذَا أُلْقُوا مِنْهَا مَكَانًا ضَيِّقًا مُقَرَّنِينَ دَعَوْا هُنَالِكَ ثُبُورًا"
  },
  {
    "id": 24,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 210.41,
    "end": 218.73,
    "text": "لَا تَدْعُوا الْيَوْمَ ثُبُورًا وَاحِدًا وَادْعُوا ثُبُورًا كَثِيرًا"
  },
  {
    "id": 25,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 219.89,
    "end": 227.9,
    "text": "قُلْ أَذَلِكَ خَيْرٌ أَمْ جَنَّةُ الْخُلْدِ الَّتِي وُعِدَ الْمُتَّقُونَ"
  },
  {
    "id": 26,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 228.83,
    "end": 234.02,
    "text": "يَا نَتْلَهُمْ جَزَاءً وَمَصِيرًا"
  },
  {
    "id": 27,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 235.11,
    "end": 240.75,
    "text": "لَهُمْ فِيهَا مَا يَشَاءُونَ خَالِدِينَ"
  },
  {
    "id": 28,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 241.58,
    "end": 244.75,
    "text": "وَيَا نَعْلَى رَبِّكَ وَعْدًا"
  },
  {
    "id": 29,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 245.06,
    "end": 245.83,
    "text": "وَالْعُولَى"
  },
  {
    "id": 30,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 246.85,
    "end": 263.34,
    "text": "وَيَوْمَ يَحْشُرُهُمْ وَمَا يَعْبُدُونَ مِنْ دُونِ اللَّهِ فَيَقُولُ أَأَنْتُمْ أَضْلَلْتُمْ عِبَادِي هَؤُلَاءِ أَمْ هُمْ ضَلُّوا السَّبِيلَ"
  },
  {
    "id": 31,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 264.46,
    "end": 278.32,
    "text": "قَالُوا سُبْحَانَكَ مَا كَانَ يَنْبَغِي لَنَا أَنْ نَتَّخِذَ مِنْ دُونِكَ مِنْ أَوْلِيَاءَ وَلَكِنْ مَتَّعْتَهُمْ"
  },
  {
    "id": 32,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 279.82,
    "end": 289.86,
    "text": "وَلَكِنْ مَتَّعْتَهُمْ وَآبَاءَهُمْ حَتَّى نَسُوا الذِّكْرَ وَكَانُوا قَوْمًا بُورًا"
  },
  {
    "id": 33,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 291.27,
    "end": 299.84,
    "text": "لَقَدْ كَذَّبُوكُمْ بِمَا تَقُولُونَ فَمَا تَسْتَطِيعُونَ صَرْفًا وَلَا نَصْرًا"
  },
  {
    "id": 34,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 300.83,
    "end": 307.76,
    "text": "وَمَنْ يَظْلِمْ مِنْكُمْ نُذِقْهُ عَذَابًا كَبِيرًا"
  },
  {
    "id": 35,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 308.86,
    "end": 321.27,
    "text": "وَمَا أَرْسَلْنَا قَبْلَكَ مِنَ الْمُرْسَلِينَ إِلَّا إِنَّهُمْ لَيَأْكُلُونَ الطَّعَامَ وَيَمْشُونَ فِي الْأَسْوَاقِ"
  },
  {
    "id": 36,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 322.37,
    "end": 329.12,
    "text": "وَجَعَلْنَا بَعْضَكُمْ لِبَعْضٍ فِتْنَةً أَتَصْبِرُونَ"
  },
  {
    "id": 37,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 330.03,
    "end": 332.85,
    "text": "وَكَانَ رَبُّكَ بَصِيرًا"
  },
  {
    "id": 38,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 334.03,
    "end": 346.2,
    "text": "وَقَالَ الَّذِينَ لَا يَرْجُونَ لِقَاءَنَا لَوْلَا أُنْزِلَ عَلَيْنَا الْمَلَائِكَةُ أَوْ نَرَى رَبَّنَا"
  },
  {
    "id": 39,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 347.42,
    "end": 355.14,
    "text": "لَقَدِ اسْتَكْبَرُوا فِي أَنْفُسِهِمْ وَعَتَوْا عُتُوًّا كَبِيرًا"
  },
  {
    "id": 40,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 356.27,
    "end": 366.04,
    "text": "يَوْمَ يَرَوْنَ الْمَلَائِكَةَ لَا بُشْرَى يَوْمَئِذٍ لِلْمُجْرِمِينَ وَيَقُولُونَ حِجْرًا"
  },
  {
    "id": 41,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 366.4,
    "end": 367.12,
    "text": "سُورًا"
  },
  {
    "id": 42,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 368.34,
    "end": 378.78,
    "text": "وَقَدِمْنَا إِلَى مَا عَمِلُوا مِنْ عَمَلٍ فَجَعَلْنَاهُ هَبَاءً مَنْثُورًا"
  },
  {
    "id": 43,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 380.14,
    "end": 388.26,
    "text": "أَصْحَابُ الْجَنَّةِ يَوْمَئِذٍ خَيْرٌ مُسْتَقَرًّا وَأَحْسَنُ مَقِيلًا"
  },
  {
    "id": 44,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 389.42,
    "end": 399.18,
    "text": "وَيَوْمَ تَشَقَّقُ السَّمَاءُ بِالْغَمَامِ وَنُزِّلَ الْمَلَائِكَةُ تَنْزِيلًا"
  },
  {
    "id": 45,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 400.25,
    "end": 404.95,
    "text": "الْمُلْكُ يَوْمَئِذٍ الْحَقُّ لِلرَّحْمَنِ"
  },
  {
    "id": 46,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 405.84,
    "end": 410.27,
    "text": "وَكَانَ يَوْمًا عَلَى الْكَافِرِينَ عَسِيرًا"
  },
  {
    "id": 47,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 411.41,
    "end": 420.46,
    "text": "وَيَوْمَ يَعَضُّ الظَّالِمُ عَلَى يَدَيْهِ يَقُولُ يَا لَيْتَنِي اتَّخَذْتُ مَعَ الرَّسُولِ سَبِيلًا"
  },
  {
    "id": 48,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 421.25,
    "end": 426.82,
    "text": "يَا وَيْلَتَى لَيْتَنِي لَمْ أَتَّخِذْ فُلَانًا خَرِينًا"
  },
  {
    "id": 49,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 427.68,
    "end": 433.42,
    "text": "لَقَدْ أَضَلَّنِي عَنِ الذِّكْرِ بَعْدَ إِذْ جَاءَنِي"
  },
  {
    "id": 50,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 434.21,
    "end": 439.22,
    "text": "وَكَانَ الشَّيْطَانُ لِلْإِنْسَانِ خَذُولًا"
  },
  {
    "id": 51,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 440.1,
    "end": 447.94,
    "text": "وَقَالَ الرَّسُولُ يَا رَبِّ إِنَّ قَوْمِي اتَّخَذُوا هَذَا الْقُرْآنَ مَهْجُورًا"
  },
  {
    "id": 52,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 448.92,
    "end": 456.49,
    "text": "وَكَذَلِكَ جَعَلْنَا لِكُلِّ نَبِيٍّ عَدُوًّا مِنَ الْمُجْرِمِينَ"
  },
  {
    "id": 53,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 457.24,
    "end": 462.03,
    "text": "وَكَفَى بِرَبِّكَ هَادِيًا وَنَصِيرًا"
  },
  {
    "id": 54,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 462.99,
    "end": 470.97,
    "text": "وَقَالَ الَّذِينَ كَفَرُوا لَوْلَا نُزِّلَ عَلَيْهِ الْقُرْآنُ جُمْلَةً وَاحِدَةً"
  },
  {
    "id": 55,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 471.84,
    "end": 475.3,
    "text": "كَذَلِكَ لِنُثَبِّتَ بِهِ"
  },
  {
    "id": 56,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 476.12,
    "end": 476.34,
    "text": "وَرَ"
  },
  {
    "id": 57,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 476.64,
    "end": 478.92,
    "text": "بَلْ لَاهُ تَرْتِيلًا"
  },
  {
    "id": 58,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 480.08,
    "end": 484.87,
    "text": "وَلَا يَأْتُونَكَ بِمَثَلٍ إِلَّا جِئْنَاكَ بِالْحَقِّ"
  },
  {
    "id": 59,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 485.19,
    "end": 487.52,
    "text": "وَأَحْسَنَ تَفْسِيرًا"
  },
  {
    "id": 60,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 488.71,
    "end": 500.79,
    "text": "الَّذِينَ يُحْشَرُونَ عَلَى وُجُوهِهِمْ إِلَى جَهَنَّمَ أُولَئِكَ شَرٌّ مَكَانًا وَأَضَلُّ سَبِيلًا"
  },
  {
    "id": 61,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 502.06,
    "end": 510.69,
    "text": "وَلَقَدْ آتَيْنَا مُوسَى الْكِتَابَ وَجَعَلْنَا مَعَهُ أَخَاهُ هَارُونَ وَزِيرًا"
  },
  {
    "id": 62,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 512.0,
    "end": 522.17,
    "text": "فَقُلْنَا اذْهَبَا إِلَى الْقَوْمِ الَّذِينَ كَذَّبُوا بِآيَاتِنَا فَدَمَّرْنَاهُمْ تَدْمِيرًا"
  },
  {
    "id": 63,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 523.4,
    "end": 532.83,
    "text": "وَقَوْمَ نُوحٍ لَمَّا كَذَّبُوا الرُّسُلَ أَغْرَقْنَاهُمْ وَجَعَلْنَاهُمْ لِلنَّاسِ آيَةً"
  },
  {
    "id": 64,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 533.68,
    "end": 538.6,
    "text": "وَأَعْتَدْنَا لِلظَّالِمِينَ عَذَابًا أَلِيمًا"
  },
  {
    "id": 65,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 539.92,
    "end": 547.73,
    "text": "وَعَادًا وَثَمُودَ وَأَصْحَابَ الرَّسِّ وَقُرُونًا بَيْنَ ذَلِكَ كَثِيرًا"
  },
  {
    "id": 66,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 548.73,
    "end": 553.11,
    "text": "وَكُلًّا ضَرَبْنَا لَهُ الْأَمْثَالَ"
  },
  {
    "id": 67,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 553.84,
    "end": 557.54,
    "text": "وَكُلًّا تَبَّرْنَا تَتْبِيرًا"
  },
  {
    "id": 68,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 558.78,
    "end": 566.2,
    "text": "وَلَقَدْ أَتَوْا عَلَى الْقَرْيَةِ الَّتِي أُمْطِرَتْ مَطَرَ السَّوْءِ"
  },
  {
    "id": 69,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 567.12,
    "end": 570.01,
    "text": "أَفَلَمْ يَكُونُوا يَرَوْنَهَا"
  },
  {
    "id": 70,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 570.91,
    "end": 574.46,
    "text": "بَلْ كَانُوا لَا يَرْجُونَ نُشُورًا"
  },
  {
    "id": 71,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 575.75,
    "end": 584.69,
    "text": "وَإِذَا رَأَوْكَ إِنْ يَتَّخِذُونَكَ إِلَّا هُزُوًا أَهَذَا الَّذِي بَعَثَ اللَّهُ رَسُولًا"
  },
  {
    "id": 72,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 585.84,
    "end": 593.94,
    "text": "إِنْ كَادَ لَيُضِلُّنَا عَنْ آلِهَتِنَا لَوْلَا أَنْ صَبَرْنَا عَلَيْهَا"
  },
  {
    "id": 73,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 595.01,
    "end": 601.26,
    "text": "وَسَوْفَ يَعْلَمُونَ حِينَ يَرَوْنَ الْعَذَابَ مَنْ أَضَلُّ سَبِيلًا"
  },
  {
    "id": 74,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 602.25,
    "end": 610.38,
    "text": "أَا رَأَيْتَ مَنِ اتَّخَذَ إِلَهَهُ هَوَاهُ أَفَأَنْتَ تَكُونُ عَلَيْهِ وَكِيلًا"
  },
  {
    "id": 75,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 611.49,
    "end": 617.72,
    "text": "أَمْ تَحْسَبُ أَنَّ أَكْثَرَهُمْ يَسْمَعُونَ أَوْ يَعْقِلُونَ"
  },
  {
    "id": 76,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 618.66,
    "end": 624.33,
    "text": "إِنْ هُمْ إِلَّا كَالْأَنْعَامِ بَلْ هُمْ أَضَلُّ سَبِيلًا"
  },
  {
    "id": 77,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 625.77,
    "end": 638.66,
    "text": "أَلَمْ تَرَ إِلَى رَبِّكَ كَيْفَ مَدَّ الظِّلَّ وَلَوْ شَاءَ لَجَعَلَهُ سَاكِنًا ثُمَّ جَعَلْنَا الشَّمْسَ عَلَيْهِ دَلِيلًا"
  },
  {
    "id": 78,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 639.9,
    "end": 645.01,
    "text": "ثُمَّ قَبَضْنَاهُ إِلَيْنَا قَبْضًا يَسِيرًا"
  },
  {
    "id": 79,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 646.3,
    "end": 656.65,
    "text": "وَهُوَ الَّذِي جَعَلَ لَكُمُ اللَّيْلَ لِبَاسًا وَالنَّوْمَ سُبَاتًا وَجَعَلَ النَّهَارَ نُشُورًا"
  },
  {
    "id": 80,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 657.97,
    "end": 664.98,
    "text": "وَهُوَ الَّذِي أَرْسَلَ الرِّيَاحَ بُشْرًا بَيْنَ يَدَيْ رَحْمَتِهِ"
  },
  {
    "id": 81,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 665.95,
    "end": 672.53,
    "text": "وَأَنْزَلْنَا مِنَ السَّمَاءِ مَاءً طَهُورًا"
  },
  {
    "id": 82,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 673.58,
    "end": 685.32,
    "text": "لِنُحْيِيَ بِهِ بَلْدَةً مَيْتًا وَنُسْقِيَهُ مِمَّا خَلَقْنَا أَنْعَامًا وَأَنَاسِيَّ كَثِيرًا"
  },
  {
    "id": 83,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 686.44,
    "end": 696.09,
    "text": "وَلَقَدْ صَرَّفْنَاهُ بَيْنَهُمْ لِيَذَّكَّرُوا فَأَبَى أَكْثَرُ النَّاسِ إِلَّا كُفُورًا"
  },
  {
    "id": 84,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 697.43,
    "end": 703.03,
    "text": "وَلَوْ شِئْنَا لَبَعَثْنَا فِي كُلِّ قَرْيَةٍ نَذِيرًا"
  },
  {
    "id": 85,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 704.13,
    "end": 711.67,
    "text": "فَلَا تُطِعِ الْكَافِرِينَ وَجَاهِدْهُمْ بِهِ جِهَادًا كَبِيرًا"
  },
  {
    "id": 86,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 713.07,
    "end": 727.02,
    "text": "وَهُوَ الَّذِي مَرَجَ الْبَحْرَيْنِ هَذَا عَذْبٌ فُرَاتٌ وَهَذَا مِلْحٌ أُجَاجٌ وَجَعَلَ بَيْنَهُمَا بَرْزَخًا وَحِجْرًا"
  },
  {
    "id": 87,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 727.35,
    "end": 728.12,
    "text": "عُورًا"
  },
  {
    "id": 88,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 729.27,
    "end": 737.38,
    "text": "وَهُوَ الَّذِي خَلَقَ مِنَ الْمَاءِ بَشَرًا فَجَعَلَهُ نَسَبًا وَصِهْرًا"
  },
  {
    "id": 89,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 738.18,
    "end": 740.8,
    "text": "وَكَانَ رَبُّكَ قَدِيرًا"
  },
  {
    "id": 90,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 741.93,
    "end": 749.15,
    "text": "وَيَعْبُدُونَ مِنْ دُونِ اللَّهِ مَا لَا يَنْفَعُهُمْ وَلَا يَضُرُّهُمْ"
  },
  {
    "id": 91,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 750.07,
    "end": 754.57,
    "text": "وَكَانَ الْكَافِرُ عَلَى رَبِّهِ ظَهِيرًا"
  },
  {
    "id": 92,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 755.77,
    "end": 761.48,
    "text": "وَمَا أَرْسَلْنَاكَ إِلَّا مُبَشِّرًا وَنَذِيرًا"
  },
  {
    "id": 93,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 762.3,
    "end": 773.71,
    "text": "قُلْ مَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ إِلَّا مَنْ شَاءَ أَنْ يَتَّخِذَ إِلَى رَبِّهِ سَبِيلًا"
  },
  {
    "id": 94,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 774.9,
    "end": 780.8,
    "text": "وَتَوَكَّلْ عَلَى الْحَيِّ الَّذِي لَا يَمُوتُ وَسَبِّحْ بِحَمْدِهِ"
  },
  {
    "id": 95,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 781.74,
    "end": 786.44,
    "text": "وَكَفَى بِهِ بِذُنُوبِ عِبَادِهِ خَبِيرًا"
  },
  {
    "id": 96,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 787.44,
    "end": 798.18,
    "text": "الَّذِي خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ وَمَا بَيْنَهُمَا فِي سِتَّةِ أَيَّامٍ ثُمَّ اسْتَوَى عَلَى الْعَظِيمِ"
  },
  {
    "id": 97,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 799.32,
    "end": 803.12,
    "text": "الرَّحْمَنُ فَاسْأَلْ بِهِ خَبِيرًا"
  },
  {
    "id": 98,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 804.36,
    "end": 815.67,
    "text": "وَإِذَا قِيلَ لَهُمُ اسْجُدُوا لِلرَّحْمَنِ قَالُوا وَمَا الرَّحْمَنُ أَنَسْجُدُ لِمَا تَأْمُرُنَا وَزَادَهُمْ نُفُورًا"
  },
  {
    "id": 99,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 816.89,
    "end": 827.51,
    "text": "تَبَارَكَ الَّذِي جَعَلَ فِي السَّمَاءِ بُرُوجًا وَجَعَلَ فِيهَا سِرَاجًا وَقَمَرًا مُنِيرًا"
  },
  {
    "id": 100,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 828.64,
    "end": 839.11,
    "text": "وَهُوَ الَّذِي جَعَلَ اللَّيْلَ وَالنَّهَارَ خِلْفَةً لِمَنْ أَرَادَ أَنْ يَذَّكَّرَ أَوْ أَرَادَ شُكُورًا"
  },
  {
    "id": 101,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 840.44,
    "end": 851.82,
    "text": "وَعِبَادُ الرَّحْمَنِ الَّذِينَ يَمْشُونَ عَلَى الْأَرْضِ هَوْنًا وَإِذَا خَاطَبَهُمُ الْجَاهِلُونَ قَالُوا سَلَامًا"
  },
  {
    "id": 102,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 852.99,
    "end": 858.82,
    "text": "وَالَّذِينَ يَبِيتُونَ لِرَبِّهِمْ سُجَّدًا وَقِيَامًا"
  },
  {
    "id": 103,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 859.88,
    "end": 866.66,
    "text": "وَالَّذِينَ يَقُولُونَ رَبَّنَا اصْرِفْ عَنَّا عَذَابَ جَهَنَّمَ"
  },
  {
    "id": 104,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 867.67,
    "end": 871.43,
    "text": "إِنَّ عَذَابَهَا كَانَ غَرَامًا"
  },
  {
    "id": 105,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 872.75,
    "end": 878.22,
    "text": "إِنَّهَا سَاءَتْ مُسْتَقَرًّا وَمُقَامًا"
  },
  {
    "id": 106,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 879.24,
    "end": 889.17,
    "text": "وَالَّذِينَ إِذَا أَنْفَقُوا لَمْ يُسْرِفُوا وَلَمْ يَقْتُرُوا وَكَانَ بَيْنَ ذَلِكَ قَوَامًا"
  },
  {
    "id": 107,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 890.3,
    "end": 904.58,
    "text": "وَالَّذِينَ لَا يَدْعُونَ مَعَ اللَّهِ إِلَهًا آخَرَ وَلَا يَقْتُلُونَ النَّفْسَ الَّتِي حَرَّمَ اللَّهُ إِلَّا بِالْحَقِّ وَلَا يَزْنُونَ"
  },
  {
    "id": 108,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 905.6,
    "end": 909.56,
    "text": "وَمَنْ يَفْعَلْ ذَلِكَ يَلْقَ أَثَامًا"
  },
  {
    "id": 109,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 910.51,
    "end": 917.39,
    "text": "يُضَاعَفْ لَهُ الْعَذَابُ يَوْمَ الْقِيَامَةِ وَيَخْلُدْ فِيهِ مُهَانًا"
  },
  {
    "id": 110,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 918.49,
    "end": 931.58,
    "text": "إِلَّا مَنْ تَابَ وَآمَنَ وَعَمِلَ عَمَلًا صَالِحًا فَأُولَئِكَ يُبَدِّلُ اللَّهُ سَيِّئَاتِهِمْ حَسَنَاتٍ"
  },
  {
    "id": 111,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 932.82,
    "end": 936.39,
    "text": "وَكَانَ اللَّهُ غَفُورًا رَحِيمًا"
  },
  {
    "id": 112,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 937.62,
    "end": 945.86,
    "text": "وَمَنْ تَابَ وَعَمِلَ صَالِحًا فَإِنَّهُ يَتُوبُ إِلَى اللَّهِ مَتَابًا"
  },
  {
    "id": 113,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 947.13,
    "end": 954.46,
    "text": "وَالَّذِينَ لَا يَشْهَدُونَ الزُّورَ وَإِذَا مَرُّوا بِاللَّغْوِ مَرُّوا كِرَامًا"
  },
  {
    "id": 114,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 955.73,
    "end": 965.57,
    "text": "وَالَّذِينَ إِذَا ذُكِّرُوا بِآيَاتِ رَبِّهِمْ لَمْ يَخِرُّوا عَلَيْهَا صُمًّا وَعُمْيَانًا"
  },
  {
    "id": 115,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 966.58,
    "end": 980.36,
    "text": "وَالَّذِينَ يَقُولُونَ رَبَّنَا هَبْ لَنَا مِنْ أَزْوَاجِنَا وَذُرِّيَّاتِنَا قُرَّةَ أَعْيُنٍ وَاجْعَلْنَا لِلْمُتَّقِينَ إِمَامًا"
  },
  {
    "id": 116,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 981.51,
    "end": 987.11,
    "text": "أُولَئِكَ يُجْزَوْنَ الْغُرْفَةَ بِمَا صَبَرُوا وَيُلَمَ"
  },
  {
    "id": 117,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 987.43,
    "end": 991.53,
    "text": "قَالُوا لَفِيهَا تَحِيَّةً وَسَلَامًا"
  },
  {
    "id": 118,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 992.42,
    "end": 994.4,
    "text": "خَالِدِينَ فِيهَا"
  },
  {
    "id": 119,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 995.23,
    "end": 999.31,
    "text": "يَسُنَتْ مُسْتَقَرًّا وَمُقَامًا"
  },
  {
    "id": 120,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 1000.14,
    "end": 1005.89,
    "text": "قُلْ مَا يَعْبَأُ بِكُمْ رَبِّي لَوْلَا دُعَاؤُكُمْ"
  },
  {
    "id": 121,
    "sura_id": 25,
    "recitation_uuid": "f78ca788-3fd5-4e3e-85eb-63eacd0948ea",
    "start": 1006.8,
    "end": 1011.91,
    "text": "وَقَدْ كَذَّبْتُمْ فَسَوْفَ يَكُونُ لِزَامًا"
  }
]