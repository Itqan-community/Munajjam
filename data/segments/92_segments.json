[
  {
    "id": 2,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 5.86,
    "end": 8.27,
    "text": "وَاللَّيْلِ إِذَا يَغْشَى"
  },
  {
    "id": 3,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 9.16,
    "end": 12.22,
    "text": "وَالنَّهَارِ إِذَا تَجَلَّى"
  },
  {
    "id": 4,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 13.14,
    "end": 16.93,
    "text": "وَمَا خَلَقَ الذَّكَرَ وَالْأُنْثَى"
  },
  {
    "id": 5,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 17.9,
    "end": 20.77,
    "text": "إِنَّ سَعْيَكُمْ لَشَتَّى"
  },
  {
    "id": 6,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 21.92,
    "end": 25.35,
    "text": "فَأَمَّا مَنْ أَعْطَى وَاتَّقَى"
  },
  {
    "id": 7,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 26.14,
    "end": 28.33,
    "text": "وَصَدَّقَ بِالْحُسْنَى"
  },
  {
    "id": 8,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 29.21,
    "end": 32.29,
    "text": "فَسَنُيَسِّرُهُ لِلْيُسْرَى"
  },
  {
    "id": 9,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 33.66,
    "end": 37.53,
    "text": "وَأَمَّا مَنْ بَخِلَ وَاسْتَغْنَى"
  },
  {
    "id": 10,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 38.41,
    "end": 40.57,
    "text": "وَكَذَّبَ بِالْحُسْنَى"
  },
  {
    "id": 11,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 41.5,
    "end": 44.52,
    "text": "فَسَنُيَسِّرُهُ رِيبْعُسْرًا"
  },
  {
    "id": 12,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 45.62,
    "end": 51.17,
    "text": "وَمَا يُغْنِي عَنْهُ مَالُهُ إِذَا تَرَدَّى"
  },
  {
    "id": 13,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 52.2,
    "end": 55.07,
    "text": "إِنَّ عَلَيْنَا لَلْهُدَى"
  },
  {
    "id": 14,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 56.04,
    "end": 60.28,
    "text": "وَإِنَّ لَنَا لَلْآخِرَةَ وَالْأُولَى"
  },
  {
    "id": 15,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 61.26,
    "end": 65.84,
    "text": "فَأَنْذَرْتُكُمْ نَارًا تَلَظَّى"
  },
  {
    "id": 16,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 66.73,
    "end": 70.71,
    "text": "لَا يَصْلَاهَا إِلَّا الْأَشْقَى"
  },
  {
    "id": 17,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 71.66,
    "end": 74.67,
    "text": "الَّذِي كَذَّبَ وَتَوَلَّى"
  },
  {
    "id": 18,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 75.67,
    "end": 78.66,
    "text": "وَسَيُجَنَّبُهَا الْأَتْقَى"
  },
  {
    "id": 19,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 79.45,
    "end": 83.37,
    "text": "الَّذِي يُؤْتِي مَالَهُ يَتَزَكَّى"
  },
  {
    "id": 20,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 84.32,
    "end": 90.49,
    "text": "وَمَا لِأَحَدٍ عِنْدَهُ مِنْ نِعْمَةٍ تُجْزَى"
  },
  {
    "id": 21,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 91.26,
    "end": 96.1,
    "text": "إِلَّا ابْتِغَاءَ وَجْهِ رَبِّهِ الْأَعْلَى"
  },
  {
    "id": 22,
    "sura_id": 92,
    "recitation_uuid": "ecbe98d4-a375-421d-ac36-3c28728f2758",
    "start": 96.94,
    "end": 99.0,
    "text": "وَلَسَوْفَ يَرْضَى"
  }
]