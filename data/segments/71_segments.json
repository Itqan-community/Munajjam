[
  {
    "id": 2,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 5.97,
    "end": 20.46,
    "text": "إِنَّا أَرْسَلْنَا نُوحًا إِلَى قَوْمِهِ أَنْ أَنْذِرْ قَوْمَكَ مِنْ قَبْلِ أَنْ يَأْتِيَهُمْ عَذَابٌ أَلِيمٌ"
  },
  {
    "id": 3,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 21.67,
    "end": 27.5,
    "text": "قَالَ يَا قَوْمِ إِنِّي لَكُمْ نَذِيرٌ مُبِينٌ"
  },
  {
    "id": 4,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 28.29,
    "end": 33.19,
    "text": "أَنِ اعْبُدُوا اللَّهَ وَاتَّقُوهُ وَأَطِيعُونِ"
  },
  {
    "id": 5,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 34.05,
    "end": 45.09,
    "text": "لِيَغْفِرْ لَكُمْ مِنْ ذُنُوبِكُمْ وَيُؤَخِّرْكُمْ إِلَى أَجَلٍ مُسَمًّى"
  },
  {
    "id": 6,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 46.2,
    "end": 51.87,
    "text": "إِنَّ أَجَلَ اللَّهِ إِذَا جَاءَ لَا يُؤَخَّرُ"
  },
  {
    "id": 7,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 52.75,
    "end": 56.64,
    "text": "لَوْ كُنْتُمْ تَعْلَمُونَ"
  },
  {
    "id": 8,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 57.92,
    "end": 64.12,
    "text": "قَالَ رَبِّ إِنِّي دَعَوْتُ قَوْمِي لَيْلًا وَنَهَارًا"
  },
  {
    "id": 9,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 65.04,
    "end": 70.59,
    "text": "فَلَمْ يَزِدْهُمْ دُعَائِي إِلَّا فِرَارًا"
  },
  {
    "id": 10,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 71.7,
    "end": 85.34,
    "text": "وَإِنِّي كُلَّمَا دَعَوْتُهُمْ لِتَغْفِرَ لَهُمْ جَعَلُوا أَصَابِعَهُمْ فِي آذَانِهِمْ وَاسْتَغْشَوْا ثِيَابَهُمْ وَأَصَرُّوا"
  },
  {
    "id": 11,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 86.64,
    "end": 92.73,
    "text": "وَاسْتَغْشَوْا ثِيَابَهُمْ وَأَصَرُّوا وَاسْتَكْبَرُوا اسْتِكْبَارًا"
  },
  {
    "id": 12,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 93.98,
    "end": 98.27,
    "text": "ثُمَّ إِنِّي دَعَوْتُهُمْ جِهَارًا"
  },
  {
    "id": 13,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 99.06,
    "end": 106.93,
    "text": "ثُمَّ إِنِّي أَعْلَنْتُ لَهُمْ وَأَسْرَرْتُ لَهُمْ إِسْرَارًا"
  },
  {
    "id": 14,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 108.04,
    "end": 114.17,
    "text": "فَقُلْتُ اسْتَغْفِرُوا رَبَّكُمْ إِنَّهُ كَانَ غَفَّارًا"
  },
  {
    "id": 15,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 115.3,
    "end": 120.34,
    "text": "يُرْسِلِ السَّمَاءَ عَلَيْكُمْ مِدْرَارًا"
  },
  {
    "id": 16,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 121.32,
    "end": 132.05,
    "text": "وَيُمْدِدْكُمْ بِأَمْوَالٍ وَبَنِينَ وَيَجْعَلْ لَكُمْ جَنَّاتٍ وَيَجْعَلْ لَكُمْ أَنْهَارًا"
  },
  {
    "id": 17,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 133.24,
    "end": 137.88,
    "text": "مَا لَكُمْ لَا تَرْجُونَ لِلَّهِ وَقَارًا"
  },
  {
    "id": 18,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 138.77,
    "end": 141.74,
    "text": "وَقَدْ خَلَقَكُمْ أَطْوَارًا"
  },
  {
    "id": 19,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 142.88,
    "end": 149.91,
    "text": "أَلَمْ تَرَوْا كَيْفَ خَلَقَ اللَّهُ سَبْعَ سَمَاوَاتٍ طِبَاقًا"
  },
  {
    "id": 20,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 151.09,
    "end": 157.76,
    "text": "وَجَعَلَ الْقَمَرَ فِيهِنَّ نُورًا وَجَعَلَ الشَّمْسَ سِرَاجًا"
  },
  {
    "id": 21,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 158.81,
    "end": 163.97,
    "text": "وَاللَّهُ أَنْبَتَكُمْ مِنَ الْأَرْضِ نَبَاتًا"
  },
  {
    "id": 22,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 164.82,
    "end": 170.46,
    "text": "ثُمَّ يُعِيدُكُمْ فِيهَا وَيُخْرِجُكُمْ إِخْرَاجًا"
  },
  {
    "id": 23,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 171.55,
    "end": 175.61,
    "text": "وَاللَّهُ جَعَلَ لَكُمُ الْأَرْضَ بِسَاطًا"
  },
  {
    "id": 24,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 176.38,
    "end": 180.82,
    "text": "لِتَسْلُكُوا مِنْهَا سُبُلًا فِجَاجًا"
  },
  {
    "id": 25,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 182.09,
    "end": 193.51,
    "text": "قَالَ نُوحٌ رَبِّ إِنَّهُمْ عَصَوْنِي وَاتَّبَعُوا مَنْ لَمْ يَزِدْهُ مَالُهُ وَوَلَدُهُ إِلَّا خَسَارًا"
  },
  {
    "id": 26,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 194.61,
    "end": 198.02,
    "text": "وَمَكَرُوا مَكْرًا كُبَّارًا"
  },
  {
    "id": 27,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 198.88,
    "end": 211.22,
    "text": "وَقَالُوا لَا تَذَرُنَّ آلِهَتَكُمْ وَلَا تَذَرُنَّ وَدًّا وَلَا سُوَاعًا وَلَا يَغُوثَ وَيَعُوقَ وَنَسْرًا"
  },
  {
    "id": 28,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 212.22,
    "end": 214.9,
    "text": "وَقَدْ أَضَلُّوا كَثِيرًا"
  },
  {
    "id": 29,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 215.57,
    "end": 220.05,
    "text": "وَلَا تَزِدِ الظَّالِمِينَ إِلَّا ضَلَالًا"
  },
  {
    "id": 30,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 221.12,
    "end": 233.87,
    "text": "مِمَّا خَطِيئَاتِهِمْ أُغْرِقُوا فَأُدْخِلُوا نَارًا فَلَمْ يَجِدُوا لَهُمْ مِنْ دُونِ اللَّهِ أَنْصَارًا"
  },
  {
    "id": 31,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 235.37,
    "end": 242.49,
    "text": "وَقَالَ نُوحٌ رَبِّ لَا تَذَرْ عَلَى الْأَرْضِ مِنَ الْكَافِرِينَ دَيَّارًا"
  },
  {
    "id": 32,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 243.71,
    "end": 252.88,
    "text": "إِنَّكَ إِنْ تَذَرْهُمْ يُضِلُّوا عِبَادَكَ وَلَا يَلِدُوا إِلَّا فَاجِرًا"
  },
  {
    "id": 33,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 253.23,
    "end": 254.01,
    "text": "تَرْهَارًا"
  },
  {
    "id": 34,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 255.13,
    "end": 266.09,
    "text": "رَبِّ اغْفِرْ لِي وَلِوَالِدَيَّ وَلِمَنْ دَخَلَ بَيْتِيَ مُؤْمِنًا وَلِلْمُؤْمِنِينَ وَالْمُؤْمِنَاتِ"
  },
  {
    "id": 35,
    "sura_id": 71,
    "recitation_uuid": "9dda141a-adad-4a6b-9aaf-bfda46a45c39",
    "start": 267.01,
    "end": 271.74,
    "text": "وَلَا تَزِدِ الظَّالِمِينَ إِلَّا تَبَارًا"
  }
]