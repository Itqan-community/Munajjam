    "start": 5.63,
    "end": 6.97,
    "text": "مَا الْقَارِعَةُ"
  },
  {
    "id": 3,
    "sura_id": 101,
    "recitation_uuid": "be0e0e95-0b5b-483a-ab1a-ae59e14804c2",
    "start": 7.79,
    "end": 9.24,
    "text": "مَا الْقَارِعَةُ"
  },
  {
    "id": 4,
    "sura_id": 101,
    "recitation_uuid": "be0e0e95-0b5b-483a-ab1a-ae59e14804c2",
    "start": 9.98,
    "end": 13.69,
    "text": "وَمَا أَدْرَاكَ مَا الْقَارِعَةُ"
  },
  {
    "id": 5,
    "sura_id": 101,
    "recitation_uuid": "be0e0e95-0b5b-483a-ab1a-ae59e14804c2",
    "start": 14.78,
    "end": 20.18,
    "text": "يَوْمَ يَكُونُ النَّاسُ كَالْفَرَاشِ الْمَبْثُوثِ"
  },
  {
    "id": 6,
    "sura_id": 101,
    "recitation_uuid": "be0e0e95-0b5b-483a-ab1a-ae59e14804c2",
    "start": 21.16,
    "end": 26.87,
    "text": "وَتَكُونُ الْجِبَالُ كَالْعِهْنِ الْمَنْفُوشِ"
  },
  {
    "id": 7,
    "sura_id": 101,
    "recitation_uuid": "be0e0e95-0b5b-483a-ab1a-ae59e14804c2",
    "start": 27.8,
    "end": 31.81,
    "text": "فَأَمَّا مَنْ ثَقُلَتْ مَوَازِينُهُ"
  },
  {
    "id": 8,
    "sura_id": 101,
    "recitation_uuid": "be0e0e95-0b5b-483a-ab1a-ae59e14804c2",
    "start": 32.51,
    "end": 35.34,
    "text": "فَهُوَ فِي عِيشَةٍ رَاضِيَةٍ"
  },
  {
    "id": 9,
    "sura_id": 101,
    "recitation_uuid": "be0e0e95-0b5b-483a-ab1a-ae59e14804c2",
    "start": 36.26,
    "end": 40.11,
    "text": "وَأَمَّا مَنْ خَفَّتْ مَوَازِينُهُ"
  },
  {
    "id": 10,
    "sura_id": 101,
    "recitation_uuid": "be0e0e95-0b5b-483a-ab1a-ae59e14804c2",
    "start": 40.8,
    "end": 43.36,
    "text": "فَأُمُّهُ هَاوِيَةٌ"
  },
  {
    "id": 11,
    "sura_id": 101,
    "recitation_uuid": "be0e0e95-0b5b-483a-ab1a-ae59e14804c2",
    "start": 44.23,
    "end": 47.7,
    "text": "وَمَا أَدْرَاكَ مَا هِيَهْ"
  },
  {
    "id": 12,
    "sura_id": 101,
    "recitation_uuid": "be0e0e95-0b5b-483a-ab1a-ae59e14804c2",
    "start": 48.54,
    "end": 50.49,
    "text": "نَارٌ حَامِيَةٌ"
  }
]