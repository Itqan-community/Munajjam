[
  {
    "id": 2,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 5.78,
    "end": 7.92,
    "text": "وَالطُّورِ"
  },
  {
    "id": 3,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 8.72,
    "end": 12.65,
    "text": "وَكِتَابٍ مَسْطُورٍ"
  },
  {
    "id": 4,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 13.54,
    "end": 18.21,
    "text": "فِي رَقٍّ مَنْشُورٍ"
  },
  {
    "id": 5,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 19.08,
    "end": 21.98,
    "text": "وَالْبَيْتِ الْمَعْمُورِ"
  },
  {
    "id": 6,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 22.75,
    "end": 25.87,
    "text": "وَالسَّقْفِ الْمَرْفُوعِ"
  },
  {
    "id": 7,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 26.66,
    "end": 29.84,
    "text": "وَالْبَحْرِ الْمَسْجُورِ"
  },
  {
    "id": 8,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 30.69,
    "end": 33.9,
    "text": "إِنَّ عَذَابَ رَبِّكَ لَوَاقِعٌ"
  },
  {
    "id": 9,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 34.74,
    "end": 37.31,
    "text": "مَا لَهُ مِنْ دَافِعٍ"
  },
  {
    "id": 10,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 38.25,
    "end": 42.14,
    "text": "يَوْمَ تَمُورُ السَّمَاءُ مَوْرًا"
  },
  {
    "id": 11,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 42.88,
    "end": 45.76,
    "text": "وَتَسِيرُ الْجِبَالُ سَيْرًا"
  },
  {
    "id": 12,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 46.52,
    "end": 51.65,
    "text": "فَوَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 13,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 52.54,
    "end": 57.92,
    "text": "الَّذِينَ هُمْ فِي خَوْضٍ يَلْعَبُونَ"
  },
  {
    "id": 14,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 58.62,
    "end": 63.63,
    "text": "يَوْمَ يُدَعُّونَ إِلَى نَارِ جَهَنَّمَ دَعًّا"
  },
  {
    "id": 15,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 64.44,
    "end": 71.55,
    "text": "هَذِهِ النَّارُ الَّتِي كُنْتُمْ بِهَا تُكَذِّبُونَ"
  },
  {
    "id": 16,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 72.62,
    "end": 78.99,
    "text": "أَفَسِحْرٌ هَذَا أَمْ أَنْتُمْ لَا تُبْصِرُونَ"
  },
  {
    "id": 17,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 80.1,
    "end": 87.45,
    "text": "اصْلَوْهَا فَاصْبِرُوا أَوْ لَا تَصْبِرُوا سَوَاءٌ عَلَيْكُمْ"
  },
  {
    "id": 18,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 88.22,
    "end": 95.03,
    "text": "إِنَّمَا تُجْزَوْنَ مَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 19,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 96.05,
    "end": 102.59,
    "text": "إِنَّ الْمُتَّقِينَ فِي جَنَّاتٍ وَنَعِيمٍ"
  },
  {
    "id": 20,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 103.6,
    "end": 112.82,
    "text": "فَاكِهِينَ بِمَا آتَاهُمْ رَبُّهُمْ وَوَقَاهُمْ رَبُّهُمْ عَذَابَ الْجَحِيمِ"
  },
  {
    "id": 21,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 113.81,
    "end": 121.78,
    "text": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 22,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 122.74,
    "end": 126.75,
    "text": "مُتَّكِئِينَ عَلَى سُرُرٍ مَصْفُوفَةٍ"
  },
  {
    "id": 23,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 127.52,
    "end": 131.97,
    "text": "وَزَوَّجْنَاهُمْ بِحُورٍ عِينٍ"
  },
  {
    "id": 24,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 132.96,
    "end": 149.94,
    "text": "وَالَّذِينَ آمَنُوا وَاتَّبَعَتْهُمْ ذُرِّيَّتُهُمْ بِإِيمَانٍ أَلْحَقْنَا بِهِمْ ذُرِّيَّتَهُمْ وَمَا أَلَتْنَاهُمْ مِنْ عَمَلِهِمْ مِنْ شَيْءٍ"
  },
  {
    "id": 25,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 151.31,
    "end": 156.26,
    "text": "كُلُّ امْرِئٍ بِمَا كَسَبَ رَهِينٌ"
  },
  {
    "id": 26,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 157.68,
    "end": 165.5,
    "text": "وَأَمْدَدْنَاهُمْ بِفَاكِهَةٍ وَلَحْمٍ مِمَّا يَشْتَهُونَ"
  },
  {
    "id": 27,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 166.4,
    "end": 174.2,
    "text": "يَتَنَازَعُونَ فِيهَا كَأْسًا لَا لَغْوٌ فِيهَا وَلَا تَأْثِيمٌ"
  },
  {
    "id": 28,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 175.34,
    "end": 183.28,
    "text": "وَيَطُوفُ عَلَيْهِمْ غِلْمَانٌ لَهُمْ كَأَنَّهُمْ لُؤْلُؤٌ مَكْنُونٌ"
  },
  {
    "id": 29,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 184.24,
    "end": 191.69,
    "text": "وَأَقْبَلَ بَعْضُهُمْ عَلَى بَعْضٍ يَتَسَاءَلُونَ"
  },
  {
    "id": 30,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 192.54,
    "end": 201.06,
    "text": "قَالُوا إِنَّا كُنَّا قَبْلُ فِي أَهْلِنَا مُشْفِقِينَ"
  },
  {
    "id": 31,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 201.88,
    "end": 208.46,
    "text": "فَمَنَّ اللَّهُ عَلَيْنَا وَوَقَانَا عَذَابَ السَّمُومِ"
  },
  {
    "id": 32,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 209.43,
    "end": 214.38,
    "text": "إِنَّا كُنَّا مِنْ قَبْلُ نَدْعُوهُ"
  },
  {
    "id": 33,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 215.03,
    "end": 219.38,
    "text": "إِنَّهُ هُوَ الْبَرُّ الرَّحِيمُ"
  },
  {
    "id": 34,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 220.55,
    "end": 229.16,
    "text": "فَذَكِّرْ فَمَا أَنْتَ بِنِعْمَتِ رَبِّكَ بِكَاهِنٍ وَلَا مَجْنُونٍ"
  },
  {
    "id": 35,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 230.19,
    "end": 236.77,
    "text": "أَمْ يَقُولُونَ شَاعِرٌ نَتَرَبَّصُ بِهِ رَيْبَ الْمَنُونِ"
  },
  {
    "id": 36,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 237.62,
    "end": 244.42,
    "text": "قُلْ تَرَبَّصُوا فَإِنِّي مَعَكُمْ مِنَ الْمُتَرَبِّصِينَ"
  },
  {
    "id": 37,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 245.95,
    "end": 250.07,
    "text": "أَمْ تَأْمُرُهُمْ أَحْلَامُهُمْ بِهَذَا"
  },
  {
    "id": 38,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 250.94,
    "end": 254.44,
    "text": "أَمْ هُمْ قَوْمٌ طَاغُونَ"
  },
  {
    "id": 39,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 255.35,
    "end": 257.98,
    "text": "أَمْ يَقُولُونَ تَقَوَّلَهُ"
  },
  {
    "id": 40,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 258.73,
    "end": 261.15,
    "text": "كَلَّا يُؤْمِنُونَ"
  },
  {
    "id": 41,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 262.0,
    "end": 270.01,
    "text": "فَلْيَأْتُوا بِحَدِيثٍ مِثْلِهِ إِنْ كَانُوا صَادِقِينَ"
  },
  {
    "id": 42,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 271.18,
    "end": 276.72,
    "text": "أَمْ خُلِقُوا مِنْ غَيْرِ شَيْءٍ أَمْ هُمُ الْخَالِقُونَ"
  },
  {
    "id": 43,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 277.45,
    "end": 280.59,
    "text": "أَمْ خَلَقُوا السَّمَاوَاتِ وَالْأَرْضَ"
  },
  {
    "id": 44,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 281.39,
    "end": 283.91,
    "text": "كَلَّا يُوقِنُونَ"
  },
  {
    "id": 45,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 284.66,
    "end": 292.71,
    "text": "أَمْ عِنْدَهُمْ خَزَائِنُ رَبِّكَ أَمْ هُمُ الْمُصَيْطِرُونَ"
  },
  {
    "id": 46,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 293.83,
    "end": 298.51,
    "text": "أَمْ لَهُمْ سُلَّمٌ يَسْتَمِعُونَ فِيهِ"
  },
  {
    "id": 47,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 299.17,
    "end": 304.96,
    "text": "فَلْيَأْتِ مُسْتَمِعُهُمْ بِسُلْطَانٍ مُبِينٍ"
  },
  {
    "id": 48,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 306.02,
    "end": 309.96,
    "text": "أَمْ لَهُ الْبَنَاتُ وَلَكُمُ الْبَنُونَ"
  },
  {
    "id": 49,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 310.73,
    "end": 318.35,
    "text": "أَمْ تَسْأَلُهُمْ أَجْرًا فَهُمْ مِنْ مَغْرَمٍ مُثْقَلُونَ"
  },
  {
    "id": 50,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 319.24,
    "end": 324.26,
    "text": "أَمْ عِنْدَهُمُ الْغَيْبُ فَهُمْ يَكْتُبُونَ"
  },
  {
    "id": 51,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 324.92,
    "end": 327.33,
    "text": "أَمْ يُرِيدُونَ كَيْدًا"
  },
  {
    "id": 52,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 328.12,
    "end": 332.55,
    "text": "الَّذِينَ كَفَرُوا هُمُ الْمَكِيدُونَ"
  },
  {
    "id": 53,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 333.48,
    "end": 337.17,
    "text": "أَمْ لَهُمْ إِلَهٌ غَيْرُ اللَّهِ"
  },
  {
    "id": 54,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 337.92,
    "end": 342.5,
    "text": "سُبْحَانَ اللَّهِ عَمَّا يُشْرِكُونَ"
  },
  {
    "id": 55,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 343.64,
    "end": 353.24,
    "text": "وَإِنْ يَرَوْا كِسْفًا مِنَ السَّمَاءِ سَاقِطًا يَقُولُوا سَحَابٌ مَرْكُومٌ"
  },
  {
    "id": 56,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 354.06,
    "end": 361.08,
    "text": "فَذَرْهُمْ حَتَّى يُلَاقُوا يَوْمَهُمُ الَّذِي فِيهِ يُصْعَقُونَ"
  },
  {
    "id": 57,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 362.1,
    "end": 370.25,
    "text": "يَوْمَ لَا يُغْنِي عَنْهُمْ كَيْدُهُمْ شَيْئًا وَلَا هُمْ يُنْصَرُونَ"
  },
  {
    "id": 58,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 371.23,
    "end": 381.33,
    "text": "وَإِنَّ لِلَّذِينَ ظَلَمُوا عَذَابًا دُونَ ذَلِكَ وَلَكِنَّ أَكْثَرَهُمْ لَا يَعْلَمُونَ"
  },
  {
    "id": 59,
    "sura_id": 52,
    "recitation_uuid": "b89aa1e9-e578-4dd6-b200-706e094c7dd2",
    "start": 382.22,
    "end": 387.04,
    "text": "وَاصْبِرْ لِحُكْمِ رَبِّكَ فَإِنَّكَ بِأَعْيُنِنَا"
  },
  {
    "id": 60,
    "sura_id": 52,
