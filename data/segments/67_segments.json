[
  {
    "id": 2,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 5.62,
    "end": 13.24,
    "text": "تَبَارَكَ الَّذِي بِيَدِهِ الْمُلْكُ وَهُوَ عَلَى كُلِّ شَيْءٍ قَدِيرٌ"
  },
  {
    "id": 3,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 14.42,
    "end": 21.38,
    "text": "الَّذِي خَلَقَ الْمَوْتَ وَالْحَيَاةَ لِيَبْلُوَكُمْ أَيُّكُمْ أَحْسَنُ عَمَلًا"
  },
  {
    "id": 4,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 22.59,
    "end": 25.57,
    "text": "وَهُوَ الْعَزِيزُ الْغَفُورُ"
  },
  {
    "id": 5,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 26.83,
    "end": 31.75,
    "text": "الَّذِي خَلَقَ سَبْعَ سَمَاوَاتٍ طِبَاقًا"
  },
  {
    "id": 6,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 32.92,
    "end": 37.56,
    "text": "مَا تَرَى فِي خَلْقِ الرَّحْمَنِ مِنْ تَفَاوُرٍ"
  },
  {
    "id": 7,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 38.71,
    "end": 43.24,
    "text": "تَرْجِعِ الْبَصَرَ هَلْ تَرَى مِنْ فُطُورٍ"
  },
  {
    "id": 8,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 44.45,
    "end": 54.32,
    "text": "ثُمَّ ارْجِعِ الْبَصَرَ كَرَّتَيْنِ يَنْقَلِبْ إِلَيْكَ الْبَصَرُ خَاسِئًا وَهُوَ حَسِيرٌ"
  },
  {
    "id": 9,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 55.37,
    "end": 65.53,
    "text": "وَلَقَدْ زَيَّنَّا السَّمَاءَ الدُّنْيَا بِمَصَابِيحَ وَجَعَلْنَاهَا رُجُومًا لِلشَّيَاطِينِ"
  },
  {
    "id": 10,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 66.53,
    "end": 70.67,
    "text": "وَأَعْتَدْنَا لَهُمْ عَذَابَ السَّعِيرِ"
  },
  {
    "id": 11,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 71.7,
    "end": 76.99,
    "text": "وَلِلَّذِينَ كَفَرُوا بِرَبِّهِمْ عَذَابُ جَهَنَّمَ"
  },
  {
    "id": 12,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 77.76,
    "end": 80.08,
    "text": "وَبِئْسَ الْمَصِيرُ"
  },
  {
    "id": 13,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 81.1,
    "end": 88.39,
    "text": "إِذَا أُلْقُوا فِيهَا سَمِعُوا لَهَا شَهِيقًا وَهِيَ تَفُورُ"
  },
  {
    "id": 14,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 89.25,
    "end": 92.83,
    "text": "تَكَادُ تَمَيَّزُ مِنَ الْغَيْظِ"
  },
  {
    "id": 15,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 93.83,
    "end": 103.94,
    "text": "كُلَّمَا أُلْقِيَ فِيهَا فَوْجٌ سَأَلَهُمْ خَزَنَتُهَا أَلَمْ يَأْتِكُمْ نَذِيرٌ"
  },
  {
    "id": 16,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 104.85,
    "end": 121.62,
    "text": "قَالُوا بَلَى قَدْ جَاءَنَا نَذِيرٌ فَكَذَّبْنَا وَقُلْنَا مَا نَزَّلَ اللَّهُ مِنْ شَيْءٍ إِنْ أَنْتُمْ إِلَّا فِي ضَلَالٍ كَبِيرٍ"
  },
  {
    "id": 17,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 122.97,
    "end": 132.42,
    "text": "وَقَالُوا لَوْ كُنَّا نَسْمَعُ أَوْ نَعْقِلُ مَا كُنَّا فِي أَصْحَابِ السَّعِيرِ"
  },
  {
    "id": 18,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 133.38,
    "end": 139.96,
    "text": "فَاعْتَرَفُوا بِذَنْبِهِمْ فَسُحْقًا لِأَصْحَابِ السِّعِيرِ"
  },
  {
    "id": 19,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 141.45,
    "end": 151.83,
    "text": "إِنَّ الَّذِينَ يَخْشَوْنَ رَبَّهُمْ بِالْغَيْبِ لَهُمْ مَغْفِرَةٌ وَأَجْرٌ كَبِيرٌ"
  },
  {
    "id": 20,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 153.23,
    "end": 156.66,
    "text": "وَأَسِرُّوا قَوْلَكُمْ أَوِ اجْهَرُوا بِهِ"
  },
  {
    "id": 21,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 157.45,
    "end": 162.34,
    "text": "إِنَّهُ عَلِيمٌ بِذَاتِ الصُّدُورِ"
  },
  {
    "id": 22,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 163.51,
    "end": 169.02,
    "text": "أَلَا يَعْلَمُ مَنْ خَلَقَ وَهُوَ اللَّطِيفُ الْخَبِيرُ"
  },
  {
    "id": 23,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 170.09,
    "end": 178.84,
    "text": "هُوَ الَّذِي جَعَلَ لَكُمُ الْأَرْضَ ذَلُولًا فَامْشُوا فِي مَنَاكِبِهَا وَكُلُوا مِنْ رِزْقِهِ"
  },
  {
    "id": 24,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 179.68,
    "end": 182.78,
    "text": "وَإِلَيْهِ النُّشُورُ"
  },
  {
    "id": 25,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 183.98,
    "end": 193.88,
    "text": "أَأَمِنْتُمْ مَنْ فِي السَّمَاءِ أَنْ يَخْسِفَ بِكُمُ الْأَرْضَ فَإِذَا هِيَ تَمُورُ"
  },
  {
    "id": 26,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 194.99,
    "end": 203.29,
    "text": "أَمْ أَمِنْتُمْ مَنْ فِي السَّمَاءِ أَنْ يُرْسِلَ عَلَيْكُمْ حَاصِبًا"
  },
  {
    "id": 27,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 204.22,
    "end": 207.88,
    "text": "فَسَتَعْلَمُونَ كَيْفَ نَذِيرِ"
  },
  {
    "id": 28,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 209.34,
    "end": 216.61,
    "text": "وَلَقَدْ كَذَّبَ الَّذِينَ مِنْ قَبْلِهِمْ فَكَيْفَ كَانَ نَكِيرِ"
  },
  {
    "id": 29,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 217.95,
    "end": 225.56,
    "text": "أَوَلَمْ يَرَوْا إِلَى الطَّيْرِ فَوْقَهُمْ صَافَّاتٍ وَيَقْبِضْنَ"
  },
  {
    "id": 30,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 226.66,
    "end": 231.01,
    "text": "مَا يُمْسِكُهُنَّ إِلَّا الرَّحْمَنُ"
  },
  {
    "id": 31,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 231.77,
    "end": 236.94,
    "text": "إِنَّهُ بِكُلِّ شَيْءٍ بَصِيرٌ"
  },
  {
    "id": 32,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 238.38,
    "end": 247.97,
    "text": "أَمَّنْ هَذَا الَّذِي هُوَ جُنْدٌ لَكُمْ يَنْصُرُكُمْ مِنْ دُونِ الرَّحْمَنِ"
  },
  {
    "id": 33,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 248.94,
    "end": 253.17,
    "text": "إِنِ الْكَافِرُونَ إِلَّا فِي غُرُورٍ"
  },
  {
    "id": 34,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 254.18,
    "end": 259.57,
    "text": "أَمَّنْ هَذَا الَّذِي يَرْزُقُكُمْ إِنْ أَمْسَكَ رِزْقَهُ"
  },
  {
    "id": 35,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 260.46,
    "end": 265.37,
    "text": "بَلْ نَجُّوا فِي عُتُوٍّ وَنُفُورٍ"
  },
  {
    "id": 36,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 266.95,
    "end": 281.11,
    "text": "أَفَمَنْ يَمْشِي مُكِبًّا عَلَى وَجْهِهِ أَهْدَى أَمَّنْ يَمْشِي سَوِيًّا عَلَى صِرَاطٍ مُسْتَقِيمٍ"
  },
  {
    "id": 37,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 282.34,
    "end": 290.54,
    "text": "قُلْ هُوَ الَّذِي أَنْشَأَكُمْ وَجَعَلَ لَكُمُ السَّمْعَ وَالْأَبْصَارَ وَالْأَفْئِدَةَ"
  },
  {
    "id": 38,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 291.69,
    "end": 295.46,
    "text": "وَلِيلًا مَا تَشْكُرُونَ"
  },
  {
    "id": 39,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 296.74,
    "end": 302.9,
    "text": "قُلْ هُوَ الَّذِي ذَرَأَكُمْ فِي الْأَرْضِ وَإِلَيْهِ تُحْشَرُونَ"
  },
  {
    "id": 40,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 303.81,
    "end": 311.19,
    "text": "وَيَقُولُونَ مَتَى هَذَا الْوَعْدُ إِنْ كُنْتُمْ صَادِقِينَ"
  },
  {
    "id": 41,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 312.35,
    "end": 321.87,
    "text": "قُلْ إِنَّمَا الْعِلْمُ عِنْدَ اللَّهِ وَإِنَّمَا أَنَا نَذِيرٌ مُبِينٌ"
  },
  {
    "id": 42,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 323.46,
    "end": 337.36,
    "text": "فَلَمَّا رَأَوْهُ زُلْفَةً سِيئَتْ وُجُوهُ الَّذِينَ كَفَرُوا وَقِيلَ هَذَا الَّذِي كُنْتُمْ بِهِ تَدَّعُونَ"
  },
  {
    "id": 43,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 338.73,
    "end": 351.58,
    "text": "قُلْ أَرَأَيْتُمْ إِنْ أَهْلَكَنِيَ اللَّهُ وَمَنْ مَعِيَ أَوْ رَحِمَنَا فَمَنْ يُجِيرُ الْكَافِرِينَ مِنْ عَذَابٍ أَلِيمٍ"
  },
  {
    "id": 44,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 352.7,
    "end": 359.12,
    "text": "قُلْ هُوَ الرَّحْمَنُ آمَنَّا بِهِ وَعَلَيْهِ تَوَكَّلْنَى"
  },
  {
    "id": 45,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 359.95,
    "end": 365.79,
    "text": "فَسَتَعْلَمُونَ مَنْ هُوَ فِي ضَلَالٍ مُبِينٍ"
  },
  {
    "id": 46,
    "sura_id": 67,
    "recitation_uuid": "38773b37-57db-4e27-8731-cf6921d88dbb",
    "start": 367.04,
    "end": 379.76,
    "text": "قُلْ أَرَأَيْتُمْ إِنْ أَصْبَحَ مَاؤُكُمْ غَوْرًا فَمَنْ يَأْتِيكُمْ بِمَاءٍ مَعِينٍ"
  }
]