[
  {
    "id": 2,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 6.38,
    "end": 9.14,
    "text": "طسم"
  },
  {
    "id": 3,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 10.11,
    "end": 16.19,
    "text": "تِلْكَ آيَاتُ الْقُرْآنِ وَكِتَابٍ مُبِينٍ"
  },
  {
    "id": 4,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 17.16,
    "end": 21.3,
    "text": "هُدًى وَبُشْرَى لِلْمُؤْمِنِينَ"
  },
  {
    "id": 5,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 22.35,
    "end": 32.32,
    "text": "الَّذِينَ يُقِيمُونَ الصَّلَاةَ وَيُؤْتُونَ الزَّكَاةَ وَهُمْ بِالْآخِرَةِ هُمْ يُوقِنُونَ"
  },
  {
    "id": 6,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 33.35,
    "end": 44.04,
    "text": "إِنَّ الَّذِينَ لَا يُؤْمِنُونَ بِالْآخِرَةِ زَيَّنَّا لَهُمْ أَعْمَالَهُمْ فَهُمْ يَعْمَهُونَ"
  },
  {
    "id": 7,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 45.26,
    "end": 54.05,
    "text": "أُولَئِكَ الَّذِينَ لَهُمْ سُوءُ الْعَذَابِ وَهُمْ فِي الْآخِرَةِ هُمُ الْأَرْضُ"
  },
  {
    "id": 8,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 54.4,
    "end": 56.06,
    "text": "وَيَرُونَ"
  },
  {
    "id": 9,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 57.48,
    "end": 64.64,
    "text": "وَإِنَّكَ لَتُلَقَّى الْقُرْآنَ مِنْ لَدُنْ حَكِيمٍ عَلِيمٍ"
  },
  {
    "id": 10,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 65.8,
    "end": 77.13,
    "text": "إِذْ قَالَ مُوسَى لِأَهْلِهِ إِنِّي آنَسْتُ نَارًا سَآتِيكُمْ مِنْهَا بِخَبَرٍ"
  },
  {
    "id": 11,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 78.63,
    "end": 88.24,
    "text": "لَآتِيكُمْ مِنْهَا بِخَبَرٍ أَوْ آتِيكُمْ بِشِهَابٍ قَبَسٍ لَعَلَّكُمْ"
  },
  {
    "id": 12,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 88.56,
    "end": 90.02,
    "text": "قَالُونَ"
  },
  {
    "id": 13,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 91.41,
    "end": 105.08,
    "text": "فَلَمَّا جَاءَهَا نُودِيَ أَنْ بُورِكَ مَنْ فِي النَّارِ وَمَنْ حَوْلَهَا وَسُبْحَانَ اللَّهِ رَبِّ الْعَالَمِينَ"
  },
  {
    "id": 14,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 106.39,
    "end": 115.25,
    "text": "يَا مُوسَى إِنَّهُ أَنَا اللَّهُ الْعَزِيزُ الْحَكِيمُ"
  },
  {
    "id": 15,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 116.26,
    "end": 118.57,
    "text": "وَأَلْقِ عَصَاهُ"
  },
  {
    "id": 16,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 119.5,
    "end": 132.96,
    "text": "فَلَمَّا رَآهَا تَهْتَزُّ كَأَنَّهَا جَانٌّ وَلَّى مُدْبِرًا وَلَمْ يُعَقِّبْ"
  },
  {
    "id": 17,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 134.25,
    "end": 142.14,
    "text": "يَا مُوسَى لَا تَخَفْ إِنِّي لَا يَخَافُ لَدَيَّ الْمُرْسَلُونَ"
  },
  {
    "id": 18,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 143.08,
    "end": 155.07,
    "text": "إِلَّا مَنْ ظَلَمَ ثُمَّ بَدَّلَ حُسْنًا بَعْدَ سُوءٍ فَإِنِّي غَفُورٌ رَحِيمٌ"
  },
  {
    "id": 19,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 156.41,
    "end": 163.85,
    "text": "وَأَدْخِلْ يَدَكَ فِي جَيْبِكَ تَخْرُجْ بَيْضَاءَ مِنْ غَيْرِ سُوءٍ"
  },
  {
    "id": 20,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 164.67,
    "end": 169.1,
    "text": "فِي تِسْعِ آيَاتٍ إِلَى فِرْعَوْنَ وَقَوْمِهِ"
  },
  {
    "id": 21,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 169.87,
    "end": 175.39,
    "text": "إِنَّهُمْ كَانُوا قَوْمًا فَاسِقِينَ"
  },
  {
    "id": 22,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 176.7,
    "end": 187.04,
    "text": "فَلَمَّا جَاءَتْهُمْ آيَاتُنَا مُبْصِرَةً قَالُوا هَذَا سِحْرٌ مُبِينٌ"
  },
  {
    "id": 23,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 188.14,
    "end": 196.25,
    "text": "وَجَحَدُوا بِهَا وَاسْتَيْقَنَتْهَا أَنْفُسُهُمْ ظُلْمًا وَعُلُوًّا"
  },
  {
    "id": 24,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 197.2,
    "end": 200.92,
    "text": "فَانْظُرْ كَيْفَ كَانَ عَاقِبَةُ الْمُعْرِضِ"
  },
  {
    "id": 25,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 201.24,
    "end": 202.69,
    "text": "هِدِينَ"
  },
  {
    "id": 26,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 203.82,
    "end": 208.99,
    "text": "وَلَقَدْ آتَيْنَا دَاوُودَ وَسُلَيْمَانَ عِلْمًا"
  },
  {
    "id": 27,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 209.97,
    "end": 219.63,
    "text": "وَقَالَا الْحَمْدُ لِلَّهِ الَّذِي فَضَّلَنَا عَلَى كَثِيرٍ مِنْ عِبَادِهِ الْمُؤْمِنِينَ"
  },
  {
    "id": 28,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 220.9,
    "end": 224.23,
    "text": "وَوَرِثَ سُلَيْمَانُ دَاوُودَ"
  },
  {
    "id": 29,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 225.05,
    "end": 236.25,
    "text": "وَقَالَ يَا أَيُّهَا النَّاسُ عُلِّمْنَا مَنْطِقَ الطَّيْرِ وَأُوتِينَا مِنْ كُلِّ شَيْءٍ"
  },
  {
    "id": 30,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 237.26,
    "end": 242.1,
    "text": "إِنَّ هَذَا لَهُوَ الْفَضْلُ الْمُبِينُ"
  },
  {
    "id": 31,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 243.33,
    "end": 252.86,
    "text": "وَحُشِرَ لِسُلَيْمَانَ جُنُودُهُ مِنَ الْجِنِّ وَالْإِنْسِ وَالطَّيْرِ فَهُمْ يُوزَعُونَ"
  },
  {
    "id": 32,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 253.57,
    "end": 253.63,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 33,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 253.94,
    "end": 267.86,
    "text": "جَاءِ جَاءَتَوْا عَلَى وَادِ النَّمْلِ قَالَتْ نَمْلَةٌ يَا أَيُّهَا النَّمْلُ ادْخُلُوا مَسَاكِنَكُمْ"
  },
  {
    "id": 34,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 268.93,
    "end": 279.05,
    "text": "ادْخُلُوا مَسَاكِنَكُمْ لَا يَحْطِمَنَّكُمْ سُلَيْمَانُ وَجُنُودُهُ وَهُمْ لَا يَشْعُرُونَ"
  },
  {
    "id": 35,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 280.08,
    "end": 289.82,
    "text": "فَتَبَسَّمَ ضَاحِكًا مِنْ قَوْلِهَا وَقَالَ رَبِّ أَوْزِعْنِي أَنْ أَشْكُرَ نِعْمَتَهُ"
  },
  {
    "id": 36,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 291.06,
    "end": 306.52,
    "text": "وَقَالَ رَبِّ أَوْزِعْنِي أَنْ أَشْكُرَ نِعْمَتَكَ الَّتِي أَنْعَمْتَ عَلَيَّ وَعَلَى وَالِدَيَّ وَأَنْ أَعْمَلَ صَالِحًا تَرْضَاهُ"
  },
  {
    "id": 37,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 307.51,
    "end": 317.62,
    "text": "وَأَنْ أَعْمَلَ صَالِحًا تَرْضَاهُ وَأَدْخِلْنِي بِرَحْمَتِكَ فِي عِبَادِكَ الصَّالِحِينَ"
  },
  {
    "id": 38,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 319.05,
    "end": 329.83,
    "text": "وَتَفَقَّدَ الطَّيْرَ فَقَالَ مَا لِيَ لَا أَرَى الْهُدْهُدَ أَمْ كَانَ مِنَ الْغَائِبِينَ"
  },
  {
    "id": 39,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 330.88,
    "end": 345.03,
    "text": "لَأُعَذِّبَنَّهُ عَذَابًا شَدِيدًا أَوْ لَأَذْبَحَنَّهُ أَوْ لَيَأْتِيَنِّي بِسُلْطَانٍ مُبِينٍ"
  },
  {
    "id": 40,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 346.02,
    "end": 358.53,
    "text": "فَمَكَثَ غَيْرَ بَعِيدٍ فَقَالَ أَحَطْتُ بِمَا لَمْ تُحِطْ بِهِ وَجِئْتُكَ مِنْ سَبَإٍ بِنَبَإٍ يَقِينٍ"
  },
  {
    "id": 41,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 359.71,
    "end": 371.07,
    "text": "إِنِّي وَجَدْتُ امْرَأَةً تَمْلِكُهُمْ وَأُوتِيَتْ مِنْ كُلِّ شَيْءٍ وَلَهَا عَرْشٌ عَظِيمٌ"
  },
  {
    "id": 42,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 372.16,
    "end": 383.15,
    "text": "وَجَدْتُهَا وَقَوْمَهَا يَسْجُدُونَ لِلشَّمْسِ مِنْ دُونِ اللَّهِ وَزَيَّنَ لَهُمُ الشَّيْطَانُ أَعْمَالَهُمْ"
  },
  {
    "id": 43,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 384.48,
    "end": 393.94,
    "text": "وَزَيَّنَ لَهُمُ الشَّيْطَانُ أَعْمَالَهُمْ فَصَدَّهُمْ عَنِ السَّبِيلِ فَهُمْ لَا يَهْتَدُونَ"
  },
  {
    "id": 44,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 395.25,
    "end": 407.55,
    "text": "كَلَّا يَسْجُدُوا لِلَّهِ الَّذِي يُخْرِجُ الْخَبْءَ فِي السَّمَاوَاتِ وَالْأَرْضِ وَيَعْلَمُ مَا تُخْفُونَ وَمَا تُعْلِنُونَ"
  },
  {
    "id": 45,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 408.42,
    "end": 415.92,
    "text": "اللَّهُ لَا إِلَهَ إِلَّا هُوَ رَبُّ الْعَرْشِ الْعَظِيمِ"
  },
  {
    "id": 46,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 417.29,
    "end": 424.19,
    "text": "قَالَ سَنَنْظُرُ أَصَدَقْتَ أَمْ كُنْتَ مِنَ الْكَاذِبِينَ"
  },
  {
    "id": 47,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 425.25,
    "end": 436.11,
    "text": "اذْهَبْ بِكِتَابِي هَذَا فَأَلْقِهْ إِلَيْهِمْ ثُمَّ تَوَلَّ عَنْهُمْ فَانْظُرْ مَاذَا يَرْجِعُونَ"
  },
  {
    "id": 48,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 437.46,
    "end": 447.3,
    "text": "قَالَتْ يَا أَيُّهَا الْمَلَأُ إِنِّي أُلْقِيَ إِلَيَّ كِتَابٌ كَرِيمٌ"
  },
  {
    "id": 50,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 457.96,
    "end": 463.25,
    "text": "أَلَّا تَعْلُوا عَلَيَّ وَأْتُونِي مُسْلِمِينَ"
  },
  {
    "id": 51,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 464.51,
    "end": 477.74,
    "text": "قَالَتْ يَا أَيُّهَا الْمَلَأُ أَفْتُونِي فِي أَمْرِي مَا كُنْتُ قَاطِعَةً أَمْرًا حَتَّى تَشْهَدُونِ"
  },
  {
    "id": 52,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 479.15,
    "end": 492.2,
    "text": "قَالُوا نَحْنُ أُولُو قُوَّةٍ وَأُولُو بَأْسٍ شَدِيدٍ وَالْأَمْرُ إِلَيْكِ فَانْظُرِي مَاذَا تَأْمُرِينَ"
  },
  {
    "id": 53,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 492.86,
    "end": 504.31,
    "text": "قَالَتْ إِنَّ الْمُلُوكَ إِذَا دَخَلُوا قَرْيَةً أَفْسَدُوهَا وَجَعَلُوا أَعِزَّةَ أَهْلِهَا أَذِلَّةً"
  },
  {
    "id": 54,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 505.2,
    "end": 508.2,
    "text": "وَكَذَلِكَ يَفْعَلُونَ"
  },
  {
    "id": 55,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 509.33,
    "end": 520.11,
    "text": "وَإِنِّي مُرْسِلَةٌ إِلَيْهِمْ بِهَدِيَّةٍ فَنَاظِرَةٌ بِمَ يَرْجِعُ الْمُرْسَلُونَ"
  },
  {
    "id": 56,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 521.34,
    "end": 536.43,
    "text": "فَلَمَّا جَاءَ سُلَيْمَانَ قَالَ أَتُمِدُّونَنِ بِمَالٍ فَمَا آتَانِيَ اللَّهُ خَيْرٌ مِمَّا آتَاكُمْ"
  },
  {
    "id": 57,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 537.38,
    "end": 542.98,
    "text": "بَلْ أَنْتُمْ بِهَدِيَّتِكُمْ تَفْرَحُونَ"
  },
  {
    "id": 58,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 543.84,
    "end": 560.01,
    "text": "ارْجِعَ إِلَيْهِمْ فَلَنَأْتِيَنَّهُمْ بِجُنُودٍ لَا قِبَلَ لَهُمْ بِهَا وَلَنُخْرِجَنَّهُمْ مِنْهَا أَذِلَّةً وَهُمْ صَاغِرُونَ"
  },
  {
    "id": 59,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 561.13,
    "end": 569.11,
    "text": "قَالَ يَا أَيُّهَا الْمَلَأُ أَيُّكُمْ يَأْتِينِي بِعَرْشِهَا قَبْلَ أَنْ يَأْتِينِي بِعَرْشِهَا قَبْلَ أَنْ يَأْتِينِي بِعَرْشِهَا قَبْلَ أَنْ يَأْتِينَ"
  },
  {
    "id": 60,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 569.45,
    "end": 571.87,
    "text": "عُيُونِي مُسْلِمِينَ"
  },
  {
    "id": 61,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 572.86,
    "end": 581.9,
    "text": "قَالَ عِفْرِيتٌ مِنَ الْجِنِّ أَنَا آتِيكَ بِهِ قَبْلَ أَنْ تَقُومَ مِنْ مَقَامِ"
  },
  {
    "id": 62,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 582.84,
    "end": 587.57,
    "text": "وَإِنِّي عَلَيْهِ لَقَوِيٌّ أَمِينٌ"
  },
  {
    "id": 63,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 588.43,
    "end": 599.38,
    "text": "قَالَ الَّذِي عِنْدَهُ عِلْمٌ مِنَ الْكِتَابِ أَنَا آتِيكَ بِهِ قَبْلَ أَنْ يَرْتَدَّ إِلَيْكَ طَرْفٌ"
  },
  {
    "id": 64,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 600.49,
    "end": 613.22,
    "text": "فَلَمَّا رَآهُ مُسْتَقِرًّا عِنْدَهُ قَالَ هَذَا مِنْ فَضْلِ رَبِّي لِيَبْلُوَنِي أَشْكُرُ أَمْ أَكْفُرُ"
  },
  {
    "id": 65,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 614.04,
    "end": 618.68,
    "text": "وَمَنْ شَكَرَ فَإِنَّمَا يَشْكُرُ لِنَفْسِهِ"
  },
  {
    "id": 66,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 619.46,
    "end": 625.95,
    "text": "وَمَنْ كَفَرَ فَإِنَّ رَبِّي غَنِيٌّ كَرِيمٌ"
  },
  {
    "id": 67,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 626.94,
    "end": 637.52,
    "text": "قَالَ نَكِّرُوا لَهَا عَرْشَهَا نَنْظُرْ أَتَهْتَدِي أَمْ تَكُونُ مِنَ الَّذِينَ لَا يَهْتَدُونَ"
  },
  {
    "id": 68,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 638.38,
    "end": 643.41,
    "text": "فَلَمَّا جَاءَتْ قِيلَ أَهَكَذَا عَرْشٌ"
  },
  {
    "id": 69,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 644.38,
    "end": 647.14,
    "text": "قَالَتْ كَأَنَّهُ"
  },
  {
    "id": 70,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 647.91,
    "end": 654.94,
    "text": "وَأُوتِينَا الْعِلْمَ مِنْ قَبْلِهَا وَكُنَّا مُسْلِمِينَ"
  },
  {
    "id": 71,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 656.07,
    "end": 661.86,
    "text": "وَصَدَّهَا مَا كَانَتْ تَعْبُدُ مِنْ دُونِ اللَّهِ"
  },
  {
    "id": 72,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 662.65,
    "end": 668.84,
    "text": "إِنَّهَا كَانَتْ مِنْ قَوْمٍ كَافِرِينَ"
  },
  {
    "id": 73,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 669.9,
    "end": 671.82,
    "text": "قِيلَ لَهَا ادْخُلِي الصَّرْحَ"
  },
  {
    "id": 74,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 673.06,
    "end": 680.68,
    "text": "فَلَمَّا رَأَتْهُ حَسِبَتْهُ نُجَّةً وَكَشَفَتْ عَنْ سَاقَيْهَا"
  },
  {
    "id": 75,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 681.63,
    "end": 688.89,
    "text": "قَالَ إِنَّهُ صَرْحٌ مُمَرَّدٌ مِنْ قَوَارِينَ"
  },
  {
    "id": 76,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 689.83,
    "end": 700.65,
    "text": "قَالَتْ رَبِّ إِنِّي ظَلَمْتُ نَفْسِي وَأَسْلَمْتُ مَعَ سُلَيْمَانَ لِلَّهِ رَبِّ الْعَالَمِينَ"
  },
  {
    "id": 77,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 701.79,
    "end": 713.59,
    "text": "وَلَقَدْ أَرْسَلْنَا إِلَى ثَمُودَ أَخَاهُمْ صَالِحًا أَنِ اعْبُدُوا اللَّهَ فَإِذَا هُمْ فَرِيقَانِ يَخْتَصِمُونَ"
  },
  {
    "id": 78,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 714.5,
    "end": 720.8,
    "text": "قَالَ يَا قَوْمِ لِمَ تَسْتَعْجِلُونَ بِالسَّيِّئَةِ قَبْلَ الْحَسَنَةِ"
  },
  {
    "id": 79,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 721.55,
    "end": 727.8,
    "text": "لَوْلَا تَسْتَغْفِرُونَ اللَّهَ لَعَلَّكُمْ تُرْحَمُونَ"
  },
  {
    "id": 80,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 728.98,
    "end": 732.77,
    "text": "قَالُوا اطَّيَّرْنَا بِكَ وَبِمَنْ مَعَ"
  },
  {
    "id": 81,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 733.8,
    "end": 738.59,
    "text": "قَالَ طَائِرُكُمْ عِنْدَ اللَّهِ"
  },
  {
    "id": 82,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 739.4,
    "end": 744.37,
    "text": "بَلْ أَنْتُمْ قَوْمٌ تُفْتَنُونَ"
  },
  {
    "id": 83,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 745.43,
    "end": 754.16,
    "text": "وَكَانَ فِي الْمَدِينَةِ تِسْعَةُ رَهْطٍ يُفْسِدُونَ فِي الْأَرْضِ وَلَا يُصْلِحُونَ"
  },
  {
    "id": 84,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 755.09,
    "end": 772.13,
    "text": "قَالُوا تَقَاسَمُوا بِاللَّهِ لَنُبَيِّتَنَّهُ وَأَهْلَهُ ثُمَّ لَنَقُولَنَّ لِوَلِيِّهِ مَا شَهِدْنَا مَهْلِكَ أَهْلِهِ وَإِنَّا لَصَادِقُونَ"
  },
  {
    "id": 85,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 773.19,
    "end": 780.11,
    "text": "وَمَكَرُوا مَكْرًا وَمَكَرْنَا مَكْرًا وَهُمْ لَا يَشْعُرُونَ"
  },
  {
    "id": 86,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 781.15,
    "end": 791.66,
    "text": "فَانْظُرْ كَيْفَ كَانَ عَاقِبَةُ مَكْرِهِمْ أَنَّا دَمَّرْنَاهُمْ وَقَوْمَهُمْ أَجْمَعِينَ"
  },
  {
    "id": 87,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 792.73,
    "end": 798.04,
    "text": "وَتِلْكَ بُيُوتُهُمْ خَاوِيَةً بِمَا ظَلَمُونَ"
  },
  {
    "id": 88,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 798.87,
    "end": 805.35,
    "text": "إِنَّ فِي ذَلِكَ لَآيَةً لِقَوْمٍ يَعْلَمُونَ"
  },
  {
    "id": 89,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 806.37,
    "end": 812.71,
    "text": "وَأَنْجَيْنَا الَّذِينَ آمَنُوا وَكَانُوا يَتَّقُونَ"
  },
  {
    "id": 90,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 814.04,
    "end": 823.34,
    "text": "وَلُوطًا إِذْ قَالَ لِقَوْمِهِ أَتَأْتُونَ الْفَاحِشَةَ وَأَنْتُمْ تُبْصِرُونَ"
  },
  {
    "id": 91,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 824.4,
    "end": 832.95,
    "text": "أَئِنَّكُمْ لَتَأْتُونَ الرِّجَالَ شَهْوَةً مِنْ دُونِ النِّسَاءِ"
  },
  {
    "id": 92,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 833.83,
    "end": 838.78,
    "text": "بَلْ أَنْتُمْ قَوْمٌ تَجْهَلُونَ"
  },
  {
    "id": 93,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 839.99,
    "end": 853.95,
    "text": "فَمَا كَانَ جَوَابَ قَوْمِهِ إِلَّا أَنْ قَالُوا أَخْرِجُوا آلَ لُوطٍ مِنْ قَرْيَتِكُمْ"
  },
  {
    "id": 94,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 855.07,
    "end": 859.95,
    "text": "إِنَّهُمْ أُنَاسٌ يَتَطَهَّرُونَ"
  },
  {
    "id": 95,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 860.95,
    "end": 870.21,
    "text": "فَأَنْجَيْنَاهُ وَأَهْلَهُ إِلَّا امْرَأَتَهُ قَدَّرْنَاهَا مِنَ الْغَابِرِينَ"
  },
  {
    "id": 96,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 871.09,
    "end": 874.48,
    "text": "وَأَمْطَرْنَا عَلَيْهِمْ مَطَرًا"
  },
  {
    "id": 97,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 875.36,
    "end": 880.44,
    "text": "يَسَاءَ مَقْرُ الْمُنْذَرِينَ"
  },
  {
    "id": 98,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 881.14,
    "end": 887.82,
    "text": "قُلِ الْحَمْدُ لِلَّهِ وَسَلَامٌ عَلَى عِبَادِهِ الَّذِينَ اصْطَفَى"
  },
  {
    "id": 99,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 889.04,
    "end": 896.02,
    "text": "اللَّهُ خَيْرٌ أَمَّا يُشْرِكُونَ"
  },
  {
    "id": 100,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 897.03,
    "end": 910.29,
    "text": "أَمَّنْ خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ وَأَنْزَلَ لَكُمْ مِنَ السَّمَاءِ مَاءً فَأَنْبَتْنَا بِهِ"
  },
  {
    "id": 101,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 911.31,
    "end": 922.41,
    "text": "فَأَنْبَتْنَا بِهِ حَدَائِقَ ذَاتَ بَهْجَةٍ مَا كَانَ لَكُمْ أَنْ تُنْبِتُوا شَجَرَهَا"
  },
  {
    "id": 102,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 923.48,
    "end": 926.84,
    "text": "فَإِلَهٌ مَعَ اللَّهِ"
  },
  {
    "id": 103,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 927.7,
    "end": 931.6,
    "text": "بَلْ هُمْ قَوْمٌ يَعْدِلُونَ"
  },
  {
    "id": 104,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 932.87,
    "end": 947.7,
    "text": "أَمَّنْ جَعَلَ الْأَرْضَ قَرَارًا وَجَعَلَ خِلَالَهَا أَنْهَارًا وَجَعَلَ نَهَارَ وَاسِيَ وَجَعَلَ بَيْنَ الْبَحْرَيْنِ حَاجِزًا"
  },
  {
    "id": 105,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 948.8,
    "end": 952.28,
    "text": "فَإِلَهٌ مَعَ اللَّهِ"
  },
  {
    "id": 106,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 952.91,
    "end": 956.7,
    "text": "بَلْ أَكْثَرُهُمْ لَا يَعْلَمُونَ"
  },
  {
    "id": 107,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 957.83,
    "end": 969.39,
    "text": "أَمَّنْ يُجِيبُ الْمُضْطَرَّ إِذَا دَعَاهُ وَيَكْشِفُ السُّوءَ وَيَجْعَلُكُمْ خُلَفَاءَ الْأَرْضِ"
  },
  {
    "id": 108,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 970.39,
    "end": 973.75,
    "text": "فَإِلَهٌ مَعَ اللَّهِ"
  },
  {
    "id": 109,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 974.67,
    "end": 978.37,
    "text": "وَلِيلًا مَا تَذَكَّرُونَ"
  },
  {
    "id": 110,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 979.67,
    "end": 991.64,
    "text": "أَمَّنْ يَهْدِيكُمْ فِي ظُلُمَاتِ الْبَرِّ وَالْبَحْرِ وَمَنْ يُرْسِلُ الرِّيَاحَ بُشْرًا بَيْنَ يَدَيْ رَحْمَتِهِ"
  },
  {
    "id": 111,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 992.77,
    "end": 996.03,
    "text": "فَإِلَهٌ مَعَ اللَّهِ"
  },
  {
    "id": 112,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 996.76,
    "end": 1001.58,
    "text": "بِعَالَى اللَّهُ عَمَّا يُشْرِكُونَ"
  },
  {
    "id": 113,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1003.0,
    "end": 1014.71,
    "text": "أَمَّنْ يَبْدَأُ الْخَلْقَ ثُمَّ يُعِيدُهُ وَمَنْ يَرْزُقُكُمْ مِنَ السَّمَاءِ وَالْأَرْضِ"
  },
  {
    "id": 114,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1015.76,
    "end": 1019.26,
    "text": "فَإِلَهٌ مَعَ اللَّهِ"
  },
  {
    "id": 115,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1020.23,
    "end": 1027.15,
    "text": "قُلْ هَاتُوا بُرْهَانَكُمْ إِنْ كُنْتُمْ صَادِقِينَ"
  },
  {
    "id": 116,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1028.44,
    "end": 1035.92,
    "text": "قُلْ لَا يَعْلَمُ مَنْ فِي السَّمَاوَاتِ وَالْأَرْضِ الْغَيْبَ إِلَّا اللَّهُ"
  },
  {
    "id": 117,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1036.74,
    "end": 1041.19,
    "text": "وَمَا يَشْعُرُونَ أَيَّانَ يُبْعَثُونَ"
  },
  {
    "id": 118,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1042.11,
    "end": 1045.65,
    "text": "بَلِ ادَّارَكَ عِلْمُهُمْ فِي الْآخِرَةِ"
  },
  {
    "id": 119,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1046.55,
    "end": 1049.9,
    "text": "بَلْ هُمْ فِي شَكٍّ مِنْهَا"
  },
  {
    "id": 120,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1050.88,
    "end": 1054.22,
    "text": "بَلْ هُمْ مِنْهَا عَمُونَ"
  },
  {
    "id": 121,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1054.96,
    "end": 1067.9,
    "text": "وَقَالَ الَّذِينَ كَفَرُوا أَإِذَا كُنَّا تُرَابًا وَآبَاؤُنَا أَئِنَّا لَمُخْرَجُونَ"
  },
  {
    "id": 122,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1069.16,
    "end": 1082.42,
    "text": "لَقَدْ وُعِدْنَا هَذَا نَحْنُ وَآبَاؤُنَا مِنْ قَبْلُ إِنْ هَذَا إِلَّا أَسَاطِيرُ الْأَوَّلِينَ"
  },
  {
    "id": 123,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1083.79,
    "end": 1091.69,
    "text": "قُلْ سِيرُوا فِي الْأَرْضِ فَانْظُرُوا كَيْفَ كَانَ عَاقِبَةُ الْمُجْرِمِينَ"
  },
  {
    "id": 124,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1092.55,
    "end": 1101.75,
    "text": "وَلَا تَحْزَنْ عَلَيْهِمْ وَلَا تَكُنْ فِي ضَيْقٍ مِمَّا يَمْكُرُونَ"
  },
  {
    "id": 125,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1102.88,
    "end": 1110.57,
    "text": "وَيَقُولُونَ مَتَى هَذَا الْوَعْدُ إِنْ كُنْتُمْ صَادِقِينَ"
  },
  {
    "id": 126,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1111.67,
    "end": 1120.57,
    "text": "قُلْ عَسَى أَنْ يَكُونَ رَدِفَ لَكُمْ بَعْضُ الَّذِي تَسْتَعْجِلُونَ"
  },
  {
    "id": 127,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1121.59,
    "end": 1131.53,
    "text": "وَإِنَّ رَبَّكَ لَذُو فَضْلٍ عَلَى النَّاسِ وَلَكِنَّ أَكْثَرَهُمْ لَا يَشْكُرُونَ"
  },
  {
    "id": 128,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1132.54,
    "end": 1140.71,
    "text": "وَإِنَّ رَبَّكَ لَيَعْلَمُ مَا تُكِنُّ صُدُورُهُمْ وَمَا يُعْلِنُونَ"
  },
  {
    "id": 129,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1141.4,
    "end": 1152.03,
    "text": "وَمَا مِنْ غَائِبَةٍ فِي السَّمَاءِ وَالْأَرْضِ إِلَّا فِي كِتَابٍ مُبِينٍ"
  },
  {
    "id": 130,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1153.16,
    "end": 1165.25,
    "text": "إِنَّ هَذَا الْقُرْآنَ يَقُصُّ عَلَى بَنِي إِسْرَائِيلَ أَكْثَرَ الَّذِي هُمْ فِيهِ يَخْتَلِفُونَ"
  },
  {
    "id": 131,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1166.26,
    "end": 1172.28,
    "text": "وَإِنَّهُ لَهُدًى وَرَحْمَةٌ لِلْمُؤْمِنِينَ"
  },
  {
    "id": 132,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1173.26,
    "end": 1177.93,
    "text": "إِنَّ رَبَّكَ يَقْضِي بَيْنَهُمْ بِحُكْمِهِ"
  },
  {
    "id": 133,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1178.79,
    "end": 1181.63,
    "text": "وَهُوَ الْعَزِيزُ الْعَلِيمُ"
  },
  {
    "id": 134,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1182.62,
    "end": 1185.61,
    "text": "فَتَوَكَّلْ عَلَى اللَّهِ"
  },
  {
    "id": 135,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1186.31,
    "end": 1190.42,
    "text": "إِنَّكَ عَلَى الْحَقِّ الْمُبِينِ"
  },
  {
    "id": 136,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1191.41,
    "end": 1202.3,
    "text": "إِنَّكَ لَا تُسْمِعُ الْمَوْتَى وَلَا تُسْمِعُ الصُّمَّ الدُّعَاءَ إِذَا وَلَّوْا مُدْبِرِينَ"
  },
  {
    "id": 137,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1203.29,
    "end": 1209.61,
    "text": "وَمَا أَنْتَ بِهَادِ الْعُمْيِ عَنْ ضَلَالَتِهِمْ"
  },
  {
    "id": 138,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1210.5,
    "end": 1219.26,
    "text": "إِنْ تُسْمِعُ إِلَّا مَنْ يُؤْمِنُ بِآيَاتِنَا فَهُمْ مُسْلِمُونَ"
  },
  {
    "id": 139,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1220.43,
    "end": 1237.58,
    "text": "وَإِذَا وَقَعَ الْقَوْلُ عَلَيْهِمْ أَخْرَجْنَا لَهُمْ دَابَّةً مِنَ الْأَرْضِ تُكَلِّمُهُمْ أَنَّ النَّاسَ كَانُوا بِآيَاتِنَا لَا يُوقِنُونَ"
  },
  {
    "id": 140,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1238.98,
    "end": 1251.5,
    "text": "وَيَوْمَ نَحْشُرُ مِنْ كُلِّ أُمَّةٍ فَوْجًا مِمَّنْ يُكَذِّبُ بِآيَاتِنَا فَهُمْ يُوزَعُونَ"
  },
  {
    "id": 141,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1252.8,
    "end": 1268.51,
    "text": "حَتَّى إِذَا جَاءُوا قَالَ أَكَذَّبْتُمْ بِآيَاتِي وَلَمْ تُحِيطُوا بِهَا عِلْمًا أَمَّاذَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 142,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1269.87,
    "end": 1277.78,
    "text": "وَوَقَعَ الْقَوْلُ عَلَيْهِمْ بِمَا ظَلَمُوا فَهُمْ لَا يَنْطِقُونَ"
  },
  {
    "id": 143,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1278.98,
    "end": 1287.09,
    "text": "أَلَمْ يَرَوْا أَنَّا جَعَلْنَا اللَّيْلَ لِيَسْكُنُوا فِيهِ وَالنَّهَارَ مُبْصِرًا"
  },
  {
    "id": 144,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1288.03,
    "end": 1295.01,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِقَوْمٍ يُؤْمِنُونَ"
  },
  {
    "id": 145,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1296.34,
    "end": 1309.38,
    "text": "وَيَوْمَ يُنْفَخُ فِي الصُّورِ فَفَزِعَ مَنْ فِي السَّمَاوَاتِ وَمَنْ فِي الْأَرْضِ إِلَّا مَنْ شَاءَ اللَّهُ"
  },
  {
    "id": 146,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1310.38,
    "end": 1313.98,
    "text": "وَكُلٌّ أَتَوْهُ دَاخِرِينَ"
  },
  {
    "id": 147,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1315.06,
    "end": 1323.64,
    "text": "وَتَرَى الْجِبَالَ تَحْسَبُهَا جَامِدَةً وَهِيَ تَمُرُّ مَرَّ السَّحَابِ"
  },
  {
    "id": 148,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1324.54,
    "end": 1330.37,
    "text": "مُنْعَ اللَّهِ الَّذِي أَتْقَنَ كُلَّ شَيْءٍ"
  },
  {
    "id": 149,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1331.35,
    "end": 1336.85,
    "text": "إِنَّهُ خَبِيرٌ بِمَا تَفْعَلُونَ"
  },
  {
    "id": 150,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1337.71,
    "end": 1350.28,
    "text": "مَنْ جَاءَ بِالْحَسَنَةِ فَلَهُ خَيْرٌ مِنْهَا وَهُمْ مِنْ فَزَعٍ يَوْمَئِذٍ آمِنُونَ"
  },
  {
    "id": 151,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1351.66,
    "end": 1365.62,
    "text": "وَمَنْ جَاءَ بِالسَّيِّئَةِ فَكُبَّتْ وُجُوهُهُمْ فِي النَّارِ هَلْ تُجْزَوْنَ إِلَّا مَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 152,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1366.9,
    "end": 1377.71,
    "text": "إِنَّمَا أُمِرْتُ أَنْ أَعْبُدَ رَبَّ هَذِهِ الْبَلْدَةِ الَّذِي حَرَّمَهَا وَلَهُ كُلُّ شَيْءٍ"
  },
  {
    "id": 153,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1378.86,
    "end": 1383.44,
    "text": "وَأُمِرْتُ أَنْ أَكُونَ مِنَ الْمُسْلِمِينَ"
  },
  {
    "id": 154,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1384.24,
    "end": 1387.38,
    "text": "وَأَنْ أَتْلُوَ الْقُرْآنَ"
  },
  {
    "id": 155,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1388.27,
    "end": 1393.21,
    "text": "فَمَنِ اهْتَدَى فَإِنَّمَا يَهْتَدِي لِنَفْسِهِ"
  },
  {
    "id": 156,
    "sura_id": 27,
    "recitation_uuid": "e5cbf8cc-4cac-473c-853f-cdb12ed9244b",
    "start": 1394.1,
    "end": 1402.68,
    "text": "وَمَنْ ضَلَّ فَقُلْ إِنَّمَا أَنَا مِنَ الْمُنْذِرِينَ"
  },
  {
