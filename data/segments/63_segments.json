[
  {
    "id": 2,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 6.14,
    "end": 15.16,
    "text": "إِذَا جَاءَكَ الْمُنَافِقُونَ قَالُوا نَشْهَدُ إِنَّكَ لَرَسُولُ اللَّهِ"
  },
  {
    "id": 3,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 16.34,
    "end": 26.84,
    "text": "وَاللَّهُ يَعْلَمُ إِنَّكَ لَرَسُولُهُ وَاللَّهُ يَشْهَدُ إِنَّ الْمُنَافِقِينَ لَكَاذِبُونَ"
  },
  {
    "id": 4,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 27.93,
    "end": 36.61,
    "text": "اتَّخَذُوا أَيْمَانَهُمْ جُنَّةً فَصَدُّوا عَنْ سَبِيلِ اللَّهِ"
  },
  {
    "id": 5,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 37.48,
    "end": 43.18,
    "text": "إِنَّهُمْ سَاءَ مَا كَانُوا يَعْمَلُونَ"
  },
  {
    "id": 6,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 44.22,
    "end": 52.81,
    "text": "ذَلِكَ بِأَنَّهُمْ آمَنُوا ثُمَّ كَفَرُوا فَطُبِعَ عَلَى قُلُوبِهِمْ فَهُمْ لَا يَعْمَلُونَ"
  },
  {
    "id": 7,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 53.12,
    "end": 54.85,
    "text": "طَهُونَ"
  },
  {
    "id": 8,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 55.76,
    "end": 60.13,
    "text": "وَإِذَا رَأَيْتَهُمْ تُعْجِبُكَ أَجْسَامُهُمْ"
  },
  {
    "id": 9,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 60.88,
    "end": 64.64,
    "text": "وَإِنْ يَقُولُوا تَسْمَعْ لِقَوْلِهِمْ"
  },
  {
    "id": 10,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 65.45,
    "end": 69.91,
    "text": "كَأَنَّهُمْ خُشُبٌ مُسَنَّدَةٌ"
  },
  {
    "id": 11,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 70.79,
    "end": 74.41,
    "text": "يَحْسَبُونَ كُلَّ صَيْحَةٍ عَلَيْهِمْ"
  },
  {
    "id": 12,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 75.21,
    "end": 77.71,
    "text": "هُمُ الْعَدُوُّ فَاحْذَرْهُمْ"
  },
  {
    "id": 13,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 78.56,
    "end": 81.35,
    "text": "قَاتَلَهُ اللَّهُ"
  },
  {
    "id": 14,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 82.08,
    "end": 85.36,
    "text": "أَنَّى يُؤْفَكُونَ"
  },
  {
    "id": 15,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 86.49,
    "end": 100.37,
    "text": "وَإِذَا قِيلَ لَهُمْ تَعَالَوْا يَسْتَغْفِرْ لَكُمْ رَسُولُ اللَّهِ لَوَّوْا رُءُوسَهُمْ وَرَأَيْتَهُمْ يَصُدُّونَ وَهُمْ مُسْتَكْبِرُونَ"
  },
  {
    "id": 16,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 101.72,
    "end": 111.49,
    "text": "وَاءٌ عَلَيْهِمْ أَسْتَغْفَرْتَ لَهُمْ أَمْ لَمْ تَسْتَغْفِرْ لَهُمْ لَنْ يَغْفِرَ اللَّهُ لَهُمْ"
  },
  {
    "id": 17,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 112.56,
    "end": 118.7,
    "text": "إِنَّ اللَّهَ لَا يَهْدِي الْقَوْمَ الْفَاسِقِينَ"
  },
  {
    "id": 18,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 119.95,
    "end": 130.37,
    "text": "هُمُ الَّذِينَ يَقُولُونَ لَا تُنْفِقُوا عَلَى مَنْ عِنْدَ رَسُولِ اللَّهِ حَتَّى يَنْفَضُّونَ"
  },
  {
    "id": 19,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 131.5,
    "end": 142.37,
    "text": "وَلِلَّهِ خَزَائِنُ السَّمَاوَاتِ وَالْأَرْضِ وَلَكِنَّ الْمُنَافِقِينَ لَا يَفْقَهُونَ"
  },
  {
    "id": 20,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 143.53,
    "end": 152.28,
    "text": "يَقُولُونَ لَئِنْ رَجَعْنَا إِلَى الْمَدِينَةِ لَيُخْرِجَنَّ الْأَعَزُّ مِنْهَا الْأَذَلَّ"
  },
  {
    "id": 21,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 153.27,
    "end": 164.07,
    "text": "وَلِلَّهِ الْعِزَّةُ وَلِرَسُولِهِ وَلِلْمُؤْمِنِينَ وَلَكِنَّ الْمُنَافِقِينَ لَا يَعْلَمُونَ"
  },
  {
    "id": 22,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 165.43,
    "end": 177.33,
    "text": "يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تُلْهِكُمْ أَمْوَالُكُمْ وَلَا أَوْلَادُكُمْ عَنْ ذِكْرِ اللَّهِ"
  },
  {
    "id": 23,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 178.4,
    "end": 184.98,
    "text": "وَمَنْ يَفْعَلْ ذَلِكَ فَأُولَئِكَ هُمُ الْخَاسِرُونَ"
  },
  {
    "id": 24,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 186.11,
    "end": 197.06,
    "text": "وَأَنْفِقُوا مِنْ مَا رَزَقْنَاكُمْ مِنْ قَبْلِ أَنْ يَأْتِيَ أَحَدَكُمُ الْمَوْتُ فَيَقُولُ"
  },
  {
    "id": 25,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 198.51,
    "end": 212.36,
    "text": "فَيَقُولَ رَبِّ لَوْلَا أَخَّرْتَنِي إِلَى أَجَلٍ قَرِيبٍ فَأَصَّدَّقَ وَأَكُنْ مِنَ الصَّالِحِينَ"
  },
  {
    "id": 26,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 213.5,
    "end": 220.1,
    "text": "وَلَنْ يُؤَخِّرَ اللَّهُ نَفْسًا إِذَا جَاءَ أَجَلُهَا"
  },
  {
    "id": 27,
    "sura_id": 63,
    "recitation_uuid": "f2e8120b-d802-4df6-b2cf-2b8c5c36a30a",
    "start": 221.05,
    "end": 226.37,
    "text": "وَاللَّهُ خَبِيرٌ بِمَا تَعْمَلُونَ"
  }
]