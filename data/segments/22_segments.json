[
  {
    "id": 2,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 5.65,
    "end": 10.46,
    "text": "يَا أَيُّهَا النَّاسُ اتَّقُوا رَبَّكُمْ"
  },
  {
    "id": 3,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 11.54,
    "end": 16.51,
    "text": "إِنَّ زَلْزَلَةَ السَّاعَةِ شَيْءٌ عَظِيمٌ"
  },
  {
    "id": 4,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 17.77,
    "end": 27.91,
    "text": "يَوْمَ تَرَوْنَهَا تَذْهَلُ كُلُّ مُرْضِعَةٍ عَمَّا أَرْضَعَتْ وَتَضَعُ كُلُّ ذَاتِ حَمْلٍ حَمْلَهَا"
  },
  {
    "id": 5,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 29.24,
    "end": 38.42,
    "text": "وَتَضَعُ كُلُّ ذَاتِ حَمْلٍ حَمْلَهَا وَتَرَى النَّاسَ سُكَارَى وَمَا هُمْ بِسُكَارَى"
  },
  {
    "id": 6,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 39.4,
    "end": 46.39,
    "text": "وَمَا هُمْ بِسُكَارَى وَلَكِنَّ عَذَابَ اللَّهِ شَدِيدٌ"
  },
  {
    "id": 7,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 47.78,
    "end": 58.5,
    "text": "وَمِنَ النَّاسِ مَنْ يُجَادِلُ فِي اللَّهِ بِغَيْرِ عِلْمٍ وَيَتَّبِعُ كُلَّ شَيْطَانٍ مَرِيدٍ"
  },
  {
    "id": 8,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 59.65,
    "end": 71.02,
    "text": "يُتِبَ عَلَيْهِ أَنَّهُ مَنْ تَوَلَّاهُ فَأَنَّهُ يُضِلُّهُ وَيَهْدِيهِ إِلَى عَذَابِ السَّعِيرِ"
  },
  {
    "id": 9,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 72.31,
    "end": 86.76,
    "text": "يَا أَيُّهَا النَّاسُ إِنْ كُنْتُمْ فِي رَيْبٍ مِنَ الْبَعْثِ فَإِنَّا خَلَقْنَاكُمْ مِنْ تُرَابٍ"
  },
  {
    "id": 10,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 87.97,
    "end": 104.12,
    "text": "فَإِنَّا خَلَقْنَاكُمْ مِنْ تُرَابٍ ثُمَّ مِنْ نُطْفَةٍ ثُمَّ مِنْ عَلَقَةٍ ثُمَّ مِنْ مُضْغَى"
  },
  {
    "id": 11,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 105.42,
    "end": 114.36,
    "text": "ثُمَّ مِنْ مُضْغَةٍ مُخَلَّقَةٍ وَغَيْرِ مُخَلَّقَةٍ لِنُبَيِّنَ لَكُمْ"
  },
  {
    "id": 12,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 115.86,
    "end": 128.09,
    "text": "وَنُقِرُّ فِي الْأَرْحَامِ مَا نَشَاءُ إِلَى أَجَلٍ مُسَمًّى ثُمَّ نُخْرِجُكُمْ طِفْلًا"
  },
  {
    "id": 13,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 129.26,
    "end": 136.28,
    "text": "ثُمَّ نُخْرِجُكُمْ طِفْلًا ثُمَّ لِتَبْلُغُوا أَشُدَّكُمْ"
  },
  {
    "id": 14,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 137.43,
    "end": 153.9,
    "text": "وَمِنْكُمْ مَنْ يُتَوَفَّى وَمِنْكُمْ مَنْ يُرَدُّ إِلَى أَرْذَلِ الْعُمُرِ لِكَيْ لَا يَعْلَمَ مِنْ بَعْدِ عِلْمٍ شَيْئًا"
  },
  {
    "id": 15,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 155.0,
    "end": 166.23,
    "text": "وَتَرَى الْأَرْضَ هَامِدَةً فَإِذَا أَنْزَلْنَا عَلَيْهَا الْمَاءَ اهْتَزَّتْ وَرَبَتْ وَأَنْبَتْ"
  },
  {
    "id": 16,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 167.09,
    "end": 180.03,
    "text": "فَإِذَا أَنْزَلْنَا عَلَيْهَا الْمَاءَ اهْتَزَّتْ وَرَبَتْ وَأَنْبَتَتْ مِنْ كُلِّ زَوْجٍ بَهِيجٍ"
  },
  {
    "id": 17,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 181.43,
    "end": 193.84,
    "text": "ذَلِكَ بِأَنَّ اللَّهَ هُوَ الْحَقُّ وَأَنَّهُ يُحْيِي الْمَوْتَى وَأَنَّهُ عَلَى كُلِّ شَيْءٍ قَدِيرٌ"
  },
  {
    "id": 18,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 195.27,
    "end": 205.6,
    "text": "وَأَنَّ السَّاعَةَ آتِيَةٌ لَا رَيْبَ فِيهَا وَأَنَّ اللَّهَ يَبْعَثُ مَنْ فِي الْقُبُورِ"
  },
  {
    "id": 19,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 206.81,
    "end": 217.56,
    "text": "وَمِنَ النَّاسِ مَنْ يُجَادِلُ فِي اللَّهِ بِغَيْرِ عِلْمٍ وَلَا هُدًى وَلَا كِتَابٍ مُنِيرٍ"
  },
  {
    "id": 20,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 218.78,
    "end": 224.02,
    "text": "كَانِيَ عِطْفِهِ لِيُضِلَّ عَنْ سَبِيلِ اللَّهِ"
  },
  {
    "id": 21,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 224.93,
    "end": 227.04,
    "text": "لَهُ فِي الدُّنْيَا خِزْرٌ"
  },
  {
    "id": 22,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 228.3,
    "end": 233.95,
    "text": "وَنُذِيقُهُ يَوْمَ الْقِيَامَةِ عَذَابَ الْحَرِيقِ"
  },
  {
    "id": 23,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 235.2,
    "end": 243.9,
    "text": "ذَلِكَ بِمَا قَدَّمَتْ يَدَاكَ وَأَنَّ اللَّهَ لَيْسَ بِظَلَّامٍ لِلْعَبِيدِ"
  },
  {
    "id": 24,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 245.1,
    "end": 249.56,
    "text": "وَمِنَ النَّاسِ مَنْ يَعْبُدُ اللَّهَ عَلَى حَرَفٍ"
  },
  {
    "id": 25,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 250.8,
    "end": 254.55,
    "text": "فَإِنْ أَصَابَهُ خَيْرٌ اطْمَأَنَّ"
  },
  {
    "id": 26,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 255.7,
    "end": 263.72,
    "text": "وَإِنْ أَصَابَتْهُ فِتْنَةٌ انْقَلَبَ عَلَى وَجْهِهِ خَسِرَ الدُّنْيَا وَالْآخِرَةَ"
  },
  {
    "id": 27,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 264.77,
    "end": 268.75,
    "text": "ذَلِكَ هُوَ الْخُسْرَانُ الْمُبِينُ"
  },
  {
    "id": 28,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 270.14,
    "end": 277.13,
    "text": "يَدْعُو مِنْ دُونِ اللَّهِ مَا لَا يَضُرُّهُ وَمَا لَا يَنْفَعُ"
  },
  {
    "id": 29,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 278.11,
    "end": 281.74,
    "text": "ذَلِكَ هُوَ الضَّلَالُ الْبَعِيدُ"
  },
  {
    "id": 30,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 282.7,
    "end": 288.84,
    "text": "يَدْعُو لَمَنْ ضَرُّهُ أَقْرَبُ مِنْ نَفْعِهِ"
  },
  {
    "id": 31,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 289.92,
    "end": 292.12,
    "text": "لَبِئْسَ الْمَوْلَى"
  },
  {
    "id": 32,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 292.49,
    "end": 294.58,
    "text": "الم"
  },
  {
    "id": 33,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 295.69,
    "end": 305.88,
    "text": "إِنَّ اللَّهَ يُدْخِلُ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ جَنَّاتٍ تَجْرِي مِنْ تَحْرَى"
  },
  {
    "id": 34,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 306.2,
    "end": 308.17,
    "text": "هَلْ أَنْهَى"
  },
  {
    "id": 35,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 309.35,
    "end": 313.81,
    "text": "إِنَّ اللَّهَ يَفْعَلُ مَا يُرِيدُ"
  },
  {
    "id": 36,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 315.02,
    "end": 329.59,
    "text": "مَنْ كَانَ يَظُنُّ أَنْ لَنْ يَنْصُرَهُ اللَّهُ فِي الدُّنْيَا وَالْآخِرَةِ فَلْيَمْدُدْ بِسَبَبٍ إِلَى السَّمَاءِ ثُمَّ لْيَقْطَعَ"
  },
  {
    "id": 37,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 330.55,
    "end": 338.92,
    "text": "ثُمَّ لْيَقْطَعْ فَلْيَنْظُرْ هَلْ يُذْهِبَنَّ كَيْدُهُ مَا يَغِيظُ"
  },
  {
    "id": 38,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 340.15,
    "end": 351.6,
    "text": "وَكَذَلِكَ أَنْزَلْنَاهُ آيَاتٍ بَيِّنَاتٍ وَأَنَّ اللَّهَ يَهْدِي مَنْ يُرِيدُ"
  },
  {
    "id": 39,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 352.59,
    "end": 369.44,
    "text": "إِنَّ الَّذِينَ آمَنُوا وَالَّذِينَ هَادُوا وَالصَّابِئِينَ وَالنَّصَارَى وَالْمَجُوسَ وَالَّذِينَ أَشْرَكُوا إِنَّ اللَّهَ يَفْصِلُ بَيْنَهُمْ يَوْمَ الْقِيَامَةِ"
  },
  {
    "id": 40,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 370.39,
    "end": 376.12,
    "text": "إِنَّ اللَّهَ عَلَى كُلِّ شَيْءٍ شَهِيدٌ"
  },
  {
    "id": 41,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 376.94,
    "end": 390.19,
    "text": "أَلَمْ تَرَ أَنَّ اللَّهَ يَسْجُدُ لَهُ مَنْ فِي السَّمَاوَاتِ وَمَنْ فِي الْأَرْضِ وَالشَّمْسُ وَالْقَمَرُ وَالنُّجُومُ وَالْجِبَالُ"
  },
  {
    "id": 42,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 391.44,
    "end": 404.28,
    "text": "وَالشَّمْسُ وَالْقَمَرُ وَالنُّجُومُ وَالْجِبَالُ وَالشَّجَرُ وَالدَّوَابُّ وَكَثِيرٌ مِنَ النَّاسِ"
  },
  {
    "id": 43,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 405.34,
    "end": 409.28,
    "text": "وَكَثِيرٌ حَقَّ عَلَيْهِ الْعَذَابُ"
  },
  {
    "id": 44,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 410.32,
    "end": 414.99,
    "text": "وَمَنْ يُهِنِ اللَّهُ فَمَا لَهُ مِنْ مُكْرِمٍ"
  },
  {
    "id": 45,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 415.84,
    "end": 420.4,
    "text": "إِنَّ اللَّهَ يَفْعَلُ مَا يَشَاءُ"
  },
  {
    "id": 46,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 421.45,
    "end": 425.47,
    "text": "هَذَانِ خَصْمَانِ اخْتَصَمُوا فِي رَبِّهِمْ"
  },
  {
    "id": 47,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 426.66,
    "end": 438.58,
    "text": "الَّذِينَ كَفَرُوا قُطِّعَتْ لَهُمْ ثِيَابٌ مِنْ نَارٍ يُصَبُّ مِنْ فَوْقِ رُءُوسِهِمُ الْحَمِيمُ"
  },
  {
    "id": 48,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 439.62,
    "end": 444.83,
    "text": "يُصْهَرُ بِهِ مَا فِي بُطُونِهِمْ وَالْجُلُودُ"
  },
  {
    "id": 49,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 445.7,
    "end": 449.81,
    "text": "وَلَهُمْ مَقَامِعُ مِنْ حَدِيدٍ"
  },
  {
    "id": 50,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 451.0,
    "end": 459.64,
    "text": "كُلَّمَا أَرَادُوا أَنْ يَخْرُجُوا مِنْهَا مِنْ غَمٍّ أُعِيدُوا فِيهَا"
  },
  {
    "id": 51,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 460.79,
    "end": 466.43,
    "text": "يُعِيدُوا فِيهَا وَذُوقُوا عَذَابَ الْحَرِيقِ"
  },
  {
    "id": 52,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 467.51,
    "end": 475.74,
    "text": "إِنَّ اللَّهَ يُدْخِلُ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ جَنًّا"
  },
  {
    "id": 53,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 477.03,
    "end": 488.46,
    "text": "جَنَّاتٍ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ يُحَلَّوْنَ فِيهَا مِنْ أَسَاوِرَ مِنْ ذَهَبٍ وَلُؤْلُؤًا"
  },
  {
    "id": 54,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 489.41,
    "end": 493.01,
    "text": "وَلِبَاسُهُمْ فِيهَا حَرِيبٌ"
  },
  {
    "id": 55,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 494.27,
    "end": 502.92,
    "text": "وَهُدُوا إِلَى الطَّيِّبِ مِنَ الْقَوْلِ وَهُدُوا إِلَى صِرَاطِ الْحَمِيدِ"
  },
  {
    "id": 56,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 504.15,
    "end": 520.91,
    "text": "إِنَّ الَّذِينَ كَفَرُوا وَيَصُدُّونَ عَنْ سَبِيلِ اللَّهِ وَالْمَسْجِدِ الْحَرَامِ الَّذِي جَعَلْنَاهُ لِلنَّاسِ سَوَاءً الْعَاكِفُ فِيهِ وَالْبَادِ"
  },
  {
    "id": 57,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 521.93,
    "end": 530.49,
    "text": "وَمَنْ يُرِدْ فِيهِ بِإِلْحَادٍ بِظُلْمٍ نُذِقَهُ مِنْ عَذَابٍ أَلِيمٍ"
  },
  {
    "id": 58,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 531.5,
    "end": 538.62,
    "text": "وَإِذْ بَوَّأْنَا لِإِبْرَاهِيمَ مَكَانَ الْبَيْتِ أَنْ لَا تُشْرِكْ بِي شَيْئًا"
  },
  {
    "id": 59,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 539.57,
    "end": 551.97,
    "text": "كَلَّا تُشْرِكْ بِي شَيْئًا وَطَهِّرْ بَيْتِيَ لِلطَّائِفِينَ وَالْقَائِمِينَ وَالرُّكَّعِ السُّجُودِ"
  },
  {
    "id": 60,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 552.89,
    "end": 565.75,
    "text": "وَأَذِّنْ فِي النَّاسِ بِالْحَجِّ يَأْتُوكَ رِجَالًا وَعَلَى كُلِّ ضَامِرٍ يَأْتِينَ مِنْ كُلِّ فَجٍّ عَمِيقٍ"
  },
  {
    "id": 61,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 566.94,
    "end": 581.35,
    "text": "لِيَشْهَدُوا مَنَافِعَ لَهُمْ وَيَذْكُرُوا اسْمَ اللَّهِ فِي أَيَّامٍ مَعْلُومَاتٍ عَلَى مَا رَزَقَهُمْ مِنْ بَهِيمَةِ الْأَنْعَامِ"
  },
  {
    "id": 62,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 582.51,
    "end": 588.33,
    "text": "فَكُلُوا مِنْهَا وَأَطْعِمُوا الْبَائِسَ الْفَقِيرَ"
  },
  {
    "id": 63,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 589.28,
    "end": 594.23,
    "text": "ثُمَّ لْيَقْضُوا تَفَثَهُمْ وَلْيُوفُوا نُذُورَهُمْ"
  },
  {
    "id": 64,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 595.36,
    "end": 602.28,
    "text": "وَالْيُوفُوا نُذُورَهُمْ وَلْيَطَّوَّفُوا بِالْبَيْتِ الْعَتِيقِ"
  },
  {
    "id": 65,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 603.53,
    "end": 604.04,
    "text": "ن"
  },
  {
    "id": 66,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 604.93,
    "end": 611.8,
    "text": "وَمَنْ يُعَظِّمْ حُرُمَاتِ اللَّهِ فَهُوَ خَيْرٌ لَهُ عِنْدَ رَبِّهِ"
  },
  {
    "id": 67,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 612.78,
    "end": 618.27,
    "text": "وَأُحِلَّتْ لَكُمُ الْأَنْعَامُ إِلَّا مَا يُتْلَى عَلَيْكُمْ"
  },
  {
    "id": 68,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 619.19,
    "end": 625.28,
    "text": "اجْتَنِبُوا الرِّجْسَ مِنَ الْأَوْثَانِ وَاجْتَنِبُوا قَوْلَ الزُّونِ"
  },
  {
    "id": 69,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 626.36,
    "end": 630.85,
    "text": "وَنَفَاءَ لِلَّهِ غَيْرَ مُشْرِكِينَ"
  },
  {
    "id": 70,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 631.81,
    "end": 641.22,
    "text": "وَمَنْ يُشْرِكْ بِاللَّهِ فَكَأَنَّمَا خَرَّ مِنَ السَّمَاءِ فَتَخْطَفُهُ الطَّيْرُ"
  },
  {
    "id": 71,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 642.22,
    "end": 650.02,
    "text": "وَتَخْطَفُهُ الطَّيْرُ أَوْ تَهْوِي بِهِ الرِّيحُ فِي مَكَانٍ سَحِيقٍ"
  },
  {
    "id": 72,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 651.17,
    "end": 651.68,
    "text": "ن"
  },
  {
    "id": 73,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 652.51,
    "end": 661.5,
    "text": "وَمَنْ يُعَظِّمْ شَعَائِرَ اللَّهِ فَإِنَّهَا مِنْ تَقْوَى الْقُلُوبِ"
  },
  {
    "id": 74,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 662.47,
    "end": 675.27,
    "text": "لَكُمْ فِيهَا مَنَافِعُ إِلَى أَجَلٍ مُسَمًّى ثُمَّ مَحِلُّهَا إِلَى الْبَيْتِ الْعَتِيقِ"
  },
  {
    "id": 75,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 676.35,
    "end": 688.59,
    "text": "وَلِكُلِّ أُمَّةٍ جَعَلْنَا مَنْسَكًا لِيَذْكُرُوا اسْمَ اللَّهِ عَلَى مَا رَزَقَهُمْ مِنْ بَهِيمَةِ الْأَنْعَامِ"
  },
  {
    "id": 76,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 689.5,
    "end": 695.98,
    "text": "فَإِلَهُكُمْ إِلَهٌ وَاحِدٌ فَلَهُ أَسْلِمُونَ"
  },
  {
    "id": 77,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 696.86,
    "end": 699.73,
    "text": "وَبَشِّرِ الْمُخْبِتِينَ"
  },
  {
    "id": 78,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 700.71,
    "end": 712.16,
    "text": "الَّذِينَ إِذَا ذُكِرَ اللَّهُ وَجِلَتْ قُلُوبُهُمْ وَالصَّابِرِينَ عَلَى مَا أَصَابَهُمْ وَالْمُقِيمِي الصَّلَاةِ"
  },
  {
    "id": 79,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 713.04,
    "end": 720.06,
    "text": "وَالْمُقِيمِي الصَّلَاةِ وَمِمَّا رَزَقْنَاهُمْ يُنْفِقُونَ"
  },
  {
    "id": 80,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 720.98,
    "end": 729.93,
    "text": "وَالْبُدْنَ جَعَلْنَاهَا لَكُمْ مِنْ شَعَائِرِ اللَّهِ لَكُمْ فِيهَا خَيْرٌ"
  },
  {
    "id": 81,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 730.78,
    "end": 735.58,
    "text": "فَاذْكُرُوا اسْمَ اللَّهِ عَلَيْهَا صَوَى"
  },
  {
    "id": 82,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 736.43,
    "end": 743.35,
    "text": "فَإِذَا غَدَبَتْ جُنُوبُهَا فَكُلُوا مِنْهَا وَأَطْعِمُوا الْقَانِعَ وَالْمُعْتَرَ"
  },
  {
    "id": 83,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 744.35,
    "end": 750.33,
    "text": "كَذَلِكَ سَخَّرْنَاهَا لَكُمْ لَعَلَّكُمْ تَشْكُرُونَ"
  },
  {
    "id": 84,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 751.21,
    "end": 761.33,
    "text": "لَنْ يَنَالَ اللَّهَ لُحُومُهَا وَلَا دِمَاؤُهَا وَلَكِنْ يَنَالُهُ التَّقْوَى مِنْكُمْ"
  },
  {
    "id": 85,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 762.26,
    "end": 768.79,
    "text": "كَذَلِكَ سَخَّرَهَا لَكُمْ لِتُكَبِّرُوا اللَّهَ عَلَى مَا هَدَاكُمْ"
  },
  {
    "id": 86,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 769.61,
    "end": 770.79,
    "text": "وَبَشِّرِ الْمُعْجَلِينَ"
  },
  {
    "id": 87,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 771.1,
    "end": 772.67,
    "text": "الم"
  },
  {
    "id": 88,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 773.81,
    "end": 779.0,
    "text": "إِنَّ اللَّهَ يُدَافِعُ عَنِ الَّذِينَ آمَنُوا"
  },
  {
    "id": 89,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 779.94,
    "end": 786.73,
    "text": "إِنَّ اللَّهَ لَا يُحِبُّ كُلَّ خَوَّانٍ كَفُورٍ"
  },
  {
    "id": 90,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 788.3,
    "end": 793.79,
    "text": "أُذِنَ لِلَّذِينَ يُقَاتَلُونَ بِأَنَّهُمْ ظُلِمُونَ"
  },
  {
    "id": 91,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 794.76,
    "end": 800.1,
    "text": "وَإِنَّ اللَّهَ عَلَى نَصْرِهِمْ لَقَدِينَ"
  },
  {
    "id": 92,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 801.44,
    "end": 813.12,
    "text": "الَّذِينَ أُخْرِجُوا مِنْ دِيَارِهِمْ بِغَيْرِ حَقٍّ إِلَّا أَنْ يَقُولُوا رَبُّنَا اللَّهُ"
  },
  {
    "id": 93,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 814.37,
    "end": 830.6,
    "text": "وَلَوْلَا دَفْعُ اللَّهِ النَّاسَ بَعْضَهُمْ بِبَعْضٍ لَهُدِّمَتْ صَوَامِعُ وَبِيَعٌ وَصَلَوَاتٌ وَمَسَاجِدُ يُذْكَرُ فِيهَا اسْمُ اللَّهِ كَثِيرًا"
  },
  {
    "id": 94,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 831.62,
    "end": 837.31,
    "text": "وَلَيَنْصُرَنَّ اللَّهُ مَنْ يَنْصُرُهُ"
  },
  {
    "id": 95,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 838.6,
    "end": 843.32,
    "text": "إِنَّ اللَّهَ لَقَوِيٌّ عَزِيزٌ"
  },
  {
    "id": 96,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 844.69,
    "end": 858.62,
    "text": "الَّذِينَ إِنْ مَكَّنَّاهُمْ فِي الْأَرْضِ أَقَامُوا الصَّلَاةَ وَآتَوُا الزَّكَاةَ وَأَمَرُوا بِالْمَعْرُوفِ وَنَهَوْا عَنِ الْمُنْكَرِ"
  },
  {
    "id": 97,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 859.62,
    "end": 863.44,
    "text": "وَلِلَّهِ عَاقِبَةُ الْأُمُورِ"
  },
  {
    "id": 98,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 864.47,
    "end": 874.6,
    "text": "وَإِنْ يُكَذِّبُوكَ فَقَدْ كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍ وَعَادٌ وَثَمُودُ"
  },
  {
    "id": 99,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 875.56,
    "end": 880.0,
    "text": "وَقَوْمُ إِبْرَاهِيمَ وَقَوْمُ لُوطٍ"
  },
  {
    "id": 100,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 880.96,
    "end": 882.81,
    "text": "وَأَصْحَابُ مَدْيَنِ"
  },
  {
    "id": 101,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 883.47,
    "end": 885.53,
    "text": "وَكُذِّبَ مُوسَى"
  },
  {
    "id": 102,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 886.27,
    "end": 891.31,
    "text": "فَأَمْلَيْتُ لِلْكَافِرِينَ ثُمَّ أَخَذْتُهُمْ"
  },
  {
    "id": 103,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 892.31,
    "end": 895.24,
    "text": "فَكَيْفَ كَانَ نَكِيرِ"
  },
  {
    "id": 104,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 896.29,
    "end": 907.31,
    "text": "فَكَأَيِّنْ مِنْ قَرْيَةٍ أَهْلَكْنَاهَا وَهِيَ ظَالِمَةٌ فَهِيَ خَاوِيَةٌ عَلَى عُرُوشِهَا"
  },
  {
    "id": 105,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 908.63,
    "end": 918.57,
    "text": "لَهِيَ خَاوِيَةٌ عَلَى عُرُوشِهَا وَبِئْرٍ مُعَطَّلَةٍ وَقَصْرٍ مَشِيدٍ"
  },
  {
    "id": 106,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 919.93,
    "end": 932.32,
    "text": "أَفَلَمْ يَسِيرُوا فِي الْأَرْضِ فَتَكُونَ لَهُمْ قُلُوبٌ يَعْقِلُونَ بِهَا أَوْ آذَانٌ يَسْمَعُونَ بِهَا"
  },
  {
    "id": 107,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 933.4,
    "end": 943.17,
    "text": "فَإِنَّهَا لَا تَعْمَى الْأَبْصَارُ وَلَكِنْ تَعْمَى الْقُلُوبُ الَّتِي فِي الصُّدُورِ"
  },
  {
    "id": 108,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 944.58,
    "end": 950.93,
    "text": "وَيَسْتَعْجِلُونَكَ بِالْعَذَابِ وَلَنْ يُخْلِفَ اللَّهُ وَعْدَهُ"
  },
  {
    "id": 109,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 951.9,
    "end": 960.79,
    "text": "وَإِنَّ يَوْمًا عِنْدَ رَبِّكَ كَأَلْفِ سَنَةٍ مِمَّا تَعُدُّونَ"
  },
  {
    "id": 110,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 961.87,
    "end": 974.63,
    "text": "وَكَأَيِّنْ مِنْ قَرْيَةٍ أَمْلَيْتُ لَهَا وَهِيَ ظَالِمَةٌ ثُمَّ أَخَذْتُهَا وَإِلَيَّ الْمَصِيرُ"
  },
  {
    "id": 111,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 975.84,
    "end": 985.75,
    "text": "قُلْ يَا أَيُّهَا النَّاسُ إِنَّمَا أَنَا لَكُمْ نَذِيرٌ مُبِينٌ"
  },
  {
    "id": 112,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 986.76,
    "end": 996.76,
    "text": "الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ مَغْفِرَةٌ وَرِزْقٌ كَرِيمٌ"
  },
  {
    "id": 113,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 997.98,
    "end": 1008.78,
    "text": "وَالَّذِينَ سَعَوْا فِي آيَاتِنَا مُعَاجِزِينَ أُولَئِكَ أَصْحَابُ الْجَحِيمِ"
  },
  {
    "id": 114,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1009.62,
    "end": 1026.23,
    "text": "وَمَا أَرْسَلْنَا مِنْ قَبْلِكَ مِنْ رَسُولٍ وَلَا نَبِيٍّ إِلَّا إِذَا تَمَنَّى أَلْقَى الشَّيْطَانُ فِي أُمْنِيَّتِهِ"
  },
  {
    "id": 115,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1027.63,
    "end": 1037.72,
    "text": "بَلْ قَدْ شَيْطَانُ فِي أُمْنِيَّتِهِ فَيَنْسَخُ اللَّهُ مَا يُلْقِي الشَّيْطَانُ ثُمَّ يُرِيدُ"
  },
  {
    "id": 116,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1038.06,
    "end": 1040.24,
    "text": "فِيهِمُ اللَّهُ آيَاتِهِ"
  },
  {
    "id": 117,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1041.21,
    "end": 1044.84,
    "text": "وَاللَّهُ عَلِيمٌ حَكِيمٌ"
  },
  {
    "id": 118,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1045.96,
    "end": 1056.91,
    "text": "لِيَجْعَلَ مَا يُلْقِي الشَّيْطَانُ فِتْنَةً لِلَّذِينَ فِي قُلُوبِهِمْ مَرَضٌ وَالْقَاسِيَةِ قُلُوبُهُمْ"
  },
  {
    "id": 119,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1057.64,
    "end": 1063.98,
    "text": "وَإِنَّ الظَّالِمِينَ لَفِي شِقَاقٍ بَعِيدٍ"
  },
  {
    "id": 120,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1065.15,
    "end": 1076.08,
    "text": "وَلِيَعْلَمَ الَّذِينَ أُوتُوا الْعِلْمَ أَنَّهُ الْحَقُّ مِنْ رَبِّكَ فَيُؤْمِنُوا بِهِ فَتُخْبِتَ لَهُ قُلُوبُهُمْ"
  },
  {
    "id": 121,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1076.97,
    "end": 1086.79,
    "text": "وَإِنَّ اللَّهَ لَهَادِ الَّذِينَ آمَنُوا إِلَى صِرَاطٍ مُسْتَقِيمٍ"
  },
  {
    "id": 122,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1087.99,
    "end": 1102.21,
    "text": "وَلَا يَزَالُ الَّذِينَ كَفَرُوا فِي مِرْيَةٍ مِنْهُ حَتَّى تَأْتِيَهُمُ السَّاعَةُ بَغْتَةً أَوْ يَأْتِيَهُمْ عَذَابُ يَوْمٍ عَقِيمٍ"
  },
  {
    "id": 123,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1103.64,
    "end": 1108.72,
    "text": "الْمُلْكُ يَوْمَئِذٍ لِلَّهِ يَحْكُمُ بَيْنَهُمْ"
  },
  {
    "id": 124,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1109.73,
    "end": 1117.94,
    "text": "الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ فِي جَنَّاتِ النَّعِيمِ"
  },
  {
    "id": 125,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1119.34,
    "end": 1129.41,
    "text": "وَالَّذِينَ كَفَرُوا وَكَذَّبُوا بِآيَاتِنَا فَأُولَئِكَ لَهُمْ عَذَابٌ مُهِينٌ"
  },
  {
    "id": 126,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1130.96,
    "end": 1142.97,
    "text": "وَالَّذِينَ هَاجَرُوا فِي سَبِيلِ اللَّهِ ثُمَّ قُتِلُوا أَوْ مَاتُوا لَيَرْزُقَنَّهُمُ اللَّهُ رِزْقًا حَسَنًا"
  },
  {
    "id": 127,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1144.09,
    "end": 1149.66,
    "text": "وَإِنَّ اللَّهَ لَهُوَ خَيْرُ الرَّازِقِينَ"
  },
  {
    "id": 128,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1150.64,
    "end": 1155.37,
    "text": "لَيُدْخِلَنَّهُمْ مُدْخَلًا يَرْضَوْنَهُ"
  },
  {
    "id": 129,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1156.29,
    "end": 1160.96,
    "text": "وَإِنَّ اللَّهَ لَعَلِيمٌ حَلِيمٌ"
  },
  {
    "id": 130,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1162.21,
    "end": 1162.8,
    "text": "ذَلِكَ"
  },
  {
    "id": 131,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1163.66,
    "end": 1173.47,
    "text": "وَمَنْ عَاقَبَ بِمِثْلِ مَا عُوقِبَ بِهِ ثُمَّ بُغِيَ عَلَيْهِ لَيَنْصُرَنَّهُ اللَّهُ"
  },
  {
    "id": 132,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1174.36,
    "end": 1178.72,
    "text": "إِنَّ اللَّهَ لَعَفُوٌّ غَفُورٌ"
  },
  {
    "id": 133,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1179.68,
    "end": 1193.99,
    "text": "ذَلِكَ بِأَنَّ اللَّهَ يُولِجُ اللَّيْلَ فِي النَّهَارِ وَيُولِجُ النَّهَارَ فِي اللَّيْلِ وَأَنَّ اللَّهَ سَمِيعٌ بَصِيرٌ"
  },
  {
    "id": 134,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1195.5,
    "end": 1210.38,
    "text": "ذَلِكَ بِأَنَّ اللَّهَ هُوَ الْحَقُّ وَأَنَّ مَا يَدْعُونَ مِنْ دُونِهِ هُوَ الْبَاطِلُ وَأَنَّ اللَّهَ هُوَ الْعَلِيُّ الْكَبِيرُ"
  },
  {
    "id": 135,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1211.56,
    "end": 1222.82,
    "text": "أَلَمْ تَرَ أَنَّ اللَّهَ أَنْزَلَ مِنَ السَّمَاءِ مَاءً فَتُصْبِحُ الْأَرْضُ مُخْضَرَّةً"
  },
  {
    "id": 136,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1223.84,
    "end": 1228.03,
    "text": "إِنَّ اللَّهَ لَطِيفٌ خَبِيرٌ"
  },
  {
    "id": 137,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1229.22,
    "end": 1233.17,
    "text": "لَهُ مَا فِي السَّمَاوَاتِ وَمَا فِي الْأَرْضِ"
  },
  {
    "id": 138,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1234.04,
    "end": 1239.48,
    "text": "وَإِنَّ اللَّهَ لَهُوَ الْغَنِيُّ الْحَمِيدُ"
  },
  {
    "id": 139,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1240.61,
    "end": 1257.11,
    "text": "أَلَمْ تَرَ أَنَّ اللَّهَ سَخَّرَ لَكُمْ مَا فِي الْأَرْضِ وَالْفُلْكَ تَجْرِي فِي الْبَحْرِ بِأَمْرِهِ وَيُمْسِكُ السَّمَاءَ أَنْ تَقَعَ عَلَى الْأَرْضِ إِلَّا بِإِذْنِهِ"
  },
  {
    "id": 140,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1258.51,
    "end": 1264.48,
    "text": "إِنَّ اللَّهَ بِالنَّاسِ لَرَءُوفٌ رَحِيمٌ"
  },
  {
    "id": 141,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1265.56,
    "end": 1273.49,
    "text": "وَهُوَ الَّذِي أَحْيَاكُمْ ثُمَّ يُمِيتُكُمْ ثُمَّ يُحْيِيكُمْ"
  },
  {
    "id": 142,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1274.42,
    "end": 1278.53,
    "text": "إِنَّ الْإِنْسَانَ لَكَفُورٌ"
  },
  {
    "id": 143,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1279.76,
    "end": 1286.6,
    "text": "لِكُلِّ أُمَّةٍ جَعَلْنَا مَنْسَكًا هُمْ نَاسِكُوهُ"
  },
  {
    "id": 144,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1287.48,
    "end": 1290.62,
    "text": "فَلَا يُنَازِعُنَّكَ فِي الْأَمْرِ"
  },
  {
    "id": 145,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1291.42,
    "end": 1293.23,
    "text": "وَادْعُ إِلَى رَبِّكَ"
  },
  {
    "id": 146,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1294.23,
    "end": 1299.1,
    "text": "إِنَّكَ لَعَلَى هُدًى مُسْتَقِيمٍ"
  },
  {
    "id": 147,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1300.01,
    "end": 1306.82,
    "text": "وَإِنْ جَادَلُوكَ فَقُلِ اللَّهُ أَعْلَمُ بِمَا تَعْمَلُونَ"
  },
  {
    "id": 148,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1307.93,
    "end": 1317.41,
    "text": "اللَّهُ يَحْكُمُ بَيْنَكُمْ يَوْمَ الْقِيَامَةِ فِيمَا كُنْتُمْ فِيهِ تَخْتَلِفُونَ"
  },
  {
    "id": 149,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1318.45,
    "end": 1325.6,
    "text": "أَلَمْ تَعْلَمْ أَنَّ اللَّهَ يَعْلَمُ مَا فِي السَّمَاءِ وَالْأَرْضِ"
  },
  {
    "id": 150,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1326.63,
    "end": 1330.32,
    "text": "إِنَّ ذَلِكَ فِي كِتَابٍ"
  },
  {
    "id": 151,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1331.35,
    "end": 1335.85,
    "text": "إِنَّ ذَلِكَ عَلَى اللَّهِ يَسِيرٌ"
  },
  {
    "id": 152,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1336.96,
    "end": 1347.59,
    "text": "وَيَعْبُدُونَ مِنْ دُونِ اللَّهِ مَا لَمْ يُنَزِّلْ بِهِ سُلْطَانًا وَمَا لَيْسَ لَهُمْ بِهِ عِلْمٌ"
  },
  {
    "id": 153,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1348.43,
    "end": 1352.97,
    "text": "وَمَا لِلظَّالِمِينَ مِنْ نَصِيرٍ"
  },
  {
    "id": 154,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1354.24,
    "end": 1364.88,
    "text": "وَإِذَا تُتْلَى عَلَيْهِمْ آيَاتُنَا بَيِّنَاتٍ تَعْرِفُ فِي وُجُوهِ الَّذِينَ كَفَرُوا الْمُنْكَرَ"
  },
  {
    "id": 155,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1366.1,
    "end": 1372.66,
    "text": "يَكَادُونَ يَسْطُونَ بِالَّذِينَ يَتْلُونَ عَلَيْهِمْ آيَاتِنَا"
  },
  {
    "id": 156,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1373.54,
    "end": 1379.5,
    "text": "قُلْ أَفَأُنَبِّئُكُمْ بِشَرٍّ مِنْ ذَلِكُمْ"
  },
  {
    "id": 157,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1380.39,
    "end": 1385.05,
    "text": "النَّارُ وَعَدَهَا اللَّهُ الَّذِينَ كَفَرُوا"
  },
  {
    "id": 158,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1385.87,
    "end": 1386.13,
    "text": "وَبِينَ"
  },
  {
    "id": 159,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1386.46,
    "end": 1388.57,
    "text": "الْمَصِيرُ"
  },
  {
    "id": 160,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1389.55,
    "end": 1396.43,
    "text": "يَا أَيُّهَا النَّاسُ ضُرِبَ مَثَلٌ فَاسْتَمِعُونَ"
  },
  {
    "id": 161,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1397.58,
    "end": 1407.48,
    "text": "إِنَّ الَّذِينَ تَدْعُونَ مِنْ دُونِ اللَّهِ لَنْ يَخْلُقُوا ذُبَابًا وَلَوِ اجْتَمَعُوا"
  },
  {
    "id": 162,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1408.46,
    "end": 1414.99,
    "text": "وَإِنْ يَسْلُبْهُمُ الذُّبَابُ شَيْئًا لَا يَسْتَنْقِذُوهُ مِنْهُ"
  },
  {
    "id": 163,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1415.89,
    "end": 1419.56,
    "text": "وَعُفَطَّالِبُ وَالْمَطْلُوبُ"
  },
  {
    "id": 164,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1420.43,
    "end": 1423.48,
    "text": "مَا قَدَرُوا اللَّهَ حَقَّ قَدْرِهِ"
  },
  {
    "id": 165,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1424.42,
    "end": 1428.88,
    "text": "إِنَّ اللَّهَ لَقَوِيٌّ عَزِيزٌ"
  },
  {
    "id": 166,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1430.05,
    "end": 1437.92,
    "text": "اللَّهُ يَصْطَفِي مِنَ الْمَلَائِكَةِ رُسُلًا وَمِنَ النَّاسِ"
  },
  {
    "id": 167,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1438.92,
    "end": 1443.47,
    "text": "إِنَّ اللَّهَ سَمِيعٌ بَصِيرٌ"
  },
  {
    "id": 168,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1444.86,
    "end": 1449.24,
    "text": "يَعْلَمُ مَا بَيْنَ أَيْدِيهِمْ وَمَا خَلْفَهُمْ"
  },
  {
    "id": 169,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1450.08,
    "end": 1453.96,
    "text": "وَإِلَى اللَّهِ تُرْجَعُ الْأُمُورُ"
  },
  {
    "id": 170,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1455.09,
    "end": 1467.78,
    "text": "يَا أَيُّهَا الَّذِينَ آمَنُوا ارْكَعُوا وَاسْجُدُوا وَاعْبُدُوا رَبَّكُمْ وَافْعَلُوا الْخَيْرَ لَعَلَّكُمْ تُفْلِحُونَ"
  },
  {
    "id": 171,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1469.16,
    "end": 1472.81,
    "text": "وَجَاهِدُوا فِي اللَّهِ حَقَّ جِهَادِهِ"
  },
  {
    "id": 172,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1473.78,
    "end": 1479.3,
    "text": "وَاجْتَبَاكُمْ وَمَا جَعَلَ عَلَيْكُمْ فِي الدِّينِ مِنْ حَرَجٍ"
  },
  {
    "id": 173,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1480.2,
    "end": 1483.95,
    "text": "إِلَّةَ أَبِيكُمْ إِبْرَاهِيمَ"
  },
  {
    "id": 174,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1484.92,
    "end": 1494.76,
    "text": "وَسَمَّاكُمُ الْمُسْلِمِينَ مِنْ قَبْلُ وَفِيهَا ذَلِيَكُونَ الرَّسُولُ شَهِيدًا عَلَيْكُمْ"
  },
  {
    "id": 175,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1495.82,
    "end": 1506.54,
    "text": "وَفِيهَا ذَلِيَكُونَ الرَّسُولُ شَهِيدًا عَلَيْكُمْ وَتَكُونُوا شُهَدَاءَ عَلَى النَّاسِ"
  },
  {
    "id": 176,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1507.64,
    "end": 1515.19,
    "text": "فَأَقِيمُوا الصَّلَاةَ وَآتُوا الزَّكَاةَ وَاعْتَصِمُوا بِاللَّهِ هُوَ مَوْلَاكُمْ"
  },
  {
    "id": 177,
    "sura_id": 22,
    "recitation_uuid": "92f3d3a0-5924-4440-adce-a27562fb66a8",
    "start": 1516.06,
    "end": 1520.92,
    "text": "وَنِعْمَ الْمَوْلَى وَنِعْمَ النَّصِيرُ"
  }
]