[
  {
    "id": 2,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 6.06,
    "end": 15.37,
    "text": "هَلْ أَتَى عَلَى الْإِنْسَانِ حِينٌ مِنَ الدَّهْرِ لَمْ يَكُنْ شَيْئًا مَذْكُورًا"
  },
  {
    "id": 3,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 16.74,
    "end": 28.69,
    "text": "إِنَّا خَلَقْنَا الْإِنْسَانَ مِنْ نُطْفَةٍ أَمْشَاجٍ نَبْتَلِيهِ فَجَعَلْنَاهُ سَمِيعًا بَصِيرًا"
  },
  {
    "id": 4,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 30.14,
    "end": 38.54,
    "text": "إِنَّا هَدَيْنَاهُ السَّبِيلَ إِمَّا شَاكِرًا وَإِمَّا كَفُورًا"
  },
  {
    "id": 5,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 39.85,
    "end": 48.5,
    "text": "إِنَّا أَعْتَدْنَا لِلْكَافِرِينَ سَلَاسِلَ وَأَغْلَالًا وَسَعِيرًا"
  },
  {
    "id": 6,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 49.76,
    "end": 58.35,
    "text": "إِنَّ الْأَبْرَارَ يَشْرَبُونَ مِنْ كَأْسٍ كَانَ مِزَاجُهَا كَافُورًا"
  },
  {
    "id": 7,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 59.9,
    "end": 67.55,
    "text": "وَعَيْنًا يَشْرَبُ بِهَا عِبَادُ اللَّهِ يُفَجِّرُونَهَا تَفْجِيرًا"
  },
  {
    "id": 8,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 68.72,
    "end": 76.66,
    "text": "يُوفُونَ بِالنَّذْرِ وَيَخَافُونَ يَوْمًا كَانَ شَرُّهُ مُسْتَطِيرًا"
  },
  {
    "id": 9,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 78.13,
    "end": 82.13,
    "text": "وَيُطْعِمُونَ الطَّعَامَ عَلَى حُبِّهِ"
  },
  {
    "id": 10,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 82.47,
    "end": 86.93,
    "text": "وَيَتِيمًا وَأَسِيرًا"
  },
  {
    "id": 11,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 88.32,
    "end": 98.93,
    "text": "إِنَّمَا نُطْعِمُكُمْ لِوَجْهِ اللَّهِ لَا نُرِيدُ مِنْكُمْ جَزَاءً وَلَا شُكُورًا"
  },
  {
    "id": 12,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 100.33,
    "end": 107.36,
    "text": "إِنَّا نَخَافُ مِنْ رَبِّنَا يَوْمًا عَبُوسًا قَمْطَرِيرًا"
  },
  {
    "id": 13,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 108.49,
    "end": 116.91,
    "text": "فَوَقَاهُمُ اللَّهُ شَرَّ ذَلِكَ الْيَوْمِ وَلَقَّاهُمْ نَضْرَةً وَسُرُورًا"
  },
  {
    "id": 14,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 118.28,
    "end": 124.45,
    "text": "وَجَزَاهُمْ بِمَا صَبَرُوا جَنَّةً وَحَرِيرًا"
  },
  {
    "id": 15,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 125.64,
    "end": 129.97,
    "text": "مُتَّكِئِينَ فِيهَا عَلَى الْأَرَائِكِ"
  },
  {
    "id": 16,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 130.84,
    "end": 136.12,
    "text": "لَا يَرَوْنَ فِيهَا شَمْسًا وَلَا زَمْهَرِيرًا"
  },
  {
    "id": 17,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 137.42,
    "end": 144.5,
    "text": "وَدَانِيَةً عَلَيْهِمْ ظِلَالُهَا وَذُلِّلَتْ قُطُوفُهَا تَذْلِيلًا"
  },
  {
    "id": 18,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 145.59,
    "end": 156.12,
    "text": "وَيُطَافُ عَلَيْهِمْ بِآنِيَةٍ مِنْ فِضَّةٍ وَأَكْوَابٍ كَانَتْ قَوَارِيرَا"
  },
  {
    "id": 19,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 157.18,
    "end": 163.36,
    "text": "قَوَارِيرَ مِنْ فِضَّةٍ قَدَّرُوهَا تَقْدِيرًا"
  },
  {
    "id": 20,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 164.15,
    "end": 170.95,
    "text": "وَيُسْقَوْنَ فِيهَا كَأْسًا كَانَ مِزَاجُهَا زَنْجَبِيلًا"
  },
  {
    "id": 21,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 171.87,
    "end": 177.26,
    "text": "عَيْنًا فِيهَا تُسَمَّى سَلْسَبِيلًا"
  },
  {
    "id": 22,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 178.6,
    "end": 189.49,
    "text": "وَيَطُوفُ عَلَيْهِمْ وِلْدَانٌ مُخَلَّدُونَ إِذَا رَأَيْتَهُمْ حَسِبْتَهُمْ لُؤْلُؤًا مَنْثُورًا"
  },
  {
    "id": 23,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 190.85,
    "end": 198.22,
    "text": "وَإِذَا رَأَيْتَ ثَمَّ رَأَيْتَ نَعِيمًا وَمُلْكًا كَبِيرًا"
  },
  {
    "id": 24,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 199.65,
    "end": 205.57,
    "text": "عَالِيَهُمْ ثِيَابُ سُنْدُسٍ خُضْرٌ وَإِسْتَبْرَقٌ"
  },
  {
    "id": 25,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 206.37,
    "end": 215.78,
    "text": "وَحُلُّوا أَسَاوِرَ مِنْ فِضَّةٍ وَسَقَاهُمْ رَبُّهُمْ شَرَابًا طَهُورًا"
  },
  {
    "id": 26,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 217.11,
    "end": 225.76,
    "text": "إِنَّ هَذَا كَانَ لَكُمْ جَزَاءً وَكَانَ سَعْيُكُمْ مَشْكُورًا"
  },
  {
    "id": 27,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 226.88,
    "end": 233.17,
    "text": "إِنَّا نَحْنُ نَزَّلْنَا عَلَيْكَ الْقُرْآنَ تَنْزِيلًا"
  },
  {
    "id": 28,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 234.14,
    "end": 240.88,
    "text": "فَاصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تُطِعْ مِنْهُمْ آثِمًا أَوْ كَفُورًا"
  },
  {
    "id": 29,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 242.09,
    "end": 246.87,
    "text": "وَاذْكُرِ اسْمَ رَبِّكَ بُكْرَةً وَأَصِيلًا"
  },
  {
    "id": 30,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 247.98,
    "end": 254.43,
    "text": "وَمِنَ اللَّيْلِ فَاسْجُدْ لَهُ وَسَبِّحْهُ لَيْلًا طَوِينًا"
  },
  {
    "id": 31,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 255.56,
    "end": 267.46,
    "text": "إِنَّ هَؤُلَاءِ يُحِبُّونَ الْعَاجِلَةَ وَيَذَرُونَ وَرَاءَهُمْ يَوْمًا ثَقِيلًا"
  },
  {
    "id": 32,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 268.57,
    "end": 273.01,
    "text": "نَحْنُ خَلَقْنَاهُمْ وَشَدَدْنَا أَسْرَهُمْ"
  },
  {
    "id": 33,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 273.99,
    "end": 280.42,
    "text": "وَإِذَا شِئْنَا بَدَّلْنَا أَمْثَالَهُمْ تَبْدِيلًا"
  },
  {
    "id": 34,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 281.42,
    "end": 284.42,
    "text": "إِنَّ هَذِهِ تَذْكِرَةٌ"
  },
  {
    "id": 35,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 285.39,
    "end": 290.96,
    "text": "فَمَنْ شَاءَ اتَّخَذَ إِلَى رَبِّهِ سَبِيلًا"
  },
  {
    "id": 36,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 292.04,
    "end": 300.24,
    "text": "وَمَا تَشَاءُونَ إِلَّا أَنْ يَشَاءَ اللَّهُ"
  },
  {
    "id": 37,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 301.21,
    "end": 305.65,
    "text": "إِنَّ اللَّهَ كَانَ عَلِيمًا حَكِيمًا"
  },
  {
    "id": 38,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 306.68,
    "end": 311.0,
    "text": "يُدْخِلُ مَنْ يَشَاءُ فِي رَحْمَتِهِ"
  },
  {
    "id": 39,
    "sura_id": 76,
    "recitation_uuid": "6cac079e-16cf-4976-a7a7-682fe2b3cf43",
    "start": 311.77,
    "end": 317.23,
    "text": "الظَّالِمِينَ أَعَدَّ لَهُمْ عَذَابًا أَلِيمًا"
  }
]