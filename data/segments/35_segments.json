[
  {
    "id": 2,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 5.45,
    "end": 19.92,
    "text": "الْحَمْدُ لِلَّهِ فَاطِرِ السَّمَاوَاتِ وَالْأَرْضِ جَاعِلِ الْمَلَائِكَةِ رُسُلًا أُولِي أَجْنِحَةٍ مَثْنَى وَثُلَاثَ وَرُبَاعَ"
  },
  {
    "id": 3,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 21.23,
    "end": 24.74,
    "text": "يَزِيدُ فِي الْخَلْقِ مَا يَشَاءُ"
  },
  {
    "id": 4,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 25.59,
    "end": 31.56,
    "text": "إِنَّ اللَّهَ عَلَى كُلِّ شَيْءٍ قَدِيرٌ"
  },
  {
    "id": 5,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 33.06,
    "end": 39.83,
    "text": "مَا يَفْتَحِ اللَّهُ لِلنَّاسِ مِنْ رَحْمَةٍ فَلَا مُمْسِكَ لَهَا"
  },
  {
    "id": 6,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 40.58,
    "end": 45.31,
    "text": "وَمَا يُمْسِكْ فَلَا مُرْسِلَ لَهُ مِنْ بَعْدِهِ"
  },
  {
    "id": 7,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 46.13,
    "end": 49.5,
    "text": "وَهُوَ الْعَزِيزُ الْحَكِيمُ"
  },
  {
    "id": 8,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 50.82,
    "end": 57.94,
    "text": "يَا أَيُّهَا النَّاسُ اذْكُرُوا نِعْمَتَ اللَّهِ عَلَيْكُمْ"
  },
  {
    "id": 9,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 58.97,
    "end": 66.56,
    "text": "بِسَلْ مِنْ خَالِقٍ غَيْرُ اللَّهِ يَرْزُقُكُمْ مِنَ السَّمَاءِ وَالْأَرْضِ"
  },
  {
    "id": 10,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 67.61,
    "end": 71.22,
    "text": "لَا إِلَهَ إِلَّا هُوَ"
  },
  {
    "id": 11,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 72.26,
    "end": 76.07,
    "text": "كَأَنَّى تُؤْفَكُونَ"
  },
  {
    "id": 12,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 77.4,
    "end": 84.26,
    "text": "وَإِنْ يُكَذِّبُوكَ فَقَدْ كُذِّبَتْ رُسُلٌ مِنْ قَبْلِكَ"
  },
  {
    "id": 13,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 85.1,
    "end": 88.83,
    "text": "وَإِلَى اللَّهِ تُرْجَعُ الْأُمُورُ"
  },
  {
    "id": 14,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 90.0,
    "end": 95.71,
    "text": "يَا أَيُّهَا النَّاسُ إِنَّ وَعْدَ اللَّهِ"
  },
  {
    "id": 15,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 96.08,
    "end": 96.18,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 16,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 96.97,
    "end": 107.22,
    "text": "فَلَا تَغُرَّنَّكُمُ الْحَيَاةُ الدُّنْيَا وَلَا يَغُرَّنَّكُمْ بِاللَّهِ الْغَرُورُ"
  },
  {
    "id": 17,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 108.28,
    "end": 114.88,
    "text": "إِنَّ الشَّيْطَانَ لَكُمْ عَدُوٌّ فَاتَّخِذُوهُ عَدُوًّا"
  },
  {
    "id": 18,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 115.63,
    "end": 122.91,
    "text": "إِنَّمَا يَدْعُو حِزْبَهُ لِيَكُونُوا مِنْ أَصْحَابِ السَّعِيرِ"
  },
  {
    "id": 19,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 123.95,
    "end": 129.29,
    "text": "الَّذِينَ كَفَرُوا لَهُمْ عَذَابٌ شَدِيدٌ"
  },
  {
    "id": 20,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 130.06,
    "end": 139.64,
    "text": "وَالَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ مَغْفِرَةٌ وَأَجْرٌ كَبِيرٌ"
  },
  {
    "id": 21,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 141.15,
    "end": 147.69,
    "text": "فَمَنْ زُيِّنَ لَهُ سُوءُ عَمَلِهِ فَرَآهُ حَسَنًا"
  },
  {
    "id": 22,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 148.85,
    "end": 157.2,
    "text": "فَإِنَّ اللَّهَ يُضِلُّ مَنْ يَشَاءُ وَيَهْدِي مَنْ يَشَاءُ"
  },
  {
    "id": 23,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 158.11,
    "end": 162.77,
    "text": "فَلَا تَذْهَبْ نَفْسُكَ عَلَيْهِمْ حَسَرَاتٍ"
  },
  {
    "id": 24,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 163.67,
    "end": 169.25,
    "text": "إِنَّ اللَّهَ عَلِيمٌ بِمَا يَصْنَعُونَ"
  },
  {
    "id": 25,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 170.57,
    "end": 185.31,
    "text": "وَاللَّهُ الَّذِي أَرْسَلَ الرِّيَاحَ فَتُثِيرُ سَحَابًا فَسُقْنَاهُ إِلَى بَلَدٍ مَيِّتٍ فَأَحْيَيْنَا بِهِ الْأَرْضَ بَعْدَ مَوْتِهَا"
  },
  {
    "id": 26,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 186.42,
    "end": 189.46,
    "text": "كَذَلِكَ النُّشُورُ"
  },
  {
    "id": 27,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 190.81,
    "end": 197.1,
    "text": "مَنْ كَانَ يُرِيدُ الْعِزَّةَ فَلِلَّهِ الْعِزَّةُ جَمِيعًا"
  },
  {
    "id": 28,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 197.95,
    "end": 203.86,
    "text": "إِلَيْهِ يَصْعَدُ الْكَذِرَ الطَّيِّبُ وَالْعَمَلُ الصَّالِحُ يَرْفَعُهُ"
  },
  {
    "id": 29,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 204.93,
    "end": 211.87,
    "text": "وَالَّذِينَ يَمْكُرُونَ السَّيِّئَاتِ لَهُمْ عَذَابٌ شَدِيدٌ"
  },
  {
    "id": 30,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 212.64,
    "end": 217.39,
    "text": "وَمَكْرُ أُولَئِكَ هُوَ يَبُورُ"
  },
  {
    "id": 31,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 218.73,
    "end": 229.55,
    "text": "وَاللَّهُ خَلَقَكُمْ مِنْ تُرَابٍ ثُمَّ مِنْ نُطْفَةٍ ثُمَّ جَعَلَكُمْ أَزْوَاجًا"
  },
  {
    "id": 32,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 230.56,
    "end": 236.06,
    "text": "وَمَا تَحْمِلُ مِنْ أُنْثَى وَلَا تَضْعُ إِلَّا بِعِلْمِهِ"
  },
  {
    "id": 33,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 237.16,
    "end": 249.17,
    "text": "وَمَا يُعَمَّرُ مِنْ مُعَمَّرٍ وَلَا يُنْقَصُ مِنْ عُمُرِهِ إِلَّا فِي كِتَابٍ"
  },
  {
    "id": 34,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 250.15,
    "end": 254.57,
    "text": "إِنَّ ذَلِكَ عَلَى اللَّهِ يَسِيرٌ"
  },
  {
    "id": 35,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 255.99,
    "end": 268.43,
    "text": "وَمَا يَسْتَوِي الْبَحْرَانِ هَذَا عَذْبٌ فُرَاتٌ سَائِغٌ شَرَابُهُ وَهَذَا مِلْحٌ أُجَاجٌ"
  },
  {
    "id": 36,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 269.63,
    "end": 279.89,
    "text": "وَمِنْ كُلٍّ تَأْكُلُونَ لَحْمًا طَرِيًّا وَتَسْتَخْرِجُونَ حِلْيَةً تَلْبَسُونَهَا"
  },
  {
    "id": 37,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 280.95,
    "end": 290.13,
    "text": "وَتَرَى الْفُلْكَ فِيهِ مَوَاخِرَ لِتَبْتَغُوا مِنْ فَضْلِهِ وَلَعَلَّكُمْ تَشْكُرُونَ"
  },
  {
    "id": 38,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 291.82,
    "end": 306.56,
    "text": "يُولِجُ اللَّيْلَ فِي النَّهَارِ وَيُولِجُ النَّهَارَ فِي اللَّيْلِ وَسَخَّرَ الشَّمْسَ وَالْقَمَرَ كُلٌّ يَجْرِي لِأَجَلٍ مُسَمًّى"
  },
  {
    "id": 39,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 307.92,
    "end": 311.58,
    "text": "ذَلِكُمُ اللَّهُ رَبُّكُمْ لَهُ الْمُلْكُونَ"
  },
  {
    "id": 40,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 312.63,
    "end": 320.66,
    "text": "وَالَّذِينَ تَدْعُونَ مِنْ دُونِهِ مَا يَمْلِكُونَ مِنْ قِطْمِيرٍ"
  },
  {
    "id": 41,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 321.98,
    "end": 330.4,
    "text": "إِنْ تَدْعُوهُمْ لَا يَسْمَعُوا دُعَاءَكُمْ وَلَوْ سَمِعُوا مَا اسْتَجَابُوا لَكُمْ"
  },
  {
    "id": 42,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 331.27,
    "end": 335.46,
    "text": "وَيَوْمَ الْقِيَامَةِ يَكْفُرُونَ بِشِرْكِكُمْ"
  },
  {
    "id": 43,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 336.27,
    "end": 340.32,
    "text": "وَلَا يُنَبِّئُكَ مِثْلُ خَبِيرٍ"
  },
  {
    "id": 44,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 341.59,
    "end": 349.87,
    "text": "يَا أَيُّهَا النَّاسُ أَنْتُمُ الْفُقَرَاءُ إِلَى اللَّهِ"
  },
  {
    "id": 45,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 350.84,
    "end": 354.91,
    "text": "وَاللَّهُ هُوَ الْغَنِيُّ الْحَمِيدُ"
  },
  {
    "id": 46,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 355.93,
    "end": 362.57,
    "text": "إِنْ يَشَأْ يُذْهِبْكُمْ وَيَأْتِ بِخَلْقٍ جَدِيدٍ"
  },
  {
    "id": 47,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 363.26,
    "end": 367.79,
    "text": "وَمَا ذَلِكَ عَلَى اللَّهِ بِعَزِيزٍ"
  },
  {
    "id": 48,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 368.78,
    "end": 372.96,
    "text": "وَلَا تَزِرُ وَازِرَةٌ وِزْرَ أُخْرَى"
  },
  {
    "id": 49,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 373.87,
    "end": 383.39,
    "text": "وَإِنْ تَدْعُ مُثْقَلَةٌ إِلَى حِمْلِهَا لَا يُحْمَلْ مِنْهُ شَيْءٌ وَلَوْ كَانَ ذَا قُرْبًا"
  },
  {
    "id": 50,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 384.9,
    "end": 394.01,
    "text": "إِنَّمَا تُنْذِرُ الَّذِينَ يَخْشَوْنَ رَبَّهُمْ بِالْغَيْبِ وَأَقَامُوا الصَّلَاةَ"
  },
  {
    "id": 51,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 394.77,
    "end": 400.51,
    "text": "وَمَنْ تَزَكَّى فَإِنَّمَا يَتَزَكَّى لِنَفْسِهِ"
  },
  {
    "id": 52,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 401.37,
    "end": 404.69,
    "text": "وَإِلَى اللَّهِ الْمَصِيرُ"
  },
  {
    "id": 53,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 405.87,
    "end": 410.06,
    "text": "وَمَا يَسْتَوِي الْأَمْرَ وَالْبَصِيرَ"
  },
  {
    "id": 54,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 410.85,
    "end": 414.72,
    "text": "وَلَا الظُّلُمَاتُ وَلَا النُّورُ"
  },
  {
    "id": 55,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 415.4,
    "end": 419.05,
    "text": "وَلَا الظِّلُّ وَلَا الْحَرُورُ"
  },
  {
    "id": 56,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 420.08,
    "end": 425.41,
    "text": "وَمَا يَسْتَوِي الْأَحْيَاءُ وَلَا الْأَمْوَاتُ"
  },
  {
    "id": 57,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 426.2,
    "end": 431.54,
    "text": "إِنَّ اللَّهَ يُسْمِعُ مَنْ يَشَاءُ"
  },
  {
    "id": 58,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 432.39,
    "end": 439.73,
    "text": "وَمَا أَنْتَ بِمُسْمِعٍ مَنْ فِي الْقُبُورِ"
  },
  {
    "id": 59,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 440.77,
    "end": 444.79,
    "text": "إِنْ أَنْتَ إِلَّا نَذِيرٌ"
  },
  {
    "id": 60,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 445.59,
    "end": 452.03,
    "text": "إِنَّا أَرْسَلْنَاكَ بِالْحَقِّ بَشِيرًا وَنَذِيرًا"
  },
  {
    "id": 61,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 452.88,
    "end": 458.95,
    "text": "وَإِنْ مِنْ أُمَّةٍ إِلَّا خَلَا فِيهَا نَذِيرٌ"
  },
  {
    "id": 62,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 460.44,
    "end": 476.04,
    "text": "وَإِنْ يُكَذِّبُوكَ فَقَدْ كَذَّبَ الَّذِينَ مِنْ قَبْلِهِمْ جَاءَتْهُمْ رُسُلُهُمْ بِالْبَيِّنَاتِ وَبِالزُّبُرِ وَبِالْكِتَابِ الْمُنِيرِ"
  },
  {
    "id": 63,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 477.05,
    "end": 480.34,
    "text": "ثُمَّ أَخَذْتُ الَّذِينَ كَفَرُوا"
  },
  {
    "id": 64,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 481.49,
    "end": 484.54,
    "text": "فَكَيْفَ كَانَ نَكِيرِ"
  },
  {
    "id": 65,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 485.68,
    "end": 499.81,
    "text": "أَلَمْ تَرَ أَنَّ اللَّهَ أَنْزَلَ مِنَ السَّمَاءِ مَاءً فَأَخْرَجْنَا بِهِ ثَمَرَاتٍ مُخْتَلِفًا أَلْوَانُهَا"
  },
  {
    "id": 66,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 501.03,
    "end": 511.55,
    "text": "وَمِنَ الْجِبَالِ جُدَدٌ بِيضٌ وَحُمْرٌ مُخْتَلِفٌ أَلْوَانُهَا وَغَرَابِيبُ سُودٍ"
  },
  {
    "id": 67,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 512.83,
    "end": 522.84,
    "text": "وَمِنَ النَّاسِ وَالدَّوَابِّ وَالْأَنْعَامِ مُخْتَلِفٌ أَلْوَانُهُ كَذَابِ"
  },
  {
    "id": 68,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 523.91,
    "end": 530.18,
    "text": "إِنَّمَا يَخْشَى اللَّهَ مِنْ عِبَادِهِ الْعُلَمَاءُ"
  },
  {
    "id": 69,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 531.18,
    "end": 536.0,
    "text": "إِنَّ اللَّهَ عَزِيزٌ غَفُورٌ"
  },
  {
    "id": 70,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 537.79,
    "end": 551.9,
    "text": "إِنَّ الَّذِينَ يَتْلُونَ كِتَابَ اللَّهِ وَأَقَامُوا الصَّلَاةَ وَأَنْفَقُوا مِمَّا رَزَقْنَاهُمْ سِرًّا وَعَلَانِيَةً"
  },
  {
    "id": 71,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 553.75,
    "end": 562.15,
    "text": "فِرًّا وَعَلَانِيَةً يَرْجُونَ تِجَارَةً لَنْ تَبُورَ"
  },
  {
    "id": 72,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 563.55,
    "end": 569.87,
    "text": "يُوَسِّيَهُمْ أُجُورَهُمْ وَيَزِيدَهُمْ مِنْ فَضْلِهِ"
  },
  {
    "id": 73,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 570.97,
    "end": 576.24,
    "text": "إِنَّهُ غَفُورٌ شَكُورٌ"
  },
  {
    "id": 74,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 577.71,
    "end": 589.02,
    "text": "وَالَّذِي أَوْحَيْنَا إِلَيْكَ مِنَ الْكِتَابِ هُوَ الْحَقُّ مُصَدِّقًا لِمَا بَيْنَ يَدَيْهِ"
  },
  {
    "id": 75,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 590.25,
    "end": 597.11,
    "text": "إِنَّ اللَّهَ بِعِبَادِهِ لَخَبِيرٌ بَصِيرٌ"
  },
  {
    "id": 76,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 598.75,
    "end": 605.45,
    "text": "ثُمَّ أَوْرَثْنَا الْكِتَابَ الَّذِينَ اصْطَفَيْنَا مِنْ عِبَادِنَا"
  },
  {
    "id": 77,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 606.9,
    "end": 619.41,
    "text": "فَمِنْهُمْ ظَالِمٌ لِنَفْسِهِ وَمِنْهُمْ مُقْتَصِدٌ وَمِنْهُمْ سَابِقٌ بِالْخَيْرَاتِ بِإِذْنِ اللَّهِ"
  },
  {
    "id": 78,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 620.82,
    "end": 624.83,
    "text": "ذَلِكَ هُوَ الْفَضْلُ الْكَبِيرُ"
  },
  {
    "id": 79,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 626.29,
    "end": 637.3,
    "text": "جَنَّاتُ عَدْنٍ يَدْخُلُونَهَا يُحَلَّوْنَ فِيهَا مِنْ أَسَاوِرَ مِنْ ذَهَبٍ وَلُؤْلُؤًا"
  },
  {
    "id": 80,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 638.31,
    "end": 642.11,
    "text": "وَلِبَاسُهُمْ فِيهَا حَرِيمٌ"
  },
  {
    "id": 81,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 643.48,
    "end": 650.55,
    "text": "وَقَالُوا الْحَمْدُ لِلَّهِ الَّذِي أَذْهَبَ عَنَّا الْحَزَنَ"
  },
  {
    "id": 82,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 651.54,
    "end": 657.5,
    "text": "إِنَّ رَبَّنَا لَغَفُورٌ شَكُورٌ"
  },
  {
    "id": 83,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 659.04,
    "end": 673.68,
    "text": "الَّذِي أَحَلَّنَا دَارَ الْمُقَامَةِ مِنْ فَضْلِهِ لَا يَمَسُّنَا فِيهَا نَصَبٌ وَلَا يَمَسُّنَا فِيهَا لُغُوبٌ"
  },
  {
    "id": 84,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 675.32,
    "end": 687.8,
    "text": "وَالَّذِينَ كَفَرُوا لَهُمْ نَارُ جَهَنَّمَ لَا يُقْضَى عَلَيْهِمْ فَيَمُوتُوا وَلَا يُخَفَّفُ عَنْهُمْ مِنْ عَذَابِهَا"
  },
  {
    "id": 85,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 689.02,
    "end": 693.18,
    "text": "كَذَلِكَ نَجْزِي كُلَّ كَفُورٍ"
  },
  {
    "id": 86,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 694.7,
    "end": 705.1,
    "text": "وَهُمْ يَصْطَرِخُونَ فِيهَا رَبَّنَا أَخْرِجْنَا نَعْمَلْ صَالِحًا غَيْرَ الَّذِي كُنَّا نَعْمَلُ"
  },
  {
    "id": 87,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 706.69,
    "end": 718.19,
    "text": "أَوَلَمْ نُعَمِّرْكُمْ مَا يَتَذَكَّرُ فِيهِ مَنْ تَذَكَّرَ وَجَاءَكُمُ النَّذِيرُ"
  },
  {
    "id": 88,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 719.34,
    "end": 724.98,
    "text": "فَذُوقُوا فَمَا لِلظَّالِمِينَ مِنْ نَصِيرٍ"
  },
  {
    "id": 89,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 726.35,
    "end": 731.62,
    "text": "إِنَّ اللَّهَ عَالِمُ غَيْبِ السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 90,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 732.64,
    "end": 738.08,
    "text": "إِنَّهُ عَلِيمٌ بِذَاتِ الصُّدُورِ"
  },
  {
    "id": 91,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 739.34,
    "end": 743.87,
    "text": "هُوَ الَّذِي جَعَلَكُمْ خَلَائِفَ فِي الْأَرْضِ"
  },
  {
    "id": 92,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 744.84,
    "end": 747.98,
    "text": "فَمَنْ كَفَرَ فَعَلَيْهِ كُفْرُهُ"
  },
  {
    "id": 93,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 749.17,
    "end": 756.69,
    "text": "وَلَا يَزِيدُ الْكَافِرِينَ كُفْرُهُمْ عِنْدَ رَبِّهِمْ إِلَّا مَقْتًا"
  },
  {
    "id": 94,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 757.78,
    "end": 763.43,
    "text": "وَلَا يَزِيدُ الْكَافِرِينَ كُفْرُهُمْ إِلَّا خَسَارًا"
  },
  {
    "id": 95,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 764.76,
    "end": 781.03,
    "text": "قُلْ أَرَأَيْتُمْ شُرَكَاءَكُمُ الَّذِينَ تَدْعُونَ مِنْ دُونِ اللَّهِ أَرُونِي مَاذَا خَلَقُوا مِنَ الْأَرْضِ أَمْ لَهُمْ شِرْكٌ فِي السَّمَاوَاتِ"
  },
  {
    "id": 96,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 782.07,
    "end": 792.69,
    "text": "أَمْ لَهُمْ شِرْكٌ فِي السَّمَاوَاتِ أَمْ آتَيْنَاهُمْ كِتَابًا فَهُمْ عَلَى بَيِّنَةٍ مِنْهُ"
  },
  {
    "id": 97,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 794.02,
    "end": 801.46,
    "text": "بَلْ إِنْ يَعِدُ الظَّالِمُونَ بَعْضُهُمْ بَعْضًا إِلَّا غُرُورًا"
  },
  {
    "id": 98,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 802.48,
    "end": 809.47,
    "text": "إِنَّ اللَّهَ يُمْسِكُ السَّمَاوَاتِ وَالْأَرْضَ أَنْ تَزُولَا"
  },
  {
    "id": 99,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 810.76,
    "end": 818.91,
    "text": "وَلَئِنْ زَالَتَا إِنْ أَمْسَكَهُمَا مِنْ أَحَدٍ مِنْ بَعْدِهِ"
  },
  {
    "id": 100,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 819.88,
    "end": 824.01,
    "text": "إِنَّهُ كَانَ حَلِيمًا غَفُورًا"
  },
  {
    "id": 101,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 825.13,
    "end": 835.81,
    "text": "وَأَقْسَمُوا بِاللَّهِ جَهْدَ أَيْمَانِهِمْ لَئِنْ جَاءَهُمْ نَذِيرٌ لَيَكُونُنَّ أَهْدَى"
  },
  {
    "id": 102,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 836.13,
    "end": 836.99,
    "text": "وَبِالْأُمَمِينَ"
  },
  {
    "id": 103,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 838.2,
    "end": 846.06,
    "text": "فَلَمَّا جَاءَهُمْ نَذِيرٌ مَا زَادَهُمْ إِلَّا نُكُورًا"
  },
  {
    "id": 104,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 847.2,
    "end": 847.23,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 105,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 847.55,
    "end": 851.82,
    "text": "بِكْبَارًا فِي الْأَرْضِ وَمَكْرَ السَّيِّئِ"
  },
  {
    "id": 106,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 852.61,
    "end": 857.26,
    "text": "وَلَا يَحِيقُ الْمَكْرُ السَّيِّئُ إِلَّا بِأَهْلِهِ"
  },
  {
    "id": 107,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 858.05,
    "end": 863.96,
    "text": "فَهَلْ يَنْظُرُونَ إِلَّا سُنَّةَ الْأَوَّلِينَ"
  },
  {
    "id": 108,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 864.89,
    "end": 869.97,
    "text": "فَلَنْ تَجِدَ لِسُنَّةِ اللَّهِ تَبْدِيلًا"
  },
  {
    "id": 109,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 870.61,
    "end": 875.68,
    "text": "وَلَنْ تَجِدَ لِسُنَّةِ اللَّهِ تَحْوِيلًا"
  },
  {
    "id": 110,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 876.54,
    "end": 890.09,
    "text": "أَوَلَمْ يَسِيرُوا فِي الْأَرْضِ فَيَنْظُرُوا كَيْفَ كَانَ عَاقِبَةُ الَّذِينَ مِنْ قَبْلِهِمْ وَكَانُوا أَشَدَّ مِنْهُمْ قُوَّةً"
  },
  {
    "id": 111,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 891.41,
    "end": 899.91,
    "text": "وَمَا كَانَ اللَّهُ لِيُعْجِزَهُ مِنْ شَيْءٍ فِي السَّمَاوَاتِ وَلَا فِي الْأَرْضِ"
  },
  {
    "id": 112,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 900.92,
    "end": 905.62,
    "text": "إِنَّهُ كَانَ عَلِيمًا قَدِيرًا"
  },
  {
    "id": 113,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 906.64,
    "end": 921.02,
    "text": "وَلَوْ يُؤَاخِذُ اللَّهُ النَّاسَ بِمَا كَسَبُوا مَا تَرَكَ عَلَى ظَهْرِهَا مِنْ دَابَّةٍ وَلَكِنْ يُؤَخِّرُهُمْ"
  },
  {
    "id": 114,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 922.07,
    "end": 929.05,
    "text": "وَلَكِنْ يُؤَخِّرُهُمْ إِلَى أَجَلٍ مُسَمًّى"
  },
  {
    "id": 115,
    "sura_id": 35,
    "recitation_uuid": "cf3530e9-f9da-49f6-b029-9375e78b0b84",
    "start": 930.19,
    "end": 938.59,
    "text": "فَإِذَا جَاءَ أَجَلُهُمْ فَإِنَّ اللَّهَ كَانَ بِعِبَادِهِ بَصِيرًا"
  }
]