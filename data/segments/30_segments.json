[
  {
    "id": 2,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 6.16,
    "end": 11.83,
    "text": "الم"
  },
  {
    "id": 3,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 12.92,
    "end": 15.18,
    "text": "غُلِبَتِ الرُّومُ"
  },
  {
    "id": 4,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 16.35,
    "end": 24.34,
    "text": "فِي أَدْنَى الْأَرْضِ وَهُمْ مِنْ بَعْدِ غَنَبِهِمْ سَيَغْلِبُونَ"
  },
  {
    "id": 5,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 25.28,
    "end": 27.71,
    "text": "فِي بِضْعِ سِنِينَ"
  },
  {
    "id": 6,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 28.78,
    "end": 33.59,
    "text": "اللَّهِ الْأَمْرُ مِنْ قَبْلُ وَمِنْ بَعْدُ"
  },
  {
    "id": 7,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 34.55,
    "end": 39.02,
    "text": "وَيَوْمَئِذٍ يَفْرَحُ الْمُؤْمِنُونَ"
  },
  {
    "id": 8,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 39.95,
    "end": 42.06,
    "text": "بِنَصْرِ اللَّهِ"
  },
  {
    "id": 9,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 42.97,
    "end": 46.5,
    "text": "يَنْصُرُ مَنْ يَشَاءُ"
  },
  {
    "id": 10,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 47.37,
    "end": 50.88,
    "text": "وَهُوَ الْعَزِيزُ الرَّحِيمُ"
  },
  {
    "id": 11,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 51.89,
    "end": 53.89,
    "text": "وَعْدَ اللَّهِ"
  },
  {
    "id": 12,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 54.81,
    "end": 63.09,
    "text": "لَا يُخْلِفُ اللَّهُ وَعْدَهُ وَلَكِنَّ أَكْثَرَ النَّاسِ لَا يَعْلَمُونَ"
  },
  {
    "id": 13,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 64.33,
    "end": 73.29,
    "text": "يَعْلَمُونَ ظَاهِرًا مِنَ الْحَيَاةِ الدُّنْيَا وَهُمْ عَنِ الْآخِرَةِ هُمْ غَافِلُونَ"
  },
  {
    "id": 14,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 74.28,
    "end": 78.97,
    "text": "أَوَلَمْ يَتَفَكَّرُوا فِي أَنْفُسِهِمْ"
  },
  {
    "id": 15,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 79.88,
    "end": 91.47,
    "text": "مَا خَلَقَ اللَّهُ السَّمَاوَاتِ وَالْأَرْضَ وَمَا بَيْنَهُمَا إِلَّا بِالْحَقِّ وَأَجَلٍ مُسَمًّى"
  },
  {
    "id": 16,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 92.88,
    "end": 101.57,
    "text": "وَإِنَّ كَثِيرًا مِنَ النَّاسِ بِلِقَاءِ رَبِّهِمْ لَكَافِرُونَ"
  },
  {
    "id": 17,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 102.75,
    "end": 111.7,
    "text": "أَوَلَمْ يَسِيرُوا فِي الْأَرْضِ فَيَنْظُرُوا كَيْفَ كَانَ عَاقِبَةُ الَّذِينَ مِنْ قَبْلِهِمْ"
  },
  {
    "id": 18,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 112.87,
    "end": 124.11,
    "text": "كَانُوا أَشَدَّ مِنْهُمْ قُوَّةً وَأَثَارُ الْأَرْضَ وَعَمَرُوهَا أَكْثَرَ مِمَّا عَمَرُوهَا"
  },
  {
    "id": 19,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 125.64,
    "end": 138.06,
    "text": "وَأَثَارُوا الْأَرْضَ وَعَمَرُوهَا أَكْثَرَ مِمَّا عَمَرُوهَا وَجَاءَتْهُمْ رُسُلُهُمْ بِالْبَيِّنَاتِ"
  },
  {
    "id": 20,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 139.29,
    "end": 149.54,
    "text": "فَمَا كَانَ اللَّهُ لِيَظْلِمَهُمْ وَلَكِنْ كَانُوا أَنْفُسَهُمْ يَظْلِمُونَ"
  },
  {
    "id": 21,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 150.72,
    "end": 166.54,
    "text": "ثُمَّ كَانَ عَاقِبَةَ الَّذِينَ أَسَاءُوا السُّوأَى أَنْ كَذَّبُوا بِآيَاتِ اللَّهِ وَكَانُوا بِهَا يَسْتَهْزِئُونَ"
  },
  {
    "id": 22,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 168.2,
    "end": 176.21,
    "text": "اللَّهُ يَبْدَأُ الْخَلْقَ ثُمَّ يُعِيدُهُ ثُمَّ إِلَيْهِ تُرْجَعُونَ"
  },
  {
    "id": 23,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 177.66,
    "end": 182.83,
    "text": "وَيَوْمَ تَقُومُ السَّاعَةُ يُبْلِسُ الْمُجْرِمُونَ"
  },
  {
    "id": 24,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 183.8,
    "end": 196.09,
    "text": "وَلَمْ يَكُنْ لَهُمْ مِنْ شُرَكَائِهِمْ شُفَعَاءُ وَكَانُوا بِشُرَكَائِهِمْ كَافِرِينَ"
  },
  {
    "id": 25,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 197.59,
    "end": 203.89,
    "text": "وَيَوْمَ تَقُومُ السَّاعَةُ يَوْمَئِذٍ يَتَفَرَّقُونَ"
  },
  {
    "id": 26,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 205.0,
    "end": 214.67,
    "text": "فَأَمَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ فَهُمْ فِي رَوْضَةٍ يُحْبَرُونَ"
  },
  {
    "id": 27,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 215.9,
    "end": 229.85,
    "text": "وَأَمَّا الَّذِينَ كَفَرُوا وَكَذَّبُوا بِآيَاتِنَا وَرِقَاءِ الْآخِرَةِ فَأُولَئِكَ فِي الْعَذَابِ مُحْضَرُونَ"
  },
  {
    "id": 28,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 231.19,
    "end": 237.38,
    "text": "فَسُبْحَانَ اللَّهِ حِينَ تُمْسُونَ وَحِينَ تُصْبِحُونَ"
  },
  {
    "id": 29,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 238.44,
    "end": 246.58,
    "text": "وَلَهُ الْحَمْدُ فِي السَّمَاوَاتِ وَالْأَرْضِ وَعَشِيًّا وَحِينَ تُظْهِرُونَ"
  },
  {
    "id": 30,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 247.8,
    "end": 257.17,
    "text": "يُخْرِجُ الْحَيَّ مِنَ الْمَيِّتِ وَيُخْرِجُ الْمَيِّتَ مِنَ الْحَيِّ وَيُحْيِي الْأَرْضَ بَعْدَ مَوْتِهَا"
  },
  {
    "id": 31,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 258.43,
    "end": 261.57,
    "text": "وَكَذَلِكَ تُخْرَجُونَ"
  },
  {
    "id": 32,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 263.02,
    "end": 280.09,
    "text": "وَمِنْ آيَاتِهِ أَنْ خَلَقَكُمْ مِنْ تُرَابٍ ثُمَّ إِذَا أَنْتُمْ بَشَرٌ تَنْتَشِرُونَ"
  },
  {
    "id": 33,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 281.84,
    "end": 297.36,
    "text": "وَمِنْ آيَاتِهِ أَنْ خَلَقَ لَكُمْ مِنْ أَنْفُسِكُمْ أَزْوَاجًا لِتَسْكُنُوا إِلَيْهَا وَجَعَلَ بَيْنَكُمْ مَوَدَّةً وَرَحْمَةً"
  },
  {
    "id": 34,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 298.68,
    "end": 306.0,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِقَوْمٍ يَتَفَكَّرُونَ"
  },
  {
    "id": 35,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 307.54,
    "end": 316.4,
    "text": "وَمِنْ آيَاتِهِ خَلْقُ السَّمَاوَاتِ وَالْأَرْضِ وَاخْتِلَافُ أَلْسِنَتِكُمْ وَأَلْوَانِكُمْ"
  },
  {
    "id": 36,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 317.35,
    "end": 323.29,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِلْعَالِمِينَ"
  },
  {
    "id": 37,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 324.65,
    "end": 335.28,
    "text": "وَمِنْ آيَاتِهِ مَنَامُكُمْ بِاللَّيْلِ وَالنَّهَارِ وَابْتِغَاؤُكُمْ مِنْ فَضْلِهِ"
  },
  {
    "id": 38,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 336.37,
    "end": 343.1,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِقَوْمٍ يَسْمَعُونَ"
  },
  {
    "id": 39,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 344.56,
    "end": 360.48,
    "text": "وَمِنْ آيَاتِهِ يُرِيكُمُ الْبَرْقَ خَوْفًا وَطَمَعًا وَيُنَزِّلُ مِنَ السَّمَاءِ مَاءً فَيُحْيِي بِهِ الْأَرْضَ بَعْدَ مَوْتِهَا"
  },
  {
    "id": 40,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 361.73,
    "end": 368.64,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِقَوْمٍ يَعْقِلُونَ"
  },
  {
    "id": 41,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 369.88,
    "end": 377.86,
    "text": "وَمِنْ آيَاتِهِ أَنْ تَقُومَ السَّمَاءُ وَالْأَرْضُ بِأَمْرِهِ"
  },
  {
    "id": 42,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 379.11,
    "end": 389.74,
    "text": "ثُمَّ إِذَا دَعَاكُمْ دَعْوَةً مِنَ الْأَرْضِ إِذَا أَنْتُمْ تَخْرُجُونَ"
  },
  {
    "id": 43,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 390.91,
    "end": 394.92,
    "text": "وَلَهُ مَنْ فِي السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 44,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 395.88,
    "end": 399.12,
    "text": "كُلٌّ لَهُ قَانِتُونَ"
  },
  {
    "id": 45,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 400.39,
    "end": 408.15,
    "text": "وَهُوَ الَّذِي يَبْدَأُ الْخَلْقَ ثُمَّ يُعِيدُهُ وَهُوَ أَهْوَنُ عَلَيْهِ"
  },
  {
    "id": 46,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 409.29,
    "end": 414.08,
    "text": "وَلَهُ الْمَثَلُ الْأَعْلَى فِي السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 47,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 415.05,
    "end": 418.74,
    "text": "وَهُوَ الْعَزِيزُ الْحَكِيمُ"
  },
  {
    "id": 48,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 419.93,
    "end": 425.22,
    "text": "ضَرَبَ لَكُمْ مَثَلًا مِنْ أَنْفُسِكُمْ"
  },
  {
    "id": 49,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 426.64,
    "end": 448.56,
    "text": "أَلَّكُمْ مِمَّا مَلَكَتْ أَيْمَانُكُمْ مِنْ شُرَكَاءَ فِيمَا رَزَقْنَاكُمْ فَأَنْتُمْ فِيهِ سَوَاءٌ تَخَافُونَهُمْ كَخِيفَتِكُمْ أَنْفُسَكُمْ"
  },
  {
    "id": 50,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 449.83,
    "end": 456.65,
    "text": "كَذَلِكَ نُفَصِّلُ الْآيَاتِ لِقَوْمٍ يَعْقِلُونَ"
  },
  {
    "id": 51,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 458.09,
    "end": 465.43,
    "text": "بَلِ اتَّبَعَ الَّذِينَ ظَلَمُوا أَهْوَاءَهُمْ بِغَيْرِ عِلْمٍ"
  },
  {
    "id": 52,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 466.41,
    "end": 470.97,
    "text": "فَمَنْ يَهْدِي مَنْ أَضَلَّ اللَّهُ"
  },
  {
    "id": 53,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 471.8,
    "end": 476.84,
    "text": "وَمَا لَهُمْ مِنْ نَاصِرِينَ"
  },
  {
    "id": 54,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 477.88,
    "end": 481.67,
    "text": "فَأَقِمْ وَجْهَكَ لِلدِّينِ حَنِيفًا"
  },
  {
    "id": 55,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 482.43,
    "end": 487.88,
    "text": "بِضْرَةَ اللَّهِ الَّتِي فَطَرَ النَّاسَ عَلَيْهَا"
  },
  {
    "id": 56,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 488.66,
    "end": 492.25,
    "text": "لَا تَبْدِيلَ لِخَلْقِ اللَّهِ"
  },
  {
    "id": 57,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 493.22,
    "end": 501.86,
    "text": "ذَلِكَ الدِّينُ الْقَيِّمُ وَلَكِنَّ أَكْثَرَ النَّاسِ لَا يَعْلَمُونَ"
  },
  {
    "id": 58,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 503.24,
    "end": 512.92,
    "text": "مُنِيبِينَ إِلَيْهِ وَاتَّقُوهُ وَأَقِيمُوا الصَّلَاةَ وَلَا تَكُونُوا مِنَ الْمُشْرِكِينَ"
  },
  {
    "id": 59,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 513.9,
    "end": 519.22,
    "text": "وَمِنَ الَّذِينَ فَرَّقُوا دِينَهُمْ وَكَانُوا شِيَعًا"
  },
  {
    "id": 60,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 520.11,
    "end": 525.6,
    "text": "كُلُّ حِزْبٍ بِمَا لَدَيْهِمْ فَرِحُونَ"
  },
  {
    "id": 61,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 527.15,
    "end": 548.78,
    "text": "وَإِذَا مَسَّ النَّاسَ ضُرٌّ دَعَوْا رَبَّهُمْ مُنِيبِينَ إِلَيْهِ ثُمَّ إِذَا أَذَاقَهُمْ مِنْهُ رَحْمَةً إِذَا فَرِيقٌ مِنْهُمْ بِرَبِّهِمْ يُشْرِكُونَ"
  },
  {
    "id": 62,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 550.16,
    "end": 554.35,
    "text": "لِيَكْفُرُوا بِمَا آتَيْنَاهُمْ"
  },
  {
    "id": 63,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 555.1,
    "end": 559.37,
    "text": "فَتَمَتَّعُوا فَسَوْفَ تَعْلَمُونَ"
  },
  {
    "id": 64,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 560.76,
    "end": 571.5,
    "text": "أَمْ أَنْزَلْنَا عَلَيْهِمْ سُلْطَانًا فَهُوَ يَتَكَلَّمُ بِمَا كَانُوا بِهِ يُشْرِكُونَ"
  },
  {
    "id": 65,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 572.86,
    "end": 579.52,
    "text": "وَإِذَا أَذَقْنَا النَّاسَ رَحْمَةً فَرِحُوا بِهَا"
  },
  {
    "id": 66,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 580.54,
    "end": 590.5,
    "text": "وَإِنْ تُصِبْهُمْ سَيِّئَةٌ بِمَا قَدَّمَتْ أَيْدِيهِمْ إِذَا هُمْ يَقْنَطُونَ"
  },
  {
    "id": 67,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 592.0,
    "end": 600.55,
    "text": "أَوَلَمْ يَرَوْا أَنَّ اللَّهَ يَبْسُطُ الرِّزْقَ لِمَنْ يَشَاءُ وَيَقْدِرُ"
  },
  {
    "id": 68,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 601.55,
    "end": 608.78,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِقَوْمٍ يُؤْمِنُونَ"
  },
  {
    "id": 69,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 610.15,
    "end": 616.69,
    "text": "فَآتِ ذَا الْقُرْبَى حَقَّهُ وَالْمِسْكِينَ وَابْنَ السَّبِيلِ"
  },
  {
    "id": 70,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 617.54,
    "end": 623.74,
    "text": "ذَلِكَ خَيْرٌ لِلَّذِينَ يُرِيدُونَ وَجْهَ اللَّهِ"
  },
  {
    "id": 71,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 624.55,
    "end": 629.58,
    "text": "وَأُولَئِكَ هُمُ الْمُفْلِحُونَ"
  },
  {
    "id": 72,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 630.82,
    "end": 642.5,
    "text": "وَمَا آتَيْتُمْ مِنْ رِبًا لِيَرْبُوَ فِي أَمْوَالِ النَّاسِ فَلَا يَرْبُو عِنْدَ اللَّهِ"
  },
  {
    "id": 73,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 643.52,
    "end": 656.63,
    "text": "وَمَا آتَيْتُمْ مِنْ زَكَاةٍ تُرِيدُونَ وَجْهَ اللَّهِ فَأُولَئِكَ هُمُ الْمُضْعِفُونَ"
  },
  {
    "id": 74,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 658.28,
    "end": 667.9,
    "text": "اللَّهُ الَّذِي خَلَقَكُمْ ثُمَّ رَزَقَكُمْ ثُمَّ يُمِيتُكُمْ ثُمَّ يُحْيِيكُمْ"
  },
  {
    "id": 75,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 669.11,
    "end": 679.91,
    "text": "قَالْ مِنْ شُرَكَائِكُمْ مَنْ يَفْعَلُ مِنْ ذَلِكُمْ مِنْ شَيْءٍ"
  },
  {
    "id": 76,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 680.87,
    "end": 686.8,
    "text": "سُبْحَانَهُ وَتَعَالَى عَمَّا يُشْرِكُونَ"
  },
  {
    "id": 77,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 688.23,
    "end": 702.49,
    "text": "ظَهَرَ الْفَسَادُ فِي الْبَرِّ وَالْبَحْرِ بِمَا كَسَبَتْ أَيْدِي النَّاسِ لِيُذِيقَهُمْ بَعْضَ الَّذِي عَمِلُوا لَعَلَّهُمْ يَرْجِعُونَ"
  },
  {
    "id": 78,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 703.47,
    "end": 711.79,
    "text": "قُلْ سِيرُوا فِي الْأَرْضِ فَانْظُرُوا كَيْفَ كَانَ عَاقِبَةُ الَّذِينَ مِنْ قَبْلُ"
  },
  {
    "id": 79,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 712.85,
    "end": 717.14,
    "text": "كَانَ أَكْثَرُهُمْ مُشْرِكِينَ"
  },
  {
    "id": 80,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 718.34,
    "end": 729.48,
    "text": "فَأَقِمْ وَجْهَكَ لِلدِّينِ الْقَيِّمِ مِنْ قَبْلِ أَنْ يَأْتِيَ يَوْمٌ لَا مَرَدَّ لَهُ مِنَ اللَّهِ"
  },
  {
    "id": 81,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 730.34,
    "end": 734.0,
    "text": "يَوْمَئِذٍ يَصَّدَّعُونَ"
  },
  {
    "id": 82,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 735.06,
    "end": 738.18,
    "text": "مَنْ كَفَرَ فَعَلَيْهِ كُفْرُهُ"
  },
  {
    "id": 83,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 739.06,
    "end": 745.73,
    "text": "وَمَنْ عَمِلَ صَالِحًا فَرِئَ أَنْفُسِهِمْ يَمْهَدُونَ"
  },
  {
    "id": 84,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 747.02,
    "end": 753.65,
    "text": "لِيَجْزِيَ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ مِنْ فَضْلِهِ"
  },
  {
    "id": 85,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 754.38,
    "end": 759.38,
    "text": "إِنَّهُ لَا يُحِبُّ الْكَافِرِينَ"
  },
  {
    "id": 86,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 760.66,
    "end": 774.42,
    "text": "وَمِنْ آيَاتِهِ أَنْ يُرْسِلَ الرِّيَاحَ مُبَشِّرَاتٍ وَلِيُذِيقَكُمْ مِنْ رَحْمَتِهِ وَلِتَجْرِيَ الْفُلْكُ بِأَمْرِهِ"
  },
  {
    "id": 87,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 775.58,
    "end": 785.23,
    "text": "وَلِتَجْرِيَ الْفُلْكُ بِأَمْرِهِ وَلِتَبْتَغُوا مِنْ فَضْلِهِ وَلَعَلَّكُمْ تَشْكُرُونَ"
  },
  {
    "id": 88,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 786.62,
    "end": 801.32,
    "text": "وَلَقَدْ أَرْسَلْنَا مِنْ قَبْلِكَ رُسُلًا إِلَى قَوْمِهِمْ فَجَاءُوهُمْ بِالْبَيِّنَاتِ فَانْتَقَمْنَا مِنَ الَّذِينَ أَجْرَمُوا"
  },
  {
    "id": 89,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 802.4,
    "end": 803.35,
    "text": "وَكَانَ"
  },
  {
    "id": 90,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 803.66,
    "end": 807.78,
    "text": "وَأَوَّلْنَا عَلَيْنَا نَصْرُ الْمُؤْمِنِينَ"
  },
  {
    "id": 91,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 809.0,
    "end": 823.28,
    "text": "اللَّهُ الَّذِي يُرْسِلُ الرِّيَاحَ فَتُثِيرُ سَحَابًا فَيَبْسُطُهُ فِي السَّمَاءِ كَيْفَ يَشَاءُ وَيَجْعَلُهُ كِسَفًا"
  },
  {
    "id": 92,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 824.62,
    "end": 830.53,
    "text": "وَيَجْعَلُهُ كِسَفًا فَتَرَى الْوَدْقَ يَخْرُجُ مِنْ خِلَالِهِ"
  },
  {
    "id": 93,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 831.68,
    "end": 842.93,
    "text": "فَإِذَا أَصَابَ بِهِ مَنْ يَشَاءُ مِنْ عِبَادِهِ إِذَا هُمْ يَسْتَبْشِرُونَ"
  },
  {
    "id": 94,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 843.88,
    "end": 854.17,
    "text": "وَإِنْ كَانُوا مِنْ قَبْلِ أَنْ يُنَزَّلَ عَلَيْهِمْ مِنْ قَبْلِهِ لَمُبْلِسِينَ"
  },
  {
    "id": 95,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 855.2,
    "end": 863.88,
    "text": "فَانْظُرْ إِلَى آثَارِ رَحْمَتِ اللَّهِ كَيْفَ يُحْيِي الْأَرْضَ بَعْدَ مَوْتِهَا"
  },
  {
    "id": 96,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 864.86,
    "end": 868.53,
    "text": "إِنَّ ذَلِكَ لَمُحْيِي الْمَوْتَى"
  },
  {
    "id": 97,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 869.54,
    "end": 874.32,
    "text": "وَهُوَ عَلَى كُلِّ شَيْءٍ قَدِيرٌ"
  },
  {
    "id": 98,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 875.89,
    "end": 886.33,
    "text": "وَلَئِنْ أَرْسَلْنَا رِيحًا فَرَأَوْهُ مُصْفَرًّا لَظَلُّوا مِنْ بَعْدِهِ يَكْفُرُونَ"
  },
  {
    "id": 99,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 887.3,
    "end": 897.98,
    "text": "فَإِنَّكَ لَا تُسْمِعُ الْمَوْتَى وَلَا تُسْمِعُ الصُّمَّ الدُّعَاءَ إِذَا وَلَّوْا مُدْبِرِينَ"
  },
  {
    "id": 100,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 898.96,
    "end": 905.96,
    "text": "وَمَا أَنْتَ بِهَادِ الْعُمْيِ عَنْ ضَلَالَتِهِمْ"
  },
  {
    "id": 101,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 907.03,
    "end": 915.51,
    "text": "إِنْ تُسْمِعُ إِلَّا مَنْ يُؤْمِنُ بِآيَاتِنَا فَهُمْ مُسْلِمُونَ"
  },
  {
    "id": 102,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 916.73,
    "end": 935.22,
    "text": "اللَّهُ الَّذِي خَلَقَكُمْ مِنْ ضَعْفٍ ثُمَّ جَعَلَ مِنْ بَعْدِ ضَعْفٍ قُوَّةً ثُمَّ جَعَلَ مِنْ بَعْدِ قُوَّةٍ ضَعْفًا وَشَيْبَةً"
  },
  {
    "id": 103,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 936.44,
    "end": 939.05,
    "text": "يَخْلُقُ مَا يَشَاءُ"
  },
  {
    "id": 104,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 939.89,
    "end": 943.3,
    "text": "وَهُوَ الْعَلِيمُ الْقَدِيمُ"
  },
  {
    "id": 105,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 944.42,
    "end": 951.68,
    "text": "وَيَوْمَ تَقُومُ السَّاعَةُ يُقْسِمُ الْمُجْرِمُونَ مَا لَبِثُوا غَيْرَ سَاعَةٍ"
  },
  {
    "id": 106,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 952.65,
    "end": 956.38,
    "text": "كَذَلِكَ كَانُوا يُؤْفَكُونَ"
  },
  {
    "id": 107,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 957.57,
    "end": 967.24,
    "text": "وَقَالَ الَّذِينَ أُوتُوا الْعِلْمَ وَالْإِيمَانَ لَقَدْ لَبِثْتُمْ فِي كِتَابِ اللَّهِ إِلَى يَوْمِ الْبَعْدِ"
  },
  {
    "id": 108,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 968.42,
    "end": 976.46,
    "text": "فَهَذَا يَوْمُ الْبَعْثِ وَلَكِنَّكُمْ كُنْتُمْ لَا تَعْلَمُونَ"
  },
  {
    "id": 109,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 977.76,
    "end": 987.25,
    "text": "فَيَوْمَئِذٍ لَا يَنْفَعُ الَّذِينَ ظَلَمُوا مَعْذِرَتُهُمْ وَلَا هُمْ يُسْتَعْتَبُونَ"
  },
  {
    "id": 110,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 988.38,
    "end": 995.44,
    "text": "وَلَقَدْ ضَرَبْنَا لِلنَّاسِ فِي هَذَا الْقُرْآنِ مِنْ كُلِّ مَثَلٍ"
  },
  {
    "id": 111,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 996.48,
    "end": 1009.0,
    "text": "وَلَئِنْ جِئْتَهُمْ بِآيَةٍ لَيَقُولَنَّ الَّذِينَ كَفَرُوا إِنْ أَنْتُمْ إِلَّا مُبْطِلُونَ"
  },
  {
    "id": 112,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 1010.02,
    "end": 1017.2,
    "text": "كَذَلِكَ يَطْبَعُ اللَّهُ عَلَى قُلُوبِ الَّذِينَ لَا يَعْلَمُونَ"
  },
  {
    "id": 113,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 1018.17,
    "end": 1021.67,
    "text": "فَاصْبِرْ إِنَّ وَعْدَ اللَّهِ"
  },
  {
    "id": 114,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 1022.2,
    "end": 1022.23,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 115,
    "sura_id": 30,
    "recitation_uuid": "a0007bbf-1f4b-4101-89c4-42c9b9b1312f",
    "start": 1023.0,
    "end": 1029.21,
    "text": "وَلَا يَسْتَخِفَّنَّكَ الَّذِينَ لَا يُوقِنُونَ"
  }
]