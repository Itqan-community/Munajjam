[
  {
    "id": 2,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 6.05,
    "end": 9.5,
    "text": "اقْرَأْ بِاسْمِ رَبِّكَ الَّذِي خَلَقَ"
  },
  {
    "id": 3,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 10.49,
    "end": 13.61,
    "text": "خَلَقَ الْإِنْسَانَ مِنْ عَلَقٍ"
  },
  {
    "id": 4,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 14.63,
    "end": 17.21,
    "text": "اقْرَأْ وَرَبُّكَ الْأَكْرَمُ"
  },
  {
    "id": 5,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 18.22,
    "end": 20.77,
    "text": "الَّذِي عَلَّمَ بِالْقَلَمِ"
  },
  {
    "id": 6,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 21.84,
    "end": 25.85,
    "text": "عَلَّمَ الْإِنْسَانَ مَا لَمْ يَعْلَمْ"
  },
  {
    "id": 7,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 27.11,
    "end": 32.84,
    "text": "كَلَّا إِنَّ الْإِنْسَانَ لَيَطْغَى"
  },
  {
    "id": 8,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 33.77,
    "end": 35.91,
    "text": "أَنْ رَآهُ اسْتَغْنَى"
  },
  {
    "id": 9,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 36.84,
    "end": 40.14,
    "text": "إِنَّ إِلَى رَبِّكَ الرُّجْعَى"
  },
  {
    "id": 10,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 41.33,
    "end": 43.93,
    "text": "أَرَأَيْتَ الَّذِي يَنْهَى"
  },
  {
    "id": 11,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 44.67,
    "end": 47.09,
    "text": "عَبْدًا إِذَا صَلَّى"
  },
  {
    "id": 12,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 48.11,
    "end": 51.78,
    "text": "أَرَأَيْتَ إِنْ كَانَ عَلَى الْهُدَى"
  },
  {
    "id": 13,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 52.65,
    "end": 54.79,
    "text": "أَوْ أَمَرَ بِالتَّقْوَى"
  },
  {
    "id": 14,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 55.93,
    "end": 59.94,
    "text": "أَرَأَيْتَ إِنْ كَذَّبَ وَتَوَلَّى"
  },
  {
    "id": 15,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 60.9,
    "end": 65.24,
    "text": "أَلَمْ يَعْلَمْ بِأَنَّ اللَّهَ يَرَى"
  },
  {
    "id": 16,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 66.16,
    "end": 72.37,
    "text": "كَلَّا لَئِنْ لَمْ يَنْتَهِ لَنَسْفَعًا بِالنَّاصِيَةِ"
  },
  {
    "id": 17,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 73.38,
    "end": 77.07,
    "text": "نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ"
  },
  {
    "id": 18,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 78.02,
    "end": 79.81,
    "text": "فَلْيَدْعُ نَادِيَهُ"
  },
  {
    "id": 19,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 80.67,
    "end": 82.65,
    "text": "سَنَدْعُ الزَّبَانِيَةَ"
  },
  {
    "id": 20,
    "sura_id": 96,
    "recitation_uuid": "845dc9e8-6c3c-496a-a86b-310805ba60ef",
    "start": 83.64,
    "end": 87.47,
    "text": "كَلَّا لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِبْ"
  }
]