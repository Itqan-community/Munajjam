[
  {
    "id": 2,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 5.7,
    "end": 9.72,
    "text": "اقْتَرَبَتِ السَّاعَةُ وَانْشَقَّ الْقَمَرُ"
  },
  {
    "id": 3,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 10.78,
    "end": 17.14,
    "text": "وَإِنْ يَرَوْا آيَةً يُعْرِضُوا وَيَقُولُوا سِحْرٌ مُسْتَمِدٌ"
  },
  {
    "id": 4,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 18.3,
    "end": 23.09,
    "text": "وَكَذَّبُوا وَاتَّبَعُوا أَهْوَاءَهُمْ"
  },
  {
    "id": 5,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 23.85,
    "end": 26.43,
    "text": "وَكُلُّ أَمْرٍ مُسْتَقِرٌّ"
  },
  {
    "id": 6,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 27.6,
    "end": 34.85,
    "text": "وَلَقَدْ جَاءَهُمْ مِنَ الْأَنْبَاءِ مَا فِيهِ مُزْدَجَرٌ"
  },
  {
    "id": 7,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 35.75,
    "end": 38.0,
    "text": "حِكْمَةٌ بَالِغَةٌ"
  },
  {
    "id": 8,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 38.81,
    "end": 40.85,
    "text": "فَمَا تُغْنِ النُّذُرُ"
  },
  {
    "id": 9,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 41.93,
    "end": 43.48,
    "text": "فَتَوَلَّ عَنْهُمْ"
  },
  {
    "id": 10,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 44.49,
    "end": 48.65,
    "text": "يَوْمَ يَدْعُ الدَّاعِ إِلَى شَيْءٍ نُكُرٍ"
  },
  {
    "id": 11,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 49.82,
    "end": 59.32,
    "text": "خُشَّعًا أَبْصَارُهُمْ يَخْرُجُونَ مِنَ الْأَجْدَاثِ كَأَنَّهُمْ جَرَادٌ مُنْتَشِرٌ"
  },
  {
    "id": 12,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 60.39,
    "end": 63.07,
    "text": "مُهْطِعِينَ إِلَى الدَّاعِ"
  },
  {
    "id": 13,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 63.76,
    "end": 67.86,
    "text": "يَقُولُ الْكَافِرُونَ هَذَا يَوْمٌ عَسِرٌ"
  },
  {
    "id": 14,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 68.93,
    "end": 77.8,
    "text": "كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍ فَكَذَّبُوا عَبْدَنَا وَقَالُوا مَجْنُونٌ وَازْدُجِرَ"
  },
  {
    "id": 15,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 78.85,
    "end": 85.31,
    "text": "فَدَعَا رَبَّهُ أَنِّي مَغْلُوبٌ فَانْتَصِرْ"
  },
  {
    "id": 16,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 86.17,
    "end": 93.5,
    "text": "فَفَتَحْنَا أَبْوَابَ السَّمَاءِ بِمَاءٍ مُنْهَمِرٍ"
  },
  {
    "id": 17,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 94.79,
    "end": 103.5,
    "text": "وَفَجَّرْنَا الْأَرْضَ عُيُونًا فَالْتَقَى الْمَاءُ عَلَى أَمْرٍ قَدْ قُدِرَ"
  },
  {
    "id": 18,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 104.55,
    "end": 109.14,
    "text": "وَحَمَلْنَاهُ عَلَى ذَاتِ أَلْوَاحٍ وَدُسُرٍ"
  },
  {
    "id": 19,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 110.04,
    "end": 115.81,
    "text": "تَجْرِي بِأَعْيُنِنَا جَزَاءً لِمَنْ كَانَ كُفِرَ"
  },
  {
    "id": 20,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 116.75,
    "end": 122.81,
    "text": "وَلَقَدْ تَرَكْنَاهَا آيَةً فَهَلْ مِنْ مُدَّكِرٍ"
  },
  {
    "id": 21,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 123.71,
    "end": 126.79,
    "text": "فَكَيْفَ كَانَ عَذَابِي وَنُذُرِ"
  },
  {
    "id": 22,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 127.79,
    "end": 133.66,
    "text": "وَلَقَدْ يَسَّرْنَا الْقُرْآنَ لِلذِّكْرِ فَهَلْ مِنْ مُدَّكِرٍ"
  },
  {
    "id": 23,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 134.61,
    "end": 139.73,
    "text": "كَذَّبَتْ عَادٌ فَكَيْفَ كَانَ عَذَابِي وَنُذُرِ"
  },
  {
    "id": 24,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 140.65,
    "end": 149.45,
    "text": "إِنَّا أَرْسَلْنَا عَلَيْهِمْ رِيحًا صَرْصَرًا فِي يَوْمِ نَفْسٍ مُسْتَمِرٍّ"
  },
  {
    "id": 25,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 150.76,
    "end": 157.94,
    "text": "تَنْزِعُ النَّاسَ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ مُنْقَعِرٍ"
  },
  {
    "id": 26,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 158.84,
    "end": 161.95,
    "text": "فَكَيْفَ كَانَ عَذَابِي وَنُذُرِ"
  },
  {
    "id": 27,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 163.0,
    "end": 168.84,
    "text": "وَلَقَدْ يَسَّرْنَا الْقُرْآنَ لِلذِّكْرِ فَهَلْ مِنْ مُدَّكِرٍ"
  },
  {
    "id": 28,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 169.72,
    "end": 172.81,
    "text": "كَذَّبَتْ ثَمُودُ بِالنُّذُرِ"
  },
  {
    "id": 29,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 173.61,
    "end": 185.7,
    "text": "فَقَالُوا أَبَشَرًا مِنَّا وَاحِدًا نَتَّبِعُهُ إِنَّا إِذًا لَفِي ضَلَالٍ وَسُعُرٍ"
  },
  {
    "id": 30,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 186.97,
    "end": 193.29,
    "text": "سَأُلْقِيَ الذِّكْرُ عَلَيْهِ مِنْ بَيْنِنَا بَلْ هُوَ كَذَّابٌ أَشِرٌ"
  },
  {
    "id": 31,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 194.38,
    "end": 198.73,
    "text": "سَيَعْلَمُونَ غَدًا مَنِ الْكَذَّابُ الْأَشِرُ"
  },
  {
    "id": 32,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 200.02,
    "end": 206.62,
    "text": "إِنَّا مُرْسِلُو النَّاقَةِ فِتْنَةً لَهُمْ فَارْتَقِبْهُمْ وَاصْطَبِرْ"
  },
  {
    "id": 33,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 207.94,
    "end": 213.66,
    "text": "وَنَبِّئْهُمْ أَنَّ الْمَاءَ قِسْمَةٌ بَيْنَهُمْ"
  },
  {
    "id": 34,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 214.42,
    "end": 216.38,
    "text": "كُلُّ شِرْبٍ"
  },
  {
    "id": 35,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 216.68,
    "end": 217.13,
    "text": "وَالْوَرْ"
  },
  {
    "id": 36,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 218.07,
    "end": 221.97,
    "text": "فَنَادَوْا صَاحِبَهُمْ فَتَعَاطَى فَعَقَرَ"
  },
  {
    "id": 37,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 222.94,
    "end": 226.02,
    "text": "فَكَيْفَ كَانَ عَذَابِي وَنُذُرِ"
  },
  {
    "id": 38,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 226.84,
    "end": 235.66,
    "text": "إِنَّا أَرْسَلْنَا عَلَيْهِمْ صَيْحَةً وَاحِدَةً فَكَانُوا كَهَشِيمِ الْمُعْلَمِينَ"
  },
  {
    "id": 39,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 235.96,
    "end": 236.36,
    "text": "وَيَضِرَ"
  },
  {
    "id": 40,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 237.51,
    "end": 243.32,
    "text": "وَلَقَدْ يَسَّرْنَا الْقُرْآنَ لِلذِّكْرِ فَهَلْ مِنْ مُدَّكِرٍ"
  },
  {
    "id": 41,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 244.64,
    "end": 248.92,
    "text": "كَذَّبَتْ قَوْمُ لُوطٍ بِالنُّذُرِ"
  },
  {
    "id": 42,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 250.06,
    "end": 257.29,
    "text": "إِنَّا أَرْسَلْنَا عَلَيْهِمْ حَاصِبًا إِلَّا آلَ لُوطٍ"
  },
  {
    "id": 43,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 258.03,
    "end": 260.82,
    "text": "نَجَّيْنَاهُمْ بِسَحَرٍ"
  },
  {
    "id": 44,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 261.75,
    "end": 264.94,
    "text": "نِعْمَةً مِنْ عِنْدِنَا"
  },
  {
    "id": 45,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 265.72,
    "end": 268.76,
    "text": "كَذَلِكَ نَجْزِي مَنْشَكَ"
  },
  {
    "id": 46,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 269.83,
    "end": 276.0,
    "text": "وَلَقَدْ أَنْذَرَهُمْ بَطْشَتَنَا فَتَمَارَوْا بِالنُّذُرِ"
  },
  {
    "id": 47,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 276.8,
    "end": 285.65,
    "text": "وَلَقَدْ رَاوَدُوهُ عَنْ ضَيْفِهِ فَطَمَسْنَا أَعْيُنَهُمْ فَذُوقُوا عَذَابِي وَنُذُرِ"
  },
  {
    "id": 48,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 286.85,
    "end": 292.39,
    "text": "وَلَقَدْ صَبَّحَهُمْ بُكْرَةً عَذَابٌ مُسْتَقِرٌّ"
  },
  {
    "id": 49,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 293.33,
    "end": 296.03,
    "text": "فَذُوقُوا عَذَابِي وَنُذُرِ"
  },
  {
    "id": 50,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 296.84,
    "end": 302.79,
    "text": "وَلَقَدْ يَسَّرْنَا الْقُرْآنَ لِلذِّكْرِ فَهَلْ مِنْ مُدَّكِرٍ"
  },
  {
    "id": 51,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 304.03,
    "end": 308.52,
    "text": "وَلَقَدْ جَاءَ آلَ فِرْعَوْنَ النُّذُرُ"
  },
  {
    "id": 52,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 309.47,
    "end": 317.09,
    "text": "كَذَّبُوا بِآيَاتِنَا كُلِّهَا فَأَخَذْنَاهُمْ أَخْذَ عَزِيزٍ مُقْتَدِرٍ"
  },
  {
    "id": 53,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 318.5,
    "end": 328.13,
    "text": "أَكُفَّارُكُمْ خَيْرٌ مِنْ أُولَئِكُمْ أَمْ لَكُمْ بَرَاءَةٌ فِي الزُّبُرِ"
  },
  {
    "id": 54,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 329.24,
    "end": 334.03,
    "text": "أَمْ يَقُولُونَ نَحْنُ جَمِيعٌ مُنْتَصِرٌ"
  },
  {
    "id": 55,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 334.88,
    "end": 338.55,
    "text": "سَيُهْزَمُ الْجَمْعُ وَيُوَلُّونَ الدُّبُرَ"
  },
  {
    "id": 56,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 339.43,
    "end": 344.69,
    "text": "بَلِ السَّاعَةُ مَوْعِدُهُمْ وَالسَّاعَةُ أَدْهَى وَأَمَرُّ"
  },
  {
    "id": 57,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 346.04,
    "end": 350.75,
    "text": "إِنَّ الْمُجْرِمِينَ فِي ضَلَالٍ وَسُعُرٍ"
  },
  {
    "id": 58,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 351.74,
    "end": 358.36,
    "text": "يَوْمَ يُسْحَبُونَ فِي النَّارِ عَلَى وُجُوهِهِمْ ذُوقُوا مَسَّ سَقَرَ"
  },
  {
    "id": 59,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 359.48,
    "end": 363.7,
    "text": "إِنَّا كُلَّ شَيْءٍ خَلَقْنَاهُ بِقَدَرٍ"
  },
  {
    "id": 60,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 364.93,
    "end": 372.74,
    "text": "وَمَا أَمْرُنَا إِلَّا وَاحِدَةٌ كَلَمْحٍ بِالْبَصَرِ"
  },
  {
    "id": 61,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 374.11,
    "end": 379.65,
    "text": "وَلَقَدْ أَهْلَكْنَا أَشْيَاعَكُمْ فَهَلْ مِنْ مُدَّكِرٍ"
  },
  {
    "id": 62,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 380.77,
    "end": 384.56,
    "text": "وَكُلُّ شَيْءٍ فَعَلُوهُ فِي الزُّبُرِ"
  },
  {
    "id": 63,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 385.2,
    "end": 389.55,
    "text": "وَكُلُّ صَغِيرٍ وَكَبِيرٍ مُسْتَقَرٌّ"
  },
  {
    "id": 64,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 390.51,
    "end": 395.69,
    "text": "إِنَّ الْمُتَّقِينَ فِي جَنَّاتٍ وَنَهَرٍ"
  },
  {
    "id": 65,
    "sura_id": 54,
    "recitation_uuid": "c8079a86-45c8-48fd-b013-a9711add3a7f",
    "start": 396.58,
    "end": 401.96,
    "text": "فِي مَقْعَدِ صِدْقٍ عِنْدَ مَلِيكٍ مُقْتَدِرٍ"
  }
]