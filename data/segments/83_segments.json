[
  {
    "id": 2,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 5.59,
    "end": 8.84,
    "text": "وَيْلٌ لِلْمُطَفِّفِينَ"
  },
  {
    "id": 3,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 9.67,
    "end": 15.18,
    "text": "الَّذِينَ إِذَا اكْتَالُوا عَلَى النَّاسِ يَسْتَوْفُونَ"
  },
  {
    "id": 4,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 16.26,
    "end": 21.52,
    "text": "وَإِذَا كَالُوهُمْ أَوْ وَزَنُوهُمْ يُخْسِرُونَ"
  },
  {
    "id": 5,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 22.44,
    "end": 29.25,
    "text": "أَلَا يَظُنُّ أُولَئِكَ أَنَّهُمْ مَبْعُوثُونَ"
  },
  {
    "id": 6,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 30.16,
    "end": 32.52,
    "text": "لِيَوْمٍ عَظِيمٍ"
  },
  {
    "id": 7,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 33.29,
    "end": 38.48,
    "text": "يَوْمَ يَقُومُ النَّاسُ لِرَبِّ الْعَالَمِينَ"
  },
  {
    "id": 8,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 39.54,
    "end": 46.02,
    "text": "كَلَّا إِنَّ كِتَابَ الْفُجَّارِ لَفِي سِجِّينٍ"
  },
  {
    "id": 9,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 46.75,
    "end": 50.74,
    "text": "وَمَا أَدْرَاكَ مَا سِجِّينٌ"
  },
  {
    "id": 10,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 51.48,
    "end": 54.57,
    "text": "كِتَابٌ مَرْقُومٌ"
  },
  {
    "id": 11,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 55.24,
    "end": 60.44,
    "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 12,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 61.47,
    "end": 66.19,
    "text": "الَّذِينَ يُكَذِّبُونَ بِيَوْمِ الدِّينِ"
  },
  {
    "id": 13,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 66.86,
    "end": 73.46,
    "text": "وَمَا يُكَذِّبُ بِهِ إِلَّا كُلُّ مُعْتَدٍ أَثِيمٍ"
  },
  {
    "id": 14,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 74.38,
    "end": 81.69,
    "text": "إِذَا تُتْلَى عَلَيْهِ آيَاتُنَا قَالَ أَسَاطِيرُ الْأَوَّلِينَ"
  },
  {
    "id": 15,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 82.59,
    "end": 83.46,
    "text": "كَلَّا"
  },
  {
    "id": 16,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 84.29,
    "end": 84.61,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 17,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 85.15,
    "end": 91.0,
    "text": "رَانَ عَلَى قُلُوبِهِمْ مَا كَانُوا يَكْسِبُونَ"
  },
  {
    "id": 18,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 91.72,
    "end": 99.08,
    "text": "كَلَّا إِنَّهُمْ عَنْ رَبِّهِمْ يَوْمَئِذٍ لَمَحْجُوبُونَ"
  },
  {
    "id": 19,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 99.84,
    "end": 104.56,
    "text": "ثُمَّ إِنَّهُمْ لَصَالُو الْجَحِيمِ"
  },
  {
    "id": 20,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 105.35,
    "end": 113.04,
    "text": "ثُمَّ يُقَالُ هَذَا الَّذِي كُنْتُمْ بِهِ تُكَذِّبُونَ"
  },
  {
    "id": 21,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 114.03,
    "end": 121.46,
    "text": "كَلَّا إِنَّ كِتَابَ الْأَبْرَارِ لَفِي عِلِّيِّينَ"
  },
  {
    "id": 22,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 122.21,
    "end": 126.69,
    "text": "وَمَا أَدْرَاكَ مَا عِلِّيُّونَ"
  },
  {
    "id": 23,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 127.58,
    "end": 130.72,
    "text": "كِتَابٌ مَرْقُومٌ"
  },
  {
    "id": 24,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 131.33,
    "end": 134.79,
    "text": "يَشْهَدُهُ الْمُقَرَّبُونَ"
  },
  {
    "id": 25,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 135.66,
    "end": 139.73,
    "text": "إِنَّ الْأَبْرَارَ لَفِي نَعِيمٍ"
  },
  {
    "id": 26,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 140.36,
    "end": 145.13,
    "text": "عَلَى الْأَرَائِكِ يَنْظُرُونَ"
  },
  {
    "id": 27,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 145.89,
    "end": 151.02,
    "text": "تَعْرِفُ فِي وُجُوهِهِمْ نَضْرَةَ النَّعِيمِ"
  },
  {
    "id": 28,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 151.88,
    "end": 154.71,
    "text": "يُسْقَوْنَ مِنْ رَحِيقٍ"
  },
  {
    "id": 29,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 155.05,
    "end": 156.45,
    "text": "وَالْعُمُومِ"
  },
  {
    "id": 30,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 157.12,
    "end": 158.55,
    "text": "تِتَامُهُ"
  },
  {
    "id": 31,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 159.54,
    "end": 165.18,
    "text": "وَفِي ذَلِكَ فَلْيَتَنَافَسِ الْمُتَنَافِسُونَ"
  },
  {
    "id": 32,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 166.01,
    "end": 169.79,
    "text": "وَمِزَاجُهُ مِنْ تَسْنِيمٍ"
  },
  {
    "id": 33,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 170.4,
    "end": 175.2,
    "text": "عَيْنًا يَشْرَبُ بِهَا الْمُقَرَّبُونَ"
  },
  {
    "id": 34,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 175.91,
    "end": 183.9,
    "text": "إِنَّ الَّذِينَ أَجْرَمُوا كَانُوا مِنَ الَّذِينَ آمَنُوا يَضْحَكُونَ"
  },
  {
    "id": 35,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 184.97,
    "end": 189.42,
    "text": "وَإِذَا مَرُّوا بِهِمْ يَتَغَامَزُونَ"
  },
  {
    "id": 36,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 190.14,
    "end": 197.94,
    "text": "وَإِذَا انْقَلَبُوا إِلَى أَهْلِهِمُ انْقَلَبُوا فَكِهِينَ"
  },
  {
    "id": 37,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 198.85,
    "end": 210.37,
    "text": "وَإِذَا رَأَوْهُمْ قَالُوا إِنَّ هَؤُلَاءِ لَضَالُّونَ"
  },
  {
    "id": 38,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 211.25,
    "end": 216.85,
    "text": "وَمَا أُرْسِلُوا عَلَيْهِمْ حَافِظِينَ"
  },
  {
    "id": 39,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 217.72,
    "end": 224.02,
    "text": "فَالْيَوْمَ الَّذِينَ آمَنُوا مِنَ الْكُفَّارِ يَضْحَكُونَ"
  },
  {
    "id": 40,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 225.22,
    "end": 230.85,
    "text": "عَلَى الْأَرَائِكِ يَنْظُرُونَ"
  },
  {
    "id": 41,
    "sura_id": 83,
    "recitation_uuid": "2f4b5454-421a-4a15-b4b2-fa85e6bad1fd",
    "start": 231.88,
    "end": 238.51,
    "text": "هَلْ ثُوِّبَ الْكُفَّارُ مَا كَانُوا يَفْعَلُونَ"
  }
]