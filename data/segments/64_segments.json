[
  {
    "id": 2,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 5.76,
    "end": 10.81,
    "text": "يُسَبِّحُ لِلَّهِ مَا فِي السَّمَاوَاتِ وَمَا فِي الْأَرْضِ"
  },
  {
    "id": 3,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 11.84,
    "end": 14.28,
    "text": "لَهُ الْمُلْكُ وَلَهُ الْحَمْدُ"
  },
  {
    "id": 4,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 15.24,
    "end": 20.0,
    "text": "وَهُوَ عَلَى كُلِّ شَيْءٍ قَدِيرٌ"
  },
  {
    "id": 5,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 21.16,
    "end": 28.49,
    "text": "هُوَ الَّذِي خَلَقَكُمْ فَمِنْكُمْ كَافِرٌ وَمِنْكُمْ مُؤْمِنٌ"
  },
  {
    "id": 6,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 29.26,
    "end": 33.46,
    "text": "وَاللَّهُ بِمَا تَعْمَلُونَ بَصِيرٌ"
  },
  {
    "id": 7,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 34.66,
    "end": 41.69,
    "text": "وَلَقَدْ سَمَاوَاتِ وَالْأَرْضَ بِالْحَقِّ وَصَوَّرَكُمْ فَأَحْسَنَ صُوَرَكُمْ"
  },
  {
    "id": 8,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 42.45,
    "end": 45.16,
    "text": "وَإِلَيْهِ الْمَصِيرُ"
  },
  {
    "id": 9,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 46.02,
    "end": 54.34,
    "text": "يَعْلَمُ مَا فِي السَّمَاوَاتِ وَالْأَرْضِ وَيَعْلَمُ مَا تُسِرُّونَ وَمَا تُعْلِنُونَ"
  },
  {
    "id": 10,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 55.21,
    "end": 60.41,
    "text": "وَاللَّهُ عَلِيمٌ بِذَاتِ الصُّدُورِ"
  },
  {
    "id": 11,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 61.75,
    "end": 72.74,
    "text": "أَلَمْ يَأْتِكُمْ نَبَأُ الَّذِينَ كَفَرُوا مِنْ قَبْلُ فَذَاقُوا وَبَالَ أَمْرِهِمْ وَلَهُمْ عَذَابٌ أَلِيمٌ"
  },
  {
    "id": 12,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 73.82,
    "end": 87.13,
    "text": "ذَلِكَ بِأَنَّهُ كَانَتْ تَأْتِيهِمْ رُسُلُهُمْ بِالْبَيِّنَاتِ فَقَالُوا أَبَشَرٌ يَهْدُونَنَا فَكَفَرُوا وَتَوَلَّوْا"
  },
  {
    "id": 13,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 88.36,
    "end": 90.69,
    "text": "وَاسْتَغْنَى اللَّهُ"
  },
  {
    "id": 14,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 91.29,
    "end": 94.72,
    "text": "وَاللَّهُ غَنِيٌّ حَمِيدٌ"
  },
  {
    "id": 15,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 95.9,
    "end": 100.87,
    "text": "سَعَمَ الَّذِينَ كَفَرُوا أَنْ لَنْ يُبْعَثُوا"
  },
  {
    "id": 16,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 101.92,
    "end": 110.1,
    "text": "قُلْ بَلَى وَرَبِّي لَتُبْعَثُنَّ ثُمَّ لَتُنَبَّؤُنَّ بِمَا عَمِلْتُمْ"
  },
  {
    "id": 17,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 111.03,
    "end": 114.61,
    "text": "وَذَلِكَ عَلَى اللَّهِ يَسِيرٌ"
  },
  {
    "id": 18,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 115.64,
    "end": 123.16,
    "text": "لَآمِنُوا بِاللَّهِ وَرَسُولِهِ وَالنُّورِ الَّذِي أَنْزَلْنَا"
  },
  {
    "id": 19,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 124.11,
    "end": 128.33,
    "text": "وَاللَّهُ بِمَا تَعْمَلُونَ خَبِيرٌ"
  },
  {
    "id": 20,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 129.56,
    "end": 132.67,
    "text": "يَوْمَ يَجْمَعُكُمْ لِيَوْمِ الْجَمْعِ"
  },
  {
    "id": 21,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 133.62,
    "end": 136.19,
    "text": "ذَلِكَ يَوْمُ التَّغَابُنِ"
  },
  {
    "id": 22,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 137.2,
    "end": 145.19,
    "text": "وَمَنْ يُؤْمِنْ بِاللَّهِ وَيَعْمَلْ صَالِحًا يُكَفِّرْ عَنْهُ سَيِّئَاتِهِ"
  },
  {
    "id": 23,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 146.34,
    "end": 159.76,
    "text": "يُكَفِّرْ عَنْهُ سَيِّئَاتِهِ وَيُدْخِلْهُ جَنَّاتٍ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ خَالِدِينَ فِيهَا أَبَدًا"
  },
  {
    "id": 24,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 160.93,
    "end": 164.36,
    "text": "ذَلِكَ الْفَوْزُ الْعَظِيمُ"
  },
  {
    "id": 25,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 165.83,
    "end": 176.63,
    "text": "وَالَّذِينَ كَفَرُوا وَكَذَّبُوا بِآيَاتِنَا أُولَئِكَ أَصْحَابُ النَّارِ خَالِدِينَ فِيهَا"
  },
  {
    "id": 26,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 177.57,
    "end": 180.0,
    "text": "وَبِئْسَ الْمَصِيرُ"
  },
  {
    "id": 27,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 181.23,
    "end": 187.54,
    "text": "وَمَا أَصَابَ مِنْ مُصِيبَةٍ إِلَّا بِإِذْنِ اللَّهِ"
  },
  {
    "id": 28,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 188.4,
    "end": 192.62,
    "text": "وَمَنْ يُؤْمِنْ بِاللَّهِ يَهْدِ قَلْبَهُ"
  },
  {
    "id": 29,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 193.54,
    "end": 197.5,
    "text": "وَاللَّهُ بِكُلِّ شَيْءٍ عَلِيمٌ"
  },
  {
    "id": 30,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 198.66,
    "end": 202.76,
    "text": "وَأَطِيعُوا اللَّهَ وَأَطِيعُوا الرَّسُولَ"
  },
  {
    "id": 31,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 203.7,
    "end": 211.72,
    "text": "فَإِنْ تَوَلَّيْتُمْ فَإِنَّمَا عَلَى رَسُولِنَا الْبَلَاغُ الْمُبِينُ"
  },
  {
    "id": 32,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 213.42,
    "end": 217.35,
    "text": "اللَّهُ لَا إِلَهَ إِلَّا هُوَ"
  },
  {
    "id": 33,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 218.22,
    "end": 223.06,
    "text": "وَعَلَى اللَّهِ فَلْيَتَوَكَّلِ الْمُؤْمِنُونَ"
  },
  {
    "id": 34,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 224.09,
    "end": 235.75,
    "text": "يَا أَيُّهَا الَّذِينَ آمَنُوا إِنَّ مِنْ أَزْوَاجِكُمْ وَأَوْلَادِكُمْ عَدُوًّا لَكُمْ فَاحْذَرُوهُمْ"
  },
  {
    "id": 35,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 237.08,
    "end": 245.53,
    "text": "وَإِنْ تَعْفُوا وَتَصْفَحُوا وَتَغْفِرُوا فَإِنَّ اللَّهَ غَفُورٌ رَحِيمٌ"
  },
  {
    "id": 36,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 246.71,
    "end": 251.97,
    "text": "إِنَّمَا أَمْوَالُكُمْ وَأَوْلَادُكُمْ فِتْنَةٌ"
  },
  {
    "id": 37,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 252.82,
    "end": 258.33,
    "text": "وَاللَّهُ عِنْدَهُ أَجْرٌ عَظِيمٌ"
  },
  {
    "id": 38,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 259.46,
    "end": 268.86,
    "text": "فَاتَّقُوا اللَّهَ مَا اسْتَطَعْتُمْ وَاسْمَعُوا وَأَطِيعُوا وَأَنْفِقُوا خَيْرًا لِأَنْفُسِكُمْ"
  },
  {
    "id": 39,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 269.99,
    "end": 277.49,
    "text": "وَمَنْ يُوقَ شُحَّ نَفْسِهِ فَأُولَئِكَ هُمُ الْمُفْلِحُونَ"
  },
  {
    "id": 40,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 279.01,
    "end": 286.45,
    "text": "إِنْ تُقْرِضُوا اللَّهَ قَرْضًا حَسَنًا يُضَاعِفْهُ لَكُمْ وَيَغْفِرْ لَكُمْ"
  },
  {
    "id": 41,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 287.39,
    "end": 290.6,
    "text": "وَاللَّهُ شَكُورٌ حَلِيمٌ"
  },
  {
    "id": 42,
    "sura_id": 64,
    "surah_uuid": "9a55f40f-0b54-42f5-b7e5-806d59032efb",
    "start": 291.41,
    "end": 297.2,
    "text": "عَالِمُ الْغَيْبِ وَالشَّهَادَةِ الْعَزِيزُ الْحَكِيمُ"
  }
]