[
  {
    "id": 2,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 5.84,
    "end": 7.66,
    "text": "مهَّدَ"
  },
  {
    "id": 3,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 8.57,
    "end": 10.5,
    "text": "وَالْقُرْآنِ ذِي الذِّينِ"
  },
  {
    "id": 4,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 11.72,
    "end": 17.48,
    "text": "بَلِ الَّذِينَ كَفَرُوا فِي عِزَّةٍ وَشِقَاقٍ"
  },
  {
    "id": 5,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 18.82,
    "end": 28.17,
    "text": "كَمْ أَهْلَكْنَا مِنْ قَبْلِهِمْ مِنْ قَرْنٍ فَنَادَوْا وَلَاتَ حِينَ مَنَاصٍ"
  },
  {
    "id": 6,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 29.37,
    "end": 36.83,
    "text": "وَعَجِبُوا أَنْ جَاءَهُمْ مُنْذِرٌ مِنْهُمْ"
  },
  {
    "id": 7,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 37.59,
    "end": 43.7,
    "text": "وَقَالَ الْكَافِرُونَ هَذَا سَاحِرٌ كَذَّابٌ"
  },
  {
    "id": 8,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 44.94,
    "end": 49.16,
    "text": "فَجَعَلَ الْآلِهَةَ إِلَهًا وَاحِدًا"
  },
  {
    "id": 9,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 50.22,
    "end": 54.8,
    "text": "إِنَّ هَذَا لَشَيْءٌ عُجَابٌ"
  },
  {
    "id": 10,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 55.94,
    "end": 63.36,
    "text": "وَانْطَلَقَ الْمَلَأُ مِنْهُمْ أَنِ امْشُوا وَاصْبِرُوا عَلَى آلِهَتِكُمْ"
  },
  {
    "id": 11,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 64.52,
    "end": 69.27,
    "text": "إِنَّ هَذَا لَشَيْءٌ يُرَادُ"
  },
  {
    "id": 12,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 70.27,
    "end": 79.18,
    "text": "مَا سَمِعْنَا بِهَذَا فِي الْمِلَّةِ الْآخِرَةِ إِنْ هَذَا إِلَّا اخْتِلَاقٌ"
  },
  {
    "id": 13,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 80.24,
    "end": 84.86,
    "text": "أَأُنْزِلَ عَلَيْهِ الذِّكْرُ مِنْ بَيْنِنَا"
  },
  {
    "id": 14,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 85.61,
    "end": 86.9,
    "text": "بَلْ هُمْ فِي شَكٍّ"
  },
  {
    "id": 15,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 87.21,
    "end": 89.5,
    "text": "مِنْ ذِكْرِي"
  },
  {
    "id": 16,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 90.2,
    "end": 94.18,
    "text": "كَلَّمَّا يَذُوقُوا عَذَابٌ"
  },
  {
    "id": 17,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 95.32,
    "end": 103.58,
    "text": "أَمْ عِنْدَهُمْ خَزَائِنُ رَحْمَةِ رَبِّكَ الْعَزِيزِ الْوَهَّابِ"
  },
  {
    "id": 18,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 104.48,
    "end": 110.58,
    "text": "أَمْ لَهُمْ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا"
  },
  {
    "id": 19,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 111.7,
    "end": 115.22,
    "text": "فَلْيَرْتَقُوا فِي الْأَسْبَابِ"
  },
  {
    "id": 20,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 116.31,
    "end": 123.34,
    "text": "جُنْدٌ مَا هُنَالِكَ مَهْزُومٌ مِنَ الْأَحْزَابِ"
  },
  {
    "id": 21,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 124.52,
    "end": 132.9,
    "text": "كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍ وَعَادٌ وَفِرْعَوْنُ ذُو الْأَوْتَادِ"
  },
  {
    "id": 22,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 133.76,
    "end": 138.64,
    "text": "وَثَمُودُ وَقَوْمُ لُوطٍ وَأَصْحَابُ الْأَيْكَةِ"
  },
  {
    "id": 23,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 139.51,
    "end": 143.56,
    "text": "أُولَئِكَ الْأَحْزَابُ"
  },
  {
    "id": 24,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 144.71,
    "end": 151.13,
    "text": "إِنْ كُلٌّ إِلَّا كَذَّبَ الرُّسُلَ فَحَقَّ عِقَابِ"
  },
  {
    "id": 25,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 151.92,
    "end": 164.09,
    "text": "وَمَا يَنْظُرُ هَؤُلَاءِ إِلَّا صَيْحَةً وَاحِدَةً مَا لَهَا مِنْ فَوَاقٍ"
  },
  {
    "id": 26,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 165.47,
    "end": 172.63,
    "text": "وَقَالُوا رَبَّنَا عَجِّلْ لَنَا قِطَّنَا قَبْلَ يَوْمِ الْحِسَابِ"
  },
  {
    "id": 27,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 173.71,
    "end": 181.11,
    "text": "اصْبِرْ عَلَى مَا يَقُولُونَ وَاذْكُرْ عَبْدَنَا دَاوُودَ ذَا الْأَيْدِ"
  },
  {
    "id": 28,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 181.87,
    "end": 185.57,
    "text": "إِنَّهُ أَوَّابٌ"
  },
  {
    "id": 29,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 186.48,
    "end": 194.79,
    "text": "إِنَّا سَخَّرْنَا الْجِبَالَ مَعَهُ يُسَبِّحْنَ بِالْعَشِيِّ وَالْإِشْرَاقِ"
  },
  {
    "id": 30,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 195.49,
    "end": 197.53,
    "text": "وَالطَّيْرَ مَحْشُورَةٍ"
  },
  {
    "id": 31,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 198.2,
    "end": 202.24,
    "text": "كُلٌّ لَهُ أَوَّابٌ"
  },
  {
    "id": 32,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 203.19,
    "end": 210.37,
    "text": "وَشَدَدْنَا مُلْكَهُ وَآتَيْنَاهُ الْحِكْمَةَ وَفَصْلَ الْخِطَابِ"
  },
  {
    "id": 33,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 211.33,
    "end": 217.21,
    "text": "وَهَلْ أَتَاكَ نَبَأُ الْخَصْمِ إِذْ تَسَوَّرُوا الْمِحْرَابَ"
  },
  {
    "id": 34,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 218.26,
    "end": 222.52,
    "text": "إِذْ دَخَلُوا عَلَى دَاوُودَ فَفَزِعَ مِنْهُمْ"
  },
  {
    "id": 35,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 223.27,
    "end": 224.87,
    "text": "قَالُوا لَا تَخَفْ"
  },
  {
    "id": 36,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 225.69,
    "end": 240.49,
    "text": "وَأَصْمَانِ بَغَى بَعْضُنَا عَلَى بَعْضٍ فَاحْكُمْ بَيْنَنَا بِالْحَقِّ وَلَا تُشْطِطْ وَاهْدِنَا إِلَى سَوَاءِ الصِّرَاطِ"
  },
  {
    "id": 37,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 242.09,
    "end": 254.79,
    "text": "إِنَّ هَذَا أَخِي لَهُ تِسْعٌ وَتِسْعُونَ نَعْجَةً وَلِيَ نَعْجَةٌ وَاحِدَةٌ فَقَالَ أَكْفِلْنِيهَا"
  },
  {
    "id": 38,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 256.43,
    "end": 261.74,
    "text": "فَقَالَ أَكْفِلْنِيهَا وَعَزَّنِي فِي الْخِطَابِ"
  },
  {
    "id": 39,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 262.65,
    "end": 268.06,
    "text": "قَالَ لَقَدْ ظَلَمَكَ بِسُؤَالِ نَعْجَتِكَ إِلَى نِعَاجِهِ"
  },
  {
    "id": 40,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 269.34,
    "end": 284.7,
    "text": "وَإِنَّ كَثِيرًا مِنَ الْخُلَطَاءِ لَيَبْغِي بَعْضُهُمْ عَلَى بَعْضٍ إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ وَقَلِيلٌ مَا هُمْ"
  },
  {
    "id": 41,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 285.91,
    "end": 297.16,
    "text": "وَظَنَّ دَاوُودُ أَنَّمَا فَتَنَّاهُ فَاسْتَغْفَرَ رَبَّهُ وَخَرَّ رَاكِعًا وَأَنَابَ"
  },
  {
    "id": 42,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 298.29,
    "end": 300.71,
    "text": "فَغَفَرْنَا لَهُ ذَلِكَ"
  },
  {
    "id": 43,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 301.54,
    "end": 307.72,
    "text": "وَإِنَّ لَهُ عِنْدَنَا لَزُلْفَى وَحُسْنَ مَآبٍ"
  },
  {
    "id": 44,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 308.94,
    "end": 321.34,
    "text": "يَا دَاوُودُ إِنَّا جَعَلْنَاكَ خَلِيفَةً فِي الْأَرْضِ فَاحْكُمْ بَيْنَ النَّاسِ بِالْحَقِّ وَلَا تَتَّبِعِ الْهَوَى"
  },
  {
    "id": 45,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 322.71,
    "end": 329.39,
    "text": "وَلَا تَتَّبِعِ الْهَوَى فَيُضِلَّكَ عَنْ سَبِيلِ اللَّهِ"
  },
  {
    "id": 46,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 330.36,
    "end": 339.28,
    "text": "إِنَّ الَّذِينَ يَضِلُّونَ عَنْ سَبِيلِ اللَّهِ لَهُمْ عَذَابٌ شَدِيدٌ"
  },
  {
    "id": 47,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 340.22,
    "end": 347.3,
    "text": "لَهُمْ عَذَابٌ شَدِيدٌ بِمَا نَسُوا يَوْمَ الْحِسَابِ"
  },
  {
    "id": 48,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 348.35,
    "end": 355.1,
    "text": "وَمَا خَلَقْنَا السَّمَاءَ وَالْأَرْضَ وَمَا بَيْنَهُمَا بَاطِلًا"
  },
  {
    "id": 49,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 355.98,
    "end": 359.43,
    "text": "ذَلِكَ ظَنُّ الَّذِينَ كَفَرُوا"
  },
  {
    "id": 50,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 360.44,
    "end": 365.61,
    "text": "فَوَيْلٌ لِلَّذِينَ كَفَرُوا مِنَ النَّارِ"
  },
  {
    "id": 51,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 366.83,
    "end": 378.98,
    "text": "أَمْ نَجْعَلُ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ كَالْمُفْسِدِينَ فِي الْأَرْضِ أَمْ نَجْعَلُ الْمُتَّقِينَ كَالْفُجَّارِ"
  },
  {
    "id": 52,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 380.06,
    "end": 392.75,
    "text": "كِتَابٌ أَنْزَلْنَاهُ إِلَيْكَ مُبَارَكٌ لِيَدَّبَّرُوا آيَاتِهِ وَلِيَتَذَكَّرَ أُولُو الْأَلْبَابِ"
  },
  {
    "id": 53,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 393.72,
    "end": 397.9,
    "text": "وَوَهَبْنَا لِدَاوُودَ سُلَيْمَانٍ"
  },
  {
    "id": 54,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 398.69,
    "end": 404.0,
    "text": "نِعْمَ الْعَبْدُ إِنَّهُ أَوَّابٌ"
  },
  {
    "id": 55,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 405.16,
    "end": 411.19,
    "text": "إِذْ عُرِضَ عَلَيْهِ بِالْعَشِيِّ الصَّافِنَاتُ الْجِيَادُ"
  },
  {
    "id": 56,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 412.22,
    "end": 423.24,
    "text": "فَقَالَ إِنِّي أَحْبَبْتُ حُبَّ الْخَيْرِ عَنْ ذِكْرِ رَبِّي حَتَّى تَوَارَتْ بِالْحِجَابِ"
  },
  {
    "id": 57,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 424.1,
    "end": 425.8,
    "text": "رُدُّوهَا عَلَيَّ"
  },
  {
    "id": 58,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 426.71,
    "end": 431.92,
    "text": "وَالطَّفِقَ مَسْحًا بِالسُّوقِ وَالْأَعْنَاقِ"
  },
  {
    "id": 59,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 433.22,
    "end": 443.35,
    "text": "وَلَقَدْ فَتَنَّا سُلَيْمَانَ وَأَلْقَيْنَا عَلَى كُرْسِيِّهِ جَسَدًا ثُمَّ أَنَابَ"
  },
  {
    "id": 60,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 444.41,
    "end": 453.44,
    "text": "قَالَ رَبِّ اغْفِرْ لِي وَهَبْ لِي مُلْكًا لَا يَنْبَغِي لِأَحَدٍ مِنْ بَعْدِي"
  },
  {
    "id": 61,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 454.31,
    "end": 458.77,
    "text": "إِنَّكَ أَنْتَ الْوَهَّابُ"
  },
  {
    "id": 62,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 459.88,
    "end": 468.68,
    "text": "فَسَخَّرْنَا لَهُ الرِّيحَ تَجْرِي بِأَمْرِهِ رُخَاءً حَيْثُ أَصَابَ"
  },
  {
    "id": 63,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 469.67,
    "end": 476.84,
    "text": "وَالشَّيَاطِينَ كُلَّ بَنَّاءٍ وَغَوَّاصٍ"
  },
  {
    "id": 64,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 477.93,
    "end": 482.96,
    "text": "وَآخَرِينَ مُقَرَّنِينَ فِي الْأَصْفَادِ"
  },
  {
    "id": 65,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 483.9,
    "end": 491.13,
    "text": "هَذَا عَطَاؤُنَا فَامْنُنْ أَوْ أَمْسِكْ بِغَيْرِ حِسَابٍ"
  },
  {
    "id": 66,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 492.1,
    "end": 498.7,
    "text": "وَإِنَّ لَهُ عِنْدَنَا لَزُلْفَى وَحُسْنَ مَآبٍ"
  },
  {
    "id": 67,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 499.68,
    "end": 512.44,
    "text": "وَاذْكُرْ عَبْدَنَا أَيُّوبَ إِذْ نَادَى رَبَّهُ أَنِّي مَسَّنِيَ الشَّيْطَانُ بِنُصْبٍ وَعَذَابٍ"
  },
  {
    "id": 68,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 513.23,
    "end": 514.72,
    "text": "ارْكُضْ بِرِجْلٍ"
  },
  {
    "id": 69,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 515.6,
    "end": 521.49,
    "text": "هَذَا مُغْتَسَلٌ بَارِدٌ وَشَرَابٌ"
  },
  {
    "id": 70,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 522.52,
    "end": 534.23,
    "text": "وَوَهَبْنَا لَهُ أَهْلَهُ وَمِثْلَهُمْ مَعَهُمْ رَحْمَةً مِنَّا وَذِكْرَى لِأُولِي الْأَلْبَابِ"
  },
  {
    "id": 71,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 535.57,
    "end": 540.7,
    "text": "وَخُذْ بِيَدِكَ ضِغْثًا فَاضْرِبْ بِهِ وَلَا تَحْنَى"
  },
  {
    "id": 72,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 541.77,
    "end": 545.36,
    "text": "إِنَّا وَجَدْنَاهُ صَابِرًا"
  },
  {
    "id": 73,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 545.98,
    "end": 551.47,
    "text": "نِعْمَ الْعَبْدُ إِنَّهُ أَوَّابٌ"
  },
  {
    "id": 74,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 552.62,
    "end": 562.29,
    "text": "وَاذْكُرْ عِبَادَنَا إِبْرَاهِيمَ وَإِسْحَاقَ وَيَعْقُوبَ أُولِي الْأَيْدِي وَالْأَبْصَارِ"
  },
  {
    "id": 75,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 563.44,
    "end": 570.59,
    "text": "إِنَّا أَخْلَصْنَاهُمْ بِخَالِصَةٍ ذِكْرَى الدَّارِ"
  },
  {
    "id": 76,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 571.23,
    "end": 577.68,
    "text": "وَإِنَّهُمْ عِنْدَنَا لَمِنَ الْمُصْطَفَيْنَ الْأَخْيَارِ"
  },
  {
    "id": 77,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 578.79,
    "end": 582.84,
    "text": "وَاذْكُرْ إِسْمَاعِيلَ وَالْيَسَعَ وَذَا الْكِفْلِ"
  },
  {
    "id": 78,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 583.69,
    "end": 587.3,
    "text": "وَكُلٌّ مِنَ الْأَخْيَارِ"
  },
  {
    "id": 79,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 587.96,
    "end": 588.95,
    "text": "هَذَا"
  },
  {
    "id": 80,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 589.85,
    "end": 595.15,
    "text": "وَإِنَّ لِلْمُتَّقِينَ لَحُسْنَ مَآبٍ"
  },
  {
    "id": 81,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 596.3,
    "end": 602.59,
    "text": "جَنَّاتِ عَدْنٍ مُفَتَّحَةً لَهُمُ الْأَبْوَابُ"
  },
  {
    "id": 82,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 603.39,
    "end": 612.93,
    "text": "مُتَّكِئِينَ فِيهَا يَدْعُونَ فِيهَا بِفَاكِهَةٍ كَثِيرَةٍ وَشَرَابٍ"
  },
  {
    "id": 83,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 613.89,
    "end": 619.45,
    "text": "وَعِنْدَهُمْ قَاصِرَاتُ الطَّرْفِ أَتْرَابٌ"
  },
  {
    "id": 84,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 620.06,
    "end": 625.06,
    "text": "هَذَا مَا تُوعَدُونَ لِيَوْمِ الْحِسَابِ"
  },
  {
    "id": 85,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 625.83,
    "end": 632.32,
    "text": "إِنَّ هَذَا لَرِزْقُنَا مَا لَهُ مِنْ نَفَادٍ"
  },
  {
    "id": 86,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 633.23,
    "end": 634.09,
    "text": "ههه"
  },
  {
    "id": 87,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 634.78,
    "end": 639.48,
    "text": "وَإِنَّ لِلطَّاغِينَ لَشَرَّ مَآبٍ"
  },
  {
    "id": 88,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 640.34,
    "end": 645.45,
    "text": "جَهَنَّمَ يَصْلَوْنَهَا فَبِئْسَ الْمِهَادُ"
  },
  {
    "id": 89,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 646.4,
    "end": 652.14,
    "text": "وَهَذَا فَلْيَذُوقُوهُ حَمِيمٌ وَغَسَّاقٌ"
  },
  {
    "id": 90,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 652.85,
    "end": 658.14,
    "text": "وَآخَرُ مِنْ شَكْلِهِ أَزْوَاجٌ"
  },
  {
    "id": 91,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 659.19,
    "end": 666.15,
    "text": "هَذَا فَوْجٌ مُقْتَحِمٌ مَعَكُمْ لَا مَرْحَبًا بِهِمْ"
  },
  {
    "id": 92,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 667.05,
    "end": 671.1,
    "text": "إِنَّهُمْ صَالُو النَّارِ"
  },
  {
    "id": 93,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 672.18,
    "end": 677.17,
    "text": "قَالُوا بَلْ أَنْتُمْ لَا مَرْحَبًا بِكُمْ"
  },
  {
    "id": 94,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 677.87,
    "end": 681.49,
    "text": "أَنْتُمْ قَدَّمْتُمُوهُ لَنَا"
  },
  {
    "id": 95,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 682.25,
    "end": 684.83,
    "text": "بِئْسَ الْقَرَارُ"
  },
  {
    "id": 96,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 685.73,
    "end": 696.67,
    "text": "قَالُوا رَبَّنَا مَنْ قَدَّمَ لَنَا هَذَا فَزِدْهُ عَذَابًا ضِعْفًا فِي النَّارِ"
  },
  {
    "id": 97,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 697.57,
    "end": 707.08,
    "text": "وَقَالُوا مَا لَنَا لَا نَرَى رِجَالًا كُنَّا نَعُدُّهُمْ مِنَ الْأَشْرَارِ"
  },
  {
    "id": 98,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 708.15,
    "end": 714.83,
    "text": "أَتَّخَذْنَاهُمْ سِخْرِيًّا أَمْ زَاغَتْ عَنْهُمُ الْأَبْصَارُ"
  },
  {
    "id": 99,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 715.98,
    "end": 722.71,
    "text": "إِنَّ ذَلِكَ لَحَقٌّ تَخَاصُمُ أَهْلِ النَّارِ"
  },
  {
    "id": 100,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 723.92,
    "end": 727.89,
    "text": "قُلْ إِنَّمَا أَنَا مُنْذِرٌ"
  },
  {
    "id": 101,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 728.7,
    "end": 734.72,
    "text": "وَمَا مِنْ إِلَهٍ إِلَّا اللَّهُ الْوَاحِدُ الْقَهَّارُ"
  },
  {
    "id": 102,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 735.85,
    "end": 743.27,
    "text": "رَبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا الْعَزِيزُ الْغَفَّارُ"
  },
  {
    "id": 103,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 743.99,
    "end": 746.99,
    "text": "قُلْ هُوَ نَبَأٌ عَظِيمٌ"
  },
  {
    "id": 104,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 747.64,
    "end": 751.46,
    "text": "أَنْتُمْ عَنْهُ مُعْرِضُونَ"
  },
  {
    "id": 105,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 752.37,
    "end": 760.27,
    "text": "مَا كَانَ لِيَ مِنْ عِلْمٍ بِالْمَلَإِ الْأَعْلَى إِذْ يَخْتَصِمُونَ"
  },
  {
    "id": 106,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 761.16,
    "end": 771.17,
    "text": "يُوحَى إِلَيَّ إِلَّا أَنَّمَا أَنَا نَذِيرٌ مُبِينٌ"
  },
  {
    "id": 107,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 772.35,
    "end": 782.58,
    "text": "إِذْ قَالَ رَبُّكَ لِلْمَلَائِكَةِ إِنِّي خَالِقٌ بَشَرًا مِنْ طِينٍ"
  },
  {
    "id": 108,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 783.51,
    "end": 791.7,
    "text": "فَإِذَا سَوَّيْتُهُ وَنَفَخْتُ فِيهِ مِنْ رُوحِي فَقَعُوا لَهُ سَاجِدِينَ"
  },
  {
    "id": 109,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 792.48,
    "end": 798.0,
    "text": "فَسَجَدَ الْمَلَائِكَةُ كُلُّهُمْ أَجْمَعُونَ"
  },
  {
    "id": 110,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 798.89,
    "end": 805.66,
    "text": "إِلَّا إِبْلِيسَ اسْتَكْبَرَ وَكَانَ مِنَ الْكَافِرِينَ"
  },
  {
    "id": 111,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 806.73,
    "end": 814.07,
    "text": "قَالَ يَا إِبْلِيسُ مَا مَنَعَكَ أَنْ تَسْجُدَ لِمَا خَلَقْتُ بِيَدَيَّ"
  },
  {
    "id": 112,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 815.08,
    "end": 820.11,
    "text": "وَاسْتَكْبَرْتَ أَمْ كُنْتَ مِنَ الْعَالِينَ"
  },
  {
    "id": 113,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 821.15,
    "end": 830.67,
    "text": "قَالَ أَنَا خَيْرٌ مِنْهُ خَلَقْتَنِي مِنْ نَارٍ وَخَلَقْتَهُ مِنْ طِينٍ"
  },
  {
    "id": 114,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 831.62,
    "end": 836.7,
    "text": "قَالَ فَاخْرُجْ مِنْهَا فَإِنَّكَ رَجِيمٌ"
  },
  {
    "id": 115,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 837.55,
    "end": 844.35,
    "text": "وَإِنَّ عَلَيْكَ لَعْنَتِي إِلَى يَوْمِ الدِّينِ"
  },
  {
    "id": 116,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 845.08,
    "end": 852.12,
    "text": "قَالَ رَبِّ فَأَنْظِرْنِي إِلَى يَوْمِ يُبْعَثُونَ"
  },
  {
    "id": 117,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 852.95,
    "end": 858.18,
    "text": "قَالَ فَإِنَّكَ مِنَ الْمُنْظَرِينَ"
  },
  {
    "id": 118,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 858.83,
    "end": 862.53,
    "text": "إِلَى يَوْمِ الْوَقْتِ الْمَعْلُومِ"
  },
  {
    "id": 119,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 863.22,
    "end": 869.24,
    "text": "قَالَ فَبِعِزَّتِكَ لَأُغْوِيَنَّهُمْ أَجْمَعِينَ"
  },
  {
    "id": 120,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 870.26,
    "end": 875.14,
    "text": "إِلَّا عِبَادَكَ مِنْهُمُ الْمُخْلَصِينَ"
  },
  {
    "id": 121,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 875.99,
    "end": 880.03,
    "text": "قَالَ فَالْحَقُّ وَالْحَقَّ أَقُولُ"
  },
  {
    "id": 122,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 880.8,
    "end": 890.5,
    "text": "لَأَمْلَأَنَّ جَهَنَّمَ مِنْكَ وَمِمَّنْ تَبِعَكَ مِنْهُمْ أَجْمَعِينَ"
  },
  {
    "id": 123,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 891.47,
    "end": 901.21,
    "text": "قُلْ مَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ وَمَا أَنَا مِنَ الْمُتَكَلِّفِينَ"
  },
  {
    "id": 124,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 902.0,
    "end": 906.57,
    "text": "إِنْ هُوَ إِلَّا ذِكْرٌ لِلْعَالَمِينَ"
  },
  {
    "id": 125,
    "sura_id": 38,
    "recitation_uuid": "7c841837-5d58-4c00-b41a-ce17ad768354",
    "start": 907.35,
    "end": 912.59,
    "text": "وَلَتَعْلَمُنَّ نَبَأَهُ بَعْدَ حِينٍ"
  }
]