[
  {
    "id": 2,
    "sura_id": 100,
    "recitation_uuid": "5460cd37-4977-42fe-9c3e-98218e33b296",
    "start": 5.79,
    "end": 8.28,
    "text": "وَالْعَادِيَاتِ ضَبْحًا"
  },
  {
    "id": 3,
    "sura_id": 100,
    "recitation_uuid": "5460cd37-4977-42fe-9c3e-98218e33b296",
    "start": 9.23,
    "end": 11.7,
    "text": "فَالْمُورِيَاتِ قَدْحًا"
  },
  {
    "id": 4,
    "sura_id": 100,
    "recitation_uuid": "5460cd37-4977-42fe-9c3e-98218e33b296",
    "start": 12.66,
    "end": 15.15,
    "text": "فَالْمُغِيرَاتِ صُبْحًا"
  },
  {
    "id": 5,
    "sura_id": 100,
    "recitation_uuid": "5460cd37-4977-42fe-9c3e-98218e33b296",
    "start": 16.07,
    "end": 18.53,
    "text": "فَأَثَرْنَ بِهِ نَقْعًا"
  },
  {
    "id": 6,
    "sura_id": 100,
    "recitation_uuid": "5460cd37-4977-42fe-9c3e-98218e33b296",
    "start": 19.65,
    "end": 22.23,
    "text": "فَوَسَطْنَ بِهِ جَمْعًا"
  },
  {
    "id": 7,
    "sura_id": 100,
    "recitation_uuid": "5460cd37-4977-42fe-9c3e-98218e33b296",
    "start": 23.29,
    "end": 28.7,
    "text": "إِنَّ الْإِنْسَانَ لِرَبِّهِ لَكَنُودٌ"
  },
  {
    "id": 8,
    "sura_id": 100,
    "recitation_uuid": "5460cd37-4977-42fe-9c3e-98218e33b296",
    "start": 29.48,
    "end": 34.23,
    "text": "وَإِنَّهُ عَلَى ذَلِكَ لَشَهِيدٌ"
  },
  {
    "id": 9,
    "sura_id": 100,
    "recitation_uuid": "5460cd37-4977-42fe-9c3e-98218e33b296",
    "start": 34.98,
    "end": 40.07,
    "text": "وَإِنَّهُ لِحُبِّ الْخَيْرِ لَشَدِيدٌ"
  },
  {
    "id": 10,
    "sura_id": 100,
    "recitation_uuid": "5460cd37-4977-42fe-9c3e-98218e33b296",
    "start": 41.08,
    "end": 46.57,
    "text": "أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِي الْقُبُورِ"
  },
  {
    "id": 11,
    "sura_id": 100,
    "recitation_uuid": "5460cd37-4977-42fe-9c3e-98218e33b296",
    "start": 47.14,
    "end": 50.82,
    "text": "وَحُصِّلَ مَا فِي الصُّدُورِ"
  },
  {
    "id": 12,
    "sura_id": 100,
    "recitation_uuid": "5460cd37-4977-42fe-9c3e-98218e33b296",
    "start": 51.54,
    "end": 58.06,
    "text": "إِنَّ رَبَّهُمْ بِهِمْ يَوْمَئِذٍ لَخَبِيرٌ"
  }
]