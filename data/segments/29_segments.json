[
  {
    "id": 2,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 6.96,
    "end": 14.19,
    "text": "الم"
  },
  {
    "id": 3,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 15.52,
    "end": 27.36,
    "text": "أَحَسِبَ النَّاسُ أَنْ يُتْرَكُوا أَنْ يَقُولُوا آمَنَّا وَهُمْ لَا يُفْتَنُونَ"
  },
  {
    "id": 4,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 28.65,
    "end": 33.54,
    "text": "وَلَقَدْ فَتَنَّا الَّذِينَ مِنْ قَبْلِهِمْ"
  },
  {
    "id": 5,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 34.27,
    "end": 42.94,
    "text": "فَلَيَعْلَمَنَّ اللَّهُ الَّذِينَ صَدَقُوا وَلَيَعْلَمَنَّ الْكَاذِبِينَ"
  },
  {
    "id": 6,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 43.97,
    "end": 50.53,
    "text": "أَمْ حَسِبَ الَّذِينَ يَعْمَلُونَ السَّيِّئَاتِ أَنْ يَسْبِقُونَ"
  },
  {
    "id": 7,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 51.59,
    "end": 55.09,
    "text": "جَاءَ مَا يَحْكُمُونَ"
  },
  {
    "id": 8,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 56.43,
    "end": 65.04,
    "text": "مَنْ كَانَ يَرْجُو لِقَاءَ اللَّهِ فَإِنَّ أَجَلَ اللَّهِ لَآتٍ"
  },
  {
    "id": 9,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 66.05,
    "end": 69.22,
    "text": "وَهُوَ السَّمِيعُ الْعَلِيمُ"
  },
  {
    "id": 10,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 70.28,
    "end": 75.81,
    "text": "وَمَنْ جَاهَدَ فَإِنَّمَا يُجَاهِدُ لِنَفْسِهِ"
  },
  {
    "id": 11,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 76.8,
    "end": 82.48,
    "text": "إِنَّ اللَّهَ لَغَنِيٌّ عَنِ الْعَالَمِينَ"
  },
  {
    "id": 12,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 83.98,
    "end": 99.35,
    "text": "وَالَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَنُكَفِّرَنَّ عَنْهُمْ سَيِّئَاتِهِمْ وَلَنَجْزِيَنَّهُمْ أَحْسَنَ الَّذِي كَانُوا يَعْمَلُونَ"
  },
  {
    "id": 13,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 100.68,
    "end": 105.72,
    "text": "وَوَصَّيْنَا الْإِنْسَانَ بِوَالِدَيْهِ حُسْنًا"
  },
  {
    "id": 14,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 106.94,
    "end": 115.61,
    "text": "وَإِنْ جَاهَدَاكَ لِتُشْرِكَ بِي مَا لَيْسَ لَكَ بِهِ عِلْمٌ فَلَا تُطِعْهُمَا"
  },
  {
    "id": 15,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 116.77,
    "end": 124.59,
    "text": "إِلَيَّ مَرْجِعُكُمْ فَأُنَبِّئُكُمْ بِمَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 16,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 126.3,
    "end": 134.75,
    "text": "وَالَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَنُدْخِلَنَّهُمْ فِي الصَّالِحِينَ"
  },
  {
    "id": 17,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 136.71,
    "end": 150.69,
    "text": "وَمِنَ النَّاسِ مَنْ يَقُولُ آمَنَّا بِاللَّهِ فَإِذَا أُوذِيَ فِي اللَّهِ جَعَلَ فِتْنَةَ النَّاسِ كَعَذَابِ اللَّهِ"
  },
  {
    "id": 18,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 151.97,
    "end": 162.01,
    "text": "وَلَئِنْ جَاءَ نَصْرٌ مِنْ رَبِّكَ لَيَقُولُنَّ إِنَّا كُنَّا مَعَكُمْ"
  },
  {
    "id": 19,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 163.37,
    "end": 169.99,
    "text": "أَوَلَيْسَ اللَّهُ بِأَعْلَمَ بِمَا فِي صُدُورِ الْعَالَمِينَ"
  },
  {
    "id": 20,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 170.93,
    "end": 179.9,
    "text": "وَلَيَعْلَمَنَّ اللَّهُ الَّذِينَ آمَنُوا وَلَيَعْلَمَنَّ الْمُنَافِقِينَ"
  },
  {
    "id": 21,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 182.12,
    "end": 199.0,
    "text": "وَقَالَ الَّذِينَ كَفَرُوا لِلَّذِينَ آمَنُوا اتَّبِعُوا سَبِيلَنَا وَلْنَحْمِلْ خَطَايَاكُمْ وَمَا هُمْ بِحَامِلِينَ مِنْ خَطَايَاهُمْ مِنْ شَيْءٍ"
  },
  {
    "id": 22,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 200.22,
    "end": 203.63,
    "text": "إِنَّهُمْ لَكَاذِبُونَ"
  },
  {
    "id": 23,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 205.07,
    "end": 211.64,
    "text": "وَلَيَحْمِلُنَّ أَثْقَالَهُمْ وَأَثْقَالًا مَعَ أَثْقَالِهِمْ"
  },
  {
    "id": 24,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 212.37,
    "end": 220.68,
    "text": "وَلَيُسْأَلُنَّ يَوْمَ الْقِيَامَةِ عَمَّا كَانُوا يَفْتَرُونَ"
  },
  {
    "id": 25,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 223.04,
    "end": 238.06,
    "text": "وَلَقَدْ أَرْسَلْنَا نُوحًا إِلَى قَوْمِهِ فَلَبِثَ فِيهِمْ أَلْفَ سَنَةٍ إِلَّا خَمْسِينَ عَامًا فَأَخَذَهُمُ الطُّوفَانُ وَهُمْ ظَالِمُونَ"
  },
  {
    "id": 26,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 239.92,
    "end": 249.26,
    "text": "فَأَنْجَيْنَاهُ وَأَصْحَابَ السَّفِينَةِ وَجَعَلْنَاهَا آيَةً لِلْعَالَمِينَ"
  },
  {
    "id": 27,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 250.99,
    "end": 257.39,
    "text": "وَإِبْرَاهِيمَ إِذْ قَالَ لِقَوْمِهِ اعْبُدُوا اللَّهَ وَاتَّقُوهُ"
  },
  {
    "id": 28,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 258.37,
    "end": 265.35,
    "text": "ذَلِكُمْ خَيْرٌ لَكُمْ إِنْ كُنْتُمْ تَعْلَمُونَ"
  },
  {
    "id": 29,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 266.91,
    "end": 275.52,
    "text": "إِنَّمَا تَعْبُدُونَ مِنْ دُونِ اللَّهِ أَوْثَانًا وَتَخْلُقُونَ إِفْكًا"
  },
  {
    "id": 30,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 276.51,
    "end": 291.9,
    "text": "إِنَّ الَّذِينَ تَعْبُدُونَ مِنْ دُونِ اللَّهِ لَا يَمْلِكُونَ لَكُمْ رِزْقًا فَابْتَغُوا عِنْدَ اللَّهِ الرِّزْقَ وَاعْبُدُوهُ وَاشْكُرُوا لَهُ"
  },
  {
    "id": 31,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 293.0,
    "end": 295.75,
    "text": "إِلَيْهِ تُرْجَعُونَ"
  },
  {
    "id": 32,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 297.13,
    "end": 304.08,
    "text": "وَإِنْ تُكَذِّبُوا فَقَدْ كَذَّبَ أُمَمٌ مِنْ قَبْلِكُمْ"
  },
  {
    "id": 33,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 304.82,
    "end": 310.08,
    "text": "وَمَا عَلَى الرَّسُولِ إِلَّا الْبَلَاغُ الْمُبِينُ"
  },
  {
    "id": 34,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 311.63,
    "end": 317.79,
    "text": "أَوَلَمْ يَرَوْا كَيْفَ يُبْدِئُ اللَّهُ الْخَلْقَ ثُمَّ يُعِيدُهُ"
  },
  {
    "id": 35,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 318.62,
    "end": 322.98,
    "text": "إِنَّ ذَلِكَ عَلَى اللَّهِ يَسِيرٌ"
  },
  {
    "id": 36,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 324.41,
    "end": 330.03,
    "text": "قُلْ سِيرُوا فِي الْأَرْضِ فَانْظُرُوا كَيْفَ بَدَأًا خَلْقَ"
  },
  {
    "id": 37,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 331.46,
    "end": 336.87,
    "text": "ثُمَّ اللَّهُ يُنْشِئُ النَّشْأَةَ الْآخِرَةَ"
  },
  {
    "id": 38,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 338.21,
    "end": 344.17,
    "text": "إِنَّ اللَّهَ عَلَى كُلِّ شَيْءٍ قَدِيرٌ"
  },
  {
    "id": 39,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 345.33,
    "end": 352.18,
    "text": "يُعَذِّبُ مَنْ يَشَاءُ وَيَرْحَمُ مَنْ يَشَاءُ"
  },
  {
    "id": 40,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 353.22,
    "end": 356.18,
    "text": "وَإِلَيْهِ تُقْنَبُونَ"
  },
  {
    "id": 41,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 357.38,
    "end": 366.03,
    "text": "وَمَا أَنْتُمْ بِمُعْجِزِينَ فِي الْأَرْضِ وَلَا فِي السَّمَاءِ"
  },
  {
    "id": 42,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 367.11,
    "end": 375.53,
    "text": "وَمَا لَكُمْ مِنْ دُونِ اللَّهِ مِنْ وَلِيٍّ وَلَا نَصِيرٍ"
  },
  {
    "id": 43,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 376.74,
    "end": 393.05,
    "text": "وَالَّذِينَ كَفَرُوا بِآيَاتِ اللَّهِ وَلِقَائِهِ أُولَئِكَ يَئِسُوا مِنْ رَحْمَتِي وَأُولَئِكَ لَهُمْ عَذَابٌ أَلِيمٌ"
  },
  {
    "id": 44,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 394.57,
    "end": 408.84,
    "text": "فَمَا كَانَ جَوَابَ قَوْمِهِ إِلَّا أَنْ قَالُوا اقْتُلُوهُ أَوْ حَرِّقُوهُ فَأَنْجَاهُ اللَّهُ مِنَ النَّارِ"
  },
  {
    "id": 45,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 409.94,
    "end": 416.83,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِقَوْمٍ يُؤْمِنُونَ"
  },
  {
    "id": 46,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 417.98,
    "end": 429.14,
    "text": "وَقَالَ إِنَّمَا اتَّخَذْتُمْ مِنْ دُونِ اللَّهِ أَوْثَانًا مَوَدَّةَ بَيْنِكُمْ فِي الْحَيَاةِ الدُّنْيَا"
  },
  {
    "id": 47,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 430.4,
    "end": 448.57,
    "text": "ثُمَّ يَوْمَ الْقِيَامَةِ يَكْفُرُ بَعْضُكُمْ بِبَعْضٍ وَيَلْعَنُ بَعْضُكُمْ بَعْضًا وَمَأْوَاكُمُ النَّارُ وَمَا لَكُمْ مِنْ نَاصِرِينَ"
  },
  {
    "id": 48,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 449.9,
    "end": 452.6,
    "text": "فَآمَنَ لَهُ لُوطٌ"
  },
  {
    "id": 49,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 453.49,
    "end": 457.83,
    "text": "وَقَالَ إِنِّي مُهَاجِرٌ إِلَى رَبِّي"
  },
  {
    "id": 50,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 458.59,
    "end": 463.24,
    "text": "إِنَّهُ هُوَ الْعَزِيزُ الْحَكِيمُ"
  },
  {
    "id": 51,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 464.52,
    "end": 478.5,
    "text": "وَوَهَبْنَا لَهُ إِسْحَاقَ وَيَعْقُوبَ وَجَعَلْنَا فِي ذُرِّيَّتِهِ النُّبُوَّةَ وَالْكِتَابَ وَآتَيْنَاهُ أَجْرَهُ فِي الدُّنْيَا"
  },
  {
    "id": 52,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 479.73,
    "end": 485.46,
    "text": "وَإِنَّهُ فِي الْآخِرَةِ لَمِنَ الصَّالِحِينَ"
  },
  {
    "id": 53,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 486.99,
    "end": 501.38,
    "text": "وَلُوطًا إِذْ قَالَ لِقَوْمِهِ إِنَّكُمْ لَتَأْتُونَ الْفَاحِشَةَ مَا سَبَقَكُمْ بِهَا مِنْ أَحَدٍ مِنَ الْعَالَمِينَ"
  },
  {
    "id": 54,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 502.84,
    "end": 513.22,
    "text": "فَإِنَّكُمْ لَتَأْتُونَ الرِّجَالَ وَتَقْطَعُونَ السَّبِيلَ وَتَأْتُونَ فِي نَادِيكُمُ الْمُنْكَرَ"
  },
  {
    "id": 55,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 514.27,
    "end": 528.65,
    "text": "فَمَا كَانَ جَوَابَ قَوْمِهِ إِلَّا أَنْ قَالُوا ائْتِنَا بِعَذَابِ اللَّهِ إِنْ كُنْتَ مِنَ الصَّادِقِينَ"
  },
  {
    "id": 56,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 529.64,
    "end": 535.74,
    "text": "قَالَ رَبِّ انْصُرْنِي عَلَى الْقَوْمِ الْمُفْسِدِينَ"
  },
  {
    "id": 57,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 537.11,
    "end": 550.65,
    "text": "وَلَمَّا جَاءَتْ رُسُلُنَا إِبْرَاهِيمَ بِالْبُشْرَى قَالُوا إِنَّا مُهْلِكُو أَهْلِ هَذِهِ الْقَرْيَةِ"
  },
  {
    "id": 58,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 551.74,
    "end": 556.28,
    "text": "إِنَّ أَهْلَهَا كَانُوا ظَالِمِينَ"
  },
  {
    "id": 59,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 557.38,
    "end": 560.37,
    "text": "قَالَ إِنَّ فِيهَا لُوطًا"
  },
  {
    "id": 60,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 561.19,
    "end": 565.2,
    "text": "قَالُوا نَحْنُ أَعْلَمُ بِمَنْ فِيهَا"
  },
  {
    "id": 61,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 566.42,
    "end": 575.61,
    "text": "لَنُنَجِّيَنَّهُ وَأَهْلَهُ إِلَّا امْرَأَتَهُ كَانَتْ مِنَ الْغَابِرِينَ"
  },
  {
    "id": 62,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 577.31,
    "end": 587.97,
    "text": "وَلَمَّا أَنْ جَاءَتْ رُسُلُنَا لُوطًا سِيءَ بِهِمْ وَضَاقَ بِهِمْ ذَرْعًا"
  },
  {
    "id": 63,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 589.2,
    "end": 600.67,
    "text": "وَقَالُوا لَا تَخَفْ وَلَا تَحْزَنْ إِنَّا مُنَجُّوكَ وَأَهْلَكَ إِلَّا امْرَأَتَكَ كَانَتْ مِنَ الْغَابِرِينَ"
  },
  {
    "id": 64,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 601.88,
    "end": 614.9,
    "text": "إِنَّا مُنْزِلُونَ عَلَى أَهْلِ هَذِهِ الْقَرْيَةِ رِجْزًا مِنَ السَّمَاءِ بِمَا كَانُوا يَفْسُقُونَ"
  },
  {
    "id": 65,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 616.42,
    "end": 625.04,
    "text": "وَلَقَدْ تَرَكْنَا مِنْهَا آيَةً بَيِّنَةً لِقَوْمٍ يَعْقِلُونَ"
  },
  {
    "id": 66,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 626.39,
    "end": 639.4,
    "text": "وَإِلَى مَدْيَنَ أَخَاهُمْ شُعَيْبًا فَقَالَ يَا قَوْمِ اعْبُدُوا اللَّهَ وَارْجُوا الْيَوْمَ الْآخِرَ وَلَا تَعْثَوْا فِي الْأَرْضِ مُفْسِدِينَ"
  },
  {
    "id": 67,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 640.66,
    "end": 648.46,
    "text": "فَكَذَّبُوهُ فَأَخَذَتْهُمُ الرَّجْفَةُ فَأَصْبَحُوا فِي دَارِهِمْ جَاثِمِينَ"
  },
  {
    "id": 68,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 649.83,
    "end": 656.6,
    "text": "وَعَادًا وَثَمُودَ وَقَدْ تَبَيَّنَ لَكُمْ مِنْ مَسَاكِنِهِمْ"
  },
  {
    "id": 69,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 657.6,
    "end": 667.42,
    "text": "وَزَيَّنَ لَهُمُ الشَّيْطَانُ أَعْمَالَهُمْ فَصَدَّهُمْ عَنِ السَّبِيلِ وَكَانُوا مُسْتَبْصِرِينَ"
  },
  {
    "id": 70,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 668.82,
    "end": 672.88,
    "text": "وَقَارُونَ وَفِرْعَوْنَ وَهَامَانَ"
  },
  {
    "id": 71,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 673.83,
    "end": 684.24,
    "text": "وَلَقَدْ جَاءَهُمْ مُوسَى بِالْبَيِّنَاتِ فَاسْتَكْبَرُوا فِي الْأَرْضِ وَمَا كَانُوا سَابِقِينَ"
  },
  {
    "id": 72,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 685.31,
    "end": 688.73,
    "text": "فَكُلًّا أَخَذْنَا بِذَنْبِهِ"
  },
  {
    "id": 73,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 689.67,
    "end": 701.92,
    "text": "فَمِنْهُمْ مَنْ أَرْسَلْنَا عَلَيْهِ حَاصِبًا وَمِنْهُمْ مَنْ أَخَذَتْهُ الصَّيْحَةُ وَمِنْهُمْ مَنْ خَسَفْنَا بِهِ الْأَرْضُ"
  },
  {
    "id": 74,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 703.43,
    "end": 710.1,
    "text": "وَمِنْهُمْ مَنْ خَسَفْنَا بِهِ الْأَرْضَ وَمِنْهُمْ مَنْ أَغْرَقْنَا"
  },
  {
    "id": 75,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 711.03,
    "end": 720.76,
    "text": "وَمَا كَانَ اللَّهُ لِيَظْلِمَهُمْ وَلَكِنْ كَانُوا أَنْفُسَهُمْ يَظْلِمُونَ"
  },
  {
    "id": 76,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 721.43,
    "end": 732.16,
    "text": "مَثَلُ الَّذِينَ اتَّخَذُوا مِنْ دُونِ اللَّهِ أَوْلِيَاءَ كَمَثَلِ الْعَنْكَبُوتِ اتَّخَذَتْ بَيْتًا"
  },
  {
    "id": 77,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 733.48,
    "end": 739.21,
    "text": "وَإِنَّ أَوْهَنَا الْبُيُوتِ لَبَيْتُ الْعَنْكَبُونَ"
  },
  {
    "id": 78,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 739.98,
    "end": 742.74,
    "text": "لَوْ كَانُوا يَعْلَمُونَ"
  },
  {
    "id": 79,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 743.77,
    "end": 751.34,
    "text": "إِنَّ اللَّهَ يَعْلَمُ مَا يَدْعُونَ مِنْ دُونِهِ مِنْ شَيْءٍ"
  },
  {
    "id": 80,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 752.24,
    "end": 755.24,
    "text": "وَهُوَ الْعَزِيزُ الْحَكِيمُ"
  },
  {
    "id": 81,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 756.31,
    "end": 761.35,
    "text": "وَتِلْكَ الْأَمْثَالُ نَضْرِبُهَا لِلنَّاسِ"
  },
  {
    "id": 82,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 762.36,
    "end": 767.55,
    "text": "وَمَا يَعْقِلُهَا إِلَّا الْعَالِمُونَ"
  },
  {
    "id": 83,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 768.73,
    "end": 772.85,
    "text": "خَلَقَ اللَّهُ السَّمَاوَاتِ وَالْأَرْضَ"
  },
  {
    "id": 84,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 773.26,
    "end": 773.34,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 85,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 774.08,
    "end": 779.63,
    "text": "إِنَّ فِي ذَلِكَ لَآيَةً لِلْمُؤْمِنِينَ"
  },
  {
    "id": 86,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 780.95,
    "end": 787.57,
    "text": "اتْلُ مَا أُوحِيَ إِلَيْكَ مِنَ الْكِتَابِ وَأَقِمِ الصَّلَاةَ"
  },
  {
    "id": 87,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 788.42,
    "end": 795.21,
    "text": "إِنَّ الصَّلَاةَ تَنْهَى عَنِ الْفَحْشَاءِ وَالْمُنْكَرِ"
  },
  {
    "id": 88,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 796.17,
    "end": 798.54,
    "text": "وَلَذِكْرُ اللَّهِ أَكْبَرُ"
  },
  {
    "id": 89,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 799.35,
    "end": 803.38,
    "text": "وَاللَّهُ يَعْلَمُ مَا تَصْنَعُونَ"
  },
  {
    "id": 90,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 804.63,
    "end": 814.33,
    "text": "وَلَا تُجَادِلُوا أَهْلَ الْكِتَابِ إِلَّا بِالَّتِي هِيَ أَحْسَنُ إِلَّا الَّذِينَ ظَلَمُوا مِنْهُمْ"
  },
  {
    "id": 91,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 815.75,
    "end": 828.49,
    "text": "وَقُولُوا آمَنَّا بِالَّذِي أُنْزِلَ إِلَيْنَا وَأُنْزِلَ إِلَيْكُمْ وَإِلَهُنَا وَإِلَهُكُمْ وَاحِدٌ"
  },
  {
    "id": 92,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 829.5,
    "end": 836.76,
    "text": "وَإِلَهُنَا وَإِلَهُكُمْ وَاحِدٌ وَنَحْنُ لَهُ مُسْلِمُونَ"
  },
  {
    "id": 93,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 838.0,
    "end": 843.56,
    "text": "وَكَذَلِكَ أَنْزَلْنَا إِلَيْكَ الْكِتَابَ"
  },
  {
    "id": 94,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 844.5,
    "end": 849.48,
    "text": "الَّذِينَ آتَيْنَاهُمُ الْكِتَابَ يُؤْمِنُونَ بِهِ"
  },
  {
    "id": 95,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 850.37,
    "end": 855.56,
    "text": "وَمِنْ هَؤُلَاءِ مَنْ يُؤْمِنُ بِهِ"
  },
  {
    "id": 96,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 856.49,
    "end": 862.96,
    "text": "وَمَا يَجْحَدُ بِآيَاتِنَا إِلَّا الْكَافِرُونَ"
  },
  {
    "id": 97,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 864.07,
    "end": 873.37,
    "text": "وَمَا كُنْتَ تَتْلُو مِنْ قَبْلِهِ مِنْ كِتَابٍ وَلَا تَخُطُّهُ بِيَمِينٍ"
  },
  {
    "id": 98,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 874.32,
    "end": 877.85,
    "text": "إِذًا لَارْتَابَ الْمُبْطِلُونَ"
  },
  {
    "id": 99,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 878.83,
    "end": 886.11,
    "text": "بَلْ هُوَ آيَاتٌ بَيِّنَاتٌ فِي صُدُورِ الَّذِينَ أُوتُوا الْعِلْمَ سَبِيلَ"
  },
  {
    "id": 100,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 887.11,
    "end": 893.8,
    "text": "وَمَا يَجْحَدُ بِآيَاتِنَا إِلَّا الظَّالِمُونَ"
  },
  {
    "id": 101,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 895.42,
    "end": 902.79,
    "text": "وَقَالُوا لَوْلَا أُنْزِلَ عَلَيْهِ آيَاتٌ مِنْ رَبِّهِ"
  },
  {
    "id": 102,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 903.71,
    "end": 914.03,
    "text": "قُلْ إِنَّمَا الْآيَاتُ عِنْدَ اللَّهِ وَإِنَّمَا أَنَا نَذِيرٌ مُبِينٌ"
  },
  {
    "id": 103,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 914.97,
    "end": 923.88,
    "text": "أَوَلَمْ يَكْفِهِمْ أَنَّا أَنْزَلْنَا عَلَيْكَ الْكِتَابَ يُتْلَى عَلَيْهِمْ"
  },
  {
    "id": 104,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 924.87,
    "end": 932.93,
    "text": "إِنَّ فِي ذَلِكَ لَرَحْمَةً وَذِكْرَى لِقَوْمٍ يُؤْمِنُونَ"
  },
  {
    "id": 105,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 934.22,
    "end": 939.23,
    "text": "قُلْ كَفَى بِاللَّهِ بَيْنِي وَبَيْنَكُمْ شَهِيدًا"
  },
  {
    "id": 106,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 940.18,
    "end": 943.69,
    "text": "يَعْلَمُ مَا فِي السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 107,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 944.95,
    "end": 954.39,
    "text": "وَالَّذِينَ آمَنُوا بِالْبَاطِلِ وَكَفَرُوا بِاللَّهِ أُولَئِكَ هُمُ الْخَاسِرُونَ"
  },
  {
    "id": 108,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 955.82,
    "end": 966.63,
    "text": "وَيَسْتَعْجِلُونَكَ بِالْعَذَابِ وَلَوْلَا أَجَلٌ مُسَمًّى لَجَاءَهُمُ الْعَذَابُ"
  },
  {
    "id": 109,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 967.57,
    "end": 974.38,
    "text": "وَلَيَأْتِيَنَّهُمْ بَغْتَةً وَهُمْ لَا يَشْعُرُونَ"
  },
  {
    "id": 110,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 975.68,
    "end": 985.08,
    "text": "يَسْتَعْجِلُونَكَ بِالْعَذَابِ وَإِنَّ جَهَنَّمَ لَمُحِيطَةٌ بِالْكَافِرِينَ"
  },
  {
    "id": 111,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 986.66,
    "end": 1000.06,
    "text": "يَوْمَ يَغْشَاهُمُ الْعَذَابُ مِنْ فَوْقِهِمْ وَمِنْ تَحْتِ أَرْجُلِهِمْ وَيَقُولُ ذُوقُوا مَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 112,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1001.33,
    "end": 1012.16,
    "text": "يَا عِبَادِيَ الَّذِينَ آمَنُوا إِنَّ أَرْضِي وَاسِعَةٌ فَإِيَّايَ فَاعْبُدُونِ"
  },
  {
    "id": 113,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1013.17,
    "end": 1018.39,
    "text": "كُلُّ نَفْسٍ ذَائِقَةُ الْمَوْتِ"
  },
  {
    "id": 114,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1019.28,
    "end": 1023.17,
    "text": "ثُمَّ إِلَيْنَا تُرْجَعُونَ"
  },
  {
    "id": 115,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1024.45,
    "end": 1034.36,
    "text": "وَالَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَنُبَوِّئَنَّهُمْ مِنَ الْجَنَّةِ غُرَفًا"
  },
  {
    "id": 116,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1035.5,
    "end": 1043.12,
    "text": "لَنُبَوِّئَنَّهُمْ مِنَ الْجَنَّةِ غُرَفًا تَجْرِي مِنْ تَبْتَرَ"
  },
  {
    "id": 117,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1043.43,
    "end": 1047.03,
    "text": "فِيهَا الْأَنْهَارُ خَالِدِينَ فِيهَا"
  },
  {
    "id": 118,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1048.05,
    "end": 1051.24,
    "text": "نِعْمَ أَجْرُ الْعَامِلِينَ"
  },
  {
    "id": 119,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1052.36,
    "end": 1058.41,
    "text": "الَّذِينَ صَبَرُوا وَعَلَى رَبِّهِمْ يَتَوَكَّلُونَ"
  },
  {
    "id": 120,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1059.88,
    "end": 1069.68,
    "text": "وَكَأَيِّنْ مِنْ دَابَّةٍ لَا تَحْمِلُ رِزْقَهَا اللَّهُ يَرْزُقُهَا وَإِيَّاكُمْ"
  },
  {
    "id": 121,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1070.59,
    "end": 1073.64,
    "text": "وَهُوَ السَّمِيعُ الْعَلِيمُ"
  },
  {
    "id": 122,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1074.34,
    "end": 1086.54,
    "text": "وَلَئِنْ سَأَلْتَهُمْ مَنْ خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ وَسَخَّرَ الشَّمْسَ وَالْقَمَرَ لَيَقُولُنَّ اللَّهُ"
  },
  {
    "id": 123,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1087.45,
    "end": 1090.69,
    "text": "كَأَنَّى يُؤْفَكُونَ"
  },
  {
    "id": 124,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1091.96,
    "end": 1100.13,
    "text": "اللَّهُ يَبْسُطُ الرِّزْقَ لِمَنْ يَشَاءُ مِنْ عِبَادِهِ وَيَقْدِرُ"
  },
  {
    "id": 125,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1101.21,
    "end": 1106.02,
    "text": "إِنَّ اللَّهَ بِكُلِّ شَيْءٍ عَلِيمٌ"
  },
  {
    "id": 126,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1107.33,
    "end": 1123.5,
    "text": "وَلَئِنْ سَأَلْتَهُمْ مَنْ نَزَّلَ مِنَ السَّمَاءِ مَاءً فَأَحْيَا بِهِ الْأَرْضَ مِنْ بَعْدِ مَوْتِهَا لَيَقُولُنَّ اللَّهُ"
  },
  {
    "id": 127,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1124.66,
    "end": 1127.36,
    "text": "قُلِ الْحَمْدُ لِلَّهِ"
  },
  {
    "id": 128,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1128.17,
    "end": 1131.95,
    "text": "بَلْ أَكْثَرُهُمْ لَا يَعْقِلُونَ"
  },
  {
    "id": 129,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1133.41,
    "end": 1140.57,
    "text": "وَمَا هَذِهِ الْحَيَاةُ الدُّنْيَا إِلَّا لَهْوٌ وَلَعِبٌ"
  },
  {
    "id": 130,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1141.66,
    "end": 1146.99,
    "text": "وَإِنَّ الدَّارَ الْآخِرَةَ لَهِيَ الْحَيَوَانُ"
  },
  {
    "id": 131,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1147.8,
    "end": 1150.94,
    "text": "لَوْ كَانُوا يَعْلَمُونَ"
  },
  {
    "id": 132,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1152.38,
    "end": 1165.17,
    "text": "فَإِذَا رَكِبُوا فِي الْفُلْكِ دَعَوُا اللَّهَ مُخْلِصِينَ لَهُ الدِّينَ فَلَمَّا نَجَّاهُمْ إِلَى الْبَرِّ إِذَا هُمْ يُشْرِكُونَ"
  },
  {
    "id": 133,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1166.5,
    "end": 1172.15,
    "text": "لِيَكْفُرُوا بِمَا آتَيْنَاهُمْ وَلِيَتَمَتَّعُونَ"
  },
  {
    "id": 134,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1173.0,
    "end": 1175.5,
    "text": "فَسَوْفَ يَعْلَمُونَ"
  },
  {
    "id": 135,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1176.57,
    "end": 1185.8,
    "text": "أَوَلَمْ يَرَوْا أَنَّا جَعَلْنَا حَرَمًا آمِنًا وَيُتَخَطَّفُ النَّاسُ مِنْ حَوْلِهِمْ"
  },
  {
    "id": 136,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1186.87,
    "end": 1193.47,
    "text": "أَفَبِالْبَاطِلِ يُؤْمِنُونَ وَبِنِعْمَةِ اللَّهِ يَكْفُرُونَ"
  },
  {
    "id": 137,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1194.6,
    "end": 1204.56,
    "text": "وَمَنْ أَظْلَمُ مِمَّنِ افْتَرَى عَلَى اللَّهِ كَذِبًا أَوْ كَذَّبَ بِالْحَقِّ لَمَّا جَاءَهُ"
  },
  {
    "id": 138,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1205.75,
    "end": 1211.4,
    "text": "أَلَيْسَ فِي جَهَنَّمَ مَثْوًى لِلْكَافِرِينَ"
  },
  {
    "id": 139,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1212.31,
    "end": 1218.53,
    "text": "وَالَّذِينَ جَاهَدُوا فِينَا لَنَهْدِيَنَّهُمْ سُبُلَنَا"
  },
  {
    "id": 140,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1219.52,
    "end": 1222.52,
    "text": "وَإِنَّ اللَّهَ لَمَعَ الْمُعْلِمُ"
  },
  {
    "id": 141,
    "sura_id": 29,
    "recitation_uuid": "4fb239da-0b0d-42a3-97aa-721a67e1553f",
    "start": 1222.84,
    "end": 1224.61,
    "text": "الم"
  }
]