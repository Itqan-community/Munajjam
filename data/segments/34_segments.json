[
  {
    "id": 2,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 6.65,
    "end": 16.04,
    "text": "الْحَمْدُ لِلَّهِ الَّذِي لَهُ مَا فِي السَّمَاوَاتِ وَمَا فِي الْأَرْضِ وَلَهُ الْحَمْدُ فِي الْآخِرَةِ"
  },
  {
    "id": 3,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 17.2,
    "end": 20.57,
    "text": "وَهُوَ الْحَكِيمُ الْخَبِيرُ"
  },
  {
    "id": 4,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 21.96,
    "end": 33.38,
    "text": "يَعْلَمُ مَا يَلِجُ فِي الْأَرْضِ وَمَا يَخْرُجُ مِنْهَا وَمَا يَنْزِلُ مِنَ السَّمَاءِ وَمَا يَعْرُجُ فِيهَا"
  },
  {
    "id": 5,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 34.44,
    "end": 37.58,
    "text": "وَهُوَ الرَّحِيمُ الْغَفُورُ"
  },
  {
    "id": 6,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 39.14,
    "end": 43.81,
    "text": "وَقَالَ الَّذِينَ كَفَرُوا لَا تَأْتِينَا السَّاعَةُ"
  },
  {
    "id": 7,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 44.84,
    "end": 51.14,
    "text": "قُلْ بَلَى وَرَبِّي لَتَأْتِيَنَّكُمْ عَالِمِ الْغَيْبِ"
  },
  {
    "id": 8,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 52.27,
    "end": 71.52,
    "text": "لَا يَعْزُبُ عَنْهُ مِثْقَالُ ذَرَّةٍ فِي السَّمَاوَاتِ وَلَا فِي الْأَرْضِ وَلَا أَصْغَرُ مِنْ ذَلِكَ وَلَا أَكْبَرُ إِلَّا فِي كِتَابٍ مُبِينٍ"
  },
  {
    "id": 9,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 72.83,
    "end": 78.41,
    "text": "لِيَجْزِيَ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ"
  },
  {
    "id": 10,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 79.18,
    "end": 87.04,
    "text": "أُولَئِكَ لَهُمْ مَغْفِرَةٌ وَرِزْقٌ كَرِيمٌ"
  },
  {
    "id": 11,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 88.54,
    "end": 100.77,
    "text": "وَالَّذِينَ سَعَوْا فِي آيَاتِنَا مُعَاجِزِينَ أُولَئِكَ لَهُمْ عَذَابٌ مِنْ رِجْزٍ أَلِيمٌ"
  },
  {
    "id": 12,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 101.98,
    "end": 117.02,
    "text": "وَيَرَى الَّذِينَ أُوتُوا الْعِلْمَ الَّذِي أُنْزِلَ إِلَيْكَ مِنْ رَبِّكَ هُوَ الْحَقَّ وَيَهْدِي إِلَى صِرَاطِ الْعَزِيزِ الْحَمِيدِ"
  },
  {
    "id": 13,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 118.44,
    "end": 134.25,
    "text": "وَقَالَ الَّذِينَ كَفَرُوا هَلْ نَدُلُّكُمْ عَلَى رَجُلٍ يُنَبِّئُكُمْ إِذَا مُزِّقْتُمْ كُلَّ مُمَزَّقٍ إِنَّكُمْ لَفِي خَلْقٍ جَدِيدٍ"
  },
  {
    "id": 14,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 135.26,
    "end": 140.72,
    "text": "أَفْتَرَى عَلَى اللَّهِ كَذِبًا أَمْ بِهِ جِنَّةٌ"
  },
  {
    "id": 15,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 141.43,
    "end": 149.81,
    "text": "بَلِ الَّذِينَ لَا يُؤْمِنُونَ بِالْآخِرَةِ فِي الْعَذَابِ وَالضَّلَالِ الْبَعِيدِ"
  },
  {
    "id": 16,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 151.15,
    "end": 160.41,
    "text": "أَفَلَمْ يَرَوْا إِلَى مَا بَيْنَ أَيْدِيهِمْ وَمَا خَلْفَهُمْ مِنَ السَّمَاءِ وَالْأَرْضِ"
  },
  {
    "id": 17,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 161.91,
    "end": 170.96,
    "text": "إِنْ نَشَأْ نَخْسِفْ بِهِمُ الْأَرْضَ أَوْ نُسْقِطْ عَلَيْهِمْ كِسَفًا مِنَ السَّمَاءِ"
  },
  {
    "id": 18,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 171.88,
    "end": 179.27,
    "text": "إِنَّ فِي ذَلِكَ لَآيَةً لِكُلِّ عَبْدٍ مُنِيبٍ"
  },
  {
    "id": 19,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 180.5,
    "end": 185.53,
    "text": "وَلَقَدْ آتَيْنَا دَاوُودَ مِنَّا فَضْلًا"
  },
  {
    "id": 20,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 186.29,
    "end": 191.26,
    "text": "يَا جِبَالُ أَوِّبِي مَعَهُ وَالطَّيْرِ"
  },
  {
    "id": 21,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 191.91,
    "end": 195.84,
    "text": "وَأَلَنَّا لَهُ الْحَدِيدُ"
  },
  {
    "id": 22,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 196.91,
    "end": 201.6,
    "text": "أَنِ اعْمَلْ سَابِغَاتٍ وَقَدِّرْ فِي السَّرْدِ"
  },
  {
    "id": 23,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 202.41,
    "end": 204.49,
    "text": "وَاعْمَلُوا صَالِحًا"
  },
  {
    "id": 24,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 205.44,
    "end": 210.31,
    "text": "إِنِّي بِمَا تَعْمَلُونَ بَصِيرٌ"
  },
  {
    "id": 25,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 211.68,
    "end": 218.07,
    "text": "وَلِسُلَيْمَانَ الرِّيحَ غُدُوُّهَا شَهْرٌ وَرَوَاحُهَا شَهْرٌ"
  },
  {
    "id": 26,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 219.03,
    "end": 221.86,
    "text": "وَأَسَلْنَا لَهُ عَيْنَ الْقِطْرِ"
  },
  {
    "id": 27,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 223.09,
    "end": 229.08,
    "text": "وَمِنَ الْجِنِّ مَنْ يَعْمَلُ بَيْنَ يَدَيْهِ بِإِذْنِ رَبِّهِ"
  },
  {
    "id": 28,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 230.11,
    "end": 237.48,
    "text": "وَمَنْ يَزِغْ مِنْهُمْ عَنْ أَمْرِنَا نُذِقْهُ مِنْ عَذَابِ السَّعِيرِ"
  },
  {
    "id": 29,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 238.81,
    "end": 252.05,
    "text": "يَعْمَلُونَ لَهُ مَا يَشَاءُ مِنْ مَحَارِيبَ وَتَمَاثِيلَ وَجِفَانٍ كَالْجَوَابِ وَقُدُورٍ رَاسِيَاتٍ"
  },
  {
    "id": 30,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 253.07,
    "end": 257.49,
    "text": "اعْمَلُوا آلَ دَاوُودَ شُكْرًا"
  },
  {
    "id": 31,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 258.17,
    "end": 262.76,
    "text": "وَقَلِيلٌ مِنْ عِبَادِيَ الشَّكُورُ"
  },
  {
    "id": 32,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 264.19,
    "end": 278.13,
    "text": "فَلَمَّا قَضَيْنَا عَلَيْهِ الْمَوْتَ مَا دَلَّهُمْ عَلَى مَوْتِهِ إِلَّا دَابَّةُ الْأَرْضِ تَأْكُلُ مِنْسَأَتَهُ"
  },
  {
    "id": 33,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 279.31,
    "end": 291.25,
    "text": "فَلَمَّا خَرَّ تَبَيَّنَتِ الْجِنُّ أَنْ لَوْ كَانُوا يَعْلَمُونَ الْغَيْبَ مَا لَبِثُوا فِي الْعَذَابِ الْمُهِينِ"
  },
  {
    "id": 34,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 292.22,
    "end": 297.11,
    "text": "لَقَدْ كَانَ لِسَبَإٍ فِي مَسْكَنِهِمْ آيَةٌ"
  },
  {
    "id": 35,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 298.05,
    "end": 303.25,
    "text": "جَنَّتَانِ عَنْ يَمِينٍ وَشِمَالٍ"
  },
  {
    "id": 36,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 304.1,
    "end": 307.84,
    "text": "كُلُوا مِنْ رِزْقِ رَبِّكُمْ وَاشْكُرُوا"
  },
  {
    "id": 37,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 308.94,
    "end": 314.37,
    "text": "بَلْدَةٌ طَيِّبَةٌ وَرَبٌّ غَفُورٌ"
  },
  {
    "id": 38,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 315.81,
    "end": 336.11,
    "text": "فَأَعْرَضُوا فَأَرْسَلْنَا عَلَيْهِمْ سَيْلَ الْعَرِمِ وَبَدَّلْنَاهُمْ بِجَنَّتَيْهِمْ جَنَّتَيْنِ ذَوَاتَيْ أُكُلٍ خَمْطٍ وَأَثْلٍ وَشَيْءٍ مِنْ سِدْرٍ قَلِيلٍ"
  },
  {
    "id": 39,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 337.43,
    "end": 341.71,
    "text": "ذَلِكَ جَزَيْنَاهُمْ بِمَا كَفَرُوا"
  },
  {
    "id": 40,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 343.0,
    "end": 348.11,
    "text": "وَهَلْ نُجَازِي إِلَّا الْكَفُورَ"
  },
  {
    "id": 41,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 349.49,
    "end": 361.84,
    "text": "وَجَعَلْنَا بَيْنَهُمْ وَبَيْنَ الْقُرَى الَّتِي بَارَكْنَا فِيهَا قُرًى ظَاهِرَةً وَقَدَّرْنَا فِيهَا السَّيْرَ"
  },
  {
    "id": 42,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 362.7,
    "end": 368.23,
    "text": "فِي رُوفِيهَا لَيَالِيَ وَأَيَّامًا آمِنِينَ"
  },
  {
    "id": 43,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 369.35,
    "end": 383.54,
    "text": "فَقَالُوا رَبَّنَا بَاعِدْ بَيْنَ أَسْفَارِنَا وَظَلَمُوا أَنْفُسَهُمْ فَجَعَلْنَاهُمْ أَحَادِيثَ وَمَزَّقْنَاهُمْ كُلَّ مُمَزَّقٍ"
  },
  {
    "id": 44,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 384.6,
    "end": 392.28,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِكُلِّ صَبَّارٍ شَكُورٍ"
  },
  {
    "id": 45,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 393.54,
    "end": 404.73,
    "text": "وَلَقَدْ صَدَّقَ عَلَيْهِمْ إِبْلِيسُ ظَنَّهُ فَاتَّبَعُوهُ إِلَّا فَرِيقًا مِنَ الْمُؤْمِنِينَ"
  },
  {
    "id": 46,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 405.82,
    "end": 418.56,
    "text": "وَمَا كَانَ لَهُ عَلَيْهِمْ مِنْ سُلْطَانٍ إِلَّا لِنِعْلَمَ مَنْ يُؤْمِنُ بِالْآخِرَةِ مِمَّنْ هُوَ مِنْهَا فِي شَكٍّرٍ"
  },
  {
    "id": 47,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 419.66,
    "end": 424.1,
    "text": "وَرَبُّكَ عَلَى كُلِّ شَيْءٍ حَفِيظٌ"
  },
  {
    "id": 48,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 425.65,
    "end": 434.15,
    "text": "قُلِ ادْعُوا الَّذِينَ زَعَمْتُمْ مِنْ دُونِ اللَّهِ لَا يَمْلِكُونَ مِثْقَالَ ذَرَّةٍ"
  },
  {
    "id": 49,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 435.4,
    "end": 452.32,
    "text": "لَا يَمْلِكُونَ مِثْقَالَ ذَرَّةٍ فِي السَّمَاوَاتِ وَلَا فِي الْأَرْضِ وَمَا لَهُمْ فِيهِمَا مِنْ شِرْكٍ وَمَا لَهُ مِنْهُمْ مِنْ ظَهِيرٍ"
  },
  {
    "id": 50,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 453.82,
    "end": 461.17,
    "text": "وَلَا تَنْفَعُ الشَّفَاعَةُ عِنْدَهُ إِلَّا لِمَنْ أَذِنَ لَهُ"
  },
  {
    "id": 51,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 462.38,
    "end": 470.6,
    "text": "حَتَّى إِذَا فُزِّعَ عَنْ قُلُوبِهِمْ قَالُوا مَاذَا قَالَ رَبُّكُمْ"
  },
  {
    "id": 52,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 471.6,
    "end": 472.55,
    "text": "قَالُوا الْحَمُورُ"
  },
  {
    "id": 53,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 473.0,
    "end": 473.05,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 54,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 473.67,
    "end": 477.01,
    "text": "وَهُوَ الْعَلِيُّ الْكَبِيرُ"
  },
  {
    "id": 55,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 478.01,
    "end": 483.2,
    "text": "قُلْ مَنْ يَرْزُقُكُمْ مِنَ السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 56,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 484.04,
    "end": 485.58,
    "text": "بِاللَّهِ"
  },
  {
    "id": 57,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 486.47,
    "end": 495.2,
    "text": "وَإِنَّا أَوْ إِيَّاكُمْ لَعَلَى هُدًى أَوْ فِي ضَلَالٍ مُبِينٍ"
  },
  {
    "id": 58,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 496.42,
    "end": 505.2,
    "text": "قُلْ لَا تُسْأَلُونَ عَمَّا أَجْرَمْنَا وَلَا نُسْأَلُ عَمَّا تَعْمَلُونَ"
  },
  {
    "id": 59,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 506.12,
    "end": 516.14,
    "text": "قُلْ يَجْمَعُ بَيْنَنَا رَبُّنَا ثُمَّ يَفْتَحُ بَيْنَنَا بِالْحَقِّ وَهُوَ الْفَتَّاحُ الْعَلِيمُ"
  },
  {
    "id": 60,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 517.26,
    "end": 523.59,
    "text": "قُلْ أَرُونِيَ الَّذِينَ أَلْحَقْتُمْ بِهِ شُرَكَاءَ"
  },
  {
    "id": 61,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 524.32,
    "end": 525.09,
    "text": "كَلَّا"
  },
  {
    "id": 62,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 525.82,
    "end": 530.11,
    "text": "بَلْ هُوَ اللَّهُ الْعَزِيزُ الْحَكِيمُ"
  },
  {
    "id": 63,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 531.09,
    "end": 541.57,
    "text": "وَمَا أَرْسَلْنَاكَ إِلَّا كَافَّةً لِلنَّاسِ بَشِيرًا وَنَذِيرًا"
  },
  {
    "id": 64,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 542.63,
    "end": 550.86,
    "text": "بَشِيرًا وَنَذِيرًا وَلَكِنَّ أَكْثَرَ النَّاسِ لَا يَعْلَمُونَ"
  },
  {
    "id": 65,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 552.05,
    "end": 559.5,
    "text": "وَيَقُولُونَ مَتَى هَذَا الْوَعْدُ إِنْ كُنْتُمْ صَادِقِينَ"
  },
  {
    "id": 66,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 560.87,
    "end": 570.99,
    "text": "قُلْ لَكُمْ مِيعَادُ يَوْمٍ لَا تَسْتَأْخِرُونَ عَنْهُ سَاعَةً وَلَا تَسْتَقْدِمُونَ"
  },
  {
    "id": 67,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 572.01,
    "end": 581.17,
    "text": "وَقَالَ الَّذِينَ كَفَرُوا لَنْ نُؤْمِنَ بِهَذَا الْقُرْآنِ وَلَا بِالَّذِي بَيْنَ يَدَيْهِ"
  },
  {
    "id": 68,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 582.28,
    "end": 593.96,
    "text": "وَلَوْ تَرَى إِذِ الظَّالِمُونَ مَوْقُوفُونَ عِنْدَ رَبِّهِمْ يَرْجِعُ بَعْضُهُمْ إِلَى بَعْضٍ الْقَوْلِ"
  },
  {
    "id": 69,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 595.24,
    "end": 610.51,
    "text": "يَرْجِعُ بَعْضُهُمْ إِلَى بَعْضٍ الْقَوْلَ يَقُولُ الَّذِينَ اسْتُضْعِفُوا لِلَّذِينَ اسْتَكْبَرُوا لَوْلَا أَنْتُمْ لَكُنَّا مُؤْمِنِينَ"
  },
  {
    "id": 70,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 611.89,
    "end": 623.66,
    "text": "قَالَ الَّذِينَ اسْتَكْبَرُوا لِلَّذِينَ اسْتُضْعِفُوا أَنَحْنُ صَدَدْنَاكُمْ عَنِ الْهُدَى بَعْدَ إِذْ جَاءَكُمْ"
  },
  {
    "id": 71,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 624.57,
    "end": 628.71,
    "text": "بَلْ كُنْتُمْ مُجْرِمِينَ"
  },
  {
    "id": 72,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 630.03,
    "end": 638.77,
    "text": "وَقَالَ الَّذِينَ اسْتُضْعِفُوا لِلَّذِينَ اسْتَكْبَرُوا بَلْ مَكْرُ اللَّيْلِ وَالنَّهَارِ"
  },
  {
    "id": 73,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 639.95,
    "end": 652.33,
    "text": "بَلْ مَكْرُ اللَّيْلِ وَالنَّهَارِ إِذْ تَأْمُرُونَنَا أَنْ نَكْفُرَ بِاللَّهِ وَنَجْعَلَ لَهُ أَنْدَادًا"
  },
  {
    "id": 74,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 653.4,
    "end": 658.89,
    "text": "وَأَسَرُّوا النَّدَامَةَ لَمَّا رَأَوُا الْعَذَابَ"
  },
  {
    "id": 75,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 659.73,
    "end": 665.55,
    "text": "وَجَعَلْنَا الْأَغْلَالَ فِي أَعْنَاقِ الَّذِينَ كَفَرُوا"
  },
  {
    "id": 76,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 666.55,
    "end": 671.38,
    "text": "بَلْ يُجْزَوْنَ إِلَّا مَا كَانُوا يَعْمَلُونَ"
  },
  {
    "id": 77,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 672.58,
    "end": 687.8,
    "text": "وَمَا أَرْسَلْنَا فِي قَرْيَةٍ مِنْ نَذِيرٍ إِلَّا قَالَ مُتْرَفُوهَا إِنَّا بِمَا أُرْسِلْتُمْ بِهِ كَافِرُونَ"
  },
  {
    "id": 78,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 689.24,
    "end": 697.63,
    "text": "وَقَالُوا نَحْنُ أَكْثَرُ أَمْوَالًا وَأَوْلَادًا وَمَا نَحْنُ بِمُعَذَّبِينَ"
  },
  {
    "id": 79,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 698.58,
    "end": 710.86,
    "text": "قُلْ إِنَّ رَبِّي يَبْسُطُ الرِّزْقَ لِمَنْ يَشَاءُ وَيَقْدِرُ وَلَكِنَّ أَكْثَرَ النَّاسِ لَا يَعْلَمُونَ"
  },
  {
    "id": 80,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 712.03,
    "end": 724.69,
    "text": "وَمَا أَمْوَالُكُمْ وَلَا أَوْلَادُكُمْ بِالَّتِي تُقَرِّبُكُمْ عِنْدَنَا زُلْفَى إِلَّا مَنْ آمَنِينَ"
  },
  {
    "id": 81,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 725.84,
    "end": 739.55,
    "text": "إِلَّا مَنْ آمَنَ وَعَمِلَ صَالِحًا فَأُولَئِكَ لَهُمْ جَزَاءُ الضِّعْفِ بِمَا عَمِلُوا وَهُمْ فِي الْغُرُفَاتِ آمِنُونَ"
  },
  {
    "id": 82,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 740.78,
    "end": 750.88,
    "text": "وَالَّذِينَ يَسْعَوْنَ فِي آيَاتِنَا مُعَاجِزِينَ أُولَئِكَ فِي الْعَذَابِ مُحْضَرُونَ"
  },
  {
    "id": 83,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 751.86,
    "end": 761.07,
    "text": "قُلْ إِنَّ رَبِّي يَبْسُطُ الرِّزْقَ لِمَنْ يَشَاءُ مِنْ عِبَادِهِ وَيَقْدِرُ لَهُ"
  },
  {
    "id": 84,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 762.16,
    "end": 769.04,
    "text": "وَمَا أَنْفَقْتُمْ مِنْ شَيْءٍ فَهُوَ يُخْلِفُونَ"
  },
  {
    "id": 85,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 769.9,
    "end": 773.28,
    "text": "وَهُوَ خَيْرُ الرَّازِقِينَ"
  },
  {
    "id": 86,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 774.65,
    "end": 788.95,
    "text": "وَيَوْمَ يَحْشُرُهُمْ جَمِيعًا ثُمَّ يَقُولُ لِلْمَلَائِكَةِ أَهَؤُلَاءِ إِيَّاكُمْ كَانُوا يَعْبُدُونَ"
  },
  {
    "id": 87,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 790.23,
    "end": 796.07,
    "text": "قَالُوا سُبْحَانَكَ أَنْتَ وَلِيُّنَا مِنْ دُونِهِمْ"
  },
  {
    "id": 88,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 796.91,
    "end": 800.39,
    "text": "بَلْ كَانُوا يَعْبُدُونَ الْجِنَّ"
  },
  {
    "id": 89,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 801.12,
    "end": 805.82,
    "text": "فَأَكْثَرُهُمْ بِهِمْ مُؤْمِنُونَ"
  },
  {
    "id": 90,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 806.74,
    "end": 825.42,
    "text": "فَالْيَوْمَ لَا يَمْلِكُ بَعْضُكُمْ لِبَعْضٍ نَفْعًا وَلَا ضَرًّا وَنَقُولُ لِلَّذِينَ ظَلَمُوا ذُوقُوا عَذَابَ النَّارِ الَّتِي كُنْتُمْ بِهَا تُكَذِّبُونَ"
  },
  {
    "id": 91,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 826.92,
    "end": 839.64,
    "text": "وَإِذَا تُتْلَى عَلَيْهِمْ آيَاتُنَا بَيِّنَاتٍ قَالُوا مَا هَذَا إِلَّا رَجُلٌ يُرِيدُ أَنْ يَصُدَّكُمْ"
  },
  {
    "id": 92,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 840.69,
    "end": 854.52,
    "text": "يُرِيدُ أَنْ يَصُدَّكُمْ عَمَّا كَانَ يَعْبُدُ آبَاؤُكُمْ وَقَالُوا مَا هَذَا إِلَّا إِفْكٌ مُفْتَرًى"
  },
  {
    "id": 93,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 855.78,
    "end": 867.85,
    "text": "وَقَالَ الَّذِينَ كَفَرُوا لِلْحَقِّ لَمَّا جَاءَهُمْ إِنْ هَذَا إِلَّا سِحْرٌ مُبِينٌ"
  },
  {
    "id": 94,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 868.96,
    "end": 875.73,
    "text": "وَمَا آتَيْنَاهُمْ مِنْ كُتُبٍ يَدْرُسُونَهَا"
  },
  {
    "id": 95,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 876.69,
    "end": 883.6,
    "text": "وَمَا أَرْسَلْنَا إِلَيْهِمْ قَبْلَكَ مِنْ نَذِيرٍ"
  },
  {
    "id": 96,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 884.63,
    "end": 895.62,
    "text": "وَكَذَّبَ الَّذِينَ مِنْ قَبْلِهِمْ وَمَا بَلَغُوا مِعْشَارَ مَا آتَيْنَاهُمْ فَكَذَّبُوا رُسُلِينَ"
  },
  {
    "id": 97,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 896.42,
    "end": 899.37,
    "text": "فَكَيْفَ كَانَ نَكِيرِ"
  },
  {
    "id": 98,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 900.62,
    "end": 905.1,
    "text": "قُلْ إِنَّمَا أَعِظُكُمْ بِوَاحِدَةٍ"
  },
  {
    "id": 99,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 905.88,
    "end": 913.2,
    "text": "وَأَنْ تَقُومُوا لِلَّهِ مَثْنَى وَفُرَادَى ثُمَّ تَتَفَكَّرُونَ"
  },
  {
    "id": 100,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 914.01,
    "end": 918.38,
    "text": "مَا بِصَاحِبِكُمْ مِنْ جِنَّةٍ"
  },
  {
    "id": 101,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 919.24,
    "end": 927.17,
    "text": "إِنْ هُوَ إِلَّا نَذِيرٌ لَكُمْ بَيْنَ يَدَيْ عَذَابٍ شَدِيدٍ"
  },
  {
    "id": 102,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 928.5,
    "end": 933.47,
    "text": "قُلْ مَا سَأَلْتُكُمْ مِنْ أَجْرٍ فَهُوَ لَكُمْ"
  },
  {
    "id": 103,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 934.27,
    "end": 938.0,
    "text": "وَإِنْ أَجْرِيَ إِلَّا عَلَى اللَّهِ"
  },
  {
    "id": 104,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 938.69,
    "end": 943.33,
    "text": "وَهُوَ عَلَى كُلِّ شَيْءٍ شَهِيدٌ"
  },
  {
    "id": 105,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 944.29,
    "end": 950.78,
    "text": "قُلْ إِنَّ رَبِّي يَقْذِفُ بِالْحَقِّ عَلَّامُ الْغُيُوبِ"
  },
  {
    "id": 106,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 951.59,
    "end": 958.79,
    "text": "قُلْ جَاءَ الْحَقُّ وَمَا يُبْدِئُ الْبَاطِلُ وَمَا يُعِيدُ"
  },
  {
    "id": 107,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 959.92,
    "end": 966.22,
    "text": "قُلْ إِنْ ضَلَلْتُ فَإِنَّمَا أَضِلُّ عَلَى نَفْسِي"
  },
  {
    "id": 108,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 967.17,
    "end": 973.19,
    "text": "وَإِنِ اهْتَدَيْتُ فَبِمَا يُوحِي إِلَيَّ رَبِّي"
  },
  {
    "id": 109,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 974.13,
    "end": 978.73,
    "text": "إِنَّهُ سَمِيعٌ قَرِيبٌ"
  },
  {
    "id": 110,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 979.97,
    "end": 989.42,
    "text": "وَلَوْ تَرَى إِذْ فَزِعُوا فَلَا فَوْتَ وَأُخِذُوا مِنْ مَكَانٍ قَرِيبٍ"
  },
  {
    "id": 111,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 990.64,
    "end": 1001.85,
    "text": "وَقَالُوا آمَنَّا بِهِ وَأَنَّى لَهُمُ التَّنَاوُشُ مِنْ مَكَانٍ بَعِيدٍ"
  },
  {
    "id": 112,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 1002.98,
    "end": 1006.5,
    "text": "وَقَدْ كَفَرُوا بِهِ مِنْ قَبْلُ"
  },
  {
    "id": 113,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 1007.32,
    "end": 1013.88,
    "text": "وَيَقْذِفُونَ بِالْغَيْبِ مِنْ مَكَانٍ بَعِيدٍ"
  },
  {
    "id": 114,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 1014.74,
    "end": 1023.49,
    "text": "وَحِيلَ بَيْنَهُمْ وَبَيْنَ مَا يَشْتَهُونَ كَمَا فُعِلَ بِأَشْيَاعِهِمْ مِنْ قَبْلُ"
  },
  {
    "id": 115,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 1024.53,
    "end": 1027.38,
    "text": "إِنَّهُمْ كَانُوا فِي شَكًّا"
  },
  {
    "id": 116,
    "sura_id": 34,
    "recitation_uuid": "101a4a3f-a987-4078-90bc-e15b04cfffa6",
    "start": 1027.74,
    "end": 1030.36,
    "text": "مِنْ مُرِيبٍ"
  }
]