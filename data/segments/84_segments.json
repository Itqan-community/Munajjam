[
  {
    "id": 2,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 5.21,
    "end": 9.2,
    "text": "إِذَا السَّمَاءُ انْشَقَّتْ"
  },
  {
    "id": 3,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 10.14,
    "end": 12.99,
    "text": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ"
  },
  {
    "id": 4,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 13.82,
    "end": 15.73,
    "text": "وَإِذَا الْأَرْضُ مُدَّتْ"
  },
  {
    "id": 5,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 16.48,
    "end": 19.64,
    "text": "وَأَلْقَتْ مَا فِيهَا وَتَخَلَّتْ"
  },
  {
    "id": 6,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 20.57,
    "end": 23.47,
    "text": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ"
  },
  {
    "id": 7,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 24.53,
    "end": 35.27,
    "text": "يَا أَيُّهَا الْإِنْسَانُ إِنَّكَ كَادِحٌ إِلَى رَبِّكَ كَدْحًا فَمُلَاقِي"
  },
  {
    "id": 8,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 36.65,
    "end": 41.63,
    "text": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ"
  },
  {
    "id": 9,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 42.44,
    "end": 46.7,
    "text": "فَسَوْفَ يُحَاسَبُ حِسَابًا يَسِيرًا"
  },
  {
    "id": 10,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 47.7,
    "end": 52.74,
    "text": "وَيَنْقَلِبُ إِلَى أَهْلِهِ مَسْرُورًا"
  },
  {
    "id": 11,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 53.9,
    "end": 59.6,
    "text": "وَأَمَّا مَنْ أُوتِيَ كِتَابَهُ وَرَاءَ ظَهْرِهِ"
  },
  {
    "id": 12,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 60.49,
    "end": 63.16,
    "text": "فَسَوْفَ يَدْعُو ثُبُورًا"
  },
  {
    "id": 13,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 64.02,
    "end": 66.06,
    "text": "وَيَصْلَى سَعِيرًا"
  },
  {
    "id": 14,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 66.77,
    "end": 71.89,
    "text": "إِنَّهُ كَانَ فِي أَهْلِهِ مَسْرُورًا"
  },
  {
    "id": 15,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 72.81,
    "end": 78.06,
    "text": "إِنَّهُ ظَنَّ أَنْ لَنْ يَحُورَ"
  },
  {
    "id": 16,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 78.78,
    "end": 79.44,
    "text": "كَانَا"
  },
  {
    "id": 17,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 80.11,
    "end": 84.64,
    "text": "إِنَّ رَبَّهُ كَانَ بِهِ بَصِيرًا"
  },
  {
    "id": 18,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 85.46,
    "end": 88.67,
    "text": "فَلَا أُقْسِمُ بِالشَّفَقِ"
  },
  {
    "id": 19,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 89.51,
    "end": 91.75,
    "text": "وَاللَّيْلِ وَمَا وَسَقَ"
  },
  {
    "id": 20,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 92.56,
    "end": 94.86,
    "text": "وَالْقَمَرِ إِذَا اتَّسَقَ"
  },
  {
    "id": 21,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 95.85,
    "end": 100.44,
    "text": "لَتَرْكَبُنَّ طَبَقًا عَنْ طَبَقٍ"
  },
  {
    "id": 22,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 101.18,
    "end": 104.67,
    "text": "فَمَا لَهُمْ لَا يُؤْمِنُونَ"
  },
  {
    "id": 23,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 105.54,
    "end": 111.29,
    "text": "وَإِذَا قُرِئَ عَلَيْهِمُ الْقُرْآنُ لَا يَسْجُدُونَ"
  },
  {
    "id": 24,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 112.1,
    "end": 116.33,
    "text": "بَلِ الَّذِينَ كَفَرُوا يُكَذِّبُونَ"
  },
  {
    "id": 25,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 117.27,
    "end": 121.31,
    "text": "وَاللَّهُ أَعْلَمُ بِمَا يُوعُونَ"
  },
  {
    "id": 26,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 122.4,
    "end": 126.69,
    "text": "فَبَشِّرْهُمْ بِعَذَابٍ أَلِيمٍ"
  },
  {
    "id": 27,
    "sura_id": 84,
    "recitation_uuid": "e8d41928-e512-4182-b9e5-8586aa184882",
    "start": 127.62,
    "end": 136.7,
    "text": "إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ"
  }
]