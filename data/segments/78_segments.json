[
  {
    "id": 2,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 5.73,
    "end": 10.1,
    "text": "عَمَّ يَتَسَاءَلُونَ"
  },
  {
    "id": 3,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 11.17,
    "end": 14.34,
    "text": "عَنِ النَّبَإِ الْعَظِيمِ"
  },
  {
    "id": 4,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 15.26,
    "end": 19.25,
    "text": "الَّذِي هُمْ فِيهِ مُخْتَلِفُونَ"
  },
  {
    "id": 5,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 20.16,
    "end": 23.02,
    "text": "كَلَّا سَيَعْلَمُونَ"
  },
  {
    "id": 6,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 23.89,
    "end": 27.73,
    "text": "ثُمَّ كَلَّا سَيَعْلَمُونَ"
  },
  {
    "id": 7,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 28.86,
    "end": 31.96,
    "text": "أَلَمْ نَجْعَلِ الْأَرْضَ مِهَادًا"
  },
  {
    "id": 8,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 32.86,
    "end": 35.41,
    "text": "وَالْجِبَالَ أَوْتَادًا"
  },
  {
    "id": 9,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 36.34,
    "end": 39.28,
    "text": "وَخَلَقْنَاكُمْ أَزْوَاجًا"
  },
  {
    "id": 10,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 40.25,
    "end": 43.53,
    "text": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًا"
  },
  {
    "id": 11,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 44.66,
    "end": 47.6,
    "text": "وَجَعَلْنَا اللَّيْلَ لِبَاسًا"
  },
  {
    "id": 12,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 48.4,
    "end": 51.77,
    "text": "وَجَعَلْنَا النَّهَارَ مَعَاشًا"
  },
  {
    "id": 13,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 52.7,
    "end": 57.35,
    "text": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًا"
  },
  {
    "id": 14,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 58.35,
    "end": 62.21,
    "text": "وَجَعَلْنَا سِرَاجًا وَهَّاجًا"
  },
  {
    "id": 15,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 63.27,
    "end": 70.44,
    "text": "وَأَنْزَلْنَا مِنَ الْمُعْصِرَاتِ مَاءً ثَجَّاجًا"
  },
  {
    "id": 16,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 71.75,
    "end": 76.1,
    "text": "لِنُخْرِجَ بِهِ حَبًّا وَنَبَاتًا"
  },
  {
    "id": 17,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 76.92,
    "end": 79.93,
    "text": "وَجَنَّاتٍ أَلْفَافًا"
  },
  {
    "id": 18,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 80.84,
    "end": 85.14,
    "text": "إِنَّ يَوْمَ الْفَصْلِ كَانَ مِيقَاتًا"
  },
  {
    "id": 19,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 86.25,
    "end": 91.53,
    "text": "يَوْمَ يُنْفَخُ فِي الصُّورِ فَتَأْتُونَ أَفْوَاجًا"
  },
  {
    "id": 20,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 92.66,
    "end": 98.13,
    "text": "وَفُتِحَتِ السَّمَاءُ فَكَانَتْ أَبْوَابًا"
  },
  {
    "id": 21,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 99.13,
    "end": 103.69,
    "text": "وَسُيِّرَتِ الْجِبَالُ فَكَانَتْ سَرَابًا"
  },
  {
    "id": 22,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 104.78,
    "end": 109.31,
    "text": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا"
  },
  {
    "id": 23,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 110.36,
    "end": 112.87,
    "text": "لِلطَّاغِينَ مَآبًا"
  },
  {
    "id": 24,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 113.87,
    "end": 116.93,
    "text": "لَابِثِينَ فِيهَا"
  },
  {
    "id": 25,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 117.24,
    "end": 118.13,
    "text": "طه"
  },
  {
    "id": 26,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 119.24,
    "end": 124.71,
    "text": "لَا يَذُوقُونَ فِيهَا بَرْدًا وَلَا شَرَابًا"
  },
  {
    "id": 27,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 125.61,
    "end": 129.29,
    "text": "إِلَّا حَمِيمًا وَغَسَّاقًا"
  },
  {
    "id": 28,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 130.35,
    "end": 133.94,
    "text": "جَزَاءً وِفَاقًا"
  },
  {
    "id": 29,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 134.97,
    "end": 139.76,
    "text": "إِنَّهُمْ كَانُوا لَا يَرْجُونَ حِسَابًا"
  },
  {
    "id": 30,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 140.65,
    "end": 144.88,
    "text": "وَكَذَّبُوا بِآيَاتِنَا كِذَّابًا"
  },
  {
    "id": 31,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 145.75,
    "end": 150.04,
    "text": "وَكُلَّ شَيْءٍ أَحْصَيْنَاهُ كِتَابًا"
  },
  {
    "id": 32,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 151.02,
    "end": 156.86,
    "text": "فَذُوقُوا فَلَنْ نَزِيدَكُمْ إِلَّا عَذَابًا"
  },
  {
    "id": 33,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 157.81,
    "end": 161.63,
    "text": "إِنَّ لِلْمُتَّقِينَ مَفَازًا"
  },
  {
    "id": 34,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 162.38,
    "end": 166.07,
    "text": "حَدَائِقَ وَأَعْنَابًا"
  },
  {
    "id": 35,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 166.98,
    "end": 169.57,
    "text": "وَكَوَاعِبَ أَتْرَابًا"
  },
  {
    "id": 36,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 170.28,
    "end": 173.32,
    "text": "وَكَأْسًا دِهَاقًا"
  },
  {
    "id": 37,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 174.15,
    "end": 179.71,
    "text": "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا كِذَّابًا"
  },
  {
    "id": 38,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 180.73,
    "end": 187.16,
    "text": "جَزَاءً مِنْ رَبِّكَ عَطَاءً حِسَابًا"
  },
  {
    "id": 39,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 188.4,
    "end": 194.69,
    "text": "رَبِّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا الرَّحْمَنِ"
  },
  {
    "id": 40,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 195.79,
    "end": 199.4,
    "text": "لَا يَمْلِكُونَ مِنْهُ خِطَابًا"
  },
  {
    "id": 41,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 200.44,
    "end": 206.12,
    "text": "يَوْمَ يَقُومُ الرُّوحُ وَالْمَلَائِكَةُ صَفًّا"
  },
  {
    "id": 42,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 207.25,
    "end": 214.81,
    "text": "لَا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ الرَّحْمَنُ وَقَالَ صَوَابًا"
  },
  {
    "id": 43,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 215.74,
    "end": 217.81,
    "text": "ذَلِكَ الْيَوْمُ الْحَرَقُ"
  },
  {
    "id": 44,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 218.32,
    "end": 218.41,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 45,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 219.18,
    "end": 226.0,
    "text": "فَمَنْ شَاءَ اتَّخَذَ إِلَى رَبِّهِ مَآبًا"
  },
  {
    "id": 46,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 227.36,
    "end": 239.63,
    "text": "إِنَّا أَنْذَرْنَاكُمْ عَذَابًا قَرِيبًا يَوْمَ يَنْظُرُ الْمَرْءُ مَا قَدَّمَتْ يَدَاهُ"
  },
  {
    "id": 47,
    "sura_id": 78,
    "recitation_uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed",
    "start": 240.68,
    "end": 251.88,
    "text": "يَوْمَ يَنْظُرُ الْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ الْكَافِرُ يَا لَيْتَنِي كُنْتُ تُرَابًا"
  }
]