[
  {
    "id": 2,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 6.22,
    "end": 8.9,
    "text": "حم"
  },
  {
    "id": 3,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 9.89,
    "end": 19.47,
    "text": "عسق"
  },
  {
    "id": 4,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 20.75,
    "end": 30.23,
    "text": "كَذَلِكَ يُوحِي إِلَيْكَ وَإِلَى الَّذِينَ مِنْ قَبْلِكَ اللَّهُ الْعَزِيزُ الْحَكِيمُ"
  },
  {
    "id": 5,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 31.17,
    "end": 34.96,
    "text": "لَهُ مَا فِي السَّمَاوَاتِ وَمَا فِي الْأَرْضِ"
  },
  {
    "id": 6,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 35.83,
    "end": 39.33,
    "text": "وَهُوَ الْعَلِيُّ الْعَظِيمُ"
  },
  {
    "id": 7,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 40.36,
    "end": 46.14,
    "text": "تَكَادُ السَّمَاوَاتُ يَتَفَطَّرْنَ مِنْ فَوْقِهِنَّ"
  },
  {
    "id": 8,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 46.98,
    "end": 55.99,
    "text": "وَالْمَلَائِكَةُ يُسَبِّحُونَ بِحَمْدِ رَبِّهِمْ وَيَسْتَغْفِرُونَ لِمَنْ فِي الْأَرْضِ"
  },
  {
    "id": 9,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 57.06,
    "end": 63.6,
    "text": "أَلَا إِنَّ اللَّهَ هُوَ الْغَفُورُ الرَّحِيمُ"
  },
  {
    "id": 10,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 64.84,
    "end": 79.74,
    "text": "وَالَّذِينَ اتَّخَذُوا مِنْ دُونِهِ أَوْلِيَاءَ اللَّهُ حَفِيظٌ عَلَيْهِمْ وَمَا أَنْتَ عَلَيْهِمْ بِوَكِيلٍ"
  },
  {
    "id": 11,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 81.21,
    "end": 96.31,
    "text": "وَكَذَلِكَ أَوْحَيْنَا إِلَيْكَ قُرْآنًا عَرَبِيًّا لِتُنْذِرَ أُمَّ الْقُرَى وَمَنْ حَوْلَهَا وَتُنْذِرَ يَوْمَ الْجَمْعِ لَا رَيْبَ فِيهِ"
  },
  {
    "id": 12,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 97.41,
    "end": 103.98,
    "text": "فَرِيقٌ فِي الْجَنَّةِ وَفَرِيقٌ فِي السَّعِيرِ"
  },
  {
    "id": 13,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 105.1,
    "end": 117.47,
    "text": "وَلَوْ شَاءَ اللَّهُ لَجَعَلَهُمْ أُمَّةً وَاحِدَةً وَلَكِنْ يُدْخِلُ مَنْ يَشَاءُ فِي رَحْمَتِهِ"
  },
  {
    "id": 14,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 118.55,
    "end": 125.73,
    "text": "وَالظَّالِمُونَ مَا لَهُمْ مِنْ وَلِيٍّ وَلَا نَصِيرٍ"
  },
  {
    "id": 15,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 126.96,
    "end": 132.45,
    "text": "أَمِ اتَّخَذُوا مِنْ دُونِهِ أَوْلِيَاءَ"
  },
  {
    "id": 16,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 133.34,
    "end": 142.23,
    "text": "اللَّهُ هُوَ الْوَلِيُّ وَهُوَ يُحْيِي الْمَوْتَى وَهُوَ عَلَى كُلِّ شَيْءٍ قَدِيرٌ"
  },
  {
    "id": 17,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 143.47,
    "end": 150.91,
    "text": "وَمَا اخْتَلَفْتُمْ فِيهِ مِنْ شَيْءٍ فَحُكْمُهُ إِلَى اللَّهِ"
  },
  {
    "id": 18,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 151.94,
    "end": 158.64,
    "text": "ذَلِكُمُ اللَّهُ رَبِّي عَلَيْهِ تَوَكَّلْتُ وَإِلَيْهِ أُنِيبُ"
  },
  {
    "id": 19,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 159.73,
    "end": 162.62,
    "text": "فَاطِرُ السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 20,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 163.63,
    "end": 173.81,
    "text": "جَعَلَ لَكُمْ مِنْ أَنْفُسِكُمْ أَزْوَاجًا وَمِنَ الْأَنْعَامِ أَزْوَاجًا يَذْرَؤُكُمْ فِيهِ"
  },
  {
    "id": 21,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 174.77,
    "end": 177.78,
    "text": "لَيْسَكَ مِثْلِهِ شَيْءٌ"
  },
  {
    "id": 22,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 178.34,
    "end": 181.62,
    "text": "وَهُوَ السَّمِيعُ الْبَصِيرُ"
  },
  {
    "id": 23,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 182.7,
    "end": 186.46,
    "text": "لَهُ مَقَالِيدُ السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 24,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 187.36,
    "end": 192.6,
    "text": "يَبْسُطُ الرِّزْقَ لِمَنْ يَشَاءُ وَيَقْدِرُ"
  },
  {
    "id": 25,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 193.49,
    "end": 197.76,
    "text": "إِنَّهُ بِكُلِّ شَيْءٍ عَلِيمٌ"
  },
  {
    "id": 26,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 199.11,
    "end": 215.12,
    "text": "يَرَعَ لَكُمْ مِنَ الدِّينِ مَا وَصَّى بِهِ نُوحًا وَالَّذِي أَوْحَيْنَا إِلَيْكَ وَمَا وَصَّيْنَا بِهِ إِبْرَاهِيمَ وَمُوسَى وَعِيسَى"
  },
  {
    "id": 27,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 216.36,
    "end": 221.29,
    "text": "وَأَنْ أَقِيمُوا الدِّينَ وَلَا تَتَفَرَّقُوا فِيهِ"
  },
  {
    "id": 28,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 222.19,
    "end": 227.27,
    "text": "وَيَبْوَعَ عَلَى الْمُشْرِكِينَ مَا تَدْعُوهُمْ إِلَيْهِ"
  },
  {
    "id": 29,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 228.05,
    "end": 238.62,
    "text": "اللَّهُ يَجْتَبِي إِلَيْهِ مَنْ يَشَاءُ وَيَهْدِي إِلَيْهِ مَنْ يُنِيبُ"
  },
  {
    "id": 30,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 240.08,
    "end": 248.96,
    "text": "وَمَا تَفَرَّقُوا إِلَّا مِنْ بَعْدِ مَا جَاءَهُمُ الْعِلْمُ بَغْيًا بَيْنَهُمْ"
  },
  {
    "id": 31,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 250.07,
    "end": 259.66,
    "text": "وَلَوْلَا كَلِمَةٌ سَبَقَتْ مِنْ رَبِّكَ إِلَى أَجَلٍ مُسَمًّى لَقُضِيَ بَيْنَهُمْ"
  },
  {
    "id": 32,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 260.72,
    "end": 270.05,
    "text": "وَإِنَّ الَّذِينَ أُورِثُوا الْكِتَابَ مِنْ بَعْدِهِمْ لَفِي شَكٍّ مِنْهُ مُرِيبٍ"
  },
  {
    "id": 33,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 271.15,
    "end": 272.65,
    "text": "بَلِ ذَلِكَ فَادْعَ"
  },
  {
    "id": 34,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 273.54,
    "end": 276.5,
    "text": "وَاسْتَقِمْ كَمَا أُمِرْ"
  },
  {
    "id": 35,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 277.42,
    "end": 281.2,
    "text": "وَلَا تَتَّبِعْ أَهْوَاءَهُمْ"
  },
  {
    "id": 36,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 282.34,
    "end": 289.63,
    "text": "وَقُلْ آمَنْتُ بِمَا أَنْزَلَ اللَّهُ مِنْ كِتَابٍ"
  },
  {
    "id": 37,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 290.35,
    "end": 293.16,
    "text": "وَأُمِرْتُ لِأَعْدِنَ بَيْنَكُمْ"
  },
  {
    "id": 38,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 294.06,
    "end": 297.02,
    "text": "وَاللَّهُ رَبُّنَا وَرَبُّكُمْ"
  },
  {
    "id": 39,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 297.73,
    "end": 302.7,
    "text": "قَالَ نَا أَعْمَالُنَا وَلَكُمْ أَعْمَالُكُمْ"
  },
  {
    "id": 40,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 303.46,
    "end": 306.82,
    "text": "إِلَى حُجَّةَ بَيْنَنَا وَبَيْنَكُمْ"
  },
  {
    "id": 41,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 307.61,
    "end": 310.49,
    "text": "اللَّهُ يَجْمَعُ بَيْنَنَا"
  },
  {
    "id": 42,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 311.18,
    "end": 314.03,
    "text": "وَإِلَيْهِ الْمَصِيرُ"
  },
  {
    "id": 43,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 315.45,
    "end": 328.83,
    "text": "وَالَّذِينَ يُحَاجُّونَ فِي اللَّهِ مِنْ بَعْدِ مَا اسْتُجِيبَ لَهُ حُجَّتُهُمْ دَاحِضَةٌ عِنْدَ رَبِّهِمْ وَعَلَيْهِمْ غَضَبٌ"
  },
  {
    "id": 44,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 329.89,
    "end": 335.92,
    "text": "وَعَلَيْهِمْ غَضَبٌ وَلَهُمْ عَذَابٌ شَدِيدٌ"
  },
  {
    "id": 45,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 336.93,
    "end": 344.02,
    "text": "اللَّهُ الَّذِي أَنْزَلَ الْكِتَابَ بِالْحَقِّ وَالْمِيزَانَ"
  },
  {
    "id": 46,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 344.76,
    "end": 349.5,
    "text": "وَمَا يُدْرِيكَ لَعَلَّ السَّاعَةَ قَرِيبٌ"
  },
  {
    "id": 47,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 350.4,
    "end": 354.84,
    "text": "يَسْتَعْجِلُ بِهَا الَّذِينَ لَا يُؤْمِنُونَ بِهَا"
  },
  {
    "id": 48,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 355.77,
    "end": 362.25,
    "text": "وَالَّذِينَ آمَنُوا مُشْفِقُونَ مِنْهَا وَيَعْلَمُونَ أَنَّهَا الْحَقُّومُ"
  },
  {
    "id": 49,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 362.63,
    "end": 362.71,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 50,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 363.64,
    "end": 372.65,
    "text": "أَلَا إِنَّ الَّذِينَ يُمَارُونَ فِي السَّاعَةِ لَفِي ضَلَالٍ بَعِيدٍ"
  },
  {
    "id": 51,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 373.66,
    "end": 380.58,
    "text": "اللَّهُ لَطِيفٌ بِعِبَادِهِ يَرْزُقُ مَنْ يَشَاءُ"
  },
  {
    "id": 52,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 381.35,
    "end": 384.52,
    "text": "وَهُوَ الْقَوِيُّ الْعَزِيزُ"
  },
  {
    "id": 53,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 385.56,
    "end": 391.58,
    "text": "مَنْ كَانَ يُرِيدُ حَرْثَ الْآخِرَةِ نَزِدْ لَهُ فِي حَرْثِهِ"
  },
  {
    "id": 54,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 392.86,
    "end": 403.26,
    "text": "وَمَنْ كَانَ يُرِيدُ حَرْثَ الدُّنْيَا نُؤْتِهِ مِنْهَا وَمَا لَهُ فِي الْآخِرَةِ مِنْ نَصِيبٍ"
  },
  {
    "id": 55,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 404.82,
    "end": 413.41,
    "text": "أَمْ لَهُمْ شُرَكَاءُ شَرَعُونَهُمْ مِنَ الدِّينِ مَا لَمْ يَأْذَنْ بِهِ اللَّهُ"
  },
  {
    "id": 56,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 414.55,
    "end": 418.63,
    "text": "وَلَوْلَا كَلِمَةُ الْفَصْلِ لَقُضِيَ بَيْنَهُمْ"
  },
  {
    "id": 57,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 419.38,
    "end": 424.77,
    "text": "وَإِنَّ الظَّالِمِينَ لَهُمْ عَذَابٌ أَلِيمٌ"
  },
  {
    "id": 58,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 425.86,
    "end": 432.97,
    "text": "تَرَى الظَّالِمِينَ مُشْفِقِينَ مِمَّا كَسَبُوا وَهُوَ وَاقِعٌ بِهِمْ"
  },
  {
    "id": 59,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 434.13,
    "end": 441.73,
    "text": "وَالَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ فِي رَوْضَاتِ الْجَنَّادِ"
  },
  {
    "id": 60,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 442.7,
    "end": 447.85,
    "text": "لَهُمْ مَا يَشَاءُونَ عِنْدَ رَبِّهِمْ"
  },
  {
    "id": 61,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 448.63,
    "end": 452.32,
    "text": "ذَلِكَ هُوَ الْفَضْلُ الْكَبِيرُ"
  },
  {
    "id": 62,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 453.5,
    "end": 461.94,
    "text": "ذَلِكَ الَّذِي يُبَشِّرُ اللَّهُ عِبَادَهُ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ"
  },
  {
    "id": 63,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 463.04,
    "end": 469.72,
    "text": "قُلْ لَا أَسْأَلُكُمْ عَلَيْهِ أَجْرًا إِلَّا الْمَوَدَّةَ فِي الْقُرْبَى"
  },
  {
    "id": 64,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 470.65,
    "end": 476.46,
    "text": "وَمَنْ يَقْتَرِفْ حَسَنَةً نَزِدْ لَهُ فِيهَا حُسْنًا"
  },
  {
    "id": 65,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 477.26,
    "end": 481.68,
    "text": "إِنَّ اللَّهَ غَفُورٌ شَكُورٌ"
  },
  {
    "id": 66,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 482.71,
    "end": 486.9,
    "text": "أَمْ يَقُولُونَ افْتَرَى عَلَى اللَّهِ كَذِبًا"
  },
  {
    "id": 67,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 487.74,
    "end": 491.68,
    "text": "فَإِنْ يَشَإِ اللَّهُ يَخْتِمْ عَلَى قَلْبِهِ"
  },
  {
    "id": 68,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 492.62,
    "end": 498.18,
    "text": "وَيَمْحُو اللَّهُ الْبَاطِلَ وَيُحِقُّ الْحَقَّ بِكَلِمَاتِهِ"
  },
  {
    "id": 69,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 498.95,
    "end": 504.04,
    "text": "إِنَّهُ عَلِيمٌ بِذَاتِ الصُّدُورِ"
  },
  {
    "id": 70,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 505.65,
    "end": 515.76,
    "text": "وَهُوَ الَّذِي يَقْبَلُ التَّوْبَةَ عَنْ عِبَادِهِ وَيَعْفُو عَنِ السَّيِّئَاتِ وَيَعْلَمُ مَا تَفْعَلُونَ"
  },
  {
    "id": 71,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 517.04,
    "end": 525.59,
    "text": "وَيَسْتَجِيبُ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ وَيَزِيدُهُمْ مِنْ فَضْلِهِ"
  },
  {
    "id": 72,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 526.47,
    "end": 531.69,
    "text": "وَالْكَافِرُونَ لَهُمْ عَذَابٌ شَدِيدٌ"
  },
  {
    "id": 73,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 532.85,
    "end": 544.3,
    "text": "وَلَوْ بَسَطَ اللَّهُ الرِّزْقَ لِعِبَادِهِ نَبَغَوْا فِي الْأَرْضِ وَلَكِنْ يُنَزِّلُ بِقَدَرٍ مَا يَشَاءُ"
  },
  {
    "id": 74,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 545.25,
    "end": 550.95,
    "text": "إِنَّهُ بِعِبَادِهِ خَبِيرٌ بَصِيرٌ"
  },
  {
    "id": 75,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 552.19,
    "end": 560.04,
    "text": "وَهُوَ الَّذِي يُنَزِّلُ الْغَيْثَ مِنْ بَعْدِ مَا قَنَطُوا وَيَنْشُرُ رَحْمَتَهُ"
  },
  {
    "id": 76,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 560.95,
    "end": 564.22,
    "text": "وَهُوَ الْوَلِيُّ الْحَمِيدُ"
  },
  {
    "id": 77,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 565.33,
    "end": 575.43,
    "text": "وَمِنْ آيَاتِهِ خَلْقُ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَثَّ فِيهِمَا مِنْ دَابَّةٍ"
  },
  {
    "id": 78,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 576.39,
    "end": 582.49,
    "text": "وَهُوَ عَلَى جَمْعِهِمْ إِذَا يَشَاءُ قَدِيرٌ"
  },
  {
    "id": 79,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 583.97,
    "end": 595.08,
    "text": "وَمَا أَصَابَكُمْ مِنْ مُصِيبَةٍ فَبِمَا كَسَبَتْ أَيْدِيكُمْ وَيَعْفُو عَنْ كَثِيرٍ"
  },
  {
    "id": 80,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 596.2,
    "end": 601.27,
    "text": "وَمَا أَنْتُمْ بِمُعْجِزِينَ فِي الْأَرْضِ"
  },
  {
    "id": 81,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 602.06,
    "end": 609.95,
    "text": "وَمَا لَكُمْ مِنْ دُونِ اللَّهِ مِنْ وَلِيٍّ وَلَا نَصِيرٍ"
  },
  {
    "id": 82,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 611.31,
    "end": 616.7,
    "text": "وَمِنْ آيَاتِهِ الْجَوَارِ فِي الْبَحْرِ كَالْأَعْلَامِ"
  },
  {
    "id": 83,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 617.78,
    "end": 623.61,
    "text": "إِنْ يَشَأْ يُسْكِنِ الرِّيحَ فَيَظْلَلْنَ رَوَاكِدَ عَلَى ظَهْرِهِ"
  },
  {
    "id": 84,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 624.49,
    "end": 631.93,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِكُلِّ صَبَّارٍ شَكُورٍ"
  },
  {
    "id": 85,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 633.09,
    "end": 639.6,
    "text": "لَوْ يُوبِقْهُنَّ بِمَا كَسَبُوا وَيَعْفُ عَنْ كَثِيرٍ"
  },
  {
    "id": 86,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 640.53,
    "end": 649.8,
    "text": "وَيَعْلَمَ الَّذِينَ يُجَادِلُونَ فِي آيَاتِنَا مَا لَهُمْ مِنْ مَحِيصٍ"
  },
  {
    "id": 87,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 650.87,
    "end": 658.57,
    "text": "فَمَا أُوتِيتُمْ مِنْ شَيْءٍ فَمَتَاعُ الْحَيَاةِ الدُّنْيَا"
  },
  {
    "id": 88,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 659.57,
    "end": 669.84,
    "text": "وَمَا عِنْدَ اللَّهِ خَيْرٌ وَأَبْقَى لِلَّذِينَ آمَنُوا وَعَلَى رَبِّهِمْ يَتَوَكَّلُونَ"
  },
  {
    "id": 89,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 671.26,
    "end": 681.64,
    "text": "وَالَّذِينَ يَجْتَنِبُونَ كَبَائِرَ الْإِثْمِ وَالْفَوَاحِشَ وَإِذَا مَا غَضِبُوا هُمْ يَغْفِرُونَ"
  },
  {
    "id": 90,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 682.83,
    "end": 695.77,
    "text": "وَالَّذِينَ اسْتَجَابُوا لِرَبِّهِمْ وَأَقَامُوا الصَّلَاةَ وَأَمْرُهُمْ شُورَى بَيْنَهُمْ وَمِمَّا رَزَقْنَاهُمْ يُنْفِقُونَ"
  },
  {
    "id": 91,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 697.18,
    "end": 704.78,
    "text": "وَالَّذِينَ إِذَا أَصَابَهُمُ الْبَغْيُ هُمْ يَنْتَصِرُونَ"
  },
  {
    "id": 92,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 705.81,
    "end": 711.73,
    "text": "وَجَزَاءُ سَيِّئَةٍ سَيِّئَةٌ مِثْلُهَا"
  },
  {
    "id": 93,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 712.61,
    "end": 717.46,
    "text": "فَمَنْ عَفَا وَأَصْلَحَ فَأَجْرُهُ عَلَى اللَّهِ"
  },
  {
    "id": 94,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 718.34,
    "end": 722.87,
    "text": "إِنَّهُ لَا يُحِبُّ الظَّالِمِينَ"
  },
  {
    "id": 95,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 724.14,
    "end": 733.96,
    "text": "وَلَمَنِ انْتَصَرَ بَعْدَ ظُلْمِهِ فَأُولَئِكَ مَا عَلَيْهِمْ مِنْ سَبِيلٍ"
  },
  {
    "id": 96,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 735.07,
    "end": 743.67,
    "text": "إِنَّمَا السَّبِيلُ عَلَى الَّذِينَ يَظْلِمُونَ النَّاسَ وَيَبْغُونَ فِي الْأَرْضِ بِغَيْرِ الْحَقِّ"
  },
  {
    "id": 97,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 744.12,
    "end": 744.23,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 98,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 744.98,
    "end": 750.22,
    "text": "أُولَئِكَ لَهُمْ عَذَابٌ أَلِيمٌ"
  },
  {
    "id": 99,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 751.28,
    "end": 758.21,
    "text": "وَلَمَنْ صَبَرَ وَغَفَرَ إِنَّ ذَلِكَ لَمِنْ عَزْمِ الْأُمُورِ"
  },
  {
    "id": 100,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 758.97,
    "end": 766.02,
    "text": "وَمَنْ يُضْلِلِ اللَّهُ فَمَا لَهُ مِنْ وَلِيٍّ مِنْ بَعْدِهِ"
  },
  {
    "id": 101,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 767.18,
    "end": 778.11,
    "text": "وَتَرَى الظَّالِمِينَ لَمَّا رَأَوُا الْعَذَابَ يَقُولُونَ هَلْ إِلَى مَرَدٍّ مِنْ سَبِيلٍ"
  },
  {
    "id": 102,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 779.28,
    "end": 788.62,
    "text": "وَتَرَاهُمْ يُعْرَضُونَ عَلَيْهَا خَاشِعِينَ مِنَ الذُّلِّ يَنْظُرُونَ مِنْ طَرْفٍ خَفِيٍّ"
  },
  {
    "id": 103,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 789.91,
    "end": 802.6,
    "text": "وَقَالَ الَّذِينَ آمَنُوا إِنَّ الْخَاسِرِينَ الَّذِينَ خَسِرُوا أَنْفُسَهُمْ وَأَهْلِيهِمْ يَوْمَ الْقِيَامَةِ"
  },
  {
    "id": 104,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 803.77,
    "end": 810.98,
    "text": "عَلَى إِنَّ الظَّالِمِينَ فِي عَذَابٍ مُقِيمٍ"
  },
  {
    "id": 105,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 812.33,
    "end": 821.83,
    "text": "وَمَا كَانَ لَهُمْ مِنْ أَوْلِيَاءَ يَنْصُرُونَهُمْ مِنْ دُونِ اللَّهِ"
  },
  {
    "id": 106,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 822.67,
    "end": 828.8,
    "text": "وَمَنْ يُضْلِلِ اللَّهُ فَمَا لَهُ مِنْ سَبِيلٍ"
  },
  {
    "id": 107,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 829.77,
    "end": 839.86,
    "text": "اسْتَجِيبُوا لِرَبِّكُمْ مِنْ قَبْلِ أَنْ يَأْتِيَ يَوْمٌ لَا مَرَدَّ لَهُ مِنَ اللَّهِ"
  },
  {
    "id": 108,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 840.73,
    "end": 849.51,
    "text": "مَا لَكُمْ مِنْ مَلْجَإٍ يَوْمَئِذٍ وَمَا لَكُمْ مِنْ نَكِيرٍ"
  },
  {
    "id": 109,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 850.9,
    "end": 856.86,
    "text": "فَإِنْ أَعْرَضُوا فَمَا أَرْسَلْنَاكَ عَلَيْهِمْ حَفِيظًا"
  },
  {
    "id": 110,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 857.6,
    "end": 861.02,
    "text": "عَلَيْكَ إِلَّا الْبَلَاغُ"
  },
  {
    "id": 111,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 862.11,
    "end": 871.37,
    "text": "وَإِنَّا إِذَا أَذَقْنَا الْإِنْسَانَ مِنَّا رَحْمَةً فَرِحَ بِهَا"
  },
  {
    "id": 112,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 872.55,
    "end": 883.24,
    "text": "وَإِنْ تُصِبْهُمْ سَيِّئَةٌ بِمَا قَدَّمَتْ أَيْدِيهِمْ فَإِنَّ الْإِنْسَانَ كَفُورٌ"
  },
  {
    "id": 113,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 884.59,
    "end": 888.17,
    "text": "لِلَّهِ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 114,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 889.05,
    "end": 892.08,
    "text": "يَخْلُقُ مَا يَشَاءُ"
  },
  {
    "id": 115,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 893.01,
    "end": 902.48,
    "text": "يَهَبُ لِمَنْ يَشَاءُ إِنَاثًا وَيَهَبُ لِمَنْ يَشَاءُ الذُّكُورَ"
  },
  {
    "id": 116,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 903.97,
    "end": 908.19,
    "text": "أَوْ يُزَوِّجُهُمْ ذُكْرَانًا وَإِنَاثًا"
  },
  {
    "id": 117,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 908.99,
    "end": 913.35,
    "text": "وَيَجْعَلُ مَنْ يَشَاءُ عَقِيمًا"
  },
  {
    "id": 118,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 914.78,
    "end": 919.3,
    "text": "إِنَّهُ عَلِيمٌ قَدِيرٌ"
  },
  {
    "id": 119,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 920.6,
    "end": 930.37,
    "text": "وَمَا كَانَ لِبَشَرٍ أَنْ يُكَلِّمَهُ اللَّهُ إِلَّا وَحْيًا أَوْ مِنْ وَرَاءِ حِجَابٍ"
  },
  {
    "id": 120,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 931.42,
    "end": 940.96,
    "text": "يَوْمٍ وَرَاءِ حِجَابٍ أَوْ يُرْسِلَ رَسُولًا فَيُوحِيَ بِإِذْنِهِ مَا يَشَاءُ"
  },
  {
    "id": 121,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 942.0,
    "end": 946.08,
    "text": "إِنَّهُ عَلِيٌّ حَكِيمٌ"
  },
  {
    "id": 122,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 947.52,
    "end": 953.83,
    "text": "وَكَذَلِكَ أَوْحَيْنَا إِلَيْكَ رُوحًا مِنْ أَمْرِنَا"
  },
  {
    "id": 123,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 954.82,
    "end": 968.67,
    "text": "وَمَا كُنْتَ تَدْرِي مَا الْكِتَابُ وَلَا الْإِيمَانُ وَلَكِنْ جَعَلْنَاهُ نُورًا نَهْدِي بِهِ مَنْ نَشَاءُ مِنْ عِبَادِنَا"
  },
  {
    "id": 124,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 969.74,
    "end": 976.55,
    "text": "وَإِنَّكَ لَتَهْدِي إِلَى صِرَاطٍ مُسْتَقِيمٍ"
  },
  {
    "id": 125,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 977.45,
    "end": 983.51,
    "text": "صِرَاطِ اللَّهِ الَّذِي لَهُ مَا فِي السَّمَاوَاتِ وَمَا فِي الْأَرْضِ"
  },
  {
    "id": 126,
    "sura_id": 42,
    "recitation_uuid": "88f15478-9998-49b3-80ed-adfc466d4c4c",
    "start": 984.43,
    "end": 989.88,
    "text": "أَلَا إِلَى اللَّهِ تَصِيرُ الْأُمُورُ"
  }
]