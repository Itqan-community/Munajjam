[
  {
    "id": 2,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 6.38,
    "end": 14.51,
    "text": "الم"
  },
  {
    "id": 3,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 15.63,
    "end": 18.93,
    "text": "تِلْكَ آيَاتُ الْكِتَابِ"
  },
  {
    "id": 4,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 20.04,
    "end": 25.27,
    "text": "وَالَّذِي أُنْزِلَ إِلَيْكَ مِنْ رَبِّكَ الْحَمَمِ"
  },
  {
    "id": 5,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 25.59,
    "end": 31.82,
    "text": "وَلَكِنَّ أَكْثَرَ النَّاسِ لَا يُؤْمِنُونَ"
  },
  {
    "id": 6,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 32.83,
    "end": 39.77,
    "text": "اللَّهُ الَّذِي رَفَعَ السَّمَاوَاتِ بِغَيْرِ عَمَدٍ تَرَوْنَهَا"
  },
  {
    "id": 7,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 40.73,
    "end": 43.26,
    "text": "ثُمَّ اسْتَوَى عَلَى الْعَظِيمِ"
  },
  {
    "id": 8,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 44.22,
    "end": 52.26,
    "text": "وَسَخَّرَ الشَّمْسَ وَالْقَمَرَ كُلٌّ يَجْرِي لِأَجَلٍ مُسَمًّى"
  },
  {
    "id": 9,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 53.33,
    "end": 63.83,
    "text": "يُدَبِّرُ الْأَمْرَ يُفَصِّلُ الْآيَاتِ لَعَلَّكُمْ بِلِقَاءِ رَبِّكُمْ تُوقِنُونَ"
  },
  {
    "id": 10,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 64.89,
    "end": 71.35,
    "text": "وَهُوَ الَّذِي مَدَّ الْأَرْضَ وَجَعَلَ فِيهَا رَوَاسِيَ وَأَنْهَارًا"
  },
  {
    "id": 11,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 72.2,
    "end": 78.64,
    "text": "وَمِنْ كُلِّ الثَّمَرَاتِ جَعَلَ فِيهَا زَوْجَيْنِ اثْنَيْنِ"
  },
  {
    "id": 12,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 79.6,
    "end": 83.45,
    "text": "يُغْشِي اللَّيْلَ النَّهَارَ"
  },
  {
    "id": 13,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 84.34,
    "end": 91.76,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِقَوْمٍ يَتَفَكَّرُونَ"
  },
  {
    "id": 14,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 92.73,
    "end": 107.95,
    "text": "وَفِي الْأَرْضِ قِطَعٌ مُتَجَاوِرَاتٌ وَجَنَّاتٌ مِنْ أَعْنَابٍ وَزَرْعٌ وَنَخِيلٌ صِنْوَانٌ وَغَيْرُ صِنْوَانٍ"
  },
  {
    "id": 15,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 109.33,
    "end": 125.76,
    "text": "وَزَرْعٌ وَنَخِيلٌ صِنْوَانٌ وَغَيْرُ صِنْوَانٍ يُسْقَى بِمَاءٍ وَاحِدٍ وَنُفَضِّلُ بَعْضَهَا عَلَى بَعْضٍ فِي الْأُكُلِينَ"
  },
  {
    "id": 16,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 127.14,
    "end": 134.65,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِقَوْمٍ يَعْقِلُونَ"
  },
  {
    "id": 17,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 135.9,
    "end": 147.96,
    "text": "وَإِنْ تَعْجَبْ فَعَجَبٌ قَوْلُهُمْ أَإِذَا كُنَّا تُرَابًا أَإِنَّا لَفِي خَلْقٍ جَدِيدٍ"
  },
  {
    "id": 18,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 148.84,
    "end": 153.95,
    "text": "أُولَئِكَ الَّذِينَ كَفَرُوا بِرَبِّهِمْ"
  },
  {
    "id": 19,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 154.96,
    "end": 161.08,
    "text": "وَأُولَئِكَ الْأَغْلَالُ فِي أَعْنَاقِهِمْ"
  },
  {
    "id": 20,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 161.81,
    "end": 166.82,
    "text": "وَأُولَئِكَ أَصْحَابُ النَّارِ"
  },
  {
    "id": 21,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 167.59,
    "end": 170.66,
    "text": "لِيُمْفِيهَا خَالِدُونَ"
  },
  {
    "id": 22,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 171.89,
    "end": 181.72,
    "text": "وَيَسْتَعْجِلُونَكَ بِالسَّيِّئَةِ قَبْلَ الْحَسَنَةِ وَقَدْ خَلَتْ مِنْ قَبْلِهِمُ الْمَثُلَادُ"
  },
  {
    "id": 23,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 182.84,
    "end": 189.26,
    "text": "وَإِنَّ رَبَّكَ لَذُو مَغْفِرَةٍ لِلنَّاسِ عَلَى ظُلْمِهِمْ"
  },
  {
    "id": 24,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 190.08,
    "end": 194.79,
    "text": "وَإِنَّ رَبَّكَ لَشَدِيدُ الْعِقَابِ"
  },
  {
    "id": 25,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 195.91,
    "end": 204.76,
    "text": "وَيَقُولُ الَّذِينَ كَفَرُوا لَوْلَا أُنْزِلَ عَلَيْهِ آيَةٌ مِنْ رَبِّهِ"
  },
  {
    "id": 26,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 205.66,
    "end": 210.24,
    "text": "إِنَّمَا أَنْتَ مُنْذِرٌ"
  },
  {
    "id": 27,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 211.13,
    "end": 214.44,
    "text": "وَلِكُلِّ قَوْمٍ هَادٍ"
  },
  {
    "id": 28,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 215.53,
    "end": 225.33,
    "text": "وَاللَّهُ يَعْلَمُ مَا تَحْمِلُ كُلُّ أُنْثَى وَمَا تَغِيضُ الْأَرْحَامُ وَمَا تَزْدَادُ"
  },
  {
    "id": 29,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 226.28,
    "end": 231.18,
    "text": "وَكُلُّ شَيْءٍ عِنْدَهُ بِمِقْدَارٍ"
  },
  {
    "id": 30,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 232.01,
    "end": 238.04,
    "text": "عَالِمُ الْغَيْبِ وَالشَّهَادَةِ الْكَبِيرُ الْمُتَعَالِ"
  },
  {
    "id": 31,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 239.26,
    "end": 249.38,
    "text": "ذَوَاءٌ مِنْكُمْ مَنْ أَسَرَّ الْقَوْلَ وَمَنْ جَهَرَ بِهِ وَمَنْ هُوَ"
  },
  {
    "id": 32,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 249.7,
    "end": 256.15,
    "text": "وَيَخْفٍ بِاللَّيْلِ وَسَارِبٌ بِالنَّهَارِ"
  },
  {
    "id": 33,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 257.49,
    "end": 267.55,
    "text": "لَهُ مُعَقِّبَاتٌ مِنْ بَيْنِ يَدَيْهِ وَمِنْ خَلْفِهِ يَحْفَظُونَهُ مِنْ أَمْرِ اللَّهِ"
  },
  {
    "id": 34,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 268.68,
    "end": 273.26,
    "text": "إِنَّ اللَّهَ لَا يُغَيِّرُ مَا بِقَوْمٍ حَسَنٍ"
  },
  {
    "id": 35,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 273.57,
    "end": 277.4,
    "text": "لَا يُغَيِّرُوا مَا بِأَنْفُسِهِ"
  },
  {
    "id": 36,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 278.63,
    "end": 287.51,
    "text": "وَإِذَا أَرَادَ اللَّهُ بِقَوْمٍ سُوءًا فَلَا مَرَدَّنَا"
  },
  {
    "id": 37,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 288.53,
    "end": 294.64,
    "text": "وَمَا لَهُمْ مِنْ دُونِهِ مِنْ وَادٍ"
  },
  {
    "id": 38,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 295.7,
    "end": 305.25,
    "text": "هُوَ الَّذِي يُرِيكُمُ الْبَرْقَ خَوْفًا وَطَمَعًا وَيُنْشِئُ السَّحَابَ الثِّقَالَ"
  },
  {
    "id": 39,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 306.29,
    "end": 320.35,
    "text": "وَيُسَبِّحُ الرَّعْدُ بِحَمْدِهِ وَالْمَلَائِكَةُ مِنْ خِيفَتِهِ وَيُرْسِلُ الصَّوَاعِقَ فَيُصِيبُ بِهَا مَنْ يَشَاءُ"
  },
  {
    "id": 40,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 321.67,
    "end": 334.56,
    "text": "وَيُرْسِلُ الصَّوَاعِقَ فَيُصِيبُ بِهَا مَنْ يَشَاءُ وَهُمْ يُجَادِلُونَ فِي اللَّهِ وَهُوَ شَدِيدُ الْمِحَالِ"
  },
  {
    "id": 41,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 336.28,
    "end": 338.03,
    "text": "وَيَهُ دَعْوَةُ الْحَقِّ"
  },
  {
    "id": 42,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 338.46,
    "end": 338.52,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 43,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 339.57,
    "end": 355.61,
    "text": "وَالَّذِينَ يَدْعُونَ مِنْ دُونِهِ لَا يَسْتَجِيبُونَ لَهُمْ بِشَيْءٍ إِلَّا كَبَاسِطِ كَفَّيْهِ إِلَى الْمَاءِ لِيَبْلُغَ فَاهُ وَمَا هُوَ بِبَالِغِهِ"
  },
  {
    "id": 44,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 357.0,
    "end": 363.62,
    "text": "وَمَا دُعَاءُ الْكَافِرِينَ إِلَّا فِي ضَلَالٍ"
  },
  {
    "id": 45,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 365.07,
    "end": 377.75,
    "text": "وَلِلَّهِ يَسْجُدُ مَنْ فِي السَّمَاوَاتِ وَالْأَرْضِ طَوْعًا وَكَرْهًا وَظِلَالُهُمْ بِالْغُدُوِّ وَالْآصَالِ"
  },
  {
    "id": 46,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 378.83,
    "end": 383.92,
    "text": "قُلْ مَنْ رَبُّ السَّمَاوَاتِ وَالْأَرْضِ قُلِ اللَّهُ"
  },
  {
    "id": 47,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 384.93,
    "end": 397.65,
    "text": "قُلْ أَفَتَّخَذْتُمْ مِنْ دُونِهِ أَوْلِيَاءَ لَا يَمْلِكُونَ لِأَنْفُسِهِمْ نَفْعًا وَلَا ضَرًّا"
  },
  {
    "id": 48,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 398.77,
    "end": 407.47,
    "text": "قُلْ هَلْ يَسْتَوِي الْأَعْمَى وَالْبَصِيرُ أَمْ هَلْ تَسْتَوِي الظُّلُمَاتُ وَالنُّورُ"
  },
  {
    "id": 49,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 408.45,
    "end": 417.31,
    "text": "أَمْ جَعَلُوا لِلَّهِ شُرَكَاءَ خَلَقُوكَ خَلْقِهِ فَتَشَابَهَا الْخَلْقُ عَلَيْهِمْ"
  },
  {
    "id": 50,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 418.46,
    "end": 425.92,
    "text": "قُلِ اللَّهُ خَالِقُ كُلِّ شَيْءٍ وَهُوَ الْوَاحِدُ الْقَهَّارُ"
  },
  {
    "id": 51,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 427.42,
    "end": 440.01,
    "text": "تَنْزَلَ مِنَ السَّمَاءِ مَاءً فَسَالَتْ أَوْدِيَةٌ بِقَدَرِهَا فَاعْتَمَلَ السَّيْلُ زَبَدًا رَابِيَا"
  },
  {
    "id": 52,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 441.11,
    "end": 451.32,
    "text": "وَمِمَّا يُوقِدُونَ عَلَيْهِ فِي النَّارِ ابْتِغَاءَ حِلْيَةٍ أَوْ مَتَاعٍ زَبَدٌ مِثْلُهُ"
  },
  {
    "id": 53,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 452.21,
    "end": 456.5,
    "text": "كَذَلِكَ يَضْرِبُ اللَّهُ الْحَقَّ وَالْبَاطِلَ"
  },
  {
    "id": 54,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 457.57,
    "end": 462.04,
    "text": "فَأَمَّا الزَّبَدُ فَيَذْهَبُ جُفَاءً"
  },
  {
    "id": 55,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 462.91,
    "end": 468.62,
    "text": "وَأَمَّا مَا يَنْفَعُ النَّاسَ فَيَمْكُثُ فِي الْأَرْضِ"
  },
  {
    "id": 56,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 469.63,
    "end": 474.2,
    "text": "كَذَلِكَ يَضْرِبُ اللَّهُ الْأَمْثَالَ"
  },
  {
    "id": 57,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 475.34,
    "end": 479.74,
    "text": "لِلَّذِينَ اسْتَجَابُوا لِرَبِّهِمُ الْحُسْنَى"
  },
  {
    "id": 58,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 480.9,
    "end": 492.89,
    "text": "وَالَّذِينَ لَمْ يَسْتَجِيبُوا لَهُ لَوْ أَنَّ لَهُمْ مَا فِي الْأَرْضِ جَمِيعًا وَمِثْلَهُ مَعَهُ لَافْتَدَوْا"
  },
  {
    "id": 59,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 493.91,
    "end": 502.08,
    "text": "أُولَئِكَ لَهُمْ سُوءُ الْحِسَابِ وَمَأْوَاهُمْ جَهَنَّمَ"
  },
  {
    "id": 60,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 502.95,
    "end": 503.18,
    "text": "وَبِينِ"
  },
  {
    "id": 61,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 503.54,
    "end": 505.51,
    "text": "الْمِهَادِ"
  },
  {
    "id": 62,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 506.61,
    "end": 515.87,
    "text": "أَفَمَنْ يَعْلَمُ أَنَّمَا أُنْزِلَ إِلَيْكَ مِنْ رَبِّكَ الْحَقُّ كَمَنْ هُوَ أَعْمَى"
  },
  {
    "id": 63,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 516.92,
    "end": 521.71,
    "text": "إِنَّمَا يَتَذَكَّرُ أُولُو الْأَلْبَابِ"
  },
  {
    "id": 64,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 522.77,
    "end": 530.49,
    "text": "الَّذِينَ يُوفُونَ بِعَهْدِ اللَّهِ وَلَا يَنْقُضُونَ الْمِيثَاقَ"
  },
  {
    "id": 65,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 531.75,
    "end": 546.6,
    "text": "وَالَّذِينَ يَصِلُونَ مَا أَمَرَ اللَّهُ بِهِ أَنْ يُوصَلَ وَيَخْشَوْنَ رَبَّهُمْ وَيَخَافُونَ سُوءَ الْحِسَابِ"
  },
  {
    "id": 66,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 548.01,
    "end": 557.0,
    "text": "وَالَّذِينَ صَبَرُوا ابْتِغَاءَ وَجْهِ رَبِّهِمْ وَأَقَامُوا الصَّلَاةَ وَأَنْفَقُوا"
  },
  {
    "id": 67,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 558.17,
    "end": 572.38,
    "text": "وَأَنْفَقُوا مِنْ مَا رَزَقْنَاهُمْ سِرًّا وَعَلَانِيَةً وَيَدْرَءُونَ بِالْحَسَنَةِ السَّيِّئَةَ أُولَئِكَ لَهُمْ عُقْبًا"
  },
  {
    "id": 68,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 572.7,
    "end": 574.05,
    "text": "يس"
  },
  {
    "id": 69,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 575.67,
    "end": 587.13,
    "text": "جَنَّاتُ عَدْنٍ يَدْخُلُونَهَا وَمَنْ صَلَحَ مِنْ آبَائِهِمْ وَأَزْوَاجِهِمْ وَذُرِّيَّاتِهِمْ"
  },
  {
    "id": 70,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 588.35,
    "end": 596.33,
    "text": "وَالْمَلَائِكَةُ يَدْخُلُونَ عَلَيْهِمْ مِنْ كُلِّ بَابٍ"
  },
  {
    "id": 71,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 597.16,
    "end": 601.19,
    "text": "سَلَامٌ عَلَيْكُمْ بِمَا صَبَرْتُمْ"
  },
  {
    "id": 72,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 601.93,
    "end": 603.2,
    "text": "بَنِعْمَ عُقْبًا"
  },
  {
    "id": 73,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 603.53,
    "end": 605.01,
    "text": "يس"
  },
  {
    "id": 74,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 605.78,
    "end": 625.51,
    "text": "وَالَّذِينَ يَنْقُضُونَ عَهْدَ اللَّهِ مِنْ بَعْدِ مِيثَاقِهِ وَيَقْطَعُونَ مَا أَمَرَ اللَّهُ بِهِ أَنْ يُوصَلَ وَيُفْسِدُونَ فِي الْأَرْضِ أُولَئِكَ لَهُمُ اللَّعْنَةٌ"
  },
  {
    "id": 75,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 626.84,
    "end": 633.34,
    "text": "أُولَئِكَ لَهُمُ اللَّعْنَةُ وَلَهُمْ سُوءُونَ"
  },
  {
    "id": 76,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 633.66,
    "end": 634.92,
    "text": "ي"
  },
  {
    "id": 77,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 636.12,
    "end": 642.19,
    "text": "اللَّهُ يَبْسُطُ الرِّزْقَ لِمَنْ يَشَاءُ وَيَقْدِرُ"
  },
  {
    "id": 78,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 643.17,
    "end": 652.61,
    "text": "وَفَرِحُوا بِالْحَيَاةِ الدُّنْيَا وَمَا الْحَيَاةُ الدُّنْيَا فِي الْآخِرَةِ إِلَّا مَتَاعٌ"
  },
  {
    "id": 79,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 653.94,
    "end": 662.83,
    "text": "وَيَقُولُ الَّذِينَ كَفَرُوا لَوْلَا أُنْزِلَ عَلَيْهِ آيَةٌ مِنْ رَبِّهِ"
  },
  {
    "id": 80,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 664.1,
    "end": 674.23,
    "text": "قُلْ إِنَّ اللَّهَ يُضِلُّ مَنْ يَشَاءُ وَيَهْدِي إِلَيْهِ مَنْ أَنَابَ"
  },
  {
    "id": 81,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 675.25,
    "end": 682.95,
    "text": "الَّذِينَ آمَنُوا وَتَطْمَئِنُّ قُلُوبُهُمْ بِذِكْرِ اللَّهِ"
  },
  {
    "id": 82,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 683.81,
    "end": 689.55,
    "text": "لَلَا بِذِكْرِ اللَّهِ تَطْمَئِنُّ الْقُلُوبُ"
  },
  {
    "id": 83,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 690.58,
    "end": 698.36,
    "text": "الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ طُوبَى لَهُمْ وَحُسْنُ مَآبٍ"
  },
  {
    "id": 84,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 699.55,
    "end": 716.27,
    "text": "كَذَلِكَ أَرْسَلْنَاكَ فِي أُمَّةٍ قَدْ خَلَتْ مِنْ قَبْلِهَا أُمَمٌ لِتَتْلُوَ عَلَيْهِمُ الَّذِي أَوْحَيْنَا إِلَيْكَ"
  },
  {
    "id": 85,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 717.63,
    "end": 727.42,
    "text": "لِتَتْلُوَ عَلَيْهِمُ الَّذِي أَوْحَيْنَا إِلَيْكَ وَهُمْ يَكْفُرُونَ بِالرَّحْمَنِ"
  },
  {
    "id": 86,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 728.53,
    "end": 738.35,
    "text": "قُلْ هُوَ رَبِّي لَا إِلَهَ إِلَّا هُوَ عَلَيْهِ تَوَكَّلْتُ وَإِلَيْهِ مَتَابٌ"
  },
  {
    "id": 87,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 739.76,
    "end": 750.28,
    "text": "وَلَوْ أَنَّ قُرْآنًا سُيِّرَتْ بِهِ الْجِبَالُ أَوْ قُطِّعَتْ بِهِ الْأَرْضُ أَوْ كُلِّمَ بِهِ الْمَوْتَى"
  },
  {
    "id": 88,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 751.37,
    "end": 754.5,
    "text": "بَلْ لِلَّهِ الْأَمْرُ جَمِيعًا"
  },
  {
    "id": 89,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 755.77,
    "end": 765.95,
    "text": "أَفَلَمْ يَيْأَسِ الَّذِينَ آمَنُوا أَنْ لَوْ يَشَاءُ اللَّهُ لَهَدَ النَّاسَ جَمِيعًا"
  },
  {
    "id": 90,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 767.28,
    "end": 779.25,
    "text": "وَلَا يَزَالُ الَّذِينَ كَفَرُوا تُصِيبُهُمْ بِمَا صَنَعُوا قَارِعَةٌ أَوْ تَحُلُّ قَرِيبًا مِنْ دَارِهِمْ"
  },
  {
    "id": 91,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 779.58,
    "end": 780.07,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 92,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 780.43,
    "end": 782.64,
    "text": "فِيَوَعْدُ اللَّهِ"
  },
  {
    "id": 93,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 783.78,
    "end": 788.64,
    "text": "إِنَّ اللَّهَ لَا يُخْلِفُ الْمِيعَادَ"
  },
  {
    "id": 94,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 790.07,
    "end": 800.14,
    "text": "وَلَقَدِ اسْتُهْزِئَ بِرُسُلٍ مِنْ قَبْلِكَ فَأَمْلَيْتُ لِلَّذِينَ كَفَرُوا ثُمَّ أَخَذْتُهُمْ"
  },
  {
    "id": 95,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 801.05,
    "end": 804.17,
    "text": "فَكَيْفَ كَانَ عِقَابِ"
  },
  {
    "id": 96,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 805.08,
    "end": 812.31,
    "text": "أَفَمَنْ هُوَ قَائِمٌ عَلَى كُلِّ نَفْسٍ بِمَا كَسَبَ"
  },
  {
    "id": 97,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 813.47,
    "end": 818.99,
    "text": "وَجَعَلُوا لِلَّهِ شُرَكَاءَ قُلْ سَمُّوهُمْ"
  },
  {
    "id": 98,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 820.07,
    "end": 829.18,
    "text": "أَمْ تُنَبِّئُونَهُ بِمَا لَا يَعْلَمُ فِي الْأَرْضِ أَمْ بِظَاهِرٍ مِنَ الْقَوْلِ"
  },
  {
    "id": 99,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 830.14,
    "end": 837.13,
    "text": "بَلْ زُيِّنَ لِلَّذِينَ كَفَرُوا مَكْرُهُمْ وَصُدُّوا عَنِ السَّبِيلِ"
  },
  {
    "id": 100,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 838.06,
    "end": 843.78,
    "text": "وَمَنْ يُضْلِلِ اللَّهُ فَمَا لَهُ مِنْ هَادٍ"
  },
  {
    "id": 101,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 845.02,
    "end": 849.14,
    "text": "لَهُمْ عَذَابٌ فِي الْحَيَاةِ الدُّنْيَا"
  },
  {
    "id": 102,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 850.0,
    "end": 852.73,
    "text": "وَلَعَذَابُ الْآخِرَةِ أَشَرُّ"
  },
  {
    "id": 103,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 853.1,
    "end": 853.2,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 104,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 854.03,
    "end": 859.62,
    "text": "وَمَا لَهُمْ مِنَ اللَّهِ مِنْ وَاقٍ"
  },
  {
    "id": 105,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 860.82,
    "end": 866.6,
    "text": "مَثَلُ الْجَنَّةِ الَّتِي وُعِدَ الْمُتَّقُونَ"
  },
  {
    "id": 106,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 867.5,
    "end": 871.46,
    "text": "تَجْرِي مِنْ تَحْتِهَا الْأَنْهَى"
  },
  {
    "id": 107,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 872.35,
    "end": 876.73,
    "text": "وَأُكُلُهَا دَائِمٌ وَظِلُّهَا"
  },
  {
    "id": 108,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 877.59,
    "end": 880.35,
    "text": "تِلْكَ عُقْبَى الَّذِينَ اتَّقَوْا"
  },
  {
    "id": 109,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 881.12,
    "end": 885.3,
    "text": "وَعُقْبَ الْكَافِرِينَ النَّارَ"
  },
  {
    "id": 110,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 886.72,
    "end": 895.42,
    "text": "وَالَّذِينَ آتَيْنَاهُمُ الْكِتَابَ يَفْرَحُونَ بِمَا أُنْزِلَ إِلَيْهِ"
  },
  {
    "id": 111,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 896.29,
    "end": 900.73,
    "text": "وَمِنَ الْأَحْزَابِ مَنْ يُنْكِرُ بَعْضَهُ"
  },
  {
    "id": 112,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 901.66,
    "end": 909.18,
    "text": "قُلْ إِنَّمَا أُمِرْتُ أَنْ أَعْبُدَ اللَّهَ وَلَا أُشْرِكَ بِهِ"
  },
  {
    "id": 113,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 910.1,
    "end": 914.09,
    "text": "إِلَيْهِ أَدْعُو وَإِلَيْهِ مَآبٌ"
  },
  {
    "id": 114,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 915.67,
    "end": 920.46,
    "text": "وَكَذَلِكَ أَنْزَلْنَاهُ حُكْمًا عَرَبِيًّا"
  },
  {
    "id": 115,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 921.55,
    "end": 935.71,
    "text": "وَلَئِنِ اتَّبَعْتَ أَهْوَاءَهُمْ بَعْدَ مَا جَاءَكَ مِنَ الْعِلْمِ مَا لَكَ مِنَ اللَّهِ مِنْ وَلِيٍّ وَلَا وَاقٍ"
  },
  {
    "id": 116,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 937.06,
    "end": 946.07,
    "text": "وَلَقَدْ أَرْسَلْنَا رُسُلًا مِنْ قَبْلِكَ وَجَعَلْنَا لَهُمْ أَزْوَاجًا وَذُرِّيَّةً"
  },
  {
    "id": 117,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 946.93,
    "end": 954.71,
    "text": "وَمَا كَانَ لِرَسُولٍ أَنْ يَأْتِيَ بِآيَةٍ إِلَّا بِإِذْنِ اللَّهِ"
  },
  {
    "id": 118,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 955.63,
    "end": 959.51,
    "text": "لِكُلِّ أَجَلٍ كِتَابٌ"
  },
  {
    "id": 119,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 960.32,
    "end": 964.12,
    "text": "يَمْحُو اللَّهُ مَا يَشَاءُ"
  },
  {
    "id": 120,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 964.45,
    "end": 964.51,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 121,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 965.37,
    "end": 970.91,
    "text": "وَعِنْدَهُ أُمُّ الْكِتَابِ"
  },
  {
    "id": 122,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 972.37,
    "end": 986.07,
    "text": "وَإِنْ مَا نُرِيَنَّكَ بَعْضَ الَّذِي نَعِدُهُمْ أَوْ نَتَوَفَّيَنَّكَ فَإِنَّمَا عَلَيْكَ الْبَلَاغُ وَعَلَيْنَا الْحِسَابُ"
  },
  {
    "id": 123,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 987.42,
    "end": 994.59,
    "text": "أَوَلَمْ يَرَوْا أَنَّا نَأْتِي الْأَرْضَ نَنْقُصُهَا مِنْ أَطْرَافِهَا"
  },
  {
    "id": 124,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 995.48,
    "end": 999.36,
    "text": "وَاللَّهُ يَحْكُمُ لَا مُعَقِّبَ لِحُكْمِهِ"
  },
  {
    "id": 125,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 1000.18,
    "end": 1003.66,
    "text": "وَهُوَ سَرِيعُ الْحِسَابِ"
  },
  {
    "id": 126,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 1004.73,
    "end": 1011.57,
    "text": "وَقَدْ مَكَرَ الَّذِينَ مِنْ قَبْلِهِمْ فَلِلَّهِ الْمَكْرُ جَمِيعًا"
  },
  {
    "id": 127,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 1012.58,
    "end": 1015.37,
    "text": "يَعْلَمُ مَا تَكْسِبُ كُلُّونَ"
  },
  {
    "id": 128,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 1016.49,
    "end": 1017.94,
    "text": "وَسَيَعْلَمُ الْكُبُرُ"
  },
  {
    "id": 129,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 1018.26,
    "end": 1020.0,
    "text": "وَبَارُ لِمَنْ عُقْبَى"
  },
  {
    "id": 130,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 1020.31,
    "end": 1021.56,
    "text": "يَا"
  },
  {
    "id": 131,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 1022.71,
    "end": 1026.86,
    "text": "وَيَقُولُ الَّذِينَ كَفَرُوا لَسْتَ مُرْسَلًا"
  },
  {
    "id": 132,
    "sura_id": 13,
    "recitation_uuid": "33d8cb04-760e-4f81-bf42-3e978a7b85d6",
    "start": 1027.81,
    "end": 1038.35,
    "text": "قُلْ كَفَى بِاللَّهِ شَهِيدًا بَيْنِي وَبَيْنَكُمْ وَمَنْ عِنْدَهُ عِلْمُ الْكِتَابِ"
  }
]