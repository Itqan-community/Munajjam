[
  {
    "id": 2,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 6.01,
    "end": 12.12,
    "text": "الم"
  },
  {
    "id": 3,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 13.05,
    "end": 17.17,
    "text": "تِلْكَ آيَاتُ الْكِتَابِ الْحَكِيمِ"
  },
  {
    "id": 4,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 18.3,
    "end": 22.61,
    "text": "هُدًى وَرَحْمَةً لِلْمُخْسِنِينَ"
  },
  {
    "id": 5,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 23.98,
    "end": 34.22,
    "text": "الَّذِينَ يُقِيمُونَ الصَّلَاةَ وَيُؤْتُونَ الزَّكَاةَ وَهُمْ بِالْآخِرَةِ هُمْ يُوقِنُونَ"
  },
  {
    "id": 6,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 34.93,
    "end": 39.66,
    "text": "أُولَئِكَ عَلَى هُدًى مِنْ رَبِّهِمْ"
  },
  {
    "id": 7,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 40.52,
    "end": 45.66,
    "text": "وَأُولَئِكَ هُمُ الْمُفْلِحُونَ"
  },
  {
    "id": 8,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 46.88,
    "end": 59.15,
    "text": "وَمِنَ النَّاسِ مَنْ يَشْتَرِي لَهْوَ الْحَدِيثِ لِيُضِلَّ عَنْ سَبِيلِ اللَّهِ بِغَيْرِ عِلْمٍ وَيَتَّخِذَهَا هُزُوًا"
  },
  {
    "id": 9,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 60.12,
    "end": 65.39,
    "text": "أُولَئِكَ لَهُمْ عَذَابٌ مُهِينٌ"
  },
  {
    "id": 10,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 66.63,
    "end": 79.13,
    "text": "وَإِذَا تُتْلَى عَلَيْهِ آيَاتُنَا وَلَّى مُسْتَكْبِرًا كَأَنْ لَمْ يَسْمَعْهَا كَأَنَّ فِي أُذُنَيْهِ وَقْرًا"
  },
  {
    "id": 11,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 79.97,
    "end": 83.84,
    "text": "فَبَشِّرْهُ بِعَذَابٍ أَلِيمٍ"
  },
  {
    "id": 12,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 85.03,
    "end": 94.29,
    "text": "إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ جَنَّاتُ النَّعِيمِ"
  },
  {
    "id": 13,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 95.1,
    "end": 97.16,
    "text": "خَالِدِينَ فِيهَا"
  },
  {
    "id": 14,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 97.89,
    "end": 99.45,
    "text": "وَعْدَ اللَّهِ"
  },
  {
    "id": 15,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 99.8,
    "end": 100.24,
    "text": "وَالْأَوْرِ"
  },
  {
    "id": 16,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 100.97,
    "end": 104.37,
    "text": "وَهُوَ الْعَزِيزُ الْحَكِيمُ"
  },
  {
    "id": 17,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 105.49,
    "end": 110.69,
    "text": "وَلَقَدْ سَمَاوَاتِ بِغَيْرِ عَمَدٍ تَرَوْنَهَا"
  },
  {
    "id": 18,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 111.91,
    "end": 122.88,
    "text": "وَأَلْقَى فِي الْأَرْضِ رَوَاسِيَ أَنْ تَمِيدَ بِكُمْ وَبَثَّ فِيهَا مِنْ كُلِّ دَابَّةٍ"
  },
  {
    "id": 19,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 124.04,
    "end": 137.43,
    "text": "وَأَنْزَلْنَا مِنَ السَّمَاءِ مَاءً فَأَنْبَتْنَا فِيهَا مِنْ كُلِّ زَوْجٍ كَرِيمٍ"
  },
  {
    "id": 20,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 138.5,
    "end": 145.94,
    "text": "هَذَا خَلْقُ اللَّهِ فَأَرُونِي مَاذَا خَلَقَ الَّذِينَ مِنْ دُونِهِ"
  },
  {
    "id": 21,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 146.77,
    "end": 152.05,
    "text": "بَلِ الظَّالِمُونَ فِي ضَلَالٍ مُبِينٍ"
  },
  {
    "id": 22,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 153.29,
    "end": 159.5,
    "text": "وَلَقَدْ آتَيْنَا نُقْمَانَ الْحِكْمَةَ أَنِ اشْكُرْ لِلَّهِ"
  },
  {
    "id": 23,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 160.38,
    "end": 165.22,
    "text": "وَمَنْ يَشْكُرْ فَإِنَّمَا يَشْكُرُ لِنَفْسِهِ"
  },
  {
    "id": 24,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 166.28,
    "end": 172.54,
    "text": "وَمَنْ كَفَرَ فَإِنَّ اللَّهَ غَنِيٌّ حَمِيدٌ"
  },
  {
    "id": 25,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 174.02,
    "end": 182.48,
    "text": "وَإِذْ قَالَ لُقْمَانُ لِابْنِهِ وَهُوَ يَعِظُهُ يَا بُنَيَّ لَا تُشْرِكْ بِاللَّهِ"
  },
  {
    "id": 26,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 183.36,
    "end": 187.46,
    "text": "إِنَّ الشِّرْكَ لَظُلْمٌ عَظِيمٌ"
  },
  {
    "id": 27,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 188.64,
    "end": 200.79,
    "text": "وَوَصَّيْنَا الْإِنْسَانَ بِوَالِدَيْهِ حَمَلَتْهُ أُمُّهُ وَهْنًا عَلَى وَهْنٍ وَفِصَالُهُ فِي عَامَيْنِ"
  },
  {
    "id": 28,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 202.07,
    "end": 215.15,
    "text": "عَمَلَتْهُ أُمُّهُ وَهْنًا عَلَى وَهْنٍ وَفِصَالُهُ فِي عَامَيْنِ أَنِ اشْكُرْ لِي وَلِوَالِدَيْكَ إِلَيَّ الْمَصِيرُ"
  },
  {
    "id": 29,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 216.11,
    "end": 226.44,
    "text": "وَإِنْ جَاهَدَاكَ عَلَى أَنْ تُشْرِكَ بِي مَا لَيْسَ لَكَ بِهِ عِلْمٌ فَلَا تُطِعْهُمَا"
  },
  {
    "id": 30,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 227.27,
    "end": 231.26,
    "text": "وَصَاحِبْهُمَا فِي الدُّنْيَا مَعْرُوفًا"
  },
  {
    "id": 31,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 232.38,
    "end": 235.73,
    "text": "وَاتَّبِعْ سَبِيلَ مَنْ أَنَابَ إِلَيَّ"
  },
  {
    "id": 32,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 236.86,
    "end": 246.19,
    "text": "ثُمَّ إِلَيَّ مَرْجِعُكُمْ فَأُنَبِّئُكُمْ بِمَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 33,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 247.81,
    "end": 264.91,
    "text": "يَا بُنَيَّ إِنَّهَا إِنْ تَكُ مِثْقَالَ حَبَّةٍ مِنْ خَرْدَلٍ فَتَكُنْ فِي صَخْرَةٍ أَوْ فِي السَّمَاوَاتِ أَوْ فِي الْأَرْضِ يَأْتِ بِهَا اللَّهُ"
  },
  {
    "id": 34,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 266.18,
    "end": 270.68,
    "text": "إِنَّ اللَّهَ لَطِيفٌ خَبِيرٌ"
  },
  {
    "id": 35,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 272.0,
    "end": 282.89,
    "text": "يَا بُنَيَّ أَقِمِ الصَّلَاةَ وَأْمُرْ بِالْمَعْرُوفِ وَانْهَ عَنِ الْمُنْكَرِ وَاصْبِرْ عَلَى مَا أَصَابَ"
  },
  {
    "id": 36,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 283.73,
    "end": 288.55,
    "text": "إِنَّ ذَلِكَ مِنْ عَزْمِ الْأُمُورِ"
  },
  {
    "id": 37,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 289.46,
    "end": 296.22,
    "text": "وَلَا تُصَعِّرْ خَدَّكَ لِلنَّاسِ وَلَا تَمْشِ فِي الْأَرْضِ مَرَحًا"
  },
  {
    "id": 38,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 297.1,
    "end": 303.85,
    "text": "إِنَّ اللَّهَ لَا يُحِبُّ كُلَّ مُخْتَالٍ فَخُورٍ"
  },
  {
    "id": 39,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 304.96,
    "end": 309.37,
    "text": "وَاقْصِدْ فِي مَشْيِكَ وَاغْضُضْ مِنْ صَوْتِ"
  },
  {
    "id": 40,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 310.35,
    "end": 316.68,
    "text": "إِنَّ أَنْكَرَ الْأَصْوَاتِ لَصَوْتُ الْحَمِيرِ"
  },
  {
    "id": 41,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 318.45,
    "end": 331.9,
    "text": "أَلَمْ تَرَوْا أَنَّ اللَّهَ سَخَّرَ لَكُمْ مَا فِي السَّمَاوَاتِ وَمَا فِي الْأَرْضِ وَأَسْبَغَ عَلَيْكُمْ نِعَمَهُ ظَاهِرَةً وَبَاطِنَةً"
  },
  {
    "id": 42,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 333.24,
    "end": 344.67,
    "text": "وَمِنَ النَّاسِ مَنْ يُجَادِلُ فِي اللَّهِ بِغَيْرِ عِلْمٍ وَلَا هُدًى وَلَا كِتَابٍ مُنِيرٍ"
  },
  {
    "id": 43,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 345.9,
    "end": 358.05,
    "text": "وَإِذَا قِيلَ لَهُمُ اتَّبِعُوا مَا أَنْزَلَ اللَّهُ قَالُوا بَلْ نَتَّبِعُ مَا وَجَدْنَا عَلَيْهِ آبَاءَنَا"
  },
  {
    "id": 44,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 359.11,
    "end": 365.83,
    "text": "أَوَلَوْ كَانَ الشَّيْطَانُ يَدْعُوهُمْ إِلَى عَذَابِ السَّعِيرِ"
  },
  {
    "id": 45,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 366.8,
    "end": 376.25,
    "text": "وَمَنْ يُسْلِمْ وَجْهَهُ إِلَى اللَّهِ وَهُوَ مُحْسِنٌ فَقَدِ اسْتَمْسَكَ بِالْعُرْوَةِ الْخُسُورِ"
  },
  {
    "id": 46,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 376.57,
    "end": 377.0,
    "text": "وَالْقَارِ"
  },
  {
    "id": 47,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 377.93,
    "end": 381.94,
    "text": "وَإِلَى اللَّهِ عَاقِبَةُ الْأُمُورِ"
  },
  {
    "id": 48,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 382.92,
    "end": 387.18,
    "text": "وَمَنْ كَفَرَ فَلَا يَحْزُنْكَ كُفْرُهُ"
  },
  {
    "id": 49,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 387.94,
    "end": 393.56,
    "text": "إِلَيْنَا مَرْجِعُهُمْ فَنُنَبِّئُهُمْ بِمَا عَمِلُوا"
  },
  {
    "id": 50,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 394.64,
    "end": 400.24,
    "text": "إِنَّ اللَّهَ عَلِيمٌ بِذَاتِ الصُّدُورِ"
  },
  {
    "id": 51,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 401.31,
    "end": 409.71,
    "text": "نُمَتِّعُهُمْ قَلِيلًا ثُمَّ نَضْطَرُّهُمْ إِلَى عَذَابٍ غَلِيظٍ"
  },
  {
    "id": 52,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 410.89,
    "end": 419.72,
    "text": "وَلَئِنْ سَأَلْتَهُمْ مَنْ خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ لَيَقُولُنَّ اللَّهُ"
  },
  {
    "id": 53,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 420.58,
    "end": 422.91,
    "text": "قُلِ الْحَمْدُ لِلَّهِ"
  },
  {
    "id": 54,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 423.67,
    "end": 427.36,
    "text": "بَلْ أَكْثَرُهُمْ لَا يَعْلَمُونَ"
  },
  {
    "id": 55,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 428.61,
    "end": 432.2,
    "text": "اللَّهِ مَا فِي السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 56,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 433.07,
    "end": 437.87,
    "text": "إِنَّ اللَّهَ هُوَ الْغَنِيُّ الْحَمِيدُ"
  },
  {
    "id": 57,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 439.18,
    "end": 455.07,
    "text": "وَلَوْ أَنَّمَا فِي الْأَرْضِ مِنْ شَجَرَةٍ أَقْلَامٌ وَالْبَحْرُ يَمُدُّهُ مِنْ بَعْدِهِ سَبْعَةُ أَبْحُرٍ مَا نَفِدَتْ كَلِمَاتُ اللَّهِ"
  },
  {
    "id": 58,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 456.05,
    "end": 460.44,
    "text": "إِنَّ اللَّهَ عَزِيزٌ حَكِيمٌ"
  },
  {
    "id": 59,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 461.44,
    "end": 467.49,
    "text": "مَا خَلْقُكُمْ وَلَا بَعْثُكُمْ إِلَّا كَنَفْسٍ وَاحِدَةٍ"
  },
  {
    "id": 60,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 468.55,
    "end": 473.6,
    "text": "إِنَّ اللَّهَ سَمِيعٌ بَصِيرٌ"
  },
  {
    "id": 61,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 474.71,
    "end": 486.07,
    "text": "أَلَمْ تَرَ أَنَّ اللَّهَ يُولِجُ اللَّيْلَ فِي النَّهَارِ وَيُولِجُ النَّهَارَ فِي اللَّيْلِ وَسَخَّرَ الشَّمْسَ وَالْقَمَرَ"
  },
  {
    "id": 62,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 487.44,
    "end": 500.71,
    "text": "كُلٌّ يَجْرِي إِلَى أَجَلٍ مُسَمًّى وَأَنَّ اللَّهَ بِمَا تَعْمَلُونَ خَبِيرٌ"
  },
  {
    "id": 63,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 502.18,
    "end": 515.9,
    "text": "ذَلِكَ بِأَنَّ اللَّهَ هُوَ الْحَقُّ وَأَنَّ مَا يَدْعُونَ مِنْ دُونِهِ الْبَاطِلُ وَأَنَّ اللَّهَ هُوَ الْعَلِيُّ الْكَبِيرُ"
  },
  {
    "id": 64,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 517.25,
    "end": 526.02,
    "text": "أَلَمْ تَرَ أَنَّ الْفُلْكَ تَجْرِي فِي الْبَحْرِ بِنِعْمَتِ اللَّهِ لِيُرِيَكُمْ مِنْ آيَاتِهِ"
  },
  {
    "id": 65,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 526.97,
    "end": 534.43,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِكُلِّ صَبَّارٍ شَكُورٍ"
  },
  {
    "id": 66,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 535.47,
    "end": 544.21,
    "text": "وَإِذَا غَشِيَهُمْ مَوْجٌ كَالظُّلَلِ دَعَوُا اللَّهَ مُخْلِصِينَ لَهُ الدِّينَ"
  },
  {
    "id": 67,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 545.22,
    "end": 555.21,
    "text": "دَعَوُا اللَّهَ مُخْلِصِينَ لَهُ الدِّينَ فَلَمَّا نَجَّاهُمْ إِلَى الْبَرِّ فَمِنْهُمْ مُقْتَصِدٌ"
  },
  {
    "id": 68,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 556.1,
    "end": 564.55,
    "text": "وَمَا يَجْحَدُ بِآيَاتِنَا إِلَّا كُلُّ خَتَّارٍ كَفُورٍ"
  },
  {
    "id": 69,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 565.95,
    "end": 582.06,
    "text": "يَا أَيُّهَا النَّاسُ اتَّقُوا رَبَّكُمْ وَاخْشَوْا يَوْمًا لَا يَجْزِي وَالِدٌ عَنْ وَلَدِهِ وَلَا مَوْلُودٌ هُوَ جَازٍ عَنْ وَالِدِهِ شَيْئًا"
  },
  {
    "id": 70,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 583.25,
    "end": 585.83,
    "text": "إِنَّ وَعْدَ اللَّهِ"
  },
  {
    "id": 71,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 586.26,
    "end": 586.36,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 72,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 587.14,
    "end": 597.23,
    "text": "فَلَا تَغُرَّنَّكُمُ الْحَيَاةُ الدُّنْيَا وَلَا يَغُرَّنَّكُمْ بِاللَّهِ الْغَرُورُ"
  },
  {
    "id": 73,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 598.47,
    "end": 608.44,
    "text": "إِنَّ اللَّهَ عِنْدَهُ عِلْمُ السَّاعَةِ وَيُنَزِّلُ الْغَيْثَ وَيَعْلَمُ مَا فِي الْأَرْحَامِ"
  },
  {
    "id": 74,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 609.46,
    "end": 614.26,
    "text": "وَمَا تَدْرِي نَفْسٌ مَاذَا تَكْسِبُ غَدًا"
  },
  {
    "id": 75,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 615.22,
    "end": 621.55,
    "text": "وَمَا تَدْرِي نَفْسٌ بِأَيِّ أَرْضٍ تَمُورُ"
  },
  {
    "id": 76,
    "sura_id": 31,
    "recitation_uuid": "cc81341f-8632-4270-9419-62f5ca130e67",
    "start": 622.72,
    "end": 627.49,
    "text": "إِنَّ اللَّهَ عَلِيمٌ خَبِيرٌ"
  }
]