[
  {
    "id": 2,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 5.8,
    "end": 8.27,
    "text": "وَالْمُرْسَلَاتِ عُرْفًا"
  },
  {
    "id": 3,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 9.14,
    "end": 11.57,
    "text": "فَالْعَاصِفَاتِ عَصْفًا"
  },
  {
    "id": 4,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 12.55,
    "end": 15.38,
    "text": "وَالنَّاشِرَاتِ نَشْرًا"
  },
  {
    "id": 5,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 16.36,
    "end": 18.88,
    "text": "فَالْفَارِقَاتِ فَرْقًا"
  },
  {
    "id": 6,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 19.76,
    "end": 22.22,
    "text": "فَالْمُلْقِيَاتِ ذِكْرًا"
  },
  {
    "id": 7,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 23.02,
    "end": 25.12,
    "text": "عُذْرًا أَوْ نُذْرًا"
  },
  {
    "id": 8,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 26.04,
    "end": 29.46,
    "text": "إِنَّمَا تُوعَدُونَ لَوَاقِعٌ"
  },
  {
    "id": 9,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 30.55,
    "end": 33.05,
    "text": "فَإِذَا النُّجُومُ طُمِسَتْ"
  },
  {
    "id": 10,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 33.98,
    "end": 37.33,
    "text": "وَإِذَا السَّمَاءُ فُرِجَتْ"
  },
  {
    "id": 11,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 38.19,
    "end": 40.49,
    "text": "وَإِذَا الْجِبَالُ نُسِفَتْ"
  },
  {
    "id": 12,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 41.4,
    "end": 43.63,
    "text": "وَإِذَا الرُّسُلُ أُقِّتَتْ"
  },
  {
    "id": 13,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 44.66,
    "end": 47.12,
    "text": "لِأَيِّ يَوْمٍ أُجِّلَتْ"
  },
  {
    "id": 14,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 48.03,
    "end": 49.34,
    "text": "لِيَوْمِ الْفَصْلِ"
  },
  {
    "id": 15,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 50.27,
    "end": 54.62,
    "text": "وَمَا أَدْرَاكَ مَا يَوْمُ الْفَصْلِ"
  },
  {
    "id": 16,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 55.35,
    "end": 60.51,
    "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 17,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 61.48,
    "end": 64.71,
    "text": "أَلَمْ نُهْلِكِ الْأَوَّلِينَ"
  },
  {
    "id": 18,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 65.45,
    "end": 69.54,
    "text": "ثُمَّ نُتْبِعُهُمُ الْآخِرِينَ"
  },
  {
    "id": 19,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 70.27,
    "end": 74.18,
    "text": "كَذَلِكَ نَفْعَلُ بِالْمُجْرِمِينَ"
  },
  {
    "id": 20,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 74.85,
    "end": 79.94,
    "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 21,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 80.84,
    "end": 86.98,
    "text": "أَلَمْ نَخْلُقْكُمْ مِنْ مَاءٍ مَهِينٍ"
  },
  {
    "id": 22,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 87.84,
    "end": 92.37,
    "text": "فَجَعَلْنَاهُ فِي قَرَارٍ مَكِينٍ"
  },
  {
    "id": 23,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 93.11,
    "end": 96.52,
    "text": "إِلَى قَدَرٍ مَعْلُومٍ"
  },
  {
    "id": 24,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 97.2,
    "end": 101.21,
    "text": "فَقَدَرْنَا فَنِعْمَ الْقَادِرُونَ"
  },
  {
    "id": 25,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 101.81,
    "end": 106.93,
    "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 26,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 107.77,
    "end": 111.04,
    "text": "أَلَمْ نَجْعَلِ الْأَرْضَ كِفَاتًا"
  },
  {
    "id": 27,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 111.86,
    "end": 115.88,
    "text": "أَحْيَاءً وَأَمْوَاتًا"
  },
  {
    "id": 28,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 116.78,
    "end": 127.54,
    "text": "وَجَعَلْنَا فِيهَا رَوَاسِيَ شَامِخَاتٍ وَأَسْقَيْنَاكُمْ مَاءً فُرَاتًا"
  },
  {
    "id": 29,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 128.52,
    "end": 133.32,
    "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 30,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 134.23,
    "end": 142.16,
    "text": "انْطَلِقُوا إِلَى مَا كُنْتُمْ بِهِ تُكَذِّبُونَ"
  },
  {
    "id": 31,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 142.93,
    "end": 148.93,
    "text": "انْطَلِقُوا إِلَى ظِلٍّ ذِي ثَلَاثِ شُعَبٍ"
  },
  {
    "id": 32,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 149.67,
    "end": 154.0,
    "text": "لَا ظَلِيلٍ وَلَا يُغْنِي مِنَ اللَّهَبِ"
  },
  {
    "id": 33,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 154.77,
    "end": 159.35,
    "text": "إِنَّهَا تَرْمِي بِشَرَرٍ كَالْقَصْرِ"
  },
  {
    "id": 34,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 160.22,
    "end": 164.0,
    "text": "كَأَنَّهُ جِمَالَتٌ صُفْرٌ"
  },
  {
    "id": 35,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 164.84,
    "end": 169.81,
    "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 36,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 170.77,
    "end": 175.48,
    "text": "هَذَا يَوْمُ لَا يَنْطِقُونَ"
  },
  {
    "id": 37,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 176.2,
    "end": 180.35,
    "text": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ"
  },
  {
    "id": 38,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 181.02,
    "end": 186.37,
    "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 39,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 187.21,
    "end": 189.27,
    "text": "هَذَا يَوْمُ الْفَصْلِ"
  },
  {
    "id": 40,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 190.1,
    "end": 193.68,
    "text": "جَمَعْنَاكُمْ وَالْأَوَّلِينَ"
  },
  {
    "id": 41,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 194.38,
    "end": 199.91,
    "text": "فَإِنْ كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ"
  },
  {
    "id": 42,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 200.47,
    "end": 205.36,
    "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 43,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 206.17,
    "end": 211.84,
    "text": "إِنَّ الْمُتَّقِينَ فِي ظِلَالٍ وَعُيُونٍ"
  },
  {
    "id": 44,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 212.5,
    "end": 216.98,
    "text": "وَفَوَاكِهَ مِمَّا يَشْتَهُونَ"
  },
  {
    "id": 45,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 217.72,
    "end": 225.71,
    "text": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 46,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 226.4,
    "end": 229.44,
    "text": "إِنَّا كَذَلِكَ نَجْزِي الْوَتَادِ"
  },
  {
    "id": 47,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 229.76,
    "end": 231.39,
    "text": "الم"
  },
  {
    "id": 48,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 232.24,
    "end": 237.15,
    "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 49,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 237.93,
    "end": 244.72,
    "text": "كُلُوا وَتَمَتَّعُوا قَلِيلًا إِنَّكُمْ مُجْرِمُونَ"
  },
  {
    "id": 50,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 245.35,
    "end": 250.39,
    "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 51,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 251.01,
    "end": 256.08,
    "text": "وَإِذَا قِيلَ لَهُمُ ارْكَعُوا لَا يَرْكَعُونَ"
  },
  {
    "id": 52,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 256.76,
    "end": 261.82,
    "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 53,
    "sura_id": 77,
    "recitation_uuid": "d2a31147-deb8-44dc-b905-c625527f575b",
    "start": 262.5,
    "end": 268.1,
    "text": "فَبِأَيِّ حَدِيثٍ بَعْدَهُ يُؤْمِنُونَ"
  }
]