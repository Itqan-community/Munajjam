[
  {
    "id": 2,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 5.75,
    "end": 8.44,
    "text": "حم"
  },
  {
    "id": 3,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 9.36,
    "end": 12.4,
    "text": "وَالْكِتَابِ الْمُبِينِ"
  },
  {
    "id": 4,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 13.39,
    "end": 19.88,
    "text": "إِنَّا أَنْزَلْنَاهُ فِي لَيْلَةٍ مُبَارَكَةٍ"
  },
  {
    "id": 5,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 20.85,
    "end": 25.72,
    "text": "إِنَّا كُنَّا مُنْذِرِينَ"
  },
  {
    "id": 6,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 26.7,
    "end": 31.25,
    "text": "فِيهَا يُفْرَقُ كُلُّ أَمْرٍ حَكِيمٍ"
  },
  {
    "id": 7,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 32.07,
    "end": 35.21,
    "text": "ثَمْرًا مِنْ عِنْدِنَا"
  },
  {
    "id": 8,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 36.11,
    "end": 40.91,
    "text": "إِنَّا كُنَّا مُرْسِلِينَ"
  },
  {
    "id": 9,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 41.8,
    "end": 44.48,
    "text": "رَحْمَةً مِنْ رَبِّي"
  },
  {
    "id": 10,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 45.38,
    "end": 49.82,
    "text": "إِنَّهُ هُوَ السَّمِيعُ الْعَلِيمُ"
  },
  {
    "id": 11,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 50.96,
    "end": 55.82,
    "text": "رَبِّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا"
  },
  {
    "id": 12,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 56.69,
    "end": 61.7,
    "text": "إِنْ كُنْتُمْ مُوقِنِينَ"
  },
  {
    "id": 13,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 62.5,
    "end": 68.34,
    "text": "يَا إِلَهَ إِلَّا هُوَ يُؤْيِي وَيُمِينِ"
  },
  {
    "id": 14,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 69.31,
    "end": 75.96,
    "text": "رَبُّكُمْ وَرَبُّ آبَائِكُمُ الْأَوَّلِينَ"
  },
  {
    "id": 15,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 76.8,
    "end": 78.11,
    "text": "بَلْ هُمْ فِي شَكٍّ"
  },
  {
    "id": 16,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 78.44,
    "end": 81.06,
    "text": "يَلْعَبُونَ"
  },
  {
    "id": 17,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 81.94,
    "end": 89.71,
    "text": "يَرْتَقِبْ يَوْمَ تَأْتِي السَّمَاءُ بِدُخَانٍ مُبِينٍ"
  },
  {
    "id": 18,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 90.44,
    "end": 93.15,
    "text": "يَغْشَى النَّاسَ"
  },
  {
    "id": 19,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 93.93,
    "end": 97.26,
    "text": "هَذَا عَذَابٌ أَلِيمٌ"
  },
  {
    "id": 20,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 98.11,
    "end": 104.2,
    "text": "رَبَّنَا اكْشِفْ عَنَّا الْعَذَابَ إِنَّا مُؤْمِنُونَ"
  },
  {
    "id": 21,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 105.06,
    "end": 113.46,
    "text": "كَنَّا لَهُمُ الذِّكْرَى وَقَدْ جَاءَهُمْ رَسُولٌ مُبِينٌ"
  },
  {
    "id": 22,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 114.38,
    "end": 120.98,
    "text": "ثُمَّ تَوَلَّوْا عَنْهُ وَقَالُوا مُعَلَّمٌ مَجْنُونٌ"
  },
  {
    "id": 23,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 122.07,
    "end": 126.14,
    "text": "إِنَّا كَاشِفُو الْعَذَابِ قَلِيلًا"
  },
  {
    "id": 24,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 127.02,
    "end": 131.43,
    "text": "إِنَّكُمْ عَائِدُونَ"
  },
  {
    "id": 25,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 132.37,
    "end": 140.45,
    "text": "يَوْمَ نَبْطِشُ الْبَطْشَةَ الْكُبْرَى إِنَّا مُنْتَقِمُونَ"
  },
  {
    "id": 26,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 141.58,
    "end": 151.07,
    "text": "وَلَقَدْ فَتَنَّا قَبْلَهُمْ قَوْمَ فِرْعَوْنَ وَجَاءَهُمْ رَسُولٌ كَرِيمٌ"
  },
  {
    "id": 27,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 151.99,
    "end": 157.03,
    "text": "أَنْ أَدُّوا إِلَيَّ عِبَادَ اللَّهِ"
  },
  {
    "id": 28,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 157.79,
    "end": 161.82,
    "text": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
  },
  {
    "id": 29,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 162.97,
    "end": 166.46,
    "text": "وَأَنْ لَا تَعْلُوا عَلَى اللَّهِ"
  },
  {
    "id": 30,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 167.19,
    "end": 173.72,
    "text": "إِنِّي آتِيكُمْ بِسُلْطَانٍ مُبِينٍ"
  },
  {
    "id": 31,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 174.72,
    "end": 181.38,
    "text": "وَإِنِّي عُذْتُ بِرَبِّي وَرَبِّكُمْ أَنْ تَرْجُمُونِ"
  },
  {
    "id": 32,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 181.96,
    "end": 186.21,
    "text": "وَإِنْ لَمْ تُؤْمِنُوا لِي فَاعْتَزِلُونِ"
  },
  {
    "id": 33,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 187.11,
    "end": 195.96,
    "text": "فَدَعَا رَبَّهُ أَنَّ هَؤُلَاءِ قَوْمٌ مُجْرِمُونَ"
  },
  {
    "id": 34,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 196.81,
    "end": 203.16,
    "text": "فَأَسْرِ بِعِبَادِي لَيْلًا إِنَّكُمْ مُتَّبَعُونَ"
  },
  {
    "id": 35,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 204.21,
    "end": 206.51,
    "text": "وَاتْرُكِ الْبَحْرَ وَهُوَانِ"
  },
  {
    "id": 36,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 207.32,
    "end": 212.43,
    "text": "إِنَّهُمْ جُنْدٌ مُغْرَقُونَ"
  },
  {
    "id": 37,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 213.44,
    "end": 218.92,
    "text": "أَيَمْ تَرَكُوا مِنْ جَنَّاتٍ وَعُيُونٍ"
  },
  {
    "id": 38,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 219.54,
    "end": 224.06,
    "text": "وَزُرُوعٍ وَمَقَامٍ كَرِيمٍ"
  },
  {
    "id": 39,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 224.87,
    "end": 229.78,
    "text": "وَنِعْمَةٍ كَانُوا فِيهَا فَاكِهِينَ"
  },
  {
    "id": 40,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 230.57,
    "end": 231.32,
    "text": "كَذَلِكَ"
  },
  {
    "id": 41,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 232.13,
    "end": 236.49,
    "text": "وَأَوْرَثْنَاهَا قَوْمًا آخَرِينَ"
  },
  {
    "id": 42,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 237.5,
    "end": 246.03,
    "text": "فَمَا بَكَتْ عَلَيْهِمُ السَّمَاءُ وَالْأَرْضُ وَمَا كَانُوا مُنْظَرِينَ"
  },
  {
    "id": 43,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 247.01,
    "end": 255.01,
    "text": "وَلَقَدْ نَجَّيْنَا بَنِي إِسْرَائِيلَ مِنَ الْعَذَابِ الْمُهِينِ"
  },
  {
    "id": 44,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 255.81,
    "end": 258.2,
    "text": "مِنْ فِرْعَوْنِ"
  },
  {
    "id": 45,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 258.94,
    "end": 264.65,
    "text": "إِنَّهُ كَانَ عَالِيًا مِنَ الْمُسْرِفِينَ"
  },
  {
    "id": 46,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 265.78,
    "end": 271.45,
    "text": "وَلَقَدِ اخْتَرْنَاهُمْ عَلَى عِلْمٍ عَلَى الْعَالَمِينَ"
  },
  {
    "id": 47,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 272.33,
    "end": 281.18,
    "text": "وَآتَيْنَاهُمْ مِنَ الْآيَاتِ مَا فِيهِ بَلَاءٌ مُبِينٌ"
  },
  {
    "id": 48,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 282.18,
    "end": 288.09,
    "text": "إِنَّ هَؤُلَاءِ لَيَقُولُونَ"
  },
  {
    "id": 49,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 289.05,
    "end": 296.45,
    "text": "إِنْ هِيَ إِلَّا مَوْتَتُنَا الْأُولَى وَمَا نَحْنُ بِمُنْشَرِينَ"
  },
  {
    "id": 50,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 297.3,
    "end": 305.77,
    "text": "فَأْتُوا بِآبَائِنَا إِنْ كُنْتُمْ صَادِقِينَ"
  },
  {
    "id": 51,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 306.78,
    "end": 314.98,
    "text": "لَهُمْ خَيْرٌ أَمْ قَوْمُ تُبَّعٍ وَالَّذِينَ مِنْ قَبْلِهِمْ أَهْلَكْنَاهُمْ"
  },
  {
    "id": 52,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 315.94,
    "end": 319.97,
    "text": "إِنَّهُمْ كَانُوا مُجْرِمِينَ"
  },
  {
    "id": 53,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 321.09,
    "end": 328.29,
    "text": "وَمَا خَلَقْنَا السَّمَاوَاتِ وَالْأَرْضَ وَمَا بَيْنَهُمَا لَاعِبِينَ"
  },
  {
    "id": 54,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 329.4,
    "end": 338.56,
    "text": "وَمَا خَلَقْنَاهُمَا إِلَّا بِالْحَقِّ وَلَكِنَّ أَكْثَرَهُمْ لَا يَعْلَمُونَ"
  },
  {
    "id": 55,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 339.47,
    "end": 345.3,
    "text": "إِنَّ يَوْمَ الْفَصْلِ مِيقَاتُهُمْ أَجْمَعِينَ"
  },
  {
    "id": 56,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 346.39,
    "end": 355.64,
    "text": "يَوْمَ لَا يُغْنِي مَوْلًى عَنْ مَوْلًى شَيْئًا وَلَا هُمْ يُنْصَرُونَ"
  },
  {
    "id": 57,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 356.55,
    "end": 359.77,
    "text": "إِلَّا مَنْ رَحِمَ اللَّهُ"
  },
  {
    "id": 58,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 360.46,
    "end": 364.93,
    "text": "إِنَّهُ هُوَ الْعَزِيزُ الرَّحِيمُ"
  },
  {
    "id": 59,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 365.78,
    "end": 367.89,
    "text": "إِنَّ شَجَرَتَ الزَّقَّرِ"
  },
  {
    "id": 60,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 368.23,
    "end": 369.57,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 61,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 370.21,
    "end": 372.65,
    "text": "طَعَامُ الْأَثِيمِ"
  },
  {
    "id": 62,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 373.4,
    "end": 377.27,
    "text": "كَالْمُهْلِ يَغْلِي فِي الْبُطُونِ"
  },
  {
    "id": 63,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 378.06,
    "end": 380.79,
    "text": "كَغَلْيِ الْحَمِيمِ"
  },
  {
    "id": 64,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 381.82,
    "end": 387.97,
    "text": "خُذُوهُ فَاعْتِلُوهُ إِلَى سَوَاءِ الْجَحِيمِ"
  },
  {
    "id": 65,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 388.91,
    "end": 395.27,
    "text": "ثُمَّ صُبُّوا فَوْقَ رَأْسِهِ مِنْ عَذَابِ الْحَمِيمِ"
  },
  {
    "id": 66,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 396.25,
    "end": 402.04,
    "text": "ذُقْ إِنَّكَ أَنْتَ الْعَزِيزُ الْكَرِيمُ"
  },
  {
    "id": 67,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 402.92,
    "end": 409.93,
    "text": "إِنَّ هَذَا مَا كُنْتُمْ بِهِ تَمْتَرُونَ"
  },
  {
    "id": 68,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 411.26,
    "end": 416.61,
    "text": "إِنَّ الْمُتَّقِينَ فِي مَقَامٍ أَمِينٍ"
  },
  {
    "id": 69,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 417.44,
    "end": 421.7,
    "text": "فِي جَنَّاتٍ وَعُيُونٍ"
  },
  {
    "id": 70,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 422.5,
    "end": 431.14,
    "text": "يَلْبَسُونَ مِنْ سُنْدُسٍ وَإِسْتَبْرَقٍ مُتَقَابِلِينَ"
  },
  {
    "id": 71,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 432.23,
    "end": 437.84,
    "text": "كَذَلِكَ وَزَوَّجْنَاهُمْ بِحُورٍ عِينٍ"
  },
  {
    "id": 72,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 438.85,
    "end": 444.63,
    "text": "يَدْعُونَ فِيهَا بِكُلِّ فَاكِهَةٍ آمِنِينَ"
  },
  {
    "id": 73,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 445.46,
    "end": 451.05,
    "text": "لَا يَذُوقُونَ فِيهَا الْمَوْتَ إِلَّا الْمَوْتَةَ الْأُولَى"
  },
  {
    "id": 74,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 451.87,
    "end": 455.67,
    "text": "وَوَقَاهُمْ عَذَابَ الْجَحِيمِ"
  },
  {
    "id": 75,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 456.73,
    "end": 458.78,
    "text": "فَضْلًا مِنْ رَبِّي"
  },
  {
    "id": 76,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 459.9,
    "end": 463.58,
    "text": "ذَلِكَ هُوَ الْفَوْزُ الْعَظِيمُ"
  },
  {
    "id": 77,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 464.54,
    "end": 472.02,
    "text": "فَإِنَّمَا يَسَّرْنَاهُ بِلِسَانِكَ لَعَلَّهُمْ يَتَذَكَّرُونَ"
  },
  {
    "id": 78,
    "sura_id": 44,
    "recitation_uuid": "b456dedd-6373-4ad0-a556-aed5967b5166",
    "start": 472.85,
    "end": 478.5,
    "text": "فَارْتَقِبْ إِنَّهُمْ مُرْتَقِبُونَ"
  }
]