[
  {
    "id": 2,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 5.88,
    "end": 8.97,
    "text": "وَالسَّمَاءِ وَالطَّارِقِ"
  },
  {
    "id": 3,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 9.77,
    "end": 12.81,
    "text": "وَمَا أَدْرَاكَ"
  },
  {
    "id": 4,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 13.13,
    "end": 13.89,
    "text": "طه"
  },
  {
    "id": 5,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 14.66,
    "end": 16.16,
    "text": "النَّجْمُ"
  },
  {
    "id": 6,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 16.48,
    "end": 17.2,
    "text": "ذَاقِبَةَ"
  },
  {
    "id": 7,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 18.08,
    "end": 23.36,
    "text": "إِنْ كُلُّ نَفْسٍ لَمَّا عَلَيْهَا حَافِظٌ"
  },
  {
    "id": 8,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 24.47,
    "end": 29.0,
    "text": "فَلْيَنْظُرِ الْإِنْسَانُ مِمَّ خُلِقَ"
  },
  {
    "id": 9,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 29.81,
    "end": 35.1,
    "text": "خُلِقَ مِنْ مَاءٍ دَافِقٍ"
  },
  {
    "id": 10,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 36.0,
    "end": 41.48,
    "text": "يَخْرُجُ مِنْ بَيْنِ الصُّلْبِ وَالتَّرَائِبِ"
  },
  {
    "id": 11,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 42.4,
    "end": 46.38,
    "text": "إِنَّهُ عَلَى رَجْعِهِ لَقَادِرٌ"
  },
  {
    "id": 12,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 47.24,
    "end": 50.62,
    "text": "يَوْمَ تُبْلَى السَّرَاءِ"
  },
  {
    "id": 13,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 51.48,
    "end": 56.46,
    "text": "فَمَا لَهُ مِنْ قُوَّةٍ وَلَا نَاصِرٍ"
  },
  {
    "id": 14,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 57.62,
    "end": 61.02,
    "text": "وَالسَّمَاءِ ذَاتِ الرَّجْعِ"
  },
  {
    "id": 15,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 61.83,
    "end": 64.02,
    "text": "وَالْأَرْضِ ذَاتِ الصَّدْعِ"
  },
  {
    "id": 16,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 64.85,
    "end": 68.35,
    "text": "إِنَّهُ لَقَوْلٌ فَصْلٌ"
  },
  {
    "id": 17,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 69.11,
    "end": 70.95,
    "text": "وَمَا هُوَ بِالْهَزْلِ"
  },
  {
    "id": 18,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 71.72,
    "end": 75.31,
    "text": "إِنَّهُمْ يَكِيدُونَ كَيْدًا"
  },
  {
    "id": 19,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 76.08,
    "end": 78.14,
    "text": "وَأَكِيدُ كَيْدًا"
  },
  {
    "id": 20,
    "sura_id": 86,
    "recitation_uuid": "a1222af9-88b1-4cd3-98f6-55d07d5d78a3",
    "start": 78.98,
    "end": 83.9,
    "text": "فَمَهِّلِ الْكَافِرِينَ أَمْهِلْهُمْ رُوَيْدًا"
  }
]