[
  {
    "id": 2,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 6.32,
    "end": 9.18,
    "text": "مَا الْحَاقُ"
  },
  {
    "id": 3,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 9.59,
    "end": 9.86,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 4,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 10.84,
    "end": 14.07,
    "text": "مَا الْحَاقُ"
  },
  {
    "id": 5,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 14.53,
    "end": 14.7,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 6,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 16.0,
    "end": 21.75,
    "text": "وَمَا أَدْرَاكَ مَا الْحَاقُ"
  },
  {
    "id": 7,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 22.3,
    "end": 22.47,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 8,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 23.6,
    "end": 28.06,
    "text": "كَذَّبَتْ ثَمُودُ وَعَادٌ بِالْقَارِعَةِ"
  },
  {
    "id": 9,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 29.21,
    "end": 33.69,
    "text": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا بِالطَّاغِيَةِ"
  },
  {
    "id": 10,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 34.61,
    "end": 41.74,
    "text": "وَأَمَّا عَادٌ فَأُهْلِكُوا بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ"
  },
  {
    "id": 11,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 42.94,
    "end": 50.47,
    "text": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَانِيَةَ أَيَّامٍ حُسُومًا"
  },
  {
    "id": 12,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 51.61,
    "end": 58.98,
    "text": "فَتَرَى الْقَوْمَ فِيهَا صَرْعَى كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ"
  },
  {
    "id": 13,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 60.14,
    "end": 64.16,
    "text": "فَهَلْ تَرَى لَهُمْ مِنْ بَاقِيَةٍ"
  },
  {
    "id": 14,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 65.14,
    "end": 72.95,
    "text": "وَجَاءَ فِرْعَوْنُ وَمَنْ قَبْلَهُ وَالْمُؤْتَفِكَاتُ بِالْخَاطِئَةِ"
  },
  {
    "id": 15,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 74.11,
    "end": 79.71,
    "text": "فَعَصَوْا رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَابِيَةً"
  },
  {
    "id": 16,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 80.85,
    "end": 88.13,
    "text": "إِنَّا لَمَّا طَغَى الْمَاءُ حَمَلْنَاكُمْ فِي الْجَارِيَةِ"
  },
  {
    "id": 17,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 89.1,
    "end": 97.11,
    "text": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَا أُذُنٌ وَاعِيَةٌ"
  },
  {
    "id": 18,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 98.61,
    "end": 103.34,
    "text": "فَإِذَا نُفِخَ فِي الصُّورِ نَفْخَةٌ وَاحِدَةٌ"
  },
  {
    "id": 19,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 104.44,
    "end": 111.05,
    "text": "وَحُمِلَتِ الْأَرْضُ وَالْجِبَالُ فَدُكَّتَا دَكَّةً وَاحِدَةً"
  },
  {
    "id": 20,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 111.87,
    "end": 115.46,
    "text": "فَيَوْمَئِذٍ وَقَعَتِ الْوَاقِعَةُ"
  },
  {
    "id": 21,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 116.42,
    "end": 123.05,
    "text": "وَانْشَقَّتِ السَّمَاءُ فَهِيَ يَوْمَئِذٍ وَاهِيَةٌ"
  },
  {
    "id": 22,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 124.13,
    "end": 128.93,
    "text": "وَالْمَلَكُ عَلَى أَرْجَائِهَا"
  },
  {
    "id": 23,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 129.68,
    "end": 135.85,
    "text": "وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَانِيَةٌ"
  },
  {
    "id": 24,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 137.03,
    "end": 143.57,
    "text": "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَى مِنْكُمْ خَافِيَةٌ"
  },
  {
    "id": 25,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 144.68,
    "end": 154.43,
    "text": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ فَيَقُولُ هَاؤُمُ اقْرَءُوا كِتَابِيَهْ"
  },
  {
    "id": 26,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 155.43,
    "end": 161.09,
    "text": "إِنِّي ظَنَنْتُ أَنِّي مُلَاقٍ حِسَابِيَهْ"
  },
  {
    "id": 27,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 161.84,
    "end": 164.83,
    "text": "فَهُوَ فِي عِيشَةٍ رَاضِيَةٍ"
  },
  {
    "id": 28,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 165.68,
    "end": 168.4,
    "text": "فِي جَنَّةٍ عَالِيَةٍ"
  },
  {
    "id": 29,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 169.03,
    "end": 171.27,
    "text": "قُطُوفُهَا دَانِيَةٌ"
  },
  {
    "id": 30,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 172.03,
    "end": 180.77,
    "text": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا أَسْلَفْتُمْ فِي الْأَيَّامِ الْخَالِيَةِ"
  },
  {
    "id": 31,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 181.84,
    "end": 191.35,
    "text": "وَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِشِمَالِهِ فَيَقُولُ يَا لَيْتَنِي لَمْ أُوتَ كِتَابِيَهْ"
  },
  {
    "id": 32,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 192.23,
    "end": 194.99,
    "text": "وَلَمْ أَدْرِ مَا حِسَابِيَهْ"
  },
  {
    "id": 33,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 195.84,
    "end": 199.41,
    "text": "يَا لَيْتَهَا كَانَتِ الْقَاضِيَةَ"
  },
  {
    "id": 34,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 200.18,
    "end": 204.43,
    "text": "وَمَا أَغْنَى عَنِّي مَالِيَهْ"
  },
  {
    "id": 35,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 205.61,
    "end": 208.96,
    "text": "هَلَكَ عَنِّي سُلْطَانِيَهْ"
  },
  {
    "id": 36,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 209.94,
    "end": 212.54,
    "text": "خُذُوهُ فَغُلُّوهُ"
  },
  {
    "id": 37,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 213.16,
    "end": 216.74,
    "text": "ثُمَّ الْجَحِيمَ صَلُّوهُ"
  },
  {
    "id": 38,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 217.54,
    "end": 225.48,
    "text": "ثُمَّ فِي سِلْسِلَةٍ ذَرْعُهَا سَبْعُونَ ذِرَاعًا فَاسْلُكُوهُ"
  },
  {
    "id": 39,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 226.63,
    "end": 232.41,
    "text": "إِنَّهُ كَانَ لَا يُؤْمِنُ بِاللَّهِ الْعَظِيمِ"
  },
  {
    "id": 40,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 233.36,
    "end": 238.09,
    "text": "وَلَا يَحُضُّ عَلَى طَعَامِ الْمِسْكِينِ"
  },
  {
    "id": 41,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 238.8,
    "end": 243.07,
    "text": "فَلَيْسَ لَهُ الْيَوْمَ هَاهُنَا حَمِيمٌ"
  },
  {
    "id": 42,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 244.03,
    "end": 248.36,
    "text": "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍ"
  },
  {
    "id": 43,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 249.1,
    "end": 254.47,
    "text": "لَا يَأْكُلُهُ إِلَّا الْخَاطِئُونَ"
  },
  {
    "id": 44,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 255.71,
    "end": 260.88,
    "text": "فَلَا أُقْسِمُ بِمَا تُبْصِرُونَ"
  },
  {
    "id": 45,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 261.63,
    "end": 265.23,
    "text": "وَمَا لَا تُبْصِرُونَ"
  },
  {
    "id": 46,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 266.08,
    "end": 271.42,
    "text": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ"
  },
  {
    "id": 47,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 272.09,
    "end": 274.56,
    "text": "وَمَا هُوَ بِقَوْلِ شَاعِرٍ"
  },
  {
    "id": 48,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 275.38,
    "end": 278.82,
    "text": "وَلِيلًا مَا تُؤْمِنُونَ"
  },
  {
    "id": 49,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 279.93,
    "end": 282.05,
    "text": "وَلَا بِقَوْلِ كَاهِنٍ"
  },
  {
    "id": 50,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 282.86,
    "end": 286.6,
    "text": "وَلِيلًا مَا تَذَكَّرُونَ"
  },
  {
    "id": 51,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 287.87,
    "end": 292.65,
    "text": "تَنْزِيلٌ مِنْ رَبِّ الْعَالَمِينَ"
  },
  {
    "id": 52,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 293.86,
    "end": 299.06,
    "text": "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ الْأَقَاوِيلِ"
  },
  {
    "id": 53,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 299.7,
    "end": 303.3,
    "text": "لَأَخَذْنَا مِنْهُ بِالْيَمِينِ"
  },
  {
    "id": 54,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 304.24,
    "end": 308.78,
    "text": "ثُمَّ لَقَطَعْنَا مِنْهُ الْوَتِينَ"
  },
  {
    "id": 55,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 309.49,
    "end": 315.86,
    "text": "فَمَا مِنْكُمْ مِنْ أَحَدٍ عَنْهُ حَاجِزِينَ"
  },
  {
    "id": 56,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 316.81,
    "end": 322.07,
    "text": "وَإِنَّهُ لَتَذْكِرَةٌ لِلْمُتَّقِينَ"
  },
  {
    "id": 57,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 322.87,
    "end": 329.8,
    "text": "وَإِنَّا لَنَعْلَمُ أَنَّ مِنْكُمْ مُكَذِّبِينَ"
  },
  {
    "id": 58,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 330.69,
    "end": 336.0,
    "text": "وَإِنَّهُ لَحَسْرَةٌ عَلَى الْكَافِرِينَ"
  },
  {
    "id": 59,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 336.78,
    "end": 338.7,
    "text": "وَإِنَّهُ لَحَمَالٌ"
  },
  {
    "id": 60,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 339.03,
    "end": 341.04,
    "text": "مُلْيَقِينَ"
  },
  {
    "id": 61,
    "sura_id": 69,
    "recitation_uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6",
    "start": 341.94,
    "end": 346.47,
    "text": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيمِ"
  }
]