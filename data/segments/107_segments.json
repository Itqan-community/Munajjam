[
  {
    "id": 2,
    "sura_id": 107,
    "recitation_uuid": "810d7ab1-6842-44b2-be25-471deaa6a386",
    "start": 5.94,
    "end": 10.29,
    "text": "أَرَأَيْتَ الَّذِي يُكَذِّبُ بِالدِّينِ"
  },
  {
    "id": 3,
    "sura_id": 107,
    "recitation_uuid": "810d7ab1-6842-44b2-be25-471deaa6a386",
    "start": 11.17,
    "end": 15.4,
    "text": "فَذَلِكَ الَّذِي يَدْعُ الْيَتِيمَ"
  },
  {
    "id": 4,
    "sura_id": 107,
    "recitation_uuid": "810d7ab1-6842-44b2-be25-471deaa6a386",
    "start": 16.13,
    "end": 20.66,
    "text": "وَلَا يَحُضُّ عَلَى طَعَامِ الْمِسْكِينِ"
  },
  {
    "id": 5,
    "sura_id": 107,
    "recitation_uuid": "810d7ab1-6842-44b2-be25-471deaa6a386",
    "start": 21.41,
    "end": 24.63,
    "text": "فَوَيْلٌ لِلْمُصَلِّينَ"
  },
  {
    "id": 6,
    "sura_id": 107,
    "recitation_uuid": "810d7ab1-6842-44b2-be25-471deaa6a386",
    "start": 25.65,
    "end": 31.15,
    "text": "الَّذِينَ هُمْ عَنْ صَلَاتِهِمْ سَاهُونَ"
  },
  {
    "id": 7,
    "sura_id": 107,
    "recitation_uuid": "810d7ab1-6842-44b2-be25-471deaa6a386",
    "start": 32.08,
    "end": 36.25,
    "text": "الَّذِينَ هُمْ يُرَاءُونَ"
  },
  {
    "id": 8,
    "sura_id": 107,
    "recitation_uuid": "810d7ab1-6842-44b2-be25-471deaa6a386",
    "start": 37.0,
    "end": 40.3,
    "text": "وَيَمْنَعُونَ الْمَاعُونَ"
  }
]