[
  {
    "id": 2,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 5.78,
    "end": 7.96,
    "text": "ق"
  },
  {
    "id": 3,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 9.01,
    "end": 11.73,
    "text": "وَالْقُرْآنِ الْمَجِيدِ"
  },
  {
    "id": 4,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 12.96,
    "end": 25.5,
    "text": "بَلْ عَجِبُوا أَنْ جَاءَهُمْ مُنْذِرٌ مِنْهُمْ فَقَالَ الْكَافِرُونَ هَذَا شَيْءٌ عَجِيبٌ"
  },
  {
    "id": 5,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 26.84,
    "end": 30.57,
    "text": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا"
  },
  {
    "id": 6,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 31.29,
    "end": 34.77,
    "text": "ذَلِكَ رَجْعٌ بَعِيدٌ"
  },
  {
    "id": 7,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 35.79,
    "end": 40.05,
    "text": "قَدْ عَلِمْنَا مَا تَنْقُصُ الْأَرْضُ مِنْهُمْ"
  },
  {
    "id": 8,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 40.8,
    "end": 44.73,
    "text": "وَعِنْدَنَا كِتَابٌ حَفِيظٌ"
  },
  {
    "id": 9,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 45.89,
    "end": 55.15,
    "text": "بَلْ كَذَّبُوا بِالْحَقِّ لَمَّا جَاءَهُمْ فَهُمْ فِي أَمْرٍ مَرِيجٍ"
  },
  {
    "id": 10,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 56.48,
    "end": 69.83,
    "text": "أَفَلَمْ يَنْظُرُوا إِلَى السَّمَاءِ فَوْقَهُمْ كَيْفَ بَنَيْنَاهَا وَزَيَّنَّاهَا وَمَا لَهَا مِنْ فُرُوجٍ"
  },
  {
    "id": 11,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 71.17,
    "end": 82.93,
    "text": "وَالْأَرْضَ مَدَدْنَاهَا وَأَلْقَيْنَا فِيهَا رَوَاسِيَ وَأَنْبَتْنَا فِيهَا مِنْ كُلِّ زَوْجٍ بَهِيجٍ"
  },
  {
    "id": 12,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 83.92,
    "end": 89.62,
    "text": "تَبْصِرَةً وَذِكْرَى لِكُلِّ عَبْدٍ مُنِيبٍ"
  },
  {
    "id": 13,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 90.37,
    "end": 104.32,
    "text": "وَنَزَّلْنَا مِنَ السَّمَاءِ مَاءً مُبَارَكًا فَأَنْبَتْنَا بِهِ جَنَّاتٍ وَحَبَّ الْحَصِيدِ"
  },
  {
    "id": 14,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 105.24,
    "end": 111.3,
    "text": "وَالنَّخْلَ بَاسِقَاتٍ لَهَا طَلْعٌ نَضِيدٌ"
  },
  {
    "id": 15,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 112.01,
    "end": 115.05,
    "text": "رِزْقًا لِلْعِبَادِ"
  },
  {
    "id": 16,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 115.87,
    "end": 119.98,
    "text": "وَأَحْيَيْنَا بِهِ بَلْدَةً مَيْتًا"
  },
  {
    "id": 17,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 120.78,
    "end": 123.49,
    "text": "كَذَلِكَ الْخُرُوجُ"
  },
  {
    "id": 18,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 124.59,
    "end": 131.48,
    "text": "كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍ وَأَصْحَابُ الرَّسِّ وَثَمُودُ"
  },
  {
    "id": 19,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 132.18,
    "end": 137.16,
    "text": "وَعَادٌ وَفِرْعَوْنُ وَإِخْوَانُ لُوطٍ"
  },
  {
    "id": 20,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 138.19,
    "end": 141.61,
    "text": "وَأَصْحَابُ الْأَيْكَةِ وَقَوْمُ تُبَّعٍ"
  },
  {
    "id": 21,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 142.41,
    "end": 147.69,
    "text": "كُلٌّ كَذَّبَ الرُّسُلَ فَحَقَّ وَعِيدِ"
  },
  {
    "id": 22,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 148.59,
    "end": 151.79,
    "text": "أَفَعَيِينَا بِالْخَلْقِ الْأَوَّلِ"
  },
  {
    "id": 23,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 152.77,
    "end": 158.63,
    "text": "بَلْ هُمْ فِي لَبْسٍ مِنْ خَلْقٍ جَدِيدٍ"
  },
  {
    "id": 24,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 159.7,
    "end": 166.16,
    "text": "وَلَقَدْ خَلَقْنَا الْإِنْسَانَ وَنَعْلَمُ مَا تُوَسْوِسُ بِهِ نَفْسُهُ"
  },
  {
    "id": 25,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 167.13,
    "end": 171.9,
    "text": "وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنْ حَبْلِ الْوَرِيدِ"
  },
  {
    "id": 26,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 173.05,
    "end": 180.43,
    "text": "إِذْ يَتَلَقَّى الْمُتَلَقِّيَانِ عَنِ الْيَمِينِ وَعَنِ الشِّمَالِ قَعِيدٌ"
  },
  {
    "id": 27,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 181.44,
    "end": 188.17,
    "text": "مَا يَلْفِظُ مِنْ قَوْلٍ إِلَّا لَدَيْهِ رَقِيبٌ عَتِيدٌ"
  },
  {
    "id": 28,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 188.93,
    "end": 192.64,
    "text": "وَجَاءَتْ سَكْرَةُ الْمَوْتِ بِالْحَقَرِ"
  },
  {
    "id": 29,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 193.06,
    "end": 193.14,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 30,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 193.84,
    "end": 198.45,
    "text": "ذَلِكَ مَا كُنْتَ مِنْهُ تَحِيدٌ"
  },
  {
    "id": 31,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 199.13,
    "end": 201.54,
    "text": "وَنُفِخَ فِي الصُّورِ"
  },
  {
    "id": 32,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 202.22,
    "end": 205.39,
    "text": "ذَلِكَ يَوْمُ الْوَعِيدِ"
  },
  {
    "id": 33,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 206.52,
    "end": 215.22,
    "text": "وَجَاءَتْ كُلُّ نَفْسٍ مَعَهَا سَائِقٌ وَشَهِيدٌ"
  },
  {
    "id": 34,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 215.99,
    "end": 227.91,
    "text": "لَقَدْ كُنْتَ فِي غَفْلَةٍ مِنْ هَذَا فَكَشَفْنَا عَنْكَ غِطَاءَكَ فَبَصَرُكَ الْيَوْمَ حَدِيدٌ"
  },
  {
    "id": 35,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 228.78,
    "end": 234.13,
    "text": "وَقَالَ قَرِينُهُ هَذَا مَا لَدَيَّ عَتِيدٌ"
  },
  {
    "id": 36,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 235.06,
    "end": 240.87,
    "text": "أَلْقِيَا فِي جَهَنَّمَ كُلَّ كَفَّارٍ عَنِيدٍ"
  },
  {
    "id": 37,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 241.8,
    "end": 247.43,
    "text": "مَنَّاعٍ لِلْخَيْرِ مُعْتَدٍ مُرِيبٍ"
  },
  {
    "id": 38,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 248.69,
    "end": 257.49,
    "text": "الَّذِي جَعَلَ مَعَ اللَّهِ إِلَهًا آخَرَ فَأَلْقِيَاهُ فِي الْعَذَابِ الشَّدِيدِ"
  },
  {
    "id": 39,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 258.64,
    "end": 268.97,
    "text": "قَالَ قَرِينُهُ رَبَّنَا مَا أَطْغَيْتُهُ وَلَكِنْ كَانَ فِي ضَلَالٍ بَعِيدٍ"
  },
  {
    "id": 40,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 269.92,
    "end": 278.13,
    "text": "قَالَ لَا تَخْتَصِمُوا لَدَيَّ وَقَدْ قَدَّمْتُ إِلَيْكُمْ بِالْوَعِيدِ"
  },
  {
    "id": 41,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 279.36,
    "end": 287.29,
    "text": "وَمَا يُبَدَّلُ الْقَوْلُ لَدَيَّ وَمَا أَنَا بِظَلَّامٍ لِلْعَبِيدِ"
  },
  {
    "id": 42,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 288.31,
    "end": 296.46,
    "text": "يَوْمَ نَقُولُ لِجَهَنَّمَ هَلِ امْتَلَأْتِ وَتَقُولُ هَلْ مِنْ مَزِيدٍ"
  },
  {
    "id": 43,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 297.29,
    "end": 303.76,
    "text": "وَأُزْلِفَتِ الْجَنَّةُ لِلْمُتَّقِينَ غَيْرَ بَعِيدٍ"
  },
  {
    "id": 44,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 304.44,
    "end": 310.46,
    "text": "هَذَا مَا تُوعَدُونَ لِكُلِّ أَوَّابٍ حَفِيظٍ"
  },
  {
    "id": 45,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 311.51,
    "end": 319.14,
    "text": "مَنْ خَشِيَ الرَّحْمَنَ بِالْغَيْبِ وَجَاءَ بِقَلْبٍ مُنِيبٍ"
  },
  {
    "id": 46,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 319.82,
    "end": 323.22,
    "text": "ادْخُلُوهَا بِسَلَامٍ"
  },
  {
    "id": 47,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 324.04,
    "end": 327.19,
    "text": "ذَلِكَ يَوْمُ الْخُلُودِ"
  },
  {
    "id": 48,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 328.01,
    "end": 335.59,
    "text": "لَهُمْ مَا يَشَاءُونَ فِيهَا وَلَدَيْنَا مَزِيدٌ"
  },
  {
    "id": 49,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 336.73,
    "end": 349.94,
    "text": "وَكَمْ أَهْلَكْنَا قَبْلَهُمْ مِنْ قَرْنٍ هُمْ أَشَدُّ مِنْهُمْ بَطْشًا فَنَقَّبُوا فِي الْبِلَادِ هَلْ مِنْ مَحِيصٍ"
  },
  {
    "id": 50,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 351.15,
    "end": 360.6,
    "text": "إِنَّ فِي ذَلِكَ لَذِكْرَى لِمَنْ كَانَ لَهُ قَلْبٌ أَوْ أَلْقَى السَّمْعَ وَهُوَ شَهِيدٌ"
  },
  {
    "id": 51,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 361.68,
    "end": 373.11,
    "text": "وَلَقَدْ خَلَقْنَا السَّمَاوَاتِ وَالْأَرْضَ وَمَا بَيْنَهُمَا فِي سِتَّةِ أَيَّامٍ وَمَا مَسَّنَا مِنْ لُغُوبٍ"
  },
  {
    "id": 52,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 374.24,
    "end": 384.03,
    "text": "فَاصْبِرْ عَلَى مَا يَقُولُونَ وَسَبِّحْ بِحَمْدِ رَبِّكَ قَبْلَ طُلُوعِ الشَّمْسِ وَقَبْلَ الْغُرُوبِ"
  },
  {
    "id": 53,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 384.95,
    "end": 390.3,
    "text": "وَمِنَ اللَّيْلِ فَسَبِّحْهُ وَأَدْبَارَ السُّجُودِ"
  },
  {
    "id": 54,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 391.49,
    "end": 398.47,
    "text": "وَاسْتَمِعْ يَوْمَ يُنَادِ الْمُنَادِ مِنْ مَكَانٍ قَرِيبٍ"
  },
  {
    "id": 55,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 399.29,
    "end": 402.48,
    "text": "يَوْمَ يَسْمَعُونَ الصَّيْحَةَ بِالْحَقِّ"
  },
  {
    "id": 56,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 402.88,
    "end": 402.94,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 57,
    "sura_id": 50,
    "recitation_uuid": "09870d46-bbca-41f3-8271-e2fbabd7771b",
    "start": 403.88,
    "end": 406.85,
    "text": "ذَلِكَ يَوْمُ الْخُرُوجِ"
  },
  {
    "id": 58,
    "sura_id": 50,
