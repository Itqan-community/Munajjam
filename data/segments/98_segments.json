[
  {
    "id": 2,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 5.86,
    "end": 12.79,
    "text": "لَمْ يَكُنِ الَّذِينَ كَفَرُوا مِنْ أَهْلِ الْكِتَابِ وَالْمُشْرِكِينَ"
  },
  {
    "id": 3,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 13.12,
    "end": 16.62,
    "text": "حَتَّى تَأْتِيَهُمُ الْبَيِّنَةُ"
  },
  {
    "id": 4,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 17.82,
    "end": 23.47,
    "text": "رَسُولٌ مِنَ اللَّهِ يَتْلُو صُحُفًا مُطَهَّرَةً"
  },
  {
    "id": 5,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 24.47,
    "end": 27.75,
    "text": "فِيهَا كُتُبٌ قَيِّمَةٌ"
  },
  {
    "id": 6,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 28.92,
    "end": 38.62,
    "text": "وَمَا تَفَرَّقَ الَّذِينَ أُوتُوا الْكِتَابَ إِلَّا مِنْ بَعْدِ مَا جَاءَتْهُمُ الْبَيِّنَةُ"
  },
  {
    "id": 7,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 40.11,
    "end": 53.54,
    "text": "وَمَا أُمِرُوا إِلَّا لِيَعْبُدُوا اللَّهَ مُخْلِصِينَ لَهُ الدِّينَ حُنَفَاءَ وَيُقِيمُوا الصَّلَاةَ وَيُؤْتُوا الزَّكَاةَ"
  },
  {
    "id": 8,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 54.62,
    "end": 57.2,
    "text": "وَذَلِكَ دِينُ الْقَيِّمَةِ"
  },
  {
    "id": 9,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 58.56,
    "end": 69.16,
    "text": "إِنَّ الَّذِينَ كَفَرُوا مِنْ أَهْلِ الْكِتَابِ وَالْمُشْرِكِينَ فِي نَارِ جَهَنَّمَ خَالِدِينَ فِيهَا"
  },
  {
    "id": 10,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 70.26,
    "end": 74.69,
    "text": "أُولَئِكَ هُمْ شَرُّ الْبَرِيَّةِ"
  },
  {
    "id": 11,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 76.04,
    "end": 84.94,
    "text": "إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ أُولَئِكَ هُمْ خَيْرُ الْبَرِيَّةِ"
  },
  {
    "id": 12,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 86.72,
    "end": 100.3,
    "text": "جَزَاؤُهُمْ عِنْدَ رَبِّهِمْ جَنَّاتُ عَدْنٍ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ خَالِدِينَ فِيهَا أَبَدًا"
  },
  {
    "id": 13,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 101.38,
    "end": 104.73,
    "text": "وَرَضِيَ اللَّهُ عَنْهُمْ وَرَضُوا عَنْهُ"
  },
  {
    "id": 14,
    "sura_id": 98,
    "recitation_uuid": "10787bd6-7f24-4ae5-bc35-cea1c3b848c6",
    "start": 105.59,
    "end": 108.34,
    "text": "ذَلِكَ لِمَنْ خَشِيَ رَبَّهُ"
  }
]