    "start": 5.7,
    "end": 7.96,
    "text": "أَلَمْ نَشْرَحْ لَكَ صَدْرَكَ"
  },
  {
    "id": 3,
    "sura_id": 94,
    "recitation_uuid": "ab5db5ae-beb7-45f1-95cb-1346c0933d3f",
    "start": 9.06,
    "end": 11.85,
    "text": "وَوَضَعْنَا عَنْكَ وِزْرَكَ"
  },
  {
    "id": 4,
    "sura_id": 94,
    "recitation_uuid": "ab5db5ae-beb7-45f1-95cb-1346c0933d3f",
    "start": 12.84,
    "end": 16.82,
    "text": "الَّذِي أَنْقَضَ ظَهْرَيْنِ"
  },
  {
    "id": 5,
    "sura_id": 94,
    "recitation_uuid": "ab5db5ae-beb7-45f1-95cb-1346c0933d3f",
    "start": 17.94,
    "end": 20.14,
    "text": "وَرَفَعْنَا لَكَ ذِكْرًا"
  },
  {
    "id": 6,
    "sura_id": 94,
    "recitation_uuid": "ab5db5ae-beb7-45f1-95cb-1346c0933d3f",
    "start": 21.32,
    "end": 24.31,
    "text": "فَإِنَّ مَعَ الْعُسْرِ يُسْرًا"
  },
  {
    "id": 7,
    "sura_id": 94,
    "recitation_uuid": "ab5db5ae-beb7-45f1-95cb-1346c0933d3f",
    "start": 25.31,
    "end": 28.27,
    "text": "إِنَّ مَعَ الْعُسْرِ يُسْرًا"
  },
  {
    "id": 8,
    "sura_id": 94,
    "recitation_uuid": "ab5db5ae-beb7-45f1-95cb-1346c0933d3f",
    "start": 29.34,
    "end": 32.55,
    "text": "فَإِذَا فَرَغْتَ فَانْصَبْ"
  },
  {
    "id": 9,
    "sura_id": 94,
    "recitation_uuid": "ab5db5ae-beb7-45f1-95cb-1346c0933d3f",
    "start": 33.39,
    "end": 35.86,
    "text": "وَإِلَى رَبِّكَ فَارْغَبْ"
  }
