[
  {
    "id": 2,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 5.85,
    "end": 8.22,
    "text": "وَالذَّارِيَاتِ ذَرْوًا"
  },
  {
    "id": 3,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 9.03,
    "end": 11.39,
    "text": "فَالْحَامِلَاتِ وِقْرًا"
  },
  {
    "id": 4,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 12.38,
    "end": 14.75,
    "text": "فَالْجَارِيَاتِ يُسْرًا"
  },
  {
    "id": 5,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 15.58,
    "end": 18.07,
    "text": "فَالْمُقَسِّمَاتِ أَمْرًا"
  },
  {
    "id": 6,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 19.39,
    "end": 22.79,
    "text": "إِنَّمَا تُوعَدُونَ لَصَادِقٌ"
  },
  {
    "id": 7,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 23.66,
    "end": 26.22,
    "text": "وَإِنَّ الدِّينَ لَوَاقِعٌ"
  },
  {
    "id": 8,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 27.18,
    "end": 30.27,
    "text": "وَالسَّمَاءِ ذَاتِ الْحُبُكِ"
  },
  {
    "id": 9,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 31.24,
    "end": 35.37,
    "text": "إِنَّكُمْ لَفِي قَوْلٍ مُخْتَلِفٍ"
  },
  {
    "id": 10,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 36.32,
    "end": 38.43,
    "text": "يُؤْفَكُ عَنْهُ مَنْ أُفِكَ"
  },
  {
    "id": 11,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 39.54,
    "end": 42.28,
    "text": "قُتِلَ الْخَرَّاصُونَ"
  },
  {
    "id": 12,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 42.9,
    "end": 47.96,
    "text": "الَّذِينَ هُمْ فِي غَمْرَةٍ سَاهُونَ"
  },
  {
    "id": 13,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 48.78,
    "end": 52.8,
    "text": "يَسْأَلُونَ أَيَّانَ يَوْمُ الدِّينِ"
  },
  {
    "id": 14,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 53.51,
    "end": 57.5,
    "text": "يَوْمَ هُمْ عَلَى النَّارِ يُفْتَنُونَ"
  },
  {
    "id": 15,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 58.43,
    "end": 66.21,
    "text": "ذُوقُوا فِتْنَتَكُمْ هَذَا الَّذِي كُنْتُمْ بِهِ تَسْتَعْجِلُونَ"
  },
  {
    "id": 16,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 67.0,
    "end": 73.06,
    "text": "إِنَّ الْمُتَّقِينَ فِي جَنَّاتٍ وَعُيُونٍ"
  },
  {
    "id": 17,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 73.86,
    "end": 77.92,
    "text": "آخِذِينَ مَا آتَاهُمْ رَبُّهُمْ"
  },
  {
    "id": 18,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 78.89,
    "end": 84.01,
    "text": "إِنَّهُمْ كَانُوا قَبْلَ ذَلِكَ مُحْسِنِينَ"
  },
  {
    "id": 19,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 84.91,
    "end": 90.07,
    "text": "كَانُوا قَلِيلًا مِنَ اللَّيْلِ مَا يَهْجَعُونَ"
  },
  {
    "id": 20,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 90.89,
    "end": 95.13,
    "text": "وَبِالْأَسْحَارِ هُمْ يَسْتَغْفِرُونَ"
  },
  {
    "id": 21,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 95.97,
    "end": 103.67,
    "text": "وَفِي أَمْوَالِهِمْ حَقٌّ لِلسَّائِلِ وَالْمَحْرُومِ"
  },
  {
    "id": 22,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 104.56,
    "end": 108.88,
    "text": "وَفِي الْأَرْضِ آيَاتٌ لِلْمُوقِنِينَ"
  },
  {
    "id": 23,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 109.56,
    "end": 112.62,
    "text": "وَفِي أَنْفُسِكُمْ"
  },
  {
    "id": 24,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 113.41,
    "end": 116.07,
    "text": "أَفَلَا تُبْصِرُونَ"
  },
  {
    "id": 25,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 116.86,
    "end": 122.39,
    "text": "وَفِي السَّمَاءِ رِزْقُكُمْ وَمَا تُوعَدُونَ"
  },
  {
    "id": 26,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 123.47,
    "end": 135.61,
    "text": "وَرَبِّ السَّمَاءِ وَالْأَرْضِ إِنَّهُ لَحَقٌّ مِثْلَ مَا أَنَّكُمْ تَنْطِقُونَ"
  },
  {
    "id": 27,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 137.2,
    "end": 142.82,
    "text": "هَلْ أَتَاكَ حَدِيثُ ضَيْفِ إِبْرَاهِيمَ الْمُكْرَمِينَ"
  },
  {
    "id": 28,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 143.63,
    "end": 147.4,
    "text": "إِذْ دَخَلُوا عَلَيْهِ فَقَالُوا سَلَامًا"
  },
  {
    "id": 29,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 148.39,
    "end": 153.89,
    "text": "قَالَ سَلَامٌ قَوْمٌ مُنْكَرُونَ"
  },
  {
    "id": 30,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 154.81,
    "end": 162.51,
    "text": "فَرَاغَ إِلَى أَهْلِهِ فَجَاءَ بِعِجْلٍ سَمِينٍ"
  },
  {
    "id": 31,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 163.24,
    "end": 169.2,
    "text": "فَقَرَّبَهُ إِلَيْهِمْ قَالَ أَلَا تَأْكُلُونَ"
  },
  {
    "id": 32,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 169.93,
    "end": 172.25,
    "text": "فَأَوْجَسَ مِنْهُمْ خِيفَةً"
  },
  {
    "id": 33,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 173.04,
    "end": 174.6,
    "text": "قَالُوا لَا تَخَفْ"
  },
  {
    "id": 34,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 175.44,
    "end": 179.39,
    "text": "وَبَشَّرُوهُ بِغُلَامٍ عَلِيمٍ"
  },
  {
    "id": 35,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 180.6,
    "end": 190.0,
    "text": "فَأَقْبَلَتِ امْرَأَتُهُ فِي صَرَّةٍ فَصَكَّتْ وَجْهَهَا وَقَالَتْ عَجُوزٌ عَقِيمٌ"
  },
  {
    "id": 36,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 190.72,
    "end": 193.63,
    "text": "قَالُوا كَذَلِكِ قَالَ رَبُّ"
  },
  {
    "id": 37,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 194.42,
    "end": 198.86,
    "text": "إِنَّهُ هُوَ الْحَكِيمُ الْعَلِيمُ"
  },
  {
    "id": 38,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 200.0,
    "end": 204.9,
    "text": "قَالَ فَمَا خَطْبُكُمْ أَيُّهَا الْمُرْسَلُونَ"
  },
  {
    "id": 39,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 205.71,
    "end": 213.76,
    "text": "قَالُوا إِنَّا أُرْسِلْنَا إِلَى قَوْمٍ مُجْرِمِينَ"
  },
  {
    "id": 40,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 214.62,
    "end": 220.14,
    "text": "لِنُرْسِلَ عَلَيْهِمْ حِجَارَةً مِنْ طِينٍ"
  },
  {
    "id": 41,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 220.9,
    "end": 226.02,
    "text": "مُسَوَّمَةً عِنْدَ رَبِّكَ لِلْمُسْرِفِينَ"
  },
  {
    "id": 42,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 227.14,
    "end": 232.35,
    "text": "فَأَخْرَجْنَا مَنْ كَانَ فِيهَا مِنَ الْمُؤْمِنِينَ"
  },
  {
    "id": 43,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 233.1,
    "end": 239.19,
    "text": "فَمَا وَجَدْنَا فِيهَا غَيْرَ بَيْتٍ مِنَ الْمُسْلِمِينَ"
  },
  {
    "id": 44,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 240.07,
    "end": 248.08,
    "text": "وَتَرَكْنَا فِيهَا آيَةً لِلَّذِينَ يَخَافُونَ الْعَذَابَ الْأَلِيمَ"
  },
  {
    "id": 45,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 249.17,
    "end": 257.3,
    "text": "وَفِي مُوسَى إِذْ أَرْسَلْنَاهُ إِلَى فِرْعَوْنَ بِسُلْطَانٍ مُبِينٍ"
  },
  {
    "id": 46,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 258.19,
    "end": 264.09,
    "text": "فَتَوَلَّى بِرُكْنِهِ وَقَالَ سَاحِرٌ أَوْ مَجْنُونٌ"
  },
  {
    "id": 47,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 265.16,
    "end": 272.85,
    "text": "فَأَخَذْنَاهُ وَجُنُودَهُ فَنَبَذْنَاهُمْ فِي الْيَمِّ وَهُوَ مُلِيمٌ"
  },
  {
    "id": 48,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 274.29,
    "end": 280.35,
    "text": "وَفِي عَادٍ إِذْ أَرْسَلْنَا عَلَيْهِمُ الرِّيحَ الْعَقِيمَ"
  },
  {
    "id": 49,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 281.12,
    "end": 288.42,
    "text": "مَا تَذَرُ مِنْ شَيْءٍ أَتَتْ عَلَيْهِ إِلَّا جَعَلَتْهُ كَالرَّمِيمِ"
  },
  {
    "id": 50,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 289.64,
    "end": 295.6,
    "text": "وَفِي ثَمُودَ إِذْ قِيلَ لَهُمْ تَمَتَّعُوا حَتَّى حِينٍ"
  },
  {
    "id": 51,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 296.35,
    "end": 304.26,
    "text": "فَعَتَوْا عَنْ أَمْرِ رَبِّهِمْ فَأَخَذَتْهُمُ الصَّاعِقَةُ وَهُمْ يَنْظُرُونَ"
  },
  {
    "id": 52,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 305.07,
    "end": 312.93,
    "text": "فَمَا اسْتَطَاعُوا مِنْ قِيَامٍ وَمَا كَانُوا مُنْتَصِرِينَ"
  },
  {
    "id": 53,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 313.86,
    "end": 317.07,
    "text": "وَقَوْمَ نُوحٍ مِنْ قَبْلُ"
  },
  {
    "id": 54,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 317.78,
    "end": 323.09,
    "text": "إِنَّهُمْ كَانُوا قَوْمًا فَاسِقِينَ"
  },
  {
    "id": 55,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 324.19,
    "end": 331.93,
    "text": "وَالسَّمَاءَ بَنَيْنَاهَا بِأَيْدٍ وَإِنَّا لَمُوسِعُونَ"
  },
  {
    "id": 56,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 332.84,
    "end": 337.84,
    "text": "وَالْأَرْضَ فَرَشْنَاهَا فَنِعْمَ الْمَاهِدُونَ"
  },
  {
    "id": 57,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 338.6,
    "end": 346.03,
    "text": "وَمِنْ كُلِّ شَيْءٍ خَلَقْنَا زَوْجَيْنِ لَعَلَّكُمْ تَذَكَّرُونَ"
  },
  {
    "id": 58,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 347.13,
    "end": 350.91,
    "text": "فَفِرُّوا إِلَى اللَّهِ"
  },
  {
    "id": 59,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 351.52,
    "end": 356.84,
    "text": "إِنِّي لَكُمْ مِنْهُ نَذِيرٌ مُبِينٌ"
  },
  {
    "id": 60,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 357.61,
    "end": 361.62,
    "text": "وَلَا تَجْعَلُوا مَعَ اللَّهِ إِلَهًا آخَرَ"
  },
  {
    "id": 61,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 362.46,
    "end": 368.26,
    "text": "إِنِّي لَكُمْ مِنْهُ نَذِيرٌ مُبِينٌ"
  },
  {
    "id": 62,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 369.36,
    "end": 380.04,
    "text": "كَذَلِكَ مَا أَتَى الَّذِينَ مِنْ قَبْلِهِمْ مِنْ رَسُولٍ إِلَّا قَالُوا سَاحِرٌ أَوْ مَجْنُونٌ"
  },
  {
    "id": 63,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 381.14,
    "end": 382.56,
    "text": "أَتَوَاصَوْا بِهِ"
  },
  {
    "id": 64,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 383.52,
    "end": 387.15,
    "text": "بَلْ هُمْ قَوْمٌ طَاغُونَ"
  },
  {
    "id": 65,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 388.03,
    "end": 393.53,
    "text": "فَتَوَلَّ عَنْهُمْ فَمَا أَنْتَ بِمَلُومٍ"
  },
  {
    "id": 66,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 394.39,
    "end": 400.93,
    "text": "وَذَكِّرْ فَإِنَّ الذِّكْرَى تَنْفَعُ الْمُؤْمِنِينَ"
  },
  {
    "id": 67,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 401.92,
    "end": 408.58,
    "text": "وَمَا خَلَقْتُ الْجِنَّ وَالْإِنْسَ إِلَّا لِيَعْبُدُونِ"
  },
  {
    "id": 68,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 409.29,
    "end": 418.28,
    "text": "مَا أُرِيدُ مِنْهُمْ مِنْ رِزْقٍ وَمَا أُرِيدُ أَنْ يُطْعِمُونِ"
  },
  {
    "id": 69,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 419.07,
    "end": 425.37,
    "text": "إِنَّ اللَّهَ هُوَ الرَّزَّاقُ ذُو الْقُوَّةِ الْمَتِينُ"
  },
  {
    "id": 70,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 426.33,
    "end": 435.97,
    "text": "فَإِنَّ لِلَّذِينَ ظَلَمُوا ذَنُوبًا مِثْلَ ذَنُوبِ أَصْحَابِهِمْ فَلَا يَسْتَعْجِلُونِ"
  },
  {
    "id": 71,
    "sura_id": 51,
    "recitation_uuid": "e617e436-e95e-40d5-af90-c0e66e20c007",
    "start": 436.67,
    "end": 443.96,
    "text": "فَوَيْلٌ لِلَّذِينَ كَفَرُوا مِنْ يَوْمِهِمُ الَّذِي يُوعَدُونَ"
  }
]