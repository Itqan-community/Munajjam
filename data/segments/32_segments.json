[
  {
    "id": 2,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 6.08,
    "end": 13.8,
    "text": "الم"
  },
  {
    "id": 3,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 15.1,
    "end": 22.03,
    "text": "تَنْزِيلُ الْكِتَابِ لَا رَيْبَ فِيهِ مِنْ رَبِّ الْعَالَمِينَ"
  },
  {
    "id": 4,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 23.13,
    "end": 26.09,
    "text": "أَمْ يَقُولُونَ افْتَرَاهُ"
  },
  {
    "id": 5,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 27.27,
    "end": 42.75,
    "text": "بَلْ هُوَ الْحَقُّ مِنْ رَبِّكَ لِتُنْذِرَ قَوْمًا مَا أَتَاهُمْ مِنْ نَذِيرٍ مِنْ قَبْلِكَ لَعَلَّهُمْ يَهْتَدُونَ"
  },
  {
    "id": 6,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 43.96,
    "end": 55.51,
    "text": "اللَّهُ الَّذِي خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ وَمَا بَيْنَهُمَا فِي سِتَّةِ أَيَّامٍ ثُمَّ اسْتَوَى عَلَى الْعَظِيمِ"
  },
  {
    "id": 7,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 56.78,
    "end": 64.3,
    "text": "مَا لَكُمْ مِنْ دُونِهِ مِنْ وَلِيٍّ وَلَا شَفِيعٍ"
  },
  {
    "id": 8,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 65.4,
    "end": 68.83,
    "text": "أَفَلَا تَتَذَكَّرُونَ"
  },
  {
    "id": 9,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 70.13,
    "end": 86.8,
    "text": "يُدَبِّرُ الْأَمْرَ مِنَ السَّمَاءِ إِلَى الْأَرْضِ ثُمَّ يَعْرُجُ إِلَيْهِ فِي يَوْمٍ كَانَ مِقْدَارُهُ أَلْفَ سَنَةٍ مِمَّا تَعُدُّونَ"
  },
  {
    "id": 10,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 87.95,
    "end": 94.36,
    "text": "ذَلِكَ عَالِمُ الْغَيْبِ وَالشَّهَادَةِ الْعَزِيزُ الرَّحِيمُ"
  },
  {
    "id": 11,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 95.68,
    "end": 99.92,
    "text": "الَّذِي أَحْسَنَ كُلَّ شَيْءٍ خَلَقَهُ"
  },
  {
    "id": 12,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 100.9,
    "end": 106.1,
    "text": "وَبَدَأَ خَلْقَ الْإِنْسَانِ مِنْ طِينٍ"
  },
  {
    "id": 13,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 107.06,
    "end": 117.08,
    "text": "ثُمَّ جَعَلَ نَسْلَهُ مِنْ سُلَالَةٍ مِنْ مَاءٍ مَهِينٍ"
  },
  {
    "id": 14,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 118.23,
    "end": 122.39,
    "text": "ثُمَّ سَوَّاهُ وَنَفَخَ فِيهِ مِنْ رُوحِهِ"
  },
  {
    "id": 15,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 123.38,
    "end": 127.26,
    "text": "وَجَعَلَ لَكُمُ السَّمْعَ وَالْأَبْصَارَ"
  },
  {
    "id": 16,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 127.62,
    "end": 127.94,
    "text": "وَالْمُعْبِضِ"
  },
  {
    "id": 17,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 128.81,
    "end": 132.17,
    "text": "وَلِيلًا مَا تَشْكُرُونَ"
  },
  {
    "id": 18,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 133.43,
    "end": 142.07,
    "text": "وَقَالُوا أَإِذَا ضَلَلْنَا فِي الْأَرْضِ أَإِنَّا لَفِي خَلْقٍ جَدِيدٍ"
  },
  {
    "id": 19,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 142.99,
    "end": 148.99,
    "text": "بَلْ هُمْ بِلِقَاءِ رَبِّهِمْ كَافِرُونَ"
  },
  {
    "id": 20,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 150.21,
    "end": 160.51,
    "text": "قُلْ يَتَوَفَّاكُمْ مَلَكُ الْمَوْتِ الَّذِي وُكِّلَ بِكُمْ ثُمَّ إِلَى رَبِّكُمْ تُرْجَعُونَ"
  },
  {
    "id": 21,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 161.56,
    "end": 174.37,
    "text": "وَلَوْ تَرَى إِذِ الْمُجْرِمُونَ نَاكِسُو رُءُوسِهِمْ عِنْدَ رَبِّهِمْ رَبَّنَا أَبْصَرْنَا وَسَمِعْنَا"
  },
  {
    "id": 22,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 175.54,
    "end": 185.31,
    "text": "رَبَّنَا أَبْصَرْنَا وَسَمِعْنَا فَارْجِعْنَا نَعْمَلْ صَالِحًا إِنَّا مُوقِنُونَ"
  },
  {
    "id": 23,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 186.71,
    "end": 203.6,
    "text": "وَلَوْ شِئْنَا لَآتَيْنَا كُلَّ نَفْسٍ هُدَاهَا وَلَكِنْ حَقَّ الْقَوْلُ مِنِّي لَأَمْلَأَنَّ جَهَنَّمَ مِنَ الْجِنَّةِ وَالنَّاسِ أَجْمَعِينَ"
  },
  {
    "id": 24,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 205.07,
    "end": 214.52,
    "text": "فَذُوقُوا بِمَا نَسِيتُمْ لِقَاءَ يَوْمِكُمْ هَذَا إِنَّا نَسِينَاكُمْ"
  },
  {
    "id": 25,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 215.38,
    "end": 221.91,
    "text": "وَذُوقُوا عَذَابَ الْخُلْدِ بِمَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 26,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 223.14,
    "end": 237.9,
    "text": "إِنَّمَا يُؤْمِنُ بِآيَاتِنَا الَّذِينَ إِذَا ذُكِّرُوا بِهَا خَرُّوا سُجَّدًا وَسَبَّحُوا بِحَمْدِ رَبِّهِمْ وَهُمْ لَا يَسْتَكْبِرُونَ"
  },
  {
    "id": 27,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 239.11,
    "end": 247.22,
    "text": "تَتَجَافَى جُنُوبُهُمْ عَنِ الْمَضَاجِعِ يَدْعُونَ رَبَّهُمْ خَوْفًا وَطَمَعًا"
  },
  {
    "id": 28,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 248.45,
    "end": 258.35,
    "text": "يَدْعُونَ رَبَّهُمْ خَوْفًا وَطَمَعًا وَمِمَّا رَزَقْنَاهُمْ يُنْفِقُونَ"
  },
  {
    "id": 29,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 259.55,
    "end": 273.72,
    "text": "فَلَا تَعْلَمُ نَفْسٌ مَا أُخْفِيَ لَهُمْ مِنْ قُرَّةِ أَعْيُنٍ جَزَاءً بِمَا كَانُوا يَعْمَلُونَ"
  },
  {
    "id": 30,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 274.88,
    "end": 280.65,
    "text": "أَفَمَنْ كَانَ مُؤْمِنًا كَمَنْ كَانَ فَاسِقًا"
  },
  {
    "id": 31,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 281.48,
    "end": 283.58,
    "text": "لَا يَسْتَوُونَ"
  },
  {
    "id": 32,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 284.51,
    "end": 297.16,
    "text": "أَمَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ فَلَهُمْ جَنَّاتُ الْمَأْوَى نُزُلًا بِمَا كَانُوا يَعْمَلُونَ"
  },
  {
    "id": 33,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 298.37,
    "end": 304.24,
    "text": "وَأَمَّا الَّذِينَ فَسَقُوا فَمَأْوَاهُمُ النَّارُ"
  },
  {
    "id": 34,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 305.15,
    "end": 323.52,
    "text": "ثُلَّمَا أَرَادُوا أَنْ يَخْرُجُوا مِنْهَا أُعِيدُوا فِيهَا وَقِيلَ لَهُمْ ذُوقُوا عَذَابَ النَّارِ الَّذِي كُنْتُمْ بِهِ تُكَذِّبُونَ"
  },
  {
    "id": 35,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 325.14,
    "end": 335.83,
    "text": "وَلَنُذِيقَنَّهُمْ مِنَ الْعَذَابِ الْأَدْنَى دُونَ الْعَذَابِ الْأَكْبَرِ لَعَلَّهُمْ يَرْجِعُونَ"
  },
  {
    "id": 36,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 336.81,
    "end": 344.84,
    "text": "وَمَنْ أَظْلَمُ مِمَّنْ ذُكِّرَ بِآيَاتِ رَبِّهِ ثُمَّ أَعْرَضَ عَنْهَا"
  },
  {
    "id": 37,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 345.89,
    "end": 351.38,
    "text": "إِنَّا مِنَ الْمُجْرِمِينَ مُنْتَقِمُونَ"
  },
  {
    "id": 38,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 352.59,
    "end": 361.65,
    "text": "وَلَقَدْ آتَيْنَا مُوسَى الْكِتَابَ فَلَا تَكُنْ فِي مِرْيَةٍ مِنْ لِقَائِهِ"
  },
  {
    "id": 39,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 362.62,
    "end": 368.94,
    "text": "وَجَعَلْنَاهُ هُدًى لِبَنِي إِسْرَائِيلَ"
  },
  {
    "id": 40,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 370.05,
    "end": 377.86,
    "text": "وَجَعَلْنَا مِنْهُمْ أَئِمَّةً يَهْدُونَ بِأَمْرِنَا لَمَّا صَبَرُونَ"
  },
  {
    "id": 41,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 378.66,
    "end": 382.86,
    "text": "وَكَانُوا بِآيَاتِنَا يُوقِنُونَ"
  },
  {
    "id": 42,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 383.92,
    "end": 393.83,
    "text": "إِنَّ رَبَّكَ هُوَ يَفْصِلُ بَيْنَهُمْ يَوْمَ الْقِيَامَةِ فِيمَا كَانُوا فِيهِ يَخْتَلِفُونَ"
  },
  {
    "id": 43,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 394.77,
    "end": 404.34,
    "text": "أَوَلَمْ يَهْدِ لَهُمْ كَمْ أَهْلَكْنَا مِنْ قَبْلِهِمْ مِنَ الْقُرُونِ يَمْشُونَ فِي مَسَاكِنِهِمْ"
  },
  {
    "id": 44,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 405.41,
    "end": 408.97,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ"
  },
  {
    "id": 45,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 409.87,
    "end": 412.53,
    "text": "أَفَلَا يَسْمَعُونَ"
  },
  {
    "id": 46,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 413.54,
    "end": 427.44,
    "text": "أَوَلَمْ يَرَوْا أَنَّا نَسُوقُ الْمَاءَ إِلَى الْأَرْضِ الْجُرُزِ فَنُخْرِجُ بِهِ زَرْعًا تَأْكُلُ مِنْهُ أَنْعَامُهُمْ وَأَنْفُسُهُمْ"
  },
  {
    "id": 47,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 428.47,
    "end": 431.53,
    "text": "أَفَلَا يُبْصِرُونَ"
  },
  {
    "id": 48,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 432.23,
    "end": 440.06,
    "text": "وَيَقُولُونَ مَتَى هَذَا الْفَتْحُ إِنْ كُنْتُمْ صَادِقِينَ"
  },
  {
    "id": 49,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 441.0,
    "end": 451.34,
    "text": "قُلْ يَوْمَ الْفَتْحِ لَا يَنْفَعُ الَّذِينَ كَفَرُوا إِيمَانُهُمْ وَلَا هُمْ يُنْظَرُونَ"
  },
  {
    "id": 50,
    "sura_id": 32,
    "recitation_uuid": "648a5543-6994-4951-9e0f-b82dfc214d31",
    "start": 452.34,
    "end": 460.5,
    "text": "فَأَعْرِضْ عَنْهُمْ وَانْتَظِرْ إِنَّهُمْ مُنْتَظِرُونَ"
  }
]