[
  {
    "id": 2,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 6.8,
    "end": 9.49,
    "text": "حم"
  },
  {
    "id": 3,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 10.6,
    "end": 13.62,
    "text": "وَالْكِتَابِ الْمُبِينِ"
  },
  {
    "id": 4,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 14.69,
    "end": 21.95,
    "text": "إِنَّا جَعَلْنَاهُ قُرْآنًا عَرَبِيًّا لَعَلَّكُمْ تَعْقِلُونَ"
  },
  {
    "id": 5,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 23.26,
    "end": 31.27,
    "text": "وَإِنَّهُ فِي أُمِّ الْكِتَابِ لَدَيْنَا لَعَلِيٌّ حَكِيمٌ"
  },
  {
    "id": 6,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 32.43,
    "end": 41.41,
    "text": "أَفَنَضْرِبُ عَنْكُمُ الذِّكْرَ صَفْحًا أَنْ كُنْتُمْ قَوْمًا مُسْرِفِينَ"
  },
  {
    "id": 7,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 42.37,
    "end": 48.33,
    "text": "وَكَمْ أَرْسَلْنَا مِنْ نَبِيٍّ فِي الْأَوَّلِينَ"
  },
  {
    "id": 8,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 49.15,
    "end": 56.88,
    "text": "وَمَا يَأْتِيهِمْ مِنْ نَبِيٍّ إِلَّا كَانُوا بِهِ يَسْتَهْزِئُونَ"
  },
  {
    "id": 9,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 58.02,
    "end": 65.79,
    "text": "فَأَهْلَكْنَا أَشَدَّ مِنْهُمْ بَطْشًا وَمَضَى مَثَلُ الْأَوَّلِينَ"
  },
  {
    "id": 10,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 67.06,
    "end": 79.15,
    "text": "وَلَئِنْ سَأَلْتَهُمْ مَنْ خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ لَيَقُولُنَّ خَلَقَهُنَّ الْعَزِيزُ الْعَلِيمُ"
  },
  {
    "id": 11,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 80.33,
    "end": 90.42,
    "text": "الَّذِي جَعَلَ لَكُمُ الْأَرْضَ مَهْدًا وَجَعَلَ لَكُمْ فِيهَا سُبُلًا لَعَلَّكُمْ تَهْتَدُونَ"
  },
  {
    "id": 12,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 91.7,
    "end": 103.26,
    "text": "وَالَّذِي نَزَّلَ مِنَ السَّمَاءِ مَاءً بِقَدَرٍ فَأَنْشَرْنَا بِهِ بَلْدَةً مَيْتًا"
  },
  {
    "id": 13,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 104.15,
    "end": 106.9,
    "text": "كَذَلِكَ تُخْرَجُونَ"
  },
  {
    "id": 14,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 108.14,
    "end": 118.03,
    "text": "وَالَّذِي خَلَقَ الْأَزْوَاجَ كُلَّهَا وَجَعَلَ لَكُمْ مِنَ الْفُلْكِ وَالْأَنْعَامِ مَا تَرْكَبُونَ"
  },
  {
    "id": 15,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 119.14,
    "end": 136.96,
    "text": "لِتَسْتَوُوا عَلَى ظُهُورِهِ ثُمَّ تَذْكُرُوا نِعْمَةَ رَبِّكُمْ إِذَا اسْتَوَيْتُمْ عَلَيْهِ وَتَقُولُوا سُبْحَانَ الَّذِي سَخَّرَ لَنَا هَذَا وَمَا كُنَّا لَهُ مُقْرِنِينَ"
  },
  {
    "id": 16,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 138.09,
    "end": 144.42,
    "text": "وَإِنَّا إِلَى رَبِّنَا لَمُنْقَلِبُونَ"
  },
  {
    "id": 17,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 145.72,
    "end": 149.51,
    "text": "وَجَعَلُوا لَهُ مِنْ عِبَادِهِ جُزْءًا"
  },
  {
    "id": 18,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 150.59,
    "end": 156.26,
    "text": "إِنَّ الْإِنْسَانَ لَكَفُورٌ مُبِينٌ"
  },
  {
    "id": 19,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 157.42,
    "end": 164.87,
    "text": "أَمِ اتَّخَذَ مِمَّا يَخْلُقُ بَنَاتٍ وَأَصْفَاكُمْ بِالْبَنِينَ"
  },
  {
    "id": 20,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 165.93,
    "end": 177.6,
    "text": "وَإِذَا بُشِّرَ أَحَدُهُمْ بِمَا ضَرَبَ لِلرَّحْمَنِ مَثَلًا ظَلَّ وَجْهُهُ مُسْوَدًّا وَهُوَ كَظِيمٌ"
  },
  {
    "id": 21,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 178.79,
    "end": 185.84,
    "text": "أَوَمَنْ يُنَشَّأُ فِي الْحِلْيَةِ وَهُوَ فِي الْخِصَامِ غَيْرُ مُبِينٍ"
  },
  {
    "id": 22,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 187.07,
    "end": 194.01,
    "text": "وَجَعَلُوا الْمَلَائِكَةَ الَّذِينَ هُمْ عِبَادُ الرَّحْمَنِ إِنَاثًا"
  },
  {
    "id": 23,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 194.92,
    "end": 196.85,
    "text": "لَشَهِدُوا خَلْقَهُمْ"
  },
  {
    "id": 24,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 197.69,
    "end": 202.0,
    "text": "فَتُكْتَبُ شَهَادَتُهُمْ وَيُسْأَلُونَ"
  },
  {
    "id": 25,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 203.2,
    "end": 208.27,
    "text": "وَقَالُوا لَوْ شَاءَ الرَّحْمَنُ مَا عَبَدْنَاهُمْ"
  },
  {
    "id": 26,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 209.08,
    "end": 212.01,
    "text": "مَا لَهُمْ بِذَلِكَ مِنْ عِلْمٍ"
  },
  {
    "id": 27,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 212.88,
    "end": 216.18,
    "text": "إِنْ هُمْ إِلَّا يَخْرُصُونَ"
  },
  {
    "id": 28,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 217.27,
    "end": 226.03,
    "text": "أَمْ آتَيْنَاهُمْ كِتَابًا مِنْ قَبْلِهِ فَهُمْ بِهِ مُسْتَمْسِكُونَ"
  },
  {
    "id": 29,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 227.03,
    "end": 242.87,
    "text": "بَلْ قَالُوا إِنَّا وَجَدْنَا آبَاءَنَا عَلَى أُمَّةٍ وَإِنَّا عَلَى آثَارِهِمْ مُهْتَدُونَ"
  },
  {
    "id": 30,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 244.34,
    "end": 255.01,
    "text": "وَكَذَلِكَ مَا أَرْسَلْنَا مِنْ قَبْلِكَ فِي قَرْيَةٍ مِنْ نَذِيرٍ إِلَّا قَالَ مُتْرَفُوهَا"
  },
  {
    "id": 31,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 256.3,
    "end": 274.67,
    "text": "إِلَّا قَالَ مُتْرَفُوهَا إِنَّا وَجَدْنَا آبَاءَنَا عَلَى أُمَّةٍ وَإِنَّا عَلَى آثَارِهِمْ مُقْتَدُونَ"
  },
  {
    "id": 32,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 276.07,
    "end": 284.81,
    "text": "قَالَ أَوَلَوْ جِئْتُكُمْ بِأَهْدَى مِمَّا وَجَدْتُمْ عَلَيْهِ آبَاءَكُمْ"
  },
  {
    "id": 33,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 285.72,
    "end": 293.53,
    "text": "قَالُوا إِنَّا بِمَا أُرْسِلْتُمْ بِهِ كَافِرُونَ"
  },
  {
    "id": 34,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 294.3,
    "end": 297.06,
    "text": "فَانْتَقَمْنَا مِنْهُمْ"
  },
  {
    "id": 35,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 297.81,
    "end": 304.15,
    "text": "فَانْظُرْ كَيْفَ كَانَ عَاقِبَةُ الْمُكَذِّبِينَ"
  },
  {
    "id": 36,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 305.64,
    "end": 318.97,
    "text": "وَإِذْ قَالَ إِبْرَاهِيمُ لِأَبِيهِ وَقَوْمِهِ إِنَّنِي بَرَاءٌ مِمَّا تَعْبُدُونَ"
  },
  {
    "id": 37,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 320.6,
    "end": 326.81,
    "text": "إِلَّا الَّذِي فَطَرَنِي فَإِنَّهُ سَيَهْدِينِ"
  },
  {
    "id": 38,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 328.27,
    "end": 337.72,
    "text": "وَجَعَلَهَا كَلِمَةً بَاقِيَةً فِي عَقِبِهِ لَعَلَّهُمْ يَرْجِعُونَ"
  },
  {
    "id": 39,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 339.22,
    "end": 346.79,
    "text": "بَلْ مَتَّعْتُ هَؤُلَاءِ وَآبَاءَهُمْ"
  },
  {
    "id": 40,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 347.1,
    "end": 353.68,
    "text": "لَا جَاءَهُمُ الْحَقُّ وَرَسُولٌ مُبِينٌ"
  },
  {
    "id": 41,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 355.14,
    "end": 365.94,
    "text": "وَلَمَّا جَاءَهُمُ الْحَقُّ قَالُوا هَذَا سِحْرٌ وَإِنَّا بِهِ كَافِرُونَ"
  },
  {
    "id": 42,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 367.57,
    "end": 377.02,
    "text": "وَقَالُوا لَوْلَا نُزِّلَ هَذَا الْقُرْآنُ عَلَى رَجُلٍ مِنَ الْقَرْيَتَيْنِ عَظِيمٍ"
  },
  {
    "id": 43,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 378.01,
    "end": 381.15,
    "text": "فَهُمْ يَقْسِمُونَ رَحْمَتَ رَبِّ"
  },
  {
    "id": 44,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 382.37,
    "end": 388.86,
    "text": "نَحْنُ قَسَمْنَا بَيْنَهُمْ مَعِيشَتَهُمْ فِي الْحَيَاةِ الدُّنْيَا"
  },
  {
    "id": 45,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 390.05,
    "end": 401.0,
    "text": "وَرَفَعْنَا بَعْضَهُمْ فَوْقَ بَعْضٍ دَرَجَاتٍ لِيَتَّخِذَ بَعْضُهُمْ بَعْضًا سُخْرِيًّا"
  },
  {
    "id": 46,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 402.03,
    "end": 408.64,
    "text": "وَرَحْمَتُ رَبِّكَ خَيْرٌ مِمَّا يَجْمَعُونَ"
  },
  {
    "id": 47,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 410.19,
    "end": 426.19,
    "text": "وَلَوْلَا أَنْ يَكُونَ النَّاسُ أُمَّةً وَاحِدَةً لَجَعَلْنَا لِمَنْ يَكْفُرُ بِالرَّحْمَنِ لِبُيُوتِهِمْ سُقُفًا مِنْ فِضَّةٍ"
  },
  {
    "id": 48,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 427.78,
    "end": 435.41,
    "text": "ذُكُفًا مِنْ فِضَّةٍ وَمَعَارِجَ عَلَيْهَا يَظْهَرُونَ"
  },
  {
    "id": 49,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 436.85,
    "end": 443.95,
    "text": "وَلِبُيُوتِهِمْ أَبْوَابًا وَسُرُرًا عَلَيْهَا يَتَّكِئُونَ"
  },
  {
    "id": 50,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 444.85,
    "end": 446.08,
    "text": "وَزُخْرُفًا"
  },
  {
    "id": 51,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 446.96,
    "end": 453.58,
    "text": "وَإِنْ كُلُّ ذَلِكَ لَمَّا مَتَاعُ الْحَيَاةِ الدُّنْيَا"
  },
  {
    "id": 52,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 454.62,
    "end": 460.77,
    "text": "وَالْآخِرَةُ عِنْدَ رَبِّكَ لِلْمُتَّقِينَ"
  },
  {
    "id": 53,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 462.28,
    "end": 472.19,
    "text": "وَمَنْ يَعْشُ عَنْ ذِكْرِ الرَّحْمَنِ نُقَيِّضْ لَهُ شَيْطَانًا فَهُوَ لَهُ قَرِينٌ"
  },
  {
    "id": 54,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 473.5,
    "end": 483.49,
    "text": "وَإِنَّهُمْ لَيَصُدُّونَهُمْ عَنِ السَّبِيلِ وَيَحْسَبُونَ أَنَّهُمْ مُهْتَدُونَ"
  },
  {
    "id": 55,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 484.6,
    "end": 494.96,
    "text": "حَتَّى إِذَا جَاءَنَا قَالَ يَا لَيْتَ بَيْنِي وَبَيْنَكَ بُعْدَ الْمَشْرِقَيْنِ"
  },
  {
    "id": 56,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 495.3,
    "end": 497.39,
    "text": "وَالْقَرِينِ"
  },
  {
    "id": 57,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 498.45,
    "end": 507.93,
    "text": "وَلَنْ يَنْفَعَكُمُ الْيَوْمَ إِذْ ظَلَمْتُمْ أَنَّكُمْ فِي الْعَذَابِ مُشْتَرِكُونَ"
  },
  {
    "id": 58,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 509.04,
    "end": 519.14,
    "text": "أَفَأَنْتَ تُسْمِعُ الصُّمَّ أَوْ تَهْدِي الْعُمْيَ وَمَنْ كَانَ فِي ضَلَالٍ مُبِينٍ"
  },
  {
    "id": 59,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 520.27,
    "end": 528.42,
    "text": "فَإِمَّا نَذْهَبَنَّ بِكَ فَإِنَّا مِنْهُمْ مُنْتَقِمُونَ"
  },
  {
    "id": 60,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 529.37,
    "end": 538.53,
    "text": "لَوْ نُرِيَنَّكَ الَّذِي وَعَدْنَاهُمْ فَإِنَّا عَلَيْهِمْ مُقْتَدِرُونَ"
  },
  {
    "id": 61,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 539.67,
    "end": 544.75,
    "text": "فَاسْتَمْسِكْ بِالَّذِي أُوحِيَ إِلَيْهِ"
  },
  {
    "id": 62,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 545.74,
    "end": 550.89,
    "text": "إِنَّكَ عَلَى صِرَاطٍ مُسْتَقِيمٍ"
  },
  {
    "id": 63,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 552.07,
    "end": 555.94,
    "text": "وَإِنَّهُ لَذِكْرٌ لَكَ وَلِقَوْمِ"
  },
  {
    "id": 64,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 556.83,
    "end": 559.81,
    "text": "وَسَوْفَ تُسْأَلُونَ"
  },
  {
    "id": 65,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 560.97,
    "end": 574.57,
    "text": "وَاسْأَلْ مَنْ أَرْسَلْنَا مِنْ قَبْلِكَ مِنْ رُسُلِنَا أَجَعَلْنَا مِنْ دُونِ الرَّحْمَنِ آلِهَةً يُعْبَدُونَ"
  },
  {
    "id": 66,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 575.97,
    "end": 589.67,
    "text": "وَلَقَدْ أَرْسَلْنَا مُوسَى بِآيَاتِنَا إِلَى فِرْعَوْنَ وَمَلَئِهِ فَقَالَ إِنِّي رَسُولُ رَبِّ الْعَالَمِينَ"
  },
  {
    "id": 67,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 591.28,
    "end": 601.83,
    "text": "فَلَمَّا جَاءَهُمْ بِآيَاتِنَا إِذَا هُمْ مِنْهَا يَضْحَكُونَ"
  },
  {
    "id": 68,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 603.25,
    "end": 608.84,
    "text": "وَمَا نُرِيهِمْ مِنْ آيَةٍ إِلَّا هِيَ أَكْبَرُ مِنْ أُولَى"
  },
  {
    "id": 69,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 609.17,
    "end": 609.71,
    "text": "فِيهَا"
  },
  {
    "id": 70,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 610.61,
    "end": 616.9,
    "text": "وَأَخَذْنَاهُمْ بِالْعَذَابِ لَعَلَّهُمْ يَرْجِعُونَ"
  },
  {
    "id": 71,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 618.31,
    "end": 629.68,
    "text": "وَقَالُوا يَا أَيُّهَ السَّاحِرُ ادْعُ لَنَا رَبَّكَ بِمَا عَهِدَ عِنْدَكَ إِنَّنَا لَمُهْتَدُونَ"
  },
  {
    "id": 72,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 630.82,
    "end": 638.62,
    "text": "فَلَمَّا كَشَفْنَا عَنْهُمُ الْعَذَابَ إِذَا هُمْ يَنْكُثُونَ"
  },
  {
    "id": 73,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 639.76,
    "end": 651.53,
    "text": "وَنَادَى فِرْعَوْنُ فِي قَوْمِهِ قَالَ يَا قَوْمِ أَلَيْسَ لِي مُلْكُ مِصْرَ وَهَذِهِ الْأَنْهَارُ تَجْرِي مِنْ تَبُورٍ"
  },
  {
    "id": 74,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 651.86,
    "end": 652.27,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 75,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 653.23,
    "end": 655.88,
    "text": "أَفَلَا تُبْصِرُونَ"
  },
  {
    "id": 76,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 656.97,
    "end": 665.47,
    "text": "أَمْ أَنَا خَيْرٌ مِنْ هَذَا الَّذِي هُوَ مَهِينٌ وَلَا يَكَادُ يُبِينُ"
  },
  {
    "id": 77,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 666.76,
    "end": 679.8,
    "text": "وَلَوْلَا أُلْقِيَ عَلَيْهِ أَسْوِرَةٌ مِنْ ذَهَبٍ أَوْ جَاءَ مَعَهُ الْمَلَائِكَةُ مُقْتَرِنِينَ"
  },
  {
    "id": 78,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 680.83,
    "end": 685.14,
    "text": "فَاسْتَخَفَّ قَوْمَهُ فَأَطَاعُونِ"
  },
  {
    "id": 79,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 685.85,
    "end": 691.78,
    "text": "إِنَّهُمْ كَانُوا قَوْمًا فَاسِقِينَ"
  },
  {
    "id": 80,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 692.98,
    "end": 703.01,
    "text": "فَلَمَّا آسَفُونَا انْتَقَمْنَا مِنْهُمْ فَأَغْرَقْنَاهُمْ أَجْمَعِينَ"
  },
  {
    "id": 81,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 703.85,
    "end": 709.95,
    "text": "فَجَعَلْنَاهُمْ سَلَفًا وَمَثَلًا لِلْآخِرِينَ"
  },
  {
    "id": 82,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 711.06,
    "end": 718.64,
    "text": "وَلَمَّا ضُرِبَ ابْنُ مَرْيَمَ مَثَلًا إِذَا قَوْمُكَ مِنْهُ يَصِدُّونَ"
  },
  {
    "id": 83,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 719.58,
    "end": 724.24,
    "text": "وَقَالُوا أَآلِهَتُنَا خَيْرٌ أَمْ هُوَ"
  },
  {
    "id": 84,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 725.01,
    "end": 728.54,
    "text": "وَمَا ضَرَبُوهُ لَكَ إِلَّا جَدَلًا"
  },
  {
    "id": 85,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 729.37,
    "end": 732.89,
    "text": "بَلْ هُمْ قَوْمٌ خَصِمُونَ"
  },
  {
    "id": 86,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 734.01,
    "end": 744.96,
    "text": "إِنْ هُوَ إِلَّا عَبْدٌ أَنْعَمْنَا عَلَيْهِ وَجَعَلْنَاهُ مَثَلًا لِبَنِي إِسْرَائِيلَ"
  },
  {
    "id": 87,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 746.03,
    "end": 756.86,
    "text": "وَلَوْ نَشَاءُ لَجَعَلْنَا مِنْكُمْ مَلَائِكَةً فِي الْأَرْضِ يَخْلُفُونَ"
  },
  {
    "id": 88,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 757.98,
    "end": 766.68,
    "text": "وَإِنَّهُ لَعِلْمٌ لِلسَّاعَةِ فَلَا تَمْتَرُنَّ بِهَا وَاتَّبِعُونِ"
  },
  {
    "id": 89,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 767.48,
    "end": 771.74,
    "text": "هَذَا صِرَاطٌ مُسْتَقِيمٌ"
  },
  {
    "id": 90,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 772.72,
    "end": 777.07,
    "text": "وَلَا يَصُدَّنَّكُمُ الشَّيْطَانُ"
  },
  {
    "id": 91,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 777.93,
    "end": 783.12,
    "text": "إِنَّهُ لَكُمْ عَدُوٌّ مُبِينٌ"
  },
  {
    "id": 92,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 784.37,
    "end": 799.15,
    "text": "وَلَمَّا جَاءَ عِيسَى بِالْبَيِّنَاتِ قَالَ قَدْ جِئْتُكُمْ بِالْحِكْمَةِ وَلِأُبَيِّنَ لَكُمْ بَعْضَ الَّذِي تَخْتَلِفُونَ فِيهِ"
  },
  {
    "id": 93,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 800.12,
    "end": 803.9,
    "text": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
  },
  {
    "id": 94,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 805.17,
    "end": 811.05,
    "text": "إِنَّ اللَّهَ هُوَ رَبِّي وَرَبُّكُمْ فَاعْبُدُوهُ"
  },
  {
    "id": 95,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 811.79,
    "end": 816.02,
    "text": "هَذَا صِرَاطٌ مُسْتَقِيمٌ"
  },
  {
    "id": 96,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 817.08,
    "end": 820.9,
    "text": "فَاخْتَلَفَ الْأَحْزَابُ مِنْ بَيْنِهِمْ"
  },
  {
    "id": 97,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 821.74,
    "end": 828.2,
    "text": "فَوَيْلٌ لِلَّذِينَ ظَلَمُوا مِنْ عَذَابِ يَوْمٍ أَلِيمٍ"
  },
  {
    "id": 98,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 829.36,
    "end": 839.81,
    "text": "بَلْ يَنْظُرُونَ إِلَّا السَّاعَةَ أَنْ تَأْتِيَهُمْ بَغْتَةً وَهُمْ لَا يَشْعُرُونَ"
  },
  {
    "id": 99,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 840.85,
    "end": 850.76,
    "text": "الْأَخِلَّاءُ يَوْمَئِذٍ بَعْضُهُمْ لِبَعْضٍ عَدُوٌّ إِلَّا الْمُتَّقِينَ"
  },
  {
    "id": 100,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 851.8,
    "end": 860.85,
    "text": "يَا عِبَادِ لَا خَوْفٌ عَلَيْكُمُ الْيَوْمَ وَلَا أَنْتُمْ تَحْزَنُونَ"
  },
  {
    "id": 101,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 861.75,
    "end": 868.49,
    "text": "الَّذِينَ آمَنُوا بِآيَاتِنَا وَكَانُوا مُسْلِمِينَ"
  },
  {
    "id": 102,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 869.63,
    "end": 876.28,
    "text": "ادْخُلُوا الْجَنَّةَ أَنْتُمْ وَأَزْوَاجُكُمْ تُحْبَرُونَ"
  },
  {
    "id": 103,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 877.18,
    "end": 885.03,
    "text": "يُطَافُ عَلَيْهِمْ بِصِحَافٍ مِنْ ذَهَبٍ وَأَكْوَابٍ"
  },
  {
    "id": 104,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 886.05,
    "end": 891.97,
    "text": "وَفِيهَا مَا تَشْتَهِيهِ الْأَنْفُسُ وَتَلَذُّ الْأَعْيُنُ"
  },
  {
    "id": 105,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 892.79,
    "end": 897.21,
    "text": "وَأَنْتُمْ فِيهَا خَالِدُونَ"
  },
  {
    "id": 106,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 898.5,
    "end": 907.7,
    "text": "وَتِلْكَ الْجَنَّةُ الَّتِي أُورِثْتُمُوهَا بِمَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 107,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 908.53,
    "end": 914.15,
    "text": "لَكُمْ فِيهَا فَاكِهَةٌ كَثِيرَةٌ مِنْهَاتٍ"
  },
  {
    "id": 108,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 914.48,
    "end": 915.88,
    "text": "وَيُولُونَ"
  },
  {
    "id": 109,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 917.3,
    "end": 924.1,
    "text": "إِنَّ الْمُجْرِمِينَ فِي عَذَابِ جَهَنَّمَ خَالِدُونَ"
  },
  {
    "id": 110,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 924.91,
    "end": 930.25,
    "text": "لَا يُفَتَّرُ عَنْهُمْ وَهُمْ فِيهِ مُبْلِسُونَ"
  },
  {
    "id": 111,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 931.15,
    "end": 937.98,
    "text": "وَمَا ظَلَمْنَاهُمْ وَلَكِنْ كَانُوا هُمُ الظَّالِمِينَ"
  },
  {
    "id": 112,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 938.81,
    "end": 943.28,
    "text": "وَنَادَوْا يَا مَالِكُ لِيَقْضِ عَلَيْنَا رَبُّهُ"
  },
  {
    "id": 113,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 944.14,
    "end": 948.42,
    "text": "قَالَ إِنَّكُمْ مَاكِثُونَ"
  },
  {
    "id": 114,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 949.21,
    "end": 958.33,
    "text": "لَقَدْ جِئْنَاكُمْ بِالْحَقِّ وَلَكِنَّ أَكْثَرَكُمْ لِلْحَقِّ كَارِهُونَ"
  },
  {
    "id": 115,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 959.23,
    "end": 965.92,
    "text": "أَمْ أَبْرَمُوا أَمْرًا فَإِنَّا مُبْرِمُونَ"
  },
  {
    "id": 116,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 967.0,
    "end": 972.94,
    "text": "أَمْ يَحْسَبُونَ أَنَّا لَا نَسْمَعُ سِرَّهُمْ وَنَجْوَاهُمْ"
  },
  {
    "id": 117,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 973.94,
    "end": 978.72,
    "text": "بَلَى وَرُسُلُنَا نَدَيْهِمْ يَكْتُبُونَ"
  },
  {
    "id": 118,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 980.01,
    "end": 987.93,
    "text": "قُلْ إِنْ كَانَ لِلرَّحْمَنِ وَلَدٌ فَأَنَا أَوَّلُ الْعَابِدِينَ"
  },
  {
    "id": 119,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 988.89,
    "end": 997.3,
    "text": "سُبْحَانَ رَبِّ السَّمَاوَاتِ وَالْأَرْضِ رَبِّ الْعَرْشِ عَمَّا يَصِفُونَ"
  },
  {
    "id": 120,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 998.22,
    "end": 1006.82,
    "text": "فَذَرْهُمْ يَخُوضُوا وَيَلْعَبُوا حَتَّى يُلَاقُوا يَوْمَهُمُ الَّذِي يُوعَدُونَ"
  },
  {
    "id": 121,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 1007.82,
    "end": 1015.77,
    "text": "وَهُوَ الَّذِي فِي السَّمَاءِ إِلَهٌ وَفِي الْأَرْضِ إِلَهٌ"
  },
  {
    "id": 122,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 1016.65,
    "end": 1019.77,
    "text": "وَهُوَ الْحَكِيمُ الْعَلِيمُ"
  },
  {
    "id": 123,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 1020.89,
    "end": 1034.81,
    "text": "وَتَبَارَكَ الَّذِي لَهُ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا وَعِنْدَهُ عِلْمُ السَّاعَةِ وَإِلَيْهِ تُرْجَعُونَ"
  },
  {
    "id": 124,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 1035.81,
    "end": 1047.81,
    "text": "وَلَا يَمْلِكُ الَّذِينَ يَدْعُونَ مِنْ دُونِهِ الشَّفَاعَةَ إِلَّا مَنْ شَهِدَ بِالْحَقِّ وَهُمْ يَعْلَمُونَ"
  },
  {
    "id": 125,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 1048.95,
    "end": 1056.49,
    "text": "وَلَئِنْ سَأَلْتَهُمْ مَنْ خَلَقَهُمْ لَيَقُولُنَّ اللَّهُ"
  },
  {
    "id": 126,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 1057.4,
    "end": 1061.13,
    "text": "كَأَنَّى يُؤْفَكُونَ"
  },
  {
    "id": 127,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 1062.12,
    "end": 1070.91,
    "text": "وَقِيلِهِ يَا رَبِّ إِنَّ هَؤُلَاءِ قَوْمٌ لَا يُؤْمِنُونَ"
  },
  {
    "id": 128,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 1071.73,
    "end": 1075.36,
    "text": "فَاصْفَحْ عَنْهُمْ وَقُلْ سَلَامٌ"
  },
  {
    "id": 129,
    "sura_id": 43,
    "recitation_uuid": "b2cf0860-1976-47d1-b568-eb83881d17d0",
    "start": 1076.19,
    "end": 1078.96,
    "text": "فَسَوْفَ يَعْلَمُونَ"
  }
]