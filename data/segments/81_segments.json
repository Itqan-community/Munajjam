[
  {
    "id": 2,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 5.53,
    "end": 7.59,
    "text": "إِذَا الشَّمْسُ كُوِّرَتْ"
  },
  {
    "id": 3,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 8.61,
    "end": 11.88,
    "text": "وَإِذَا النُّجُومُ انْكَدَرَتْ"
  },
  {
    "id": 4,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 12.87,
    "end": 15.44,
    "text": "وَإِذَا الْجِبَالُ سُيِّرَتْ"
  },
  {
    "id": 5,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 16.39,
    "end": 18.87,
    "text": "وَإِذَا الْعِشَارُ عُطِّلَتْ"
  },
  {
    "id": 6,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 19.87,
    "end": 22.19,
    "text": "وَإِذَا الْوُحُوشُ حُشِرَتْ"
  },
  {
    "id": 7,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 23.18,
    "end": 25.7,
    "text": "وَإِذَا الْبِحَارُ سُجِّرَتْ"
  },
  {
    "id": 8,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 26.46,
    "end": 29.57,
    "text": "وَإِذَا النُّفُوسُ زُوِّجَ"
  },
  {
    "id": 9,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 30.49,
    "end": 33.25,
    "text": "وَإِذَا الْمَوْءُودَةُ سُئِلَتْ"
  },
  {
    "id": 10,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 34.16,
    "end": 37.64,
    "text": "بِأَيِّ ذَنْبٍ قُتِلَتْ"
  },
  {
    "id": 11,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 38.5,
    "end": 40.56,
    "text": "وَإِذَا الصُّحُفُ نُشِرَتْ"
  },
  {
    "id": 12,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 41.5,
    "end": 44.59,
    "text": "وَإِذَا السَّمَاءُ كُشِطَتْ"
  },
  {
    "id": 13,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 45.66,
    "end": 48.22,
    "text": "وَإِذَا الْجَحِيمُ سُعِّرَتْ"
  },
  {
    "id": 14,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 49.17,
    "end": 52.12,
    "text": "وَإِذَا الْجَنَّةُ أُزْلِفَتْ"
  },
  {
    "id": 15,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 52.99,
    "end": 57.01,
    "text": "عَلِمَتْ نَفْسٌ مَا أَحْضَرَتْ"
  },
  {
    "id": 16,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 57.88,
    "end": 61.48,
    "text": "فَلَا أُقْسِمُ بِالْخُنَّسِ"
  },
  {
    "id": 17,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 62.25,
    "end": 64.75,
    "text": "الْجَوَارِ الْكُنَّسِ"
  },
  {
    "id": 18,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 65.47,
    "end": 67.82,
    "text": "وَاللَّيْلِ إِذَا عَسْعَسَ"
  },
  {
    "id": 19,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 68.65,
    "end": 71.13,
    "text": "وَالصُّبْحِ إِذَا تَنَفَّسَ"
  },
  {
    "id": 20,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 72.06,
    "end": 77.19,
    "text": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ"
  },
  {
    "id": 21,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 77.94,
    "end": 82.7,
    "text": "ذِي قُوَّةٍ عِنْدَ ذِي الْعَرْشِ مَكِينٍ"
  },
  {
    "id": 22,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 83.44,
    "end": 87.14,
    "text": "مُطَاعٍ ثَمَّ أَمِينٍ"
  },
  {
    "id": 23,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 87.92,
    "end": 92.04,
    "text": "وَمَا صَاحِبُكُمْ بِمَجْنُونٍ"
  },
  {
    "id": 24,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 92.8,
    "end": 97.05,
    "text": "وَلَقَدْ رَآهُ بِالْأُفُقِ الْمُبِينِ"
  },
  {
    "id": 25,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 97.83,
    "end": 101.8,
    "text": "وَمَا هُوَ عَلَى الْغَيْبِ بِضَنِينٍ"
  },
  {
    "id": 26,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 102.61,
    "end": 107.41,
    "text": "وَمَا هُوَ بِقَوْلِ شَيْطَانٍ رَجِيمٍ"
  },
  {
    "id": 27,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 108.42,
    "end": 111.31,
    "text": "فَأَيْنَ تَذْهَبُونَ"
  },
  {
    "id": 28,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 112.09,
    "end": 116.6,
    "text": "إِنْ هُوَ إِلَّا ذِكْرٌ لِلْعَالَمِينَ"
  },
  {
    "id": 29,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 117.35,
    "end": 123.64,
    "text": "لِمَنْ شَاءَ مِنْكُمْ أَنْ يَسْتَقِيمَ"
  },
  {
    "id": 30,
    "sura_id": 81,
    "recitation_uuid": "7165c9be-754b-4ca3-ae17-7159640e6fba",
    "start": 124.6,
    "end": 135.22,
    "text": "وَمَا تَشَاءُونَ إِلَّا أَنْ يَشَاءَ اللَّهُ رَبُّ الْعَالَمِينَ"
  }
]