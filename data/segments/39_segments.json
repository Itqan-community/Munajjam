[
  {
    "id": 2,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 6.15,
    "end": 12.61,
    "text": "تَنْزِيلُ الْكِتَابِ مِنَ اللَّهِ الْعَزِيزِ الْحَكِيمِ"
  },
  {
    "id": 3,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 13.75,
    "end": 25.62,
    "text": "إِنَّا أَنْزَلْنَا إِلَيْكَ الْكِتَابَ بِالْحَقِّ فَاعْبُدِ اللَّهَ مُخْلِصًا لَهُ الدِّينَ"
  },
  {
    "id": 4,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 26.74,
    "end": 29.86,
    "text": "بَلَى لِلَّهِ الدِّينُ الْخَالِ"
  },
  {
    "id": 5,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 31.21,
    "end": 51.74,
    "text": "وَالَّذِينَ اتَّخَذُوا مِنْ دُونِهِ أَوْلِيَاءَ مَا نَعْبُدُهُمْ إِلَّا لِيُقَرِّبُونَا إِلَى اللَّهِ زُلْفَى إِنَّ اللَّهَ يَحْكُمُ بَيْنَهُمْ فِي مَا هُمْ فِيهِ"
  },
  {
    "id": 6,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 52.05,
    "end": 53.75,
    "text": "الَّذِفُونَ"
  },
  {
    "id": 7,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 55.19,
    "end": 60.62,
    "text": "إِنَّ اللَّهَ لَا يَهْدِي مَنْ هُوَ كَاذِبٌ"
  },
  {
    "id": 8,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 60.96,
    "end": 62.38,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 9,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 63.7,
    "end": 72.81,
    "text": "لَوْ أَرَادَ اللَّهُ أَنْ يَتَّخِذَ وَلَدًا لَاصْطَفَى مِمَّا يَخْلُقُ مَا يَشَاءُ"
  },
  {
    "id": 10,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 73.67,
    "end": 74.72,
    "text": "سُبْحَانَهُ"
  },
  {
    "id": 11,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 75.42,
    "end": 79.68,
    "text": "وَهُوَ اللَّهُ الْوَاحِدُ الْقَهَّارُ"
  },
  {
    "id": 12,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 80.78,
    "end": 84.12,
    "text": "خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ"
  },
  {
    "id": 13,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 84.67,
    "end": 84.72,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 14,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 85.45,
    "end": 93.37,
    "text": "يُكَوِّرُ اللَّيْلَ عَلَى النَّهَارِ وَيُكَوِّرُ النَّهَارَ عَلَى اللَّيْلِ"
  },
  {
    "id": 15,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 94.34,
    "end": 96.79,
    "text": "وَسَخَّرَ الشَّمْسَ وَالْقَمَرَ"
  },
  {
    "id": 16,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 97.71,
    "end": 102.97,
    "text": "كُلٌّ يَجْرِي لِأَجَلٍ مُسَمًّى"
  },
  {
    "id": 17,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 103.91,
    "end": 106.18,
    "text": "أَنَاهُوَ الْعَزِيزُ الْغَيْبُ"
  },
  {
    "id": 18,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 106.52,
    "end": 107.88,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 19,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 108.88,
    "end": 124.46,
    "text": "خَلَقَكُمْ مِنْ نَفْسٍ وَاحِدَةٍ ثُمَّ جَعَلَ مِنْهَا زَوْجَهَا وَأَنْزَلَ لَكُمْ مِنَ الْأَنْعَامِ ثَمَانِيَةَ أَزْوَاجٍ"
  },
  {
    "id": 20,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 125.49,
    "end": 137.53,
    "text": "يَخْلُقُكُمْ فِي بُطُونِ أُمَّهَاتِكُمْ خَلْقًا مِنْ بَعْدِ خَلْقٍ فِي ظُلُمَاتٍ ثَلَاثٍ"
  },
  {
    "id": 21,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 138.4,
    "end": 142.09,
    "text": "ذَلِكُمُ اللَّهُ رَبُّكُمْ لَهُ الْمُلْكُونَ"
  },
  {
    "id": 22,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 143.0,
    "end": 146.91,
    "text": "لَا إِلَهَ إِلَّا هُوَ"
  },
  {
    "id": 23,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 147.71,
    "end": 151.16,
    "text": "كَأَنَّا تُصْرَفُونَ"
  },
  {
    "id": 24,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 152.29,
    "end": 158.07,
    "text": "إِنْ تَكْفُرُوا فَإِنَّ اللَّهَ غَنِيٌّ عَنْكُمْ"
  },
  {
    "id": 25,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 158.95,
    "end": 162.13,
    "text": "وَلَا يَرْضَى لِعِبَادِهِ الْكُفْرَ"
  },
  {
    "id": 26,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 163.12,
    "end": 166.54,
    "text": "وَإِنْ تَشْكُرُوا يَرْضَهُ لَكُمْ"
  },
  {
    "id": 27,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 167.38,
    "end": 171.54,
    "text": "وَلَا تَزِرُ وَازِرَةٌ وِزْرَ أُخْرَى"
  },
  {
    "id": 28,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 172.39,
    "end": 183.58,
    "text": "ثُمَّ إِلَى رَبِّكُمْ مَرْجِعُكُمْ فَيُنَبِّئُكُمْ بِمَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 29,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 184.45,
    "end": 189.62,
    "text": "إِنَّهُ عَلِيمٌ بِذَاتِ الصُّدُورِ"
  },
  {
    "id": 30,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 190.8,
    "end": 198.64,
    "text": "وَإِذَا مَسَّ الْإِنْسَانَ ضُرٌّ دَعَا رَبَّهُ مُنِيبًا إِلَيْهِ"
  },
  {
    "id": 31,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 199.65,
    "end": 219.75,
    "text": "وَدَعَا رَبَّهُ مُرِيبًا إِلَيْهِ ثُمَّ إِذَا خَوَّلَهُ نِعْمَةً مِنْهُ نَسِيَ مَا كَانَ يَدْعُو إِلَيْهِ مِنْ قَبْلُ وَجَعَلَ لِلَّهِ أَنْدَادًا لِيُضِلَّ عَنْ سَبِيلِهِ"
  },
  {
    "id": 32,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 220.98,
    "end": 228.74,
    "text": "قُلْ تَمَتَّعْ بِكُفْرِكَ قَلِيلًا إِنَّكَ مِنْ أَصْحَابِ النَّارِ"
  },
  {
    "id": 33,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 230.0,
    "end": 243.22,
    "text": "أَمَّنْ هُوَ قَانِتٌ آنَاءَ اللَّيْلِ سَاجِدًا وَقَائِمًا يَحْذَرُ الْآخِرَةَ وَيَرْجُو رَحْمَةَ رَبِّهِ"
  },
  {
    "id": 34,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 244.28,
    "end": 251.27,
    "text": "قُلْ هَلْ يَسْتَوِي الَّذِينَ يَعْلَمُونَ وَالَّذِينَ لَا يَعْلَمُونَ"
  },
  {
    "id": 35,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 251.93,
    "end": 257.05,
    "text": "إِنَّمَا يَتَذَكَّرُ أُولُو الْأَلْبَابِ"
  },
  {
    "id": 36,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 258.2,
    "end": 263.12,
    "text": "قُلْ يَا عِبَادِ الَّذِينَ آمَنُوا اتَّقُوا رَبَّكُمْ"
  },
  {
    "id": 37,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 264.02,
    "end": 268.92,
    "text": "الَّذِينَ أَحْسَنُوا فِي هَذِهِ الدُّنْيَا حَسَنَةٌ"
  },
  {
    "id": 38,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 269.61,
    "end": 272.07,
    "text": "وَأَرْضُ اللَّهِ وَاسِعَةٌ"
  },
  {
    "id": 39,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 272.87,
    "end": 280.19,
    "text": "إِنَّمَا يُوَفَّى الصَّابِرُونَ أَجْرَهُمْ بِغَيْرِ حِسَابٍ"
  },
  {
    "id": 40,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 281.39,
    "end": 288.79,
    "text": "قُلْ إِنِّي أُمِرْتُ أَنْ أَعْبُدَ اللَّهَ مُخْلِصًا لَهُ الدِّينَ"
  },
  {
    "id": 41,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 289.4,
    "end": 294.74,
    "text": "وَأُمِرْتُ لِأَنْ أَكُونَ أَوَّلَ الْمُسْلِمِينَ"
  },
  {
    "id": 42,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 295.47,
    "end": 303.42,
    "text": "قُلْ إِنِّي أَخَافُ إِنْ عَصَيْتُ رَبِّي عَذَابَ يَوْمٍ عَظِيمٍ"
  },
  {
    "id": 43,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 304.32,
    "end": 308.77,
    "text": "قُلِ اللَّهَ أَعْبُدُ مُخْلِصًا لَهُ دِينِي"
  },
  {
    "id": 44,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 309.64,
    "end": 313.68,
    "text": "فَاعْبُدُوا مَا شِئْتُمْ مِنْ دُونِهِ"
  },
  {
    "id": 45,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 314.37,
    "end": 323.88,
    "text": "قُلْ إِنَّ الْخَاسِرِينَ الَّذِينَ خَسِرُوا أَنْفُسَهُمْ وَأَهْلِيهِمْ يَوْمَ الْقِيَامَةِ"
  },
  {
    "id": 46,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 324.77,
    "end": 329.42,
    "text": "أَلَا ذَلِكَ هُوَ الْخُسْرَانُ الْمُبِينُ"
  },
  {
    "id": 47,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 330.42,
    "end": 338.08,
    "text": "لَهُمْ مِنْ فَوْقِهِمْ ظُلَلٌ مِنَ النَّارِ وَمِنْ تَحْتِهِمْ ظُلَلٌ"
  },
  {
    "id": 48,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 338.99,
    "end": 343.14,
    "text": "ذَلِكَ يُخَوِّفُ اللَّهُ بِهِ عِبَادَهُ"
  },
  {
    "id": 49,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 343.95,
    "end": 347.75,
    "text": "يَا عِبَادِ فَاتَّقُونِ"
  },
  {
    "id": 50,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 348.39,
    "end": 358.02,
    "text": "وَالَّذِينَ اجْتَنَبُوا الطَّاغُوتَ أَنْ يَعْبُدُوهَا وَأَنَابُوا إِلَى اللَّهِ لَهُمُ الْبُشْرَى"
  },
  {
    "id": 51,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 358.89,
    "end": 361.44,
    "text": "بَشِّرْ عِبَادِ"
  },
  {
    "id": 52,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 362.42,
    "end": 367.87,
    "text": "الَّذِينَ يَسْتَمِعُونَ الْقَوْلَ فَيَتَّبِعُونَ أَحْسَنَهُ"
  },
  {
    "id": 53,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 368.68,
    "end": 373.62,
    "text": "أُولَئِكَ الَّذِينَ هَدَاهُمُ اللَّهُ"
  },
  {
    "id": 54,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 374.44,
    "end": 379.21,
    "text": "وَأُولَئِكَ هُمْ أُولُو الْأَلْبَابِ"
  },
  {
    "id": 55,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 380.57,
    "end": 390.21,
    "text": "أَفَمَنْ حَقَّ عَلَيْهِ كَلِمَةُ الْعَذَابِ أَفَأَنْتَ تُنْقِذُ مَنْ فِي النَّارِ"
  },
  {
    "id": 56,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 391.23,
    "end": 405.32,
    "text": "لَكِنِ الَّذِينَ اتَّقَوْا رَبَّهُمْ لَهُمْ غُرَفٌ مِنْ فَوْقِهَا غُرَفٌ مَبْنِيَّةٌ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ"
  },
  {
    "id": 57,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 406.39,
    "end": 411.59,
    "text": "وَعْدَ اللَّهِ لَا يُخْلِفُ اللَّهُ الْمِيعَادَ"
  },
  {
    "id": 58,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 412.73,
    "end": 426.52,
    "text": "أَلَمْ تَرَ أَنَّ اللَّهَ أَنْزَلَ مِنَ السَّمَاءِ مَاءً فَسَلَكَهُ يَنَابِيعَ فِي الْأَرْضِ ثُمَّ يُخْرِجُ بِهِ زَرْعًا"
  },
  {
    "id": 59,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 427.83,
    "end": 441.84,
    "text": "ثُمَّ يُخْرِجُ بِهِ زَرْعًا مُخْتَلِفًا أَلْوَانُهُ ثُمَّ يَهِيجُ فَتَرَاهُ مُصْفَرًّا ثُمَّ يَجْعَلُهُ حُطَامًا"
  },
  {
    "id": 60,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 443.09,
    "end": 448.42,
    "text": "إِنَّ فِي ذَلِكَ لَذِكْرَى لِأُولِي الْأَلْبَابِ"
  },
  {
    "id": 61,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 449.84,
    "end": 457.93,
    "text": "أَفَمَنْ شَرَحَ اللَّهُ صَدْرَهُ لِلْإِسْلَامِ فَهُوَ عَلَى نُورٍ مِنْ رَبِّهِ"
  },
  {
    "id": 62,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 459.09,
    "end": 465.45,
    "text": "فَوَيْلٌ لِلْقَاسِيَةِ قُلُوبُهُمْ مِنْ ذِكْرِ اللَّهِ"
  },
  {
    "id": 63,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 466.25,
    "end": 471.38,
    "text": "أُولَئِكَ فِي ضَلَالٍ مُبِينٍ"
  },
  {
    "id": 64,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 472.52,
    "end": 485.16,
    "text": "اللَّهُ نَزَّلَ أَحْسَنَ الْحَدِيثِ كِتَابًا مُتَشَابِهًا مَثَانِيَ تَقْشَعِرُّ مِنْهُ جُلُودُ الَّذِينَ يَخْشَوْنَ رَبَّهُمْ"
  },
  {
    "id": 65,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 486.41,
    "end": 498.31,
    "text": "لَاقْشَعِ الرُّ مِنْهُ جُلُودُ الَّذِينَ يَخْشَوْنَ رَبَّهُمْ ثُمَّ تَلِينُ جُلُودُهُمْ وَقُلُوبُهُمْ إِلَى ذِكْرِ اللَّهِ"
  },
  {
    "id": 66,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 499.45,
    "end": 505.43,
    "text": "ذَلِكَ هُدَى اللَّهِ يَهْدِي بِهِ مَنْ يَشَاءُ"
  },
  {
    "id": 67,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 506.34,
    "end": 511.84,
    "text": "وَمَنْ يُضْلِلِ اللَّهُ فَمَا لَهُ مِنْ هَادٍ"
  },
  {
    "id": 68,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 513.0,
    "end": 520.29,
    "text": "أَفَمَنْ يَتَّقِي بِوَجْهِهِ سُوءَ الْعَذَابِ يَوْمَ الْقِيَامَةِ"
  },
  {
    "id": 69,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 521.09,
    "end": 527.77,
    "text": "وَقِيلَ لِلظَّالِمِينَ ذُوقُوا مَا كُنْتُمْ تَكْسِبُونَ"
  },
  {
    "id": 70,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 528.86,
    "end": 537.36,
    "text": "كَذَّبَ الَّذِينَ مِنْ قَبْلِهِمْ فَأَتَاهُمُ الْعَذَابُ مِنْ حَيْثُ لَا يَشْعُرُونَ"
  },
  {
    "id": 71,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 538.37,
    "end": 543.39,
    "text": "فَأَذَاقَهُمُ اللَّهُ الْخِزْيَ فِي الْحَيَاةِ الدُّنْيَا"
  },
  {
    "id": 72,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 544.23,
    "end": 547.17,
    "text": "وَلَعَذَابُ الْآخِرَةِ أَكْبَرُ"
  },
  {
    "id": 73,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 547.91,
    "end": 550.9,
    "text": "لَوْ كَانُوا يَعْلَمُونَ"
  },
  {
    "id": 74,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 551.98,
    "end": 562.38,
    "text": "وَلَقَدْ ضَرَبْنَا لِلنَّاسِ فِي هَذَا الْقُرْآنِ مِنْ كُلِّ مَثَلٍ لَعَلَّهُمْ يَتَذَكَّرُونَ"
  },
  {
    "id": 75,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 563.18,
    "end": 570.13,
    "text": "قُرْآنًا عَرَبِيًّا غَيْرَ ذِي عِوَجٍ لَعَلَّهُمْ يَتَّقُونَ"
  },
  {
    "id": 76,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 571.39,
    "end": 583.9,
    "text": "وَرَدَ اللَّهُ مَثَلًا رَجُلًا فِيهِ شُرَكَاءُ مُتَشَاكِسُونَ وَرَجُلًا سَلَمًا لِرَجُلٍ هَلْ يَسْتَوِيَانِ مَثَلًا"
  },
  {
    "id": 77,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 584.87,
    "end": 587.15,
    "text": "الْحَمْدُ لِلَّهِ"
  },
  {
    "id": 78,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 587.98,
    "end": 591.25,
    "text": "بَلْ أَكْثَرُهُمْ لَا يَعْلَمُونَ"
  },
  {
    "id": 79,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 592.46,
    "end": 599.4,
    "text": "إِنَّكَ مَيِّتٌ وَإِنَّهُمْ مَيِّتُونَ"
  },
  {
    "id": 80,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 600.3,
    "end": 608.64,
    "text": "ثُمَّ إِنَّكُمْ يَوْمَ الْقِيَامَةِ عِنْدَ رَبِّكُمْ تَخْتَصِمُونَ"
  },
  {
    "id": 81,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 609.92,
    "end": 618.38,
    "text": "فَمَنْ أَظْلَمُ مِمَّنْ كَذَبَ عَلَى اللَّهِ وَكَذَّبَ بِالصِّدْقِ إِذْ جَاءَهُ"
  },
  {
    "id": 82,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 619.35,
    "end": 624.98,
    "text": "أَلَيْسَ فِي جَهَنَّمَ مَثْوًى لِلْكَافِرِينَ"
  },
  {
    "id": 83,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 626.22,
    "end": 636.38,
    "text": "وَالَّذِي جَاءَ بِالصِّدْقِ وَصَدَّقَ بِهِ أُولَئِكَ هُمُ الْمُتَّقُونَ"
  },
  {
    "id": 84,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 637.35,
    "end": 642.55,
    "text": "لَهُمْ مَا يَشَاءُونَ عِنْدَ رَبِّهِمْ"
  },
  {
    "id": 85,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 643.41,
    "end": 647.59,
    "text": "ذَلِكَ جَزَاءُ الْمُحْسِنِينَ"
  },
  {
    "id": 86,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 648.75,
    "end": 660.38,
    "text": "لِيُكَفِّرَ اللَّهُ عَنْهُمْ أَسْوَأَ الَّذِي عَمِلُوا وَيَجْزِيَهُمْ أَجْرَهُمْ بِأَحْسَنِ الَّذِي كَانُوا يَعْمَلُونَ"
  },
  {
    "id": 87,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 661.77,
    "end": 665.03,
    "text": "أَلَيْسَ اللَّهُ بِكَافٍ عَبْدَهُ"
  },
  {
    "id": 88,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 665.84,
    "end": 670.47,
    "text": "وَيُخَوِّفُونَكَ بِالَّذِينَ مِنْ دُونِهِ"
  },
  {
    "id": 89,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 671.28,
    "end": 676.93,
    "text": "وَمَنْ يُضْلِلِ اللَّهُ فَمَا لَهُ مِنْ هَادٍ"
  },
  {
    "id": 90,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 677.99,
    "end": 682.8,
    "text": "وَمَنْ يَهْدِ اللَّهُ فَمَا لَهُ مِنْ مُضِلٍّ"
  },
  {
    "id": 91,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 683.64,
    "end": 689.16,
    "text": "أَلَيْسَ اللَّهُ بِعَزِيزٍ ذِي انْتِقَامٍ"
  },
  {
    "id": 92,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 690.24,
    "end": 698.83,
    "text": "وَلَئِنْ سَأَلْتَهُمْ مَنْ خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ لَيَقُولُنَّ اللَّهُ"
  },
  {
    "id": 93,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 699.99,
    "end": 711.88,
    "text": "قُلْ أَفَرَأَيْتُمْ مَا تَدْعُونَ مِنْ دُونِ اللَّهِ إِنْ أَرَادَنِيَ اللَّهُ بِضُرٍّ هَلْ هُنَّ كَاشِفَاتُ ضُرِّهِ"
  },
  {
    "id": 94,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 713.14,
    "end": 726.68,
    "text": "إِنْ أَرَادَنِيَ اللَّهُ بِضُرٍّ هَلْ هُنَّ كَاشِفَاتُ ضُرِّهِ أَوْ أَرَادَنِي بِرَحْمَةٍ هَلْ هُنَّ مُمْسِكَاتُ رَحْمَتِهِ"
  },
  {
    "id": 95,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 727.95,
    "end": 730.89,
    "text": "قُلْ حَسْبِيَ اللَّهُ"
  },
  {
    "id": 96,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 731.6,
    "end": 735.88,
    "text": "عَلَيْهِ يَتَوَكَّلُ الْمُتَوَكِّلُونَ"
  },
  {
    "id": 97,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 737.02,
    "end": 742.9,
    "text": "قُلْ يَا قَوْمِ اعْمَلُوا عَلَى مَكَانَتِكُمْ إِنِّي عَامِلٌ"
  },
  {
    "id": 98,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 743.86,
    "end": 746.7,
    "text": "فَسَوْفَ تَعْلَمُونَ"
  },
  {
    "id": 99,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 747.44,
    "end": 756.1,
    "text": "مَنْ يَأْتِيهِ عَذَابٌ يُخْزِيهِ وَيَحِلُّ عَلَيْهِ عَذَابٌ مُقِيمٌ"
  },
  {
    "id": 100,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 757.42,
    "end": 764.66,
    "text": "إِنَّا أَنْزَلْنَا عَلَيْكَ الْكِتَابَ لِلنَّاسِ بِالْحَقِّ"
  },
  {
    "id": 101,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 765.05,
    "end": 765.16,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 102,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 766.02,
    "end": 768.39,
    "text": "فَمَنِ اهْتَدَى فَلِنَفْسِهِ"
  },
  {
    "id": 103,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 769.13,
    "end": 774.23,
    "text": "وَمَنْ ضَلَّ فَإِنَّمَا يَضِلُّ عَلَيْهَا"
  },
  {
    "id": 104,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 775.22,
    "end": 781.01,
    "text": "وَمَا أَنْتَ عَلَيْهِمْ بِوَكِيلٍ"
  },
  {
    "id": 105,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 782.36,
    "end": 790.6,
    "text": "اللَّهُ يَتَوَفَّى الْأَنْفُسَ حِينَ مَوْتِهَا وَالَّتِي لَمْ تَمُتْ فِي مَنَامِهَا"
  },
  {
    "id": 106,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 791.66,
    "end": 802.55,
    "text": "لَيُمْسِكُ الَّتِي قَضَى عَلَيْهَا الْمَوْتَ وَيُرْسِلُ الْأُخْرَى إِلَى أَجَلٍ مُسَمًّى"
  },
  {
    "id": 107,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 803.55,
    "end": 810.81,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِقَوْمٍ يَتَفَكَّرُونَ"
  },
  {
    "id": 108,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 812.06,
    "end": 816.92,
    "text": "أَمِ اتَّخَذُوا مِنْ دُونِ اللَّهِ شُفَعًا"
  },
  {
    "id": 109,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 817.82,
    "end": 824.84,
    "text": "قُلْ أَوَلَوْ كَانُوا لَا يَمْلِكُونَ شَيْئًا وَلَا يَعْقِلُونَ"
  },
  {
    "id": 110,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 825.68,
    "end": 829.16,
    "text": "قُلْ لِلَّهِ الشَّفَاعَةُ جَمِيعًا"
  },
  {
    "id": 111,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 830.06,
    "end": 833.39,
    "text": "لَهُ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 112,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 834.14,
    "end": 837.73,
    "text": "ثُمَّ إِلَيْهِ تُرْجَعُونَ"
  },
  {
    "id": 113,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 838.94,
    "end": 847.24,
    "text": "وَإِذَا ذُكِرَ اللَّهُ وَحْدَهُ اشْمَأَزَّتْ قُلُوبُ الَّذِينَ لَا يُؤْمِنُونَ بِالْآخِرَةِ"
  },
  {
    "id": 114,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 848.42,
    "end": 856.55,
    "text": "وَإِذَا ذُكِرَ الَّذِينَ مِنْ دُونِهِ إِذَا هُمْ يَسْتَبْشِرُونَ"
  },
  {
    "id": 115,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 857.7,
    "end": 873.28,
    "text": "قُلِ اللَّهُمَّ فَاطِرَ السَّمَاوَاتِ وَالْأَرْضِ عَالِمَ الْغَيْبِ وَالشَّهَادَةِ أَنْتَ تَحْكُمُ بَيْنَ عِبَادِكَ فِي مَا كَانُوا فِيهِ يَخْتَلِفُونَ"
  },
  {
    "id": 116,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 874.74,
    "end": 889.13,
    "text": "وَلَوْ أَنَّ لِلَّذِينَ ظَلَمُوا مَا فِي الْأَرْضِ جَمِيعًا وَمِثْلَهُ مَعَهُ لَافْتَدَوْا بِهِ مِنْ سُوءِ الْعَذَابِ يَوْمَ الْقِيَامَةِ"
  },
  {
    "id": 117,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 890.32,
    "end": 896.98,
    "text": "وَبَدَا لَهُمْ مِنَ اللَّهِ مَا لَمْ يَكُونُوا يَحْتَسِبُونَ"
  },
  {
    "id": 118,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 897.85,
    "end": 907.25,
    "text": "وَبَدَا لَهُمْ سَيِّئَاتُ مَا كَسَبُوا وَحَاقَ بِهِمْ مَا كَانُوا بِهِ يَسْتَهْزِئُونَ"
  },
  {
    "id": 119,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 908.43,
    "end": 913.41,
    "text": "فَإِذَا مَسَّ الْإِنْسَانَ ضُرٌّ دَعَانَا"
  },
  {
    "id": 120,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 914.47,
    "end": 925.5,
    "text": "دَعَانَا ثُمَّ إِذَا خَوَّلْنَاهُ نِعْمَةً مِنَّا قَالَ إِنَّمَا أُوتِيتُهُ عَلَى عِلْمٍ"
  },
  {
    "id": 121,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 926.61,
    "end": 933.66,
    "text": "بَلْ هِيَ فِتْنَةٌ وَلَكِنَّ أَكْثَرَهُمْ لَا يَعْلَمُونَ"
  },
  {
    "id": 122,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 934.62,
    "end": 944.2,
    "text": "قَدْ قَالَهَا الَّذِينَ مِنْ قَبْلِهِمْ فَمَا أَغْنَى عَنْهُمْ مَا كَانُوا يَكْسِبُونَ"
  },
  {
    "id": 123,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 945.32,
    "end": 949.16,
    "text": "فَأَصَابَهُمْ سَيِّئَاتُ مَا كَسَبُوا"
  },
  {
    "id": 124,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 949.78,
    "end": 962.19,
    "text": "وَالَّذِينَ ظَلَمُوا مِنْ هَؤُلَاءِ سَيُصِيبُهُمْ سَيِّئَاتُ مَا كَسَبُوا وَمَا هُمْ بِمُعْجِزِينَ"
  },
  {
    "id": 125,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 963.3,
    "end": 972.47,
    "text": "أَوَلَمْ يَعْلَمُوا أَنَّ اللَّهَ يَبْسُطُ الرِّزْقَ لِمَنْ يَشَاءُ وَيَقْدِرُ"
  },
  {
    "id": 126,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 973.6,
    "end": 980.57,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِقَوْمٍ يُؤْمِنُونَ"
  },
  {
    "id": 127,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 981.94,
    "end": 992.11,
    "text": "قُلْ يَا عِبَادِيَ الَّذِينَ أَسْرَفُوا عَلَى أَنْفُسِهِمْ لَا تَقْنَطُوا مِنْ رَحْمَةِ اللَّهِ"
  },
  {
    "id": 128,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 993.02,
    "end": 997.71,
    "text": "إِنَّ اللَّهَ يَغْفِرُ الذُّنُوبَ جَمِيعًا"
  },
  {
    "id": 129,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 998.6,
    "end": 1002.92,
    "text": "إِنَّهُ هُوَ الْغَفُورُ الرَّحِيمُ"
  },
  {
    "id": 130,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1004.0,
    "end": 1016.65,
    "text": "وَأَنِيبُوا إِلَى رَبِّكُمْ وَأَسْلِمُوا لَهُ مِنْ قَبْلِ أَنْ يَأْتِيَكُمُ الْعَذَابُ ثُمَّ لَا تُنْصَرُونَ"
  },
  {
    "id": 131,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1017.78,
    "end": 1034.92,
    "text": "وَاتَّبِعُوا أَحْسَنَ مَا أُنْزِلَ إِلَيْكُمْ مِنْ رَبِّكُمْ مِنْ قَبْلِ أَنْ يَأْتِيَكُمُ الْعَذَابُ بَغْتَةً وَأَنْتُمْ لَا تَشْعُرُونَ"
  },
  {
    "id": 132,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1036.03,
    "end": 1048.83,
    "text": "أَنْ تَقُولَ نَفْسٌ يَا حَسْرَتَا عَلَى مَا فَرَّطْتُ فِي جَنْبِ اللَّهِ وَإِنْ كُنْتُ لَمِنَ السَّاخِرِينَ"
  },
  {
    "id": 133,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1049.86,
    "end": 1057.75,
    "text": "أَوْ تَقُولَ لَوْ أَنَّ اللَّهَ هَدَانِي لَكُنْتُ مِنَ الْمُتَّقِينَ"
  },
  {
    "id": 134,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1058.77,
    "end": 1068.2,
    "text": "أَوْ تَقُولَ حِينَ تَرَى الْعَذَابَ لَوْ أَنَّ لِي كَرَّةً فَأَكُونَ مِنَ الْمُحْسِنِينَ"
  },
  {
    "id": 135,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1069.23,
    "end": 1080.61,
    "text": "بَلَى قَدْ جَاءَتْكَ آيَاتِي فَكَذَّبْتَ بِهَا وَاسْتَكْبَرْتَ وَكُنْتَ مِنَ الْكَافِرِينَ"
  },
  {
    "id": 136,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1081.63,
    "end": 1089.67,
    "text": "وَيَوْمَ الْقِيَامَةِ تَرَى الَّذِينَ كَذَبُوا عَلَى اللَّهِ وُجُوهُهُمْ مُسْوَدَّةٌ"
  },
  {
    "id": 137,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1090.69,
    "end": 1097.25,
    "text": "أَلَيْسَ فِي جَهَنَّمَ مَثْوًى لِلْمُتَكَبِّرِينَ"
  },
  {
    "id": 138,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1098.24,
    "end": 1109.27,
    "text": "وَيُنَجِّي اللَّهُ الَّذِينَ اتَّقَوْا بِمَفَازَتِهِمْ لَا يَمَسُّهُمُ السُّوءُ وَلَا هُمْ يَحْزَنُونَ"
  },
  {
    "id": 139,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1110.37,
    "end": 1114.32,
    "text": "اللَّهُ خَالِقُ كُلِّ شَيْءٍ"
  },
  {
    "id": 140,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1115.04,
    "end": 1119.98,
    "text": "وَهُوَ عَلَى كُلِّ شَيْءٍ وَكِيلٌ"
  },
  {
    "id": 141,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1121.13,
    "end": 1124.99,
    "text": "لَهُ مَقَالِيدُ السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 142,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1126.09,
    "end": 1134.12,
    "text": "وَالَّذِينَ كَفَرُوا بِآيَاتِ اللَّهِ أُولَئِكَ هُمُ الْخَاسِرُونَ"
  },
  {
    "id": 143,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1135.47,
    "end": 1145.89,
    "text": "قُلْ أَفَغَيْرَ اللَّهِ تَأْمُرُونِّي أَعْبُدُ أَيُّهَا الْجَاهِلُونَ"
  },
  {
    "id": 144,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1147.13,
    "end": 1160.69,
    "text": "وَلَقَدْ أُوحِيَ إِلَيْكَ وَإِلَى الَّذِينَ مِنْ قَبْلِكَ لَئِنْ أَشْرَكْتَ لَيَحْبَطَنَّ عَمَلُكَ وَلَتَكُونَنَّ مِنَ الْخَاسِرِينَ"
  },
  {
    "id": 145,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1161.64,
    "end": 1166.97,
    "text": "بَلِ اللَّهَ فَاعْبُدْ وَكُنْ مِنَ الشَّاكِرِينَ"
  },
  {
    "id": 146,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1168.08,
    "end": 1181.7,
    "text": "وَمَا قَدَرُوا اللَّهَ حَقَّ قَدْرِهِ وَالْأَرْضُ جَمِيعًا قَبْضَتُهُ يَوْمَ الْقِيَامَةِ وَالسَّمَاوَاتُ مَطْوِيَّاتٌ بِيَمِينِهِ"
  },
  {
    "id": 147,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1182.74,
    "end": 1188.24,
    "text": "سُبْحَانَهُ وَتَعَالَى عَمَّا يُشْرِكُونَ"
  },
  {
    "id": 148,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1189.61,
    "end": 1200.62,
    "text": "وَنُفِخَ فِي الصُّورِ فَصَعِقَ مَنْ فِي السَّمَاوَاتِ وَمَنْ فِي الْأَرْضِ إِلَّا مَنْ شَاءَ اللَّهُ"
  },
  {
    "id": 149,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1201.51,
    "end": 1209.55,
    "text": "ثُمَّ نُفِخَ فِيهِ أُخْرَى فَإِذَا هُمْ قِيَامٌ يَنْظُرُونَ"
  },
  {
    "id": 150,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1210.65,
    "end": 1216.65,
    "text": "وَأَشْرَقَتِ الْأَرْضُ بِنُورِ رَبِّهَا وَوُضِعَ الْكِتَابُ"
  },
  {
    "id": 151,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1217.47,
    "end": 1230.09,
    "text": "وَوُضِعَ الْكِتَابُ وَجِيءَ بِالنَّبِيِّينَ وَالشُّهَدَاءِ وَقُضِيَ بَيْنَهُمْ بِالْحَقِّ وَهُمْ لَا يُظْلَمُونَ"
  },
  {
    "id": 152,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1231.21,
    "end": 1238.73,
    "text": "وَوُفِّيَتْ كُلُّ نَفْسٍ مَا عَمِلَتْ وَهُوَ أَعْلَمُ بِمَا يَفْعَلُونَ"
  },
  {
    "id": 153,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1239.89,
    "end": 1245.95,
    "text": "وَسِيقَ الَّذِينَ كَفَرُوا إِلَى جَهَنَّمَ زُمَرًا"
  },
  {
    "id": 154,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1247.07,
    "end": 1255.71,
    "text": "حَتَّى إِذَا جَاءُوهَا فُتِحَتْ أَبْوَابُهَا وَقَالَ لَهُمْ خَزَنَتُهَا"
  },
  {
    "id": 155,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1257.05,
    "end": 1273.92,
    "text": "وَقَالَ لَهُمْ خَزَنَتُهَا أَلَمْ يَأْتِكُمْ رُسُلٌ مِنْكُمْ يَتْلُونَ عَلَيْكُمْ آيَاتِ رَبِّكُمْ وَيُنْذِرُونَكُمْ لِقَاءَ يَوْمِكُمْ هَذَا"
  },
  {
    "id": 156,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1275.21,
    "end": 1282.28,
    "text": "قَالُوا بَلَى وَلَكِنْ أَقَّتْ كَلِمَةُ الْعَذَابِ عَلَى الْكَافِرِينَ"
  },
  {
    "id": 157,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1283.47,
    "end": 1290.01,
    "text": "قِيلَ ادْخُلُوا أَبْوَابَ جَهَنَّمَ خَالِدِينَ فِيهَا"
  },
  {
    "id": 158,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1290.95,
    "end": 1294.6,
    "text": "فَبِئْسَ مَثْوَى الْمُتَكَبِّرِينَ"
  },
  {
    "id": 159,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1295.61,
    "end": 1301.72,
    "text": "وَسِيقَ الَّذِينَ اتَّقَوْا رَبَّهُمْ إِلَى الْجَنَّةِ زُمَرًا"
  },
  {
    "id": 160,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1302.49,
    "end": 1313.46,
    "text": "حَتَّى إِذَا جَاءُوهَا وَفُتِحَتْ أَبْوَابُهَا وَقَالَ لَهُمْ خَزَنَتُهَا سَلَامٌ عَلَيْكُمْ"
  },
  {
    "id": 161,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1314.57,
    "end": 1320.47,
    "text": "سَلَامٌ عَلَيْكُمْ طِبْتُمْ فَادْخُلُوهَا خَالِدِينَ"
  },
  {
    "id": 162,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1321.61,
    "end": 1334.09,
    "text": "وَقَالُوا الْحَمْدُ لِلَّهِ الَّذِي صَدَقَنَا وَعْدَهُ وَأَوْرَثَنَا الْأَرْضَ نَتَبَوَّأُ مِنَ الْجَنَّةِ حَيْثُ نَشَاءُ"
  },
  {
    "id": 163,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1335.12,
    "end": 1338.81,
    "text": "أَنِ اعْمَأَجْرُ الْعَامِلِينَ"
  },
  {
    "id": 164,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1339.93,
    "end": 1350.66,
    "text": "وَتَرَى الْمَلَائِكَةَ حَافِّينَ مِنْ حَوْلِ الْعَرْشِ يُسَبِّحُونَ بِحَمْدِ رَبِّهِمْ"
  },
  {
    "id": 165,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1351.84,
    "end": 1354.6,
    "text": "وَقُضِيَ بَيْنَهُمْ بِالْحَقِّ"
  },
  {
    "id": 166,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1355.06,
    "end": 1355.14,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 167,
    "sura_id": 39,
    "recitation_uuid": "7ccf6125-9e99-4696-b637-ed53659665c3",
    "start": 1355.79,
    "end": 1361.3,
    "text": "وَقِيلَ الْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ"
  }
]