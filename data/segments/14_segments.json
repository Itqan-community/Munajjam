[
  {
    "id": 2,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 5.53,
    "end": 9.06,
    "text": "الم"
  },
  {
    "id": 3,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 10.08,
    "end": 24.82,
    "text": "كِتَابٌ أَنْزَلْنَاهُ إِلَيْكَ لِتُخْرِجَ النَّاسَ مِنَ الظُّلُمَاتِ إِلَى النُّورِ بِإِذْنِ رَبِّهِمْ إِلَى صِرَاطِ الْعَزِيزِ الْحَمِيدِ"
  },
  {
    "id": 4,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 25.79,
    "end": 31.23,
    "text": "اللَّهِ الَّذِي لَهُ مَا فِي السَّمَاوَاتِ وَمَا فِي الْأَرْضِ"
  },
  {
    "id": 5,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 32.07,
    "end": 38.03,
    "text": "وَوَيْلٌ لِلْكَافِرِينَ مِنْ عَذَابٍ شَدِيدٍ"
  },
  {
    "id": 6,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 39.05,
    "end": 51.35,
    "text": "الَّذِينَ يَسْتَحِبُّونَ الْحَيَاةَ الدُّنْيَا عَلَى الْآخِرَةِ وَيَصُدُّونَ عَنْ سَبِيلِ اللَّهِ وَيَبْغُونَهَا عِوَجًا"
  },
  {
    "id": 7,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 52.33,
    "end": 57.73,
    "text": "أُولَئِكَ فِي ضَلَالٍ بَعِيدٍ"
  },
  {
    "id": 8,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 59.02,
    "end": 67.17,
    "text": "وَمَا أَرْسَلْنَا مِنْ رَسُولٍ إِلَّا بِلِسَانِ قَوْمِهِ لِيُبَيِّنَ لَهُمْ"
  },
  {
    "id": 9,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 68.25,
    "end": 75.68,
    "text": "سَيُضِلُّ اللَّهُ مَنْ يَشَاءُ وَيَهْدِي مَنْ يَشَاءُ"
  },
  {
    "id": 10,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 76.5,
    "end": 79.92,
    "text": "وَهُوَ الْعَزِيزُ الْحَكِيمُ"
  },
  {
    "id": 11,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 81.21,
    "end": 95.25,
    "text": "وَلَقَدْ أَرْسَلْنَا مُوسَى بِآيَاتِنَا أَنْ أَخْرِجْ قَوْمَكَ مِنَ الظُّلُمَاتِ إِلَى النُّورِ وَذَكِّرْهُمْ بِأَيَّامِ اللَّهِ"
  },
  {
    "id": 12,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 96.25,
    "end": 103.79,
    "text": "إِنَّ فِي ذَلِكَ لَآيَاتٍ لِكُلِّ صَبَّارٍ شَكُورٍ"
  },
  {
    "id": 13,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 104.6,
    "end": 119.69,
    "text": "وَإِذْ قَالَ مُوسَى لِقَوْمِهِ اذْكُرُوا نِعْمَةَ اللَّهِ عَلَيْكُمْ إِذْ أَنْجَاكُمْ مِنْ آلِ فِرْعَوْنَ يَسُومُونَكُمْ سُوءَ الْعَذَابِ"
  },
  {
    "id": 14,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 120.92,
    "end": 132.16,
    "text": "يَسُومُونَكُمْ سُوءَ الْعَذَابِ وَيُذَبِّحُونَ أَبْنَاءَكُمْ وَيَسْتَحْيُونَ نِسَاءَكُمْ"
  },
  {
    "id": 15,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 133.07,
    "end": 140.16,
    "text": "وَفِي ذَلِكُمْ بَلَاءٌ مِنْ رَبِّكُمْ عَظِيمٌ"
  },
  {
    "id": 16,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 141.54,
    "end": 148.15,
    "text": "وَإِذْ تَأَذَّنَ رَبُّكُمْ لَئِنْ شَكَرْتُمْ لَأَزِيدَنَّكُمْ"
  },
  {
    "id": 17,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 149.15,
    "end": 155.01,
    "text": "وَلَئِنْ كَفَرْتُمْ إِنَّ عَذَابِي لَشَدِيدٌ"
  },
  {
    "id": 18,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 155.91,
    "end": 170.54,
    "text": "وَقَالَ مُوسَى إِنْ تَكْفُرُوا أَنْتُمْ وَمَنْ فِي الْأَرْضِ جَمِيعًا فَإِنَّ اللَّهَ لَغَنِيٌّ حَمِيدٌ"
  },
  {
    "id": 19,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 171.88,
    "end": 186.79,
    "text": "أَلَمْ يَأْتِكُمْ نَبَأُ الَّذِينَ مِنْ قَبْلِكُمْ قَوْمِ نُوحٍ وَعَادٍ وَثَمُودَ وَالَّذِينَ مِنْ بَعْدِهِمْ لَا يَعْلَمُهُمْ إِلَّا اللَّهُ"
  },
  {
    "id": 20,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 188.02,
    "end": 197.69,
    "text": "يَا أَتْهُمْ رُسُلُهُمْ بِالْبَيِّنَاتِ فَرَدُّوا أَيْدِيَهُمْ فِي أَفْوَاهِهِمْ"
  },
  {
    "id": 21,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 198.94,
    "end": 211.55,
    "text": "فَرَدُّوا أَيْدِيَهُمْ فِي أَفْوَاهِهِمْ وَقَالُوا إِنَّا كَفَرْنَا بِمَا أُرْسِلْتُمْ بِهِ"
  },
  {
    "id": 22,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 212.88,
    "end": 222.37,
    "text": "وَقَالُوا إِنَّا كَفَرْنَا بِمَا أُرْسِلْتُمْ بِهِ وَإِنَّا لَفِي شَكِرَكٍ"
  },
  {
    "id": 23,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 222.73,
    "end": 229.27,
    "text": "مِمَّا تَدْعُونَنَا إِلَيْهِ مُرِيبٌ"
  },
  {
    "id": 24,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 230.45,
    "end": 237.25,
    "text": "قَالَتْ رُسُلُهُمْ أَفِي اللَّهِ شَكٌّ فَاطِرِ السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 25,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 238.64,
    "end": 249.62,
    "text": "يَدْعُوكُمْ لِيَغْفِرَ لَكُمْ مِنْ ذُنُوبِكُمْ وَيُؤَخِّرَكُمْ إِلَى أَجَلٍ مُسَمًّى"
  },
  {
    "id": 26,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 250.86,
    "end": 259.74,
    "text": "قَالُوا إِنْ أَنْتُمْ إِلَّا بَشَرٌ مِثْلُنَا تُرِيدُونَ أَنْ تَصُدُّونَا"
  },
  {
    "id": 27,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 260.88,
    "end": 268.68,
    "text": "يُرِيدُونَ أَنْ تَصُدُّونَا عَمَّا كَانَ يَعْبُدُ آبَاؤُنَا"
  },
  {
    "id": 28,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 269.02,
    "end": 273.15,
    "text": "يُونَى بِسُلْطَانٍ مُبِينٍ"
  },
  {
    "id": 29,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 274.37,
    "end": 288.3,
    "text": "قَالَتْ لَهُمْ رُسُلُهُمْ إِنْ نَحْنُ إِلَّا بَشَرٌ مِثْلُكُمْ وَلَكِنَّ اللَّهَ يَمُنُّ عَلَى مَنْ يَشَاءُ مِنْ عِبَادِهِ"
  },
  {
    "id": 30,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 289.61,
    "end": 298.41,
    "text": "وَمَا كَانَ لَنَا أَنْ نَأْتِيَكُمْ بِسُلْطَانٍ إِلَّا بِإِذْنِ اللَّهِ"
  },
  {
    "id": 31,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 299.41,
    "end": 304.05,
    "text": "وَعَلَى اللَّهِ فَلْيَتَوَكَّلِ الْمُؤْمِنُونَ"
  },
  {
    "id": 32,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 305.4,
    "end": 313.03,
    "text": "وَمَا لَنَا أَلَّا نَتَوَكَّلَ عَلَى اللَّهِ وَقَدْ هَدَانَا سُبُلَنَا"
  },
  {
    "id": 33,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 313.81,
    "end": 319.65,
    "text": "وَلَنَصْبِرَنَّ عَلَى مَا آبَيْتُمُونَا"
  },
  {
    "id": 34,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 320.36,
    "end": 325.55,
    "text": "وَعَلَى اللَّهِ فَلْيَتَوَكَّلِ الْمُتَوَكِّلُونَ"
  },
  {
    "id": 35,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 326.93,
    "end": 338.55,
    "text": "وَقَالَ الَّذِينَ كَفَرُوا لِرُسُلِهِمْ لَنُخْرِجَنَّكُمْ مِنْ أَرْضِنَا أَوْ لَتَعُودُنَّ فِي مِلَّتِنَا"
  },
  {
    "id": 36,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 339.49,
    "end": 346.92,
    "text": "فَأَوْحَى إِلَيْهِمْ رَبُّهُمْ لَنُهْلِكَنَّ الظَّالِمِينَ"
  },
  {
    "id": 37,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 348.09,
    "end": 352.64,
    "text": "وَلَنُسْكِنَنَّكُمُ الْأَرْضَ مِنْ بَعْدِهِمْ"
  },
  {
    "id": 38,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 353.53,
    "end": 358.64,
    "text": "ذَلِكَ لِمَنْ خَافَ مَقَامِي وَخَافَ وَعِيدِ"
  },
  {
    "id": 39,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 359.78,
    "end": 365.18,
    "text": "وَاسْتَفْتَحُوا وَخَابَ كُلُّ جَبَّارٍ عَنِيدٍ"
  },
  {
    "id": 40,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 366.15,
    "end": 376.45,
    "text": "وَرَائِهِ جَهَنَّمُ وَيُسْقَى مِنْ مَاءٍ صَدِيدٍ"
  },
  {
    "id": 41,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 377.33,
    "end": 388.45,
    "text": "يَتَجَرَّعُهُ وَلَا يَكَادُ يُسِيغُهُ وَيَأْتِيهِ الْمَوْتُ مِنْ كُلِّ مَكَانٍ وَمَا هُوَ بِمَيِّنٍ"
  },
  {
    "id": 42,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 389.41,
    "end": 394.65,
    "text": "وَمِنْ وَرَائِهِ عَذَابٌ غَلِيظٌ"
  },
  {
    "id": 43,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 395.68,
    "end": 399.02,
    "text": "مَثَلُ الَّذِينَ كَفَرُوا بِرَبِّهِمْ"
  },
  {
    "id": 44,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 399.95,
    "end": 406.08,
    "text": "تَعْمَالُهُمْ كَرَمَادٍ اشْتَدَّتْ بِهِ الرِّيحُ فِي يَوْمٍ عَاصِمٍ"
  },
  {
    "id": 45,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 407.29,
    "end": 412.2,
    "text": "لَا يَقْدِرُونَ مِمَّا كَسَبُوا عَلَى شَيْءٍ"
  },
  {
    "id": 46,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 413.23,
    "end": 417.11,
    "text": "ذَلِكَ هُوَ الضَّلَالُ الْبَعِيدُ"
  },
  {
    "id": 47,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 418.22,
    "end": 424.08,
    "text": "أَلَمْ تَرَ أَنَّ اللَّهَ خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ بِالْحَقِّ"
  },
  {
    "id": 48,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 424.51,
    "end": 424.59,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 49,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 425.7,
    "end": 432.34,
    "text": "إِنْ يَشَأْ يُذْهِبْكُمْ وَيَأْتِ بِخَلْقٍ جَدِيدٍ"
  },
  {
    "id": 50,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 433.3,
    "end": 437.56,
    "text": "وَمَا ذَلِكَ عَلَى اللَّهِ بِعَزِيزٍ"
  },
  {
    "id": 51,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 438.69,
    "end": 456.48,
    "text": "وَبَرَزُوا لِلَّهِ جَمِيعًا فَقَالَ الضُّعَفَاءُ لِلَّذِينَ اسْتَكْبَرُوا إِنَّا كُنَّا لَكُمْ تَبَعًا فَهَلْ أَنْتُمْ مُغْنُونَ عَنَّا"
  },
  {
    "id": 52,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 457.69,
    "end": 465.7,
    "text": "فَهَلْ أَنْتُمْ مُغْنُونَ عَنَّا مِنْ عَذَابِ اللَّهِ مِنْ شَيْءٍ"
  },
  {
    "id": 53,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 466.65,
    "end": 470.74,
    "text": "قَالُوا لَوْ هَدَانَا اللَّهُ لَهَدَيْنَاكُمْ"
  },
  {
    "id": 54,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 471.79,
    "end": 481.91,
    "text": "وَلَوَاءٌ عَلَيْنَا أَجَزِعْنَا أَمْ صَبَرْنَا مَا لَنَا مِنْ مَحِيصٍ"
  },
  {
    "id": 55,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 483.19,
    "end": 493.44,
    "text": "وَقَالَ الشَّيْطَانُ لَمَّا قُضِيَ الْأَمْرُ إِنَّ اللَّهَ وَعْدَكُمْ وَعْدَ الْحَقِّ وَوَعَدْتُكُمْ فَأَخْلَى"
  },
  {
    "id": 56,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 493.75,
    "end": 494.2,
    "text": "ذُكُمْ"
  },
  {
    "id": 57,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 495.5,
    "end": 506.04,
    "text": "وَمَا كَانَ لِيَ عَلَيْكُمْ مِنْ سُلْطَانٍ إِلَّا أَنْ دَعَوْتُكُمْ فَاسْتَجَبْتُمْ"
  },
  {
    "id": 58,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 507.03,
    "end": 512.23,
    "text": "فَلَا تَلُومُونِي وَلُو مُو أَنْفُسَكُمْ"
  },
  {
    "id": 59,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 513.42,
    "end": 527.36,
    "text": "مَا أَنَا بِمُصْرِخِكُمْ وَمَا أَنْتُمْ بِمُصْرِخِيَّ إِنِّي كَفَرْتُ بِمَا أَشْرَكْتُمُونِ مِنْ قَبْلُ"
  },
  {
    "id": 60,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 528.44,
    "end": 533.78,
    "text": "إِنَّ الظَّالِمِينَ لَهُمْ عَذَابٌ أَلِيمٌ"
  },
  {
    "id": 61,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 535.09,
    "end": 545.87,
    "text": "وَأُدْخِلَ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ جَنَّاتٍ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ"
  },
  {
    "id": 62,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 547.07,
    "end": 556.01,
    "text": "جَنَّاتٍ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ خَالِدِينَ فِيهَا بِإِذْنِ رَبِّهِمْ"
  },
  {
    "id": 63,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 557.05,
    "end": 561.21,
    "text": "تِحِيَّتُهُمْ فِيهَا سَلَامٌ"
  },
  {
    "id": 64,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 562.85,
    "end": 575.7,
    "text": "أَلَمْ تَرَ كَيْفَ ضَرَبَ اللَّهُ مَثَلًا كَلِمَةً طَيِّبَةً كَشَجَرَةٍ طَيِّبَةٍ أَصْلُهَا ثَابِرٌ"
  },
  {
    "id": 65,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 576.98,
    "end": 582.99,
    "text": "فَاصْلُهَا سَابِتٌ وَفَرْعُهَا فِي السَّمَاءِ"
  },
  {
    "id": 66,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 584.17,
    "end": 584.26,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 67,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 584.58,
    "end": 590.74,
    "text": "تِيءُكُ لَهَا كُلَّ حِينٍ بِإِذْنِ رَبِّهَا"
  },
  {
    "id": 68,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 591.67,
    "end": 599.61,
    "text": "وَيَضْرِبُ اللَّهُ الْأَمْثَالَ لِلنَّاسِ لَعَلَّهُمْ يَتَذَكَّرُونَ"
  },
  {
    "id": 69,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 601.05,
    "end": 614.57,
    "text": "وَمَثَلُ كَلِمَةٍ خَبِيثَةٍ كَشَجَرَةٍ خَبِيثَةٍ اجْتُثَّتْ مِنْ فَوْقِ الْأَرْضِ مَا لَهَا مِنْ قَرَارٍ"
  },
  {
    "id": 70,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 615.79,
    "end": 625.42,
    "text": "يُثَبِّتُ اللَّهُ الَّذِينَ آمَنُوا بِالْقَوْلِ الثَّابِتِ فِي الْحَيَاةِ الدُّنْيَا وَفِي الْآخِرَةِ"
  },
  {
    "id": 71,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 626.5,
    "end": 630.74,
    "text": "وَيُضِلُّ اللَّهُ الظَّالِمِينَ"
  },
  {
    "id": 72,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 631.51,
    "end": 635.64,
    "text": "وَيَفْعَلُ اللَّهُ مَا يَشَاءُ"
  },
  {
    "id": 73,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 636.94,
    "end": 647.64,
    "text": "أَلَمْ تَرَ إِلَى الَّذِينَ بَدَّلُوا نِعْمَتَ اللَّهِ كُفْرًا وَأَحَلُّوا قَوْمَهُمْ دَارَ الْبَوَارِ"
  },
  {
    "id": 74,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 648.59,
    "end": 651.72,
    "text": "جَهَنَّمَ يَصْلَوْنَهَا"
  },
  {
    "id": 75,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 652.43,
    "end": 652.72,
    "text": "وَبِينِ"
  },
  {
    "id": 76,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 653.08,
    "end": 655.48,
    "text": "ذَا الْقَرَارِ"
  },
  {
    "id": 77,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 656.55,
    "end": 663.11,
    "text": "وَجَعَلُوا لِلَّهِ أَنْدَادًا لِيُضِلُّوا عَنْ سَبِيلِهِ"
  },
  {
    "id": 78,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 664.18,
    "end": 670.67,
    "text": "قُلْ تَمَتَّعُوا فَإِنَّ مَصِيرَكُمْ إِلَى النَّارِ"
  },
  {
    "id": 79,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 671.95,
    "end": 681.42,
    "text": "قُلْ لِعِبَادِيَ الَّذِينَ آمَنُوا يُقِيمُوا الصَّلَاةَ وَيُنْفِقُوا مِمَّا رَزَقْنَاهُمْ"
  },
  {
    "id": 80,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 682.68,
    "end": 692.87,
    "text": "وَيُنْفِقُوا مِنْ مَا رَزَقْنَاهُمْ سِرًّا وَعَلَانِيَةً مِنْ قَبْلِ أَنْ يَدَعُونِ"
  },
  {
    "id": 81,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 693.18,
    "end": 698.9,
    "text": "فِيَ يَوْمٌ لَا بَيْعٌ فِيهِ وَلَا خِلَالٌ"
  },
  {
    "id": 82,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 700.1,
    "end": 714.91,
    "text": "اللَّهُ الَّذِي خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ وَأَنْزَلَ مِنَ السَّمَاءِ مَاءً فَأَخْرَجَ بِهِ مِنَ الثَّمَرَاتِ"
  },
  {
    "id": 83,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 715.23,
    "end": 716.12,
    "text": "عَلَّكُمْ"
  },
  {
    "id": 84,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 717.27,
    "end": 722.43,
    "text": "وَسَخَّرَ لَكُمُ الْفُلْكَ لِتَجْرِيَ فِي الْبَحْرِ بِأَمْرِهِ"
  },
  {
    "id": 85,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 723.37,
    "end": 726.6,
    "text": "وَسَخَّرَ لَكُمُ الْأَنْهَارَ"
  },
  {
    "id": 86,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 727.71,
    "end": 734.58,
    "text": "وَسَخَّرَ لَكُمُ الشَّمْسَ وَالْقَمَرَ دَائِبَيْنِ"
  },
  {
    "id": 87,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 735.39,
    "end": 740.49,
    "text": "وَسَخَّرَ لَكُمُ اللَّيْلَ وَالنَّهَارَ"
  },
  {
    "id": 88,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 741.51,
    "end": 747.39,
    "text": "وَآتَاكُمْ مِنْ كُلِّ مَا سَأَلْتُمُونَ"
  },
  {
    "id": 89,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 748.38,
    "end": 752.62,
    "text": "وَإِنْ تَعُدُّوا نِعْمَةَ اللَّهِ"
  },
  {
    "id": 90,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 752.95,
    "end": 753.72,
    "text": "مُوهَى"
  },
  {
    "id": 91,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 754.71,
    "end": 759.26,
    "text": "إِنَّ الْإِنْسَانَ لَظَلُومٌ"
  },
  {
    "id": 92,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 759.64,
    "end": 761.09,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 93,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 762.49,
    "end": 774.88,
    "text": "وَإِذْ قَالَ إِبْرَاهِيمُ رَبِّ اجْعَلْ هَذَا الْبَلَدَ آمِنًا وَاجْنُبْنِي وَبَنِيَّ أَنْ نَعْبُدَ الْأَصْنَامَ"
  },
  {
    "id": 94,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 776.15,
    "end": 783.66,
    "text": "رَبِّ إِنَّهُنَّ أَضْلَلْنَ كَثِيرًا مِنَ النَّارِ"
  },
  {
    "id": 95,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 784.57,
    "end": 789.54,
    "text": "مَنْ تَبِعَنِي فَإِنَّهُ مِنِّي"
  },
  {
    "id": 96,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 790.34,
    "end": 796.36,
    "text": "وَمَنْ عَصَانِي فَإِنَّكَ غَفُورٌ رَحِيمٌ"
  },
  {
    "id": 97,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 797.4,
    "end": 814.49,
    "text": "رَبَّنَا إِنِّي أَسْكَنْتُ مِنْ ذُرِّيَّتِي بِوَادٍ غَيْرِ ذِي زَرْعٍ عِنْدَ بَيْتِكَ الْمُحَرَّمِ رَبَّنَا لِيُقِيمُوا الصَّلَاةَ"
  },
  {
    "id": 98,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 815.79,
    "end": 826.82,
    "text": "رَبَّنَا لِيُقِيمُوا الصَّلَاةَ فَاجْعَلْ أَفْئِدَةً مِنَ النَّاسِ تَهْوِي إِلَيْهِمْ وَارْزُقْهُمْ"
  },
  {
    "id": 99,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 828.11,
    "end": 835.2,
    "text": "تُرْزُقَهُمْ مِنَ الثَّمَرَاتِ لَعَلَّهُمْ يَشْكُرُونَ"
  },
  {
    "id": 100,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 836.57,
    "end": 843.42,
    "text": "رَبَّنَا إِنَّكَ تَعْلَمُ مَا نُخْفِي وَمَا نُعْلِنُ"
  },
  {
    "id": 101,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 844.76,
    "end": 853.63,
    "text": "وَمَا يَخْفَى عَلَى اللَّهِ مِنْ شَيْءٍ فِي الْأَرْضِ وَلَا فِي السَّمَاءِ"
  },
  {
    "id": 102,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 854.72,
    "end": 862.72,
    "text": "الْحَمْدُ لِلَّهِ الَّذِي وَهَبَ لِي عَلَى الْكِبَرِ إِسْمَاعِيلَ وَإِسْحَاقَ"
  },
  {
    "id": 103,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 863.67,
    "end": 869.01,
    "text": "إِنَّ رَبِّينَ سَمِيعُ الدُّعَاءِ"
  },
  {
    "id": 104,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 870.03,
    "end": 876.11,
    "text": "رَبِّ اجْعَلْنِي مُقِيمَ الصَّلَاةِ وَمِنْ ذُرِّيَّتِي"
  },
  {
    "id": 105,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 877.19,
    "end": 881.04,
    "text": "رَبَّنَا وَتَقَبَّلْ دُعَاءِ"
  },
  {
    "id": 106,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 882.04,
    "end": 890.72,
    "text": "رَبَّنَا اغْفِرْ لِي وَلِوَالِدِيَّ وَلِلْمُؤْمِنِينَ يَوْمَ يَقُومُ الْحِسَابُ"
  },
  {
    "id": 107,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 892.2,
    "end": 899.77,
    "text": "وَلَا تَحْسَبَنَّ اللَّهَ غَافِلًا عَمَّا يَعْمَلُ الظَّالِمُونَ"
  },
  {
    "id": 108,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 900.85,
    "end": 908.67,
    "text": "إِنَّمَا يُؤَخِّرُهُمْ لِيَوْمٍ تَشْخَصُ فِيهِ الْأَبْصَارُ"
  },
  {
    "id": 109,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 909.74,
    "end": 916.35,
    "text": "مُهْطِعِينَ مُقْنِعِي رُءُوسِهِمْ لَا يَرْتَدُّ إِلَيْهِمْ طَرْفُونَ"
  },
  {
    "id": 110,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 917.29,
    "end": 920.92,
    "text": "وَأَفْئِدْتُهُمْ هَوَاءٌ"
  },
  {
    "id": 111,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 921.98,
    "end": 930.48,
    "text": "وَأَنْذِرِ النَّاسَ يَوْمَ يَأْتِيهِمُ الْعَذَابُ فَيَقُولُ الَّذِينَ ظَلَمُوا"
  },
  {
    "id": 112,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 931.62,
    "end": 946.08,
    "text": "سَيَقُولُ الَّذِينَ ظَلَمُوا رَبَّنَا أَخِّرْنَا إِلَى أَجَلٍ قَرِيبٍ نُجِبْ دَعْوَتَكَ وَنَتَّبِعِ الرُّسُلُ"
  },
  {
    "id": 113,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 947.38,
    "end": 957.69,
    "text": "أَوَلَمْ تَكُونُوا أَقْسَمْتُمْ مِنْ قَبْلُ مَا لَكُمْ مِنْ زَوَالٍ"
  },
  {
    "id": 114,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 958.69,
    "end": 970.7,
    "text": "وَسَكَنْتُمْ فِي مَسَاكِنِ الَّذِينَ ظَلَمُوا أَنْفُسَهُمْ وَتَبَيَّنَ لَكُمْ كَيْفَ فَعَلْنَا بِهِمْ"
  },
  {
    "id": 115,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 971.86,
    "end": 979.64,
    "text": "وَتَبَيَّنَ لَكُمْ كَيْفَ فَعَلْنَا بِهِمْ وَضَرَبْنَا لَكُمُ الْأَمْثَالَ"
  },
  {
    "id": 116,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 980.88,
    "end": 993.16,
    "text": "وَقَدْ مَكَرُوا مَكْرَهُمْ وَعِنْدَ اللَّهِ مَكْرُهُمْ وَإِنْ كَانَ مَكْرُهُمْ لِتَزُولَ مِنْهُ الْجِبَالُ"
  },
  {
    "id": 117,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 994.5,
    "end": 999.91,
    "text": "فَلَا تَحْسَبَنَّ اللَّهَ مُخْلِفَ وَعْدِهِ رُسُلَهُ"
  },
  {
    "id": 118,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 1000.94,
    "end": 1006.81,
    "text": "إِنَّ اللَّهَ عَزِيزٌ ذُو انْتِقَامٍ"
  },
  {
    "id": 119,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 1007.9,
    "end": 1013.92,
    "text": "يَوْمَ تُبَدَّلُ الْأَرْضُ غَيْرَ الْأَرْضِ وَالسَّمَاوَاتُ"
  },
  {
    "id": 120,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 1014.72,
    "end": 1019.84,
    "text": "وَبَرَزُوا لِلَّهِ الْوَاحِدِ الْقَهَّارِ"
  },
  {
    "id": 121,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 1020.81,
    "end": 1027.93,
    "text": "وَتَرَى الْمُجْرِمِينَ يَوْمَئِذٍ مُقَرَّنِينَ فِي الْأَصْفَادِ"
  },
  {
    "id": 122,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 1028.9,
    "end": 1037.95,
    "text": "يَرَابِيلُهُمْ مِنْ قَطِرَانٍ وَتَغْشَى وُجُوهَهُمُ النَّارُ"
  },
  {
    "id": 123,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 1039.01,
    "end": 1043.6,
    "text": "لِيَجْزِيَ اللَّهُ كُلَّ نَفْسٍ مَا كَسَبَتْ"
  },
  {
    "id": 124,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 1044.37,
    "end": 1049.9,
    "text": "إِنَّ اللَّهَ سَرِيعُ الْحِسَابِ"
  },
  {
    "id": 125,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 1050.86,
    "end": 1063.32,
    "text": "هَذَا بَلَاغٌ لِلنَّاسِ وَلِيُنْذَرُوا بِهِ وَلِيَعْلَمُوا أَنَّمَا هُوَ إِلَهٌ وَاحِدٌ"
  },
  {
    "id": 126,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 1064.61,
    "end": 1072.74,
    "text": "وَلِيَعْلَمُوا أَنَّمَا هُوَ إِلَهٌ وَاحِدٌ وَلِيَزْدَى"
  },
  {
    "id": 127,
    "sura_id": 14,
    "recitation_uuid": "02bbd993-1185-42a2-b8d8-598dcc749f58",
    "start": 1073.06,
    "end": 1076.21,
    "text": "يَرَأُولُ الْأَلْبَابُ"
  }
]