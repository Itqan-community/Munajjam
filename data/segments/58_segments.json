[
  {
    "id": 2,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 5.42,
    "end": 17.76,
    "text": "قَدْ سَمِعَ اللَّهُ قَوْلَ الَّتِي تُجَادِلُكَ فِي زَوْجِهَا وَتَشْتَكِي إِلَى اللَّهِ وَاللَّهُ يَسْمَعُ تَحَاوُرَكُمَا"
  },
  {
    "id": 3,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 19.17,
    "end": 23.86,
    "text": "إِنَّ اللَّهَ سَمِيعٌ بَصِيرٌ"
  },
  {
    "id": 4,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 24.78,
    "end": 35.67,
    "text": "الَّذِينَ يُظَاهِرُونَ مِنْكُمْ مِنْ نِسَائِهِمْ مَا هُنَّ أُمَّهَاتِهِمْ"
  },
  {
    "id": 5,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 37.11,
    "end": 42.53,
    "text": "إِنْ أُمَّهَاتُهُمْ إِلَّا اللَّائِي وَلَدْنَهُمْ"
  },
  {
    "id": 6,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 43.59,
    "end": 50.54,
    "text": "وَإِنَّهُمْ لَيَقُولُونَ مُنْكَرًا مِنَ الْقَوْلِ وَزُورًا"
  },
  {
    "id": 7,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 51.74,
    "end": 56.3,
    "text": "وَإِنَّ اللَّهَ لَعَفُوٌّ غَفُورٌ"
  },
  {
    "id": 8,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 57.76,
    "end": 73.97,
    "text": "وَالَّذِينَ يُظَاهِرُونَ مِنْ نِسَائِهِمْ ثُمَّ يَعُودُونَ لِمَا قَالُوا فَتَحْرِيرُ رَقَبَةٍ مِنْ قَبْلِ أَنْ يَتَمَالَّذِينَ"
  },
  {
    "id": 9,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 74.35,
    "end": 74.69,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 10,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 75.56,
    "end": 78.0,
    "text": "ذَلِكُمْ تُوعَظُونَ"
  },
  {
    "id": 11,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 78.86,
    "end": 83.13,
    "text": "وَاللَّهُ بِمَا تَعْمَلُونَ خَبِيرٌ"
  },
  {
    "id": 12,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 84.28,
    "end": 94.57,
    "text": "فَمَنْ لَمْ يَجِدْ فَصِيَامُ شَهْرَيْنِ مُتَتَابِعَيْنِ مِنْ قَبْلِ أَنْ يَتَمَاسَّا"
  },
  {
    "id": 13,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 95.6,
    "end": 100.64,
    "text": "فَمَنْ لَمْ يَسْتَطِعْ فَإِطْعَامُ سِتِّينَ مِسْكِينًا"
  },
  {
    "id": 14,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 101.63,
    "end": 105.47,
    "text": "ذَلِكَ لِتُؤْمِنُوا بِاللَّهِ وَرَسُولِهِ"
  },
  {
    "id": 15,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 106.44,
    "end": 109.34,
    "text": "وَتِلْكَ حُدُودُ اللَّهِ"
  },
  {
    "id": 16,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 110.21,
    "end": 114.38,
    "text": "وَلِلْكَافِرِينَ عَذَابٌ أَلِيمٌ"
  },
  {
    "id": 17,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 115.74,
    "end": 127.29,
    "text": "إِنَّ الَّذِينَ يُحَادُّونَ اللَّهَ وَرَسُولَهُ كُبِتُوا كَمَا كُبِتَ الَّذِينَ مِنْ قَبْلِهِمْ"
  },
  {
    "id": 18,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 128.42,
    "end": 135.14,
    "text": "وَقَدْ أَنْزَلْنَا آيَاتٍ بَيِّنَاتٍ"
  },
  {
    "id": 19,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 136.06,
    "end": 140.72,
    "text": "وَلِلْكَافِرِينَ عَذَابٌ مُهِينٌ"
  },
  {
    "id": 20,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 141.88,
    "end": 149.28,
    "text": "يَوْمَ يَدْعَثُهُمُ اللَّهُ جَمِيعًا فَيُنَبِّئُهُمْ بِمَا عَمِلُوا"
  },
  {
    "id": 21,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 150.19,
    "end": 153.59,
    "text": "أَحْصَاهُ اللَّهُ وَنَسُوهُ"
  },
  {
    "id": 22,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 154.48,
    "end": 160.08,
    "text": "وَاللَّهُ عَلَى كُلِّ شَيْءٍ شَهِيدٌ"
  },
  {
    "id": 23,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 161.54,
    "end": 168.42,
    "text": "أَلَمْ تَرَ أَنَّ اللَّهَ يَعْلَمُ مَا فِي السَّمَاوَاتِ وَمَا فِي الْأَرْضِ"
  },
  {
    "id": 24,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 169.82,
    "end": 179.84,
    "text": "وَمَا يَكُونُ مِنْ نَجْوَى ثَلَاثَةٍ إِلَّا هُوَ رَابِعُهُمْ وَلَا خَمْسَةٍ إِلَّا هُوَ سَادِسُهُمْ"
  },
  {
    "id": 25,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 181.21,
    "end": 194.4,
    "text": "وَلَا خَمْسَةٍ إِلَّا هُوَ سَادِسُهُمْ وَلَا أَدْنَى مِنْ ذَلِكَ وَلَا أَكْثَرَ إِلَّا هُوَ مَعَهُمْ أَيْنَ مَا كَانُوا"
  },
  {
    "id": 26,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 195.79,
    "end": 201.44,
    "text": "ثُمَّ يُنَبِّئُهُمْ بِمَا عَمِلُوا يَوْمَ الْقِيَامَةِ"
  },
  {
    "id": 27,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 202.46,
    "end": 207.23,
    "text": "إِنَّ اللَّهَ بِكُلِّ شَيْءٍ عَلِيمٌ"
  },
  {
    "id": 28,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 208.68,
    "end": 223.47,
    "text": "أَلَمْ تَرَ إِلَى الَّذِينَ نُهُوا عَنِ النَّجْوَى ثُمَّ يَعُودُونَ لِمَا نُهُوا عَنْهُ وَيَتَنَاجَوْنَ بِالْإِثْمِ وَالْعُدْوَانِ وَمَعْصِيَتِ الرَّسُولِ"
  },
  {
    "id": 29,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 224.99,
    "end": 240.68,
    "text": "وَإِذَا جَاءُوكَ حَيَّوْكَ بِمَا لَمْ يُحَيِّكَ بِهِ اللَّهُ وَيَقُولُونَ فِي أَنْفُسِهِمْ لَوْلَا يُعَذِّبُنَا اللَّهُ بِمَا نَكُونَ"
  },
  {
    "id": 30,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 241.53,
    "end": 245.27,
    "text": "يَسْبُهُمْ جَهَنَّمُ يَصْلَوْنَهَا"
  },
  {
    "id": 31,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 246.13,
    "end": 246.36,
    "text": ""
  },
  {
    "id": 32,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 246.68,
    "end": 248.66,
    "text": "الْمَصِيرُ"
  },
  {
    "id": 33,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 250.06,
    "end": 265.9,
    "text": "يَا أَيُّهَا الَّذِينَ آمَنُوا إِذَا تَنَاجَيْتُمْ فَلَا تَتَنَاجَوْا بِالْإِثْمِ وَالْعُدْوَانِ وَمَعْصِيَتِ الرَّسُولِ وَتَنَاجَوْا بِالْبِرِّ وَالتَّقْوَى"
  },
  {
    "id": 34,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 267.35,
    "end": 273.28,
    "text": "وَاتَّقُوا اللَّهَ الَّذِي إِلَيْهِ تُحْشَرُونَ"
  },
  {
    "id": 35,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 274.48,
    "end": 287.89,
    "text": "إِنَّمَا النَّجْوَى مِنَ الشَّيْطَانِ لِيَحْزُنَ الَّذِينَ آمَنُوا وَلَيْسَ بِضَارِّهِمْ شَيْئًا إِلَّا بِإِذْنِ اللَّهِ"
  },
  {
    "id": 36,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 289.0,
    "end": 293.68,
    "text": "وَعَلَى اللَّهِ فَلْيَتَوَكَّلِ الْمُؤْمِنُونَ"
  },
  {
    "id": 37,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 294.93,
    "end": 306.91,
    "text": "يَا أَيُّهَا الَّذِينَ آمَنُوا إِذَا قِيلَ لَكُمْ تَفَسَّحُوا فِي الْمَجَالِسِ فَافْسَحُوا يَفْسَحِ اللَّهُ لَكُمْ"
  },
  {
    "id": 38,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 308.3,
    "end": 321.07,
    "text": "وَإِذَا قِيلَ انْشُزُوا فَانْشُزُوا يَرْفَعِ اللَّهُ الَّذِينَ آمَنُوا مِنْكُمْ وَالَّذِينَ أُوتُوا الْعِلْمَ دَرَجَانِ"
  },
  {
    "id": 39,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 322.17,
    "end": 326.41,
    "text": "وَاللَّهُ بِمَا تَعْمَلُونَ خَبِيرٌ"
  },
  {
    "id": 40,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 327.47,
    "end": 339.09,
    "text": "يَا أَيُّهَا الَّذِينَ آمَنُوا إِذَا نَاجَيْتُمُ الرَّسُولَ فَقَدِّمُوا بَيْنَ يَدَيْ نَجْوَاكُمْ صَدَقَةً"
  },
  {
    "id": 41,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 340.14,
    "end": 343.08,
    "text": "ذَلِكَ خَيْرٌ لَكُمْ وَأَطْهَرُ"
  },
  {
    "id": 42,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 344.41,
    "end": 350.86,
    "text": "فَإِنْ لَمْ تَجِدُوا فَإِنَّ اللَّهَ غَفُورٌ رَحِيمٌ"
  },
  {
    "id": 43,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 352.53,
    "end": 359.88,
    "text": "أَأَشْفَقْتُمْ أَنْ تُقَدِّمُوا بَيْنَ يَدَيْ نَجْوَاكُمْ صَدَقًا"
  },
  {
    "id": 44,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 361.21,
    "end": 371.93,
    "text": "فَإِذْ لَمْ تَفْعَلُوا وَتَابَ اللَّهُ عَلَيْكُمْ فَأَقِيمُوا الصَّلَاةَ وَآتُوا الزَّكَاةَ وَأَطِيعُوا اللَّهَ وَرَسُولَهُ"
  },
  {
    "id": 45,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 373.07,
    "end": 377.85,
    "text": "وَاللَّهُ خَبِيرٌ بِمَا تَعْمَلُونَ"
  },
  {
    "id": 46,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 379.25,
    "end": 394.44,
    "text": "أَلَمْ تَرَ إِلَى الَّذِينَ تَوَلَّوْا قَوْمًا غَضِبَ اللَّهُ عَلَيْهِمْ مَا هُمْ مِنْكُمْ وَلَا مِنْهُمْ وَيَحْلِفُونَ عَلَى الْكَذِبِ وَهُمْ يَعْلَمُونَ"
  },
  {
    "id": 47,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 396.01,
    "end": 400.45,
    "text": "أَعَدَّ اللَّهُ لَهُمْ عَذَابًا شَدِيدًا"
  },
  {
    "id": 48,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 401.35,
    "end": 406.98,
    "text": "إِنَّهُمْ سَاءَ مَا كَانُوا يَعْمَلُونَ"
  },
  {
    "id": 49,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 408.37,
    "end": 420.46,
    "text": "اتَّخَذُوا أَيْمَانَهُمْ جُنَّةً فَصَدُّوا عَنْ سَبِيلِ اللَّهِ فَلَهُمْ عَذَابٌ مُهِينٌ"
  },
  {
    "id": 50,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 421.64,
    "end": 430.42,
    "text": "لَنْ تُغْنِيَ عَنْهُمْ أَمْوَالُهُمْ وَلَا أَوْلَادُهُمْ مِنَ اللَّهِ شَيْئًا"
  },
  {
    "id": 51,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 431.47,
    "end": 436.41,
    "text": "أُولَئِكَ أَصْحَابُ النَّارِ"
  },
  {
    "id": 52,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 437.31,
    "end": 440.23,
    "text": "فِيهَا خَالِدُونَ"
  },
  {
    "id": 53,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 441.38,
    "end": 454.21,
    "text": "يَوْمَ يَبْعَثُهُمُ اللَّهُ جَمِيعًا فَيَحْلِفُونَ لَهُ كَمَا يَحْلِفُونَ لَكُمْ وَيَحْسَبُونَ أَنَّهُمْ عَلَى شَيْءٍ"
  },
  {
    "id": 54,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 455.57,
    "end": 461.11,
    "text": "كَلَا إِنَّهُمْ هُمُ الْكَاذِبُونَ"
  },
  {
    "id": 55,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 462.31,
    "end": 469.05,
    "text": "يَسْتَحْوَذَ عَلَيْهِمُ الشَّيْطَانُ فَأَنْسَاهُمْ ذِكْرَ اللَّهِ"
  },
  {
    "id": 56,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 469.87,
    "end": 474.26,
    "text": "أُولَئِكَ حِزْبُ الشَّيْطَانِ"
  },
  {
    "id": 57,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 475.23,
    "end": 482.31,
    "text": "أَلَا إِنَّ حِزْبَ الشَّيْطَانِ هُمُ الْخَاسِرُونَ"
  },
  {
    "id": 58,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 483.81,
    "end": 495.36,
    "text": "إِنَّ الَّذِينَ يُحَادُّونَ اللَّهَ وَرَسُولَهُ أُولَئِكَ فِي الْأَذَلِّينَ"
  },
  {
    "id": 59,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 496.54,
    "end": 500.97,
    "text": "كَتَبَ اللَّهُ لَأَغْلِبَنَّ أَنَا وَرُسُلِينَ"
  },
  {
    "id": 60,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 501.8,
    "end": 506.18,
    "text": "إِنَّ اللَّهَ قَوِيٌّ عَزِيزٌ"
  },
  {
    "id": 61,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 507.85,
    "end": 526.3,
    "text": "لَا تَجِدُ قَوْمًا يُؤْمِنُونَ بِاللَّهِ وَالْيَوْمِ الْآخِرِ يُوَدُّونَ مَنْ حَادَّ اللَّهَ وَرَسُولَهُ وَلَوْ كَانُوا آبَاءَهُمْ"
  },
  {
    "id": 62,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 528.13,
    "end": 537.65,
    "text": "وَلَوْ كَانُوا آبَاءَهُمْ أَوْ أَبْنَاءَهُمْ أَوْ إِخْوَانَهُمْ أَوْ عَشِيرَتَهُمْ"
  },
  {
    "id": 63,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 538.82,
    "end": 547.38,
    "text": "أُولَئِكَ كَتَبَ فِي قُلُوبِهِمُ الْإِيمَانَ وَأَيَّدَهُمْ بِرُوحٍ مِنْهُ"
  },
  {
    "id": 64,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 548.72,
    "end": 557.5,
    "text": "وَيُدْخِلُهُمْ جَنَّاتٍ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ خَالِدِينَ فِيهَا"
  },
  {
    "id": 65,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 558.44,
    "end": 561.83,
    "text": "وَضِيَ اللَّهُ عَنْهُمْ وَرَضُوا عَنْهُ"
  },
  {
    "id": 66,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 562.84,
    "end": 566.68,
    "text": "أُولَئِكَ حِزْبُ اللَّهِ"
  },
  {
    "id": 67,
    "sura_id": 58,
    "recitation_uuid": "6aca4ec5-4eae-43b0-9852-15c1f25ff0ce",
    "start": 567.52,
    "end": 574.56,
    "text": "أَنَا إِنَّ حِزْبَ اللَّهِ هُمُ الْمُفْلِحُونَ"
  }
]