[
  {
    "id": 2,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 5.82,
    "end": 8.28,
    "text": "يس"
  },
  {
    "id": 3,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 9.24,
    "end": 12.45,
    "text": "وَالْقُرْآنِ الْحَكِيمِ"
  },
  {
    "id": 4,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 13.4,
    "end": 17.06,
    "text": "إِنَّكَ لَمِنَ الْمُرْسَلِينَ"
  },
  {
    "id": 5,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 18.07,
    "end": 22.03,
    "text": "عَلَى صِرَاطٍ مُسْتَقِيمٍ"
  },
  {
    "id": 6,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 22.79,
    "end": 26.96,
    "text": "تَنْزِيلَ الْعَزِيزِ الرَّحِيمِ"
  },
  {
    "id": 7,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 28.04,
    "end": 38.31,
    "text": "لِتُنْذِرَ قَوْمًا مَا أُنْذِرَ آبَاؤُهُمْ فَهُمْ غَافِلُونَ"
  },
  {
    "id": 8,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 39.01,
    "end": 46.2,
    "text": "لَقَدْ حَقَّ الْقَوْلُ عَلَى أَكْثَرِهِمْ فَهُمْ لَا يُؤْمِنُونَ"
  },
  {
    "id": 9,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 47.14,
    "end": 58.17,
    "text": "إِنَّا جَعَلْنَا فِي أَعْنَاقِهِمْ أَغْلَالًا فَهِيَ إِلَى الْأَذْقَانِ فَهُمْ مُقْمَحُونَ"
  },
  {
    "id": 10,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 59.53,
    "end": 71.92,
    "text": "وَجَعَلْنَا مِنْ بَيْنِ أَيْدِيهِمْ سَدًّا وَمِنْ خَلْفِهِمْ سَدًّا فَأَغْشَيْنَاهُمْ فَهُمْ لَا يُبْصِرُونَ"
  },
  {
    "id": 11,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 73.31,
    "end": 82.81,
    "text": "وَسَوَاءٌ عَلَيْهِمْ أَأَنْذَرْتَهُمْ أَمْ لَمْ تُنْذِرْهُمْ لَا يُؤْمِنُونَ"
  },
  {
    "id": 12,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 83.98,
    "end": 91.91,
    "text": "إِنَّمَا تُنْذِرُ مَنِ اتَّبَعَ الذِّكْرَ وَخَشِيَ الرَّحْمَنَ بِالْغَيْبِ"
  },
  {
    "id": 13,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 92.77,
    "end": 99.12,
    "text": "فَبَشِّرْهُ بِمَغْفِرَةٍ وَأَجْرٍ كَرِيمٍ"
  },
  {
    "id": 14,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 100.09,
    "end": 107.12,
    "text": "إِنَّا نَحْنُ نُحْيِي الْمَوْتَى وَنَكْتُبُ مَا قَدَّمُوا وَآثَارَهُمْ"
  },
  {
    "id": 15,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 107.81,
    "end": 115.19,
    "text": "وَكُلَّ شَيْءٍ أَحْصَيْنَاهُ فِي إِمَامٍ مُبِينٍ"
  },
  {
    "id": 16,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 116.53,
    "end": 125.0,
    "text": "وَاضْرِبْ لَهُمْ مَثَلًا أَصْحَابَ الْقَرْيَةِ إِذْ جَاءَهَا الْمُرْسَلُونَ"
  },
  {
    "id": 17,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 126.22,
    "end": 142.51,
    "text": "إِذْ أَرْسَلْنَا إِلَيْهِمُ اثْنَيْنِ فَكَذَّبُوهُمَا فَعَزَّزْنَا بِثَالِثٍ فَقَالُوا إِنَّا إِلَيْكُمْ مُرْسَلُونَ"
  },
  {
    "id": 18,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 144.1,
    "end": 160.97,
    "text": "قَالُوا مَا أَنْتُمْ إِلَّا بَشَرٌ مِثْلُنَا وَمَا أَنْزَلَ الرَّحْمَنُ مِنْ شَيْءٍ إِنْ أَنْتُمْ إِلَّا تَكْذِبُونَ"
  },
  {
    "id": 19,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 162.08,
    "end": 169.34,
    "text": "قَالُوا رَبُّنَا يَعْلَمُ إِنَّا إِلَيْكُمْ لَمُرْسَلُونَ"
  },
  {
    "id": 20,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 170.16,
    "end": 175.77,
    "text": "وَمَا عَلَيْنَا إِلَّا الْبَلَاغُ الْمُبِينُ"
  },
  {
    "id": 21,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 176.9,
    "end": 181.34,
    "text": "قَالُوا إِنَّا تَطَيَّرْنَا بِكُمْ"
  },
  {
    "id": 22,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 182.32,
    "end": 193.37,
    "text": "وَلَئِنْ لَمْ تَنْتَهُوا لَنَرْجُمَنَّكُمْ وَلَيَمَسَّنَّكُمْ مِنَّا عَذَابٌ أَلِيمٌ"
  },
  {
    "id": 23,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 194.59,
    "end": 200.82,
    "text": "قَالُوا طَائِرُكُمْ مَعَكُمْ أَئِنْ ذُكِّرْتُمْ"
  },
  {
    "id": 24,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 201.74,
    "end": 206.98,
    "text": "بَلْ أَنْتُمْ قَوْمٌ مُسْرِفُونَ"
  },
  {
    "id": 25,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 208.32,
    "end": 218.81,
    "text": "وَجَاءَ مِنْ أَقْصَى الْمَدِينَةِ رَجُلٌ يَسْعَى قَالَ يَا قَوْمِ اتَّبِعُوا الْمُرْسَلِينَ"
  },
  {
    "id": 26,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 220.02,
    "end": 227.23,
    "text": "اتَّبِعُوا مَنْ لَا يَسْأَلُكُمْ أَجْرًا وَهُمْ مُهْتَدُونَ"
  },
  {
    "id": 27,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 228.62,
    "end": 236.13,
    "text": "وَمَا لِيَ لَا أَعْبُدُ الَّذِي فَطَرَنِي وَإِلَيْهِ تُرْجَعُونَ"
  },
  {
    "id": 28,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 237.25,
    "end": 254.3,
    "text": "أَأَتَّخِذُ مِنْ دُونِهِ آلِهَةً إِنْ يُرِدْنِ الرَّحْمَنُ بِضُرٍّ لَا تُغْنِ عَنِّي شَفَاعَتُهُمْ شَيْئًا وَلَا يُنْقِذُونِ"
  },
  {
    "id": 29,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 255.6,
    "end": 261.9,
    "text": "إِنِّي إِذًا لَفِي ضَلَالٍ مُبِينٍ"
  },
  {
    "id": 30,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 262.97,
    "end": 269.51,
    "text": "إِنِّي آمَنْتُ بِرَبِّكُمْ فَاسْمَعُونِ"
  },
  {
    "id": 31,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 270.32,
    "end": 272.95,
    "text": "فِي لَدَخُلِ الْجَنَّةِ"
  },
  {
    "id": 32,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 273.83,
    "end": 278.11,
    "text": "قَالَ يَا لَيْتَ قَوْمِي يَعْلَمُونَ"
  },
  {
    "id": 33,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 279.15,
    "end": 285.25,
    "text": "بِمَا غَفَرَ لِي رَبِّي وَجَعَلَنِي مِنَ الْمُكْرَمِينَ"
  },
  {
    "id": 34,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 286.58,
    "end": 302.52,
    "text": "وَمَا أَنْزَلْنَا عَلَى قَوْمِهِ مِنْ بَعْدِهِ مِنْ جُنْدٍ مِنَ السَّمَاءِ وَمَا كُنَّا مُنْزِلِينَ"
  },
  {
    "id": 35,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 303.8,
    "end": 312.8,
    "text": "إِنْ كَانَتْ إِلَّا صَيْحَةً وَاحِدَةً فَإِذَا هُمْ خَامِدُونَ"
  },
  {
    "id": 36,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 314.11,
    "end": 317.42,
    "text": "يَا حَسْرَةً عَلَى الْعِبَادِ"
  },
  {
    "id": 37,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 318.26,
    "end": 326.46,
    "text": "لَا يَأْتِيهِمْ مِنْ رَسُولٍ إِلَّا كَانُوا بِهِ يَسْتَهْزِئُونَ"
  },
  {
    "id": 38,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 327.85,
    "end": 338.18,
    "text": "أَلَمْ يَرَوْا كَمْ أَهْلَكْنَا قَبْلَهُمْ مِنَ الْقُرُونِ أَنَّهُمْ إِلَيْهِمْ لَا يَرْجِعُونَ"
  },
  {
    "id": 39,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 339.4,
    "end": 346.41,
    "text": "وَإِنْ كُلٌّ لَمَّا جَمِيعٌ لَدَيْنَا مُحْضَرُونَ"
  },
  {
    "id": 40,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 347.56,
    "end": 356.74,
    "text": "وَآيَةٌ لَهُمُ الْأَرْضُ الْمَيْتَةُ أَحْيَيْنَاهَا وَأَخْرَجْنَا مِنْهَا حَبًّا فَمِنْهُ"
  },
  {
    "id": 41,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 357.07,
    "end": 358.63,
    "text": "عُيُونٍ"
  },
  {
    "id": 42,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 359.98,
    "end": 372.07,
    "text": "وَجَعَلْنَا فِيهَا جَنَّاتٍ مِنْ نَخِيلٍ وَأَعْنَابٍ وَفَجَّرْنَا فِيهَا مِنَ الْعُيُونِ"
  },
  {
    "id": 43,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 373.35,
    "end": 378.8,
    "text": "لِيَأْكُلُوا مِنْ ثَمَرِهِ وَمَا عَمِلَتْهُ أَيْدِيهِمْ"
  },
  {
    "id": 44,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 379.81,
    "end": 382.82,
    "text": "أَفَلَا يَشْكُرُونَ"
  },
  {
    "id": 45,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 383.9,
    "end": 397.49,
    "text": "سُبْحَانَ الَّذِي خَلَقَ الْأَزْوَاجَ كُلَّهَا مِمَّا تُنْبِتُ الْأَرْضُ وَمِنْ أَنْفُسِهِمْ وَمِمَّا لَا يَعْلَمُونَ"
  },
  {
    "id": 46,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 399.2,
    "end": 408.09,
    "text": "وَآيَةٌ لَهُمُ اللَّيْلُ نَسْلَخُ مِنْهُ النَّهَارَ فَإِذَا هُمْ مُظْلِمُونَ"
  },
  {
    "id": 47,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 409.16,
    "end": 413.05,
    "text": "وَالشَّمْسُ تَجْرِي لِمُسْتَقَرٍّ لَهَا"
  },
  {
    "id": 48,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 413.98,
    "end": 418.66,
    "text": "ذَلِكَ تَقْدِيرُ الْعَزِيزِ الْعَلِيمِ"
  },
  {
    "id": 49,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 419.75,
    "end": 427.55,
    "text": "وَالْقَمَرَ قَدَّرْنَاهُ مَنَازِلَ حَتَّى عَادَ كَالْعُرْجُونِ الْقَدِيمِ"
  },
  {
    "id": 50,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 428.88,
    "end": 439.31,
    "text": "لَا الشَّمْسُ يَنْبَغِي لَهَا أَنْ تُدْرِكَ الْقَمَرَ وَلَا اللَّيْلُ سَابِقُ النَّهَى"
  },
  {
    "id": 51,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 440.17,
    "end": 445.24,
    "text": "وَكُلٌّ فِي فَلَكٍ يَسْبَحُونَ"
  },
  {
    "id": 52,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 446.42,
    "end": 454.52,
    "text": "وَآيَةٌ لَهُمْ أَنَّا حَمَلْنَا ذُرِّيَّتَهُمْ فِي الْفُلْكِ الْمَشْحُونِ"
  },
  {
    "id": 53,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 455.48,
    "end": 461.43,
    "text": "وَخَلَقْنَا لَهُمْ مِنْ مِثْلِهِ مَا يَرْكَبُونَ"
  },
  {
    "id": 54,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 462.77,
    "end": 470.98,
    "text": "وَإِنْ نَشَأْ نُغْرِقْهُمْ فَلَا صَرِيخَ لَهُمْ وَلَا هُمْ يُنْقَذُونَ"
  },
  {
    "id": 55,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 472.16,
    "end": 478.91,
    "text": "إِلَّا رَحْمَةً مِنَّا وَمَتَاعًا إِلَى حِينٍ"
  },
  {
    "id": 56,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 480.35,
    "end": 490.07,
    "text": "وَإِذَا قِيلَ لَهُمُ اتَّقُوا مَا بَيْنَ أَيْدِيكُمْ وَمَا خَلْفَكُمْ لَعَلَّكُمْ تُرْحَمُونَ"
  },
  {
    "id": 57,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 491.18,
    "end": 502.07,
    "text": "وَمَا تَأْتِيهِمْ مِنْ آيَةٍ مِنْ آيَاتِ رَبِّهِمْ إِلَّا كَانُوا عَنْهَا مُعْرِضِينَ"
  },
  {
    "id": 58,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 503.44,
    "end": 512.09,
    "text": "وَإِذَا قِيلَ لَهُمْ أَنْفِقُوا مِمَّا رَزَقَكُمُ اللَّهُ قَالَ الَّذِينَ كَفَرُوا"
  },
  {
    "id": 59,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 513.02,
    "end": 530.79,
    "text": "قَالَ الَّذِينَ كَفَرُوا لِلَّذِينَ آمَنُوا أَنُطْعِمُ مَنْ لَوْ يَشَاءُ اللَّهُ أَطْعَمَهُ إِنْ أَنْتُمْ إِلَّا فِي ضَلَالٍ مُبِينٍ"
  },
  {
    "id": 60,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 531.9,
    "end": 540.21,
    "text": "وَيَقُولُونَ مَتَى هَذَا الْوَعْدُ إِنْ كُنْتُمْ صَادِقِينَ"
  },
  {
    "id": 61,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 541.14,
    "end": 551.26,
    "text": "مَا يَنْظُرُونَ إِلَّا صَيْحَةً وَاحِدَةً تَأْخُذُهُمْ وَهُمْ يَخِصِّمُونَ"
  },
  {
    "id": 62,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 552.5,
    "end": 562.31,
    "text": "فَلَا يَسْتَطِيعُونَ تَوْصِيَةً وَلَا إِلَى أَهْلِهِمْ يَرْجِعُونَ"
  },
  {
    "id": 63,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 563.65,
    "end": 572.38,
    "text": "وَنُفِخَ فِي الصُّورِ فَإِذَا هُمْ مِنَ الْأَجْدَاثِ إِلَى رَبِّهِمْ يَنْسِلُونَ"
  },
  {
    "id": 64,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 573.51,
    "end": 579.52,
    "text": "قَالُوا يَا وَيْلَنَا مَنْ بَعَثَنَا مِنْ مَرْقَدِنَا"
  },
  {
    "id": 65,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 580.26,
    "end": 586.23,
    "text": "هَذَا مَا وَعَدَ الرَّحْمَنُ وَصَدَقَ الْمُرْسَلُونَ"
  },
  {
    "id": 66,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 587.24,
    "end": 597.59,
    "text": "إِنْ كَانَتْ إِلَّا صَيْحَةً وَاحِدَةً فَإِذَا هُمْ جَمِيعٌ لَدَيْنَا مُحْضَرُونَ"
  },
  {
    "id": 67,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 598.75,
    "end": 609.72,
    "text": "فَالْيَوْمَ لَا تُظْلَمُ نَفْسٌ شَيْئًا وَلَا تُجْزَوْنَ إِلَّا مَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 68,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 610.97,
    "end": 618.77,
    "text": "إِنَّ أَصْحَابَ الْجَنَّةِ الْيَوْمَ فِي شُغُلٍ فَاكِهُونَ"
  },
  {
    "id": 69,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 619.4,
    "end": 627.86,
    "text": "هُمْ وَأَزْوَاجُهُمْ فِي ظِلَالٍ عَلَى الْأَرَائِكِ مُتَّكِئُونَ"
  },
  {
    "id": 70,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 629.02,
    "end": 635.79,
    "text": "لَهُمْ فِيهَا فَاكِهَةٌ وَلَهُمْ مَا يَدَّعُونَ"
  },
  {
    "id": 71,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 636.46,
    "end": 642.1,
    "text": "بَلَى مُنْقَوْنًا مِنْ رَبٍّ رَحِيمٍ"
  },
  {
    "id": 72,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 643.49,
    "end": 648.27,
    "text": "وَامْتَازُوا الْيَوْمَ أَيُّهَا الْمُجْرِمُونَ"
  },
  {
    "id": 73,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 649.13,
    "end": 657.24,
    "text": "أَلَمْ أَعْهَدْ إِلَيْكُمْ يَا بَنِي آدَمَ أَنْ لَا تَعْبُدُوا الشَّيْطَانَ"
  },
  {
    "id": 74,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 658.03,
    "end": 663.11,
    "text": "إِنَّهُ لَكُمْ عَدُوٌّ مُبِينٌ"
  },
  {
    "id": 75,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 663.9,
    "end": 665.67,
    "text": "وَأَنِ اعْبُدُونِي"
  },
  {
    "id": 76,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 666.42,
    "end": 670.63,
    "text": "هَذَا صِرَاطٌ مُسْتَقِيمٌ"
  },
  {
    "id": 77,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 671.81,
    "end": 677.18,
    "text": "وَلَقَدْ أَضَلَّ مِنْكُمْ جِبِلًّا كَثِيرًا"
  },
  {
    "id": 78,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 677.98,
    "end": 681.71,
    "text": "أَفَلَمْ تَكُونُوا تَعْقِلُونَ"
  },
  {
    "id": 79,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 682.54,
    "end": 688.98,
    "text": "هَذِهِ جَهَنَّمُ الَّتِي كُنْتُمْ تُوعَدُونَ"
  },
  {
    "id": 80,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 689.99,
    "end": 695.51,
    "text": "اصْلَوْهَا الْيَوْمَ بِمَا كُنْتُمْ تَكْفُرُونَ"
  },
  {
    "id": 81,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 696.9,
    "end": 710.6,
    "text": "الْيَوْمَ نَخْتِمُ عَلَى أَفْوَاهِهِمْ وَتُكَلِّمُنَا أَيْدِيهِمْ وَتَشْهَدُ أَرْجُلُهُمْ بِمَا كَانُوا يَكْسِبُونَ"
  },
  {
    "id": 82,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 711.77,
    "end": 722.67,
    "text": "وَلَوْ نَشَاءُ لَطَمَسْنَا عَلَى أَعْيُنِهِمْ فَاسْتَبَقُوا الصِّرَاطَ فَأَنَّى يُبْصِرُونَ"
  },
  {
    "id": 83,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 723.68,
    "end": 734.81,
    "text": "وَلَوْ نَشَاءُ لَمَسَخْنَاهُمْ عَلَى مَكَانَتِهِمْ فَمَا اسْتَطَاعُوا مُضِيًّا وَلَا يَرْجِعُونَ"
  },
  {
    "id": 84,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 735.79,
    "end": 740.34,
    "text": "وَمَنْ نُعَمِّرْهُ نُنَكِّسْهُ فِي الْخَلْقِ"
  },
  {
    "id": 85,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 741.42,
    "end": 744.09,
    "text": "أَفَلَا يَعْقِلُونَ"
  },
  {
    "id": 86,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 744.91,
    "end": 750.04,
    "text": "وَمَا عَلَّمْنَاهُ الشِّعْرَ وَمَا يَنْبَغِي"
  },
  {
    "id": 87,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 751.09,
    "end": 757.0,
    "text": "إِنْ هُوَ إِلَّا ذِكْرٌ وَقُرْآنٌ مُبِينٌ"
  },
  {
    "id": 88,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 758.2,
    "end": 766.04,
    "text": "لِيُنْذِرَ مَنْ كَانَ حَيًّا وَيَحِقَّ الْقَوْلُ عَلَى الْكَافِرِينَ"
  },
  {
    "id": 89,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 767.13,
    "end": 780.48,
    "text": "أَوَلَمْ يَرَوْا أَنَّا خَلَقْنَا لَهُمْ مِمَّا عَمِلَتْ أَيْدِينَا أَنْعَامًا فَهُمْ لَهَا مَالِكُونَ"
  },
  {
    "id": 90,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 781.18,
    "end": 788.41,
    "text": "وَذَلَّلْنَاهَا لَهُمْ فَمِنْهَا رَكُوبُهُمْ وَمِنْهَا يَأْكُلُونَ"
  },
  {
    "id": 91,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 789.38,
    "end": 793.05,
    "text": "وَلَهُمْ فِيهَا مَنَافِعُ وَمَشَارِبُ"
  },
  {
    "id": 92,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 793.77,
    "end": 796.43,
    "text": "أَفَلَا يَشْكُرُونَ"
  },
  {
    "id": 93,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 797.34,
    "end": 805.29,
    "text": "وَاتَّخَذُوا مِنْ دُونِ اللَّهِ آلِهَةً لَعَلَّهُمْ يُنْصَرُونَ"
  },
  {
    "id": 94,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 806.17,
    "end": 813.97,
    "text": "لَا يَسْتَطِيعُونَ نَصْرَهُمْ وَهُمْ لَهُمْ جُنْدٌ مُحْضَرُونَ"
  },
  {
    "id": 95,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 814.96,
    "end": 818.04,
    "text": "فَلَا يَحْزُنْكَ قَوْلُهُمْ"
  },
  {
    "id": 96,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 818.79,
    "end": 824.87,
    "text": "إِنَّا نَعْلَمُ مَا يُسِرُّونَ وَمَا يُعْلِنُونَ"
  },
  {
    "id": 97,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 826.28,
    "end": 837.65,
    "text": "أَوَلَمْ يَرَ الْإِنْسَانُ أَنَّا خَلَقْنَاهُ مِنْ نُطْفَةٍ فَإِذَا هُوَ خَصِيمٌ مُبِينٌ"
  },
  {
    "id": 98,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 838.84,
    "end": 842.92,
    "text": "وَضَرَبَ لَنَا مَثَلًا وَنَسِيَ خَلْقَهُ"
  },
  {
    "id": 99,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 843.82,
    "end": 848.9,
    "text": "قَالَ مَنْ يُحْيِي الْعِظَامَ وَهِيَ رَمِيمٌ"
  },
  {
    "id": 100,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 849.7,
    "end": 856.78,
    "text": "قُلْ يُحْيِيهَا الَّذِي أَنْشَأَهَا أَوَّلَ مَرَّةٍ"
  },
  {
    "id": 101,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 857.64,
    "end": 861.27,
    "text": "وَهُوَ بِكُلِّ خَلْقٍ عَلِيمٍ"
  },
  {
    "id": 102,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 862.03,
    "end": 874.16,
    "text": "الَّذِي جَعَلَ لَكُمْ مِنَ الشَّجَرِ الْأَخْضَرِ نَارًا فَإِذَا أَنْتُمْ مِنْهُ تُوقِدُونَ"
  },
  {
    "id": 103,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 874.86,
    "end": 884.73,
    "text": "أَوَلَيْسَ الَّذِي خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ بِقَادِرٍ عَلَى أَنْ يَخْلُقَ مِثْلَهُمْ"
  },
  {
    "id": 104,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 885.41,
    "end": 889.59,
    "text": "بَلَى وَهُوَ الْخَلَّاقُ الْعَلِيمُ"
  },
  {
    "id": 105,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 890.5,
    "end": 902.44,
    "text": "إِنَّمَا أَمْرُهُ إِذَا أَرَادَ شَيْئًا أَنْ يَقُولَ لَهُ كُنْ فَيَكُونُ"
  },
  {
    "id": 106,
    "sura_id": 36,
    "recitation_uuid": "f353968d-f146-4601-82b9-de58289ceff9",
    "start": 903.26,
    "end": 912.25,
    "text": "فَسُبْحَانَ الَّذِي بِيَدِهِ مَلَكُوتُ كُلِّ شَيْءٍ وَإِلَيْهِ تُرْجَعُونَ"
  }
]