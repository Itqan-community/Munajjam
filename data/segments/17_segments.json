[
  {
    "id": 2,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 5.9,
    "end": 22.11,
    "text": "سُبْحَانَ الَّذِي أَسْرَى بِعَبْدِهِ لَيْلًا مِنَ الْمَسْجِدِ الْحَرَامِ إِلَى الْمَسْجِدِ الْأَقْصَى الَّذِي بَارَكْنَا حَوْلَهُ لِنُرِيَهُ مِنْ آيَاتِنَا"
  },
  {
    "id": 3,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 22.91,
    "end": 27.68,
    "text": "إِنَّهُ هُوَ السَّمِيعُ الْبَصِيرُ"
  },
  {
    "id": 4,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 28.71,
    "end": 41.88,
    "text": "وَآتَيْنَا مُوسَى الْكِتَابَ وَجَعَلْنَاهُ هُدًى لِبَنِي إِسْرَائِيلَ أَلَّا تَتَّخِذُوا مِنْ دُونِي وَكِيلًا"
  },
  {
    "id": 5,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 43.05,
    "end": 46.92,
    "text": "ذُرِّيَّةَ مَنْ حَمَلْنَا مَعْلُوما"
  },
  {
    "id": 6,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 47.83,
    "end": 52.63,
    "text": "إِنَّهُ كَانَ عَبْدًا شَكُورًا"
  },
  {
    "id": 7,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 53.75,
    "end": 69.93,
    "text": "وَقَضَيْنَا إِلَى بَنِي إِسْرَائِيلَ فِي الْكِتَابِ لَتُفْسِدُنَّ فِي الْأَرْضِ مَرَّتَيْنِ وَلَتَعْلُنَّ عُلُوًّا كَبِيرًا"
  },
  {
    "id": 8,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 71.23,
    "end": 86.75,
    "text": "فَإِذَا جَاءَ وَعْدُ أُولَاهُمَا بَعَثْنَا عَلَيْكُمْ عِبَادًا لَنَا أُولِي بَأْسٍ شَدِيدٍ فَجَاسُوا خِلَالَ الدِّيَارِ"
  },
  {
    "id": 9,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 87.72,
    "end": 91.15,
    "text": "وَكَانَ وَعْدًا مَفْعُولًا"
  },
  {
    "id": 10,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 92.12,
    "end": 104.89,
    "text": "ثُمَّ رَدَدْنَا لَكُمُ الْكَرَّةَ عَلَيْهِمْ وَأَمْدَدْنَاكُمْ بِأَمْوَالٍ وَبَنِينَ وَجَعَلْنَاكُمْ أَكْثَرَ نَفِيرًا"
  },
  {
    "id": 11,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 105.76,
    "end": 111.25,
    "text": "إِنْ أَحْسَنْتُمْ أَحْسَنْتُمْ لِأَنْفُسِكُمْ"
  },
  {
    "id": 12,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 112.12,
    "end": 114.48,
    "text": "وَإِنْ أَسَأْتُمْ فَلَهَى"
  },
  {
    "id": 13,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 115.61,
    "end": 127.84,
    "text": "فَإِذَا جَاءَ وَعْدُ الْآخِرَةِ لِيَسُوءُوا وُجُوهَكُمْ وَلِيَدْخُلُوا الْمَسْجِدَ كَمَا دَخَلُوهُ أَوَّلَ مَرَّةٍ"
  },
  {
    "id": 14,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 128.9,
    "end": 137.66,
    "text": "وَلِيَدْخُلُوا الْمَسْجِدَ كَمَا دَخَلُوهُ أَوَّلَ مَرَّةٍ وَلِيُتَبِّرُوا مَا عَلَوْا"
  },
  {
    "id": 15,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 138.0,
    "end": 138.78,
    "text": "فِي رَامْعِينَ"
  },
  {
    "id": 16,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 139.67,
    "end": 143.14,
    "text": "عَسَى رَبُّكُمْ أَنْ يَرْحَمَكُمْ"
  },
  {
    "id": 17,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 143.94,
    "end": 146.06,
    "text": "وَإِنْ عُدْتُمْ عُدْنَى"
  },
  {
    "id": 18,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 146.85,
    "end": 152.18,
    "text": "وَجَعَلْنَا جَهَنَّمَ لِلْكَافِرِينَ حَصِيرًا"
  },
  {
    "id": 19,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 153.16,
    "end": 161.87,
    "text": "إِنَّ هَذَا الْقُرْآنَ يَهْدِي لِلَّتِي هِيَ أَقْوَمُ وَيُبَشِّرُ الْمُؤْمِنِينَ"
  },
  {
    "id": 20,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 162.92,
    "end": 172.58,
    "text": "وَيُبَشِّرُ الْمُؤْمِنِينَ الَّذِينَ يَعْمَلُونَ الصَّالِحَاتِ أَنَّ لَهُمْ أَجْرًا كَبِيرًا"
  },
  {
    "id": 21,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 173.49,
    "end": 182.03,
    "text": "وَأَنَّ الَّذِينَ لَا يُؤْمِنُونَ بِالْآخِرَةِ أَعْتَدْنَا لَهُمْ عَذَابًا أَلِيمًا"
  },
  {
    "id": 22,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 183.0,
    "end": 190.12,
    "text": "وَيَدْعُ الْإِنْسَانُ بِالشَّرِّ دُعَاءَهُ بِالْخَيْرِ"
  },
  {
    "id": 23,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 190.91,
    "end": 194.46,
    "text": "وَكَانَ الْإِنْسَانُ عَجُولًا"
  },
  {
    "id": 24,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 195.41,
    "end": 199.89,
    "text": "وَجَعَلْنَا اللَّيْلَ وَالنَّهَارَ آيَتَيْنِ"
  },
  {
    "id": 25,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 200.73,
    "end": 212.74,
    "text": "فَمَحَوْنَا آيَةَ اللَّيْلِ وَجَعَلْنَا آيَةَ النَّهَارِ مُبْصِرَةً لِتَبْتَغُوا فَضْلًا مِنْ رَبِّكُمْ"
  },
  {
    "id": 26,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 213.74,
    "end": 222.06,
    "text": "لِتَبْتَغُوا فَضْلًا مِنْ رَبِّكُمْ وَلِتَعْلَمُوا عَدَدَ السِّنِينَ وَالْحِسَابَ"
  },
  {
    "id": 27,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 222.88,
    "end": 226.28,
    "text": "وَكُلَّ شَيْءٍ فَصَّلْنَاهُ"
  },
  {
    "id": 28,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 226.64,
    "end": 227.44,
    "text": "وَالْمِينَارِ"
  },
  {
    "id": 29,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 228.33,
    "end": 234.67,
    "text": "وَكُلَّ إِنْسَانٍ أَلْزَمْنَاهُ طَائِرَهُ فِي عُنُقِهِ"
  },
  {
    "id": 30,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 235.54,
    "end": 242.85,
    "text": "وَنُخْرِجُ لَهُ يَوْمَ الْقِيَامَةِ كِتَابًا يَلْقَاهُ مَنْشُورًا"
  },
  {
    "id": 31,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 243.84,
    "end": 250.05,
    "text": "اقْرَأْ كِتَابَكَ كَفَى بِنَفْسِكَ الْيَوْمَ عَلَيْكَ حَسِيبًا"
  },
  {
    "id": 32,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 251.15,
    "end": 255.38,
    "text": "فَنِ اهْتَدَى فَإِنَّمَا يَهْتَدِي لِنَفْسِهِ"
  },
  {
    "id": 33,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 256.12,
    "end": 260.93,
    "text": "وَمَنْ ضَلَّ فَإِنَّمَا يَضِلُّ عَلَيْهَا"
  },
  {
    "id": 34,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 261.8,
    "end": 265.71,
    "text": "وَلَا تَزِرُ وَازِرَةٌ وِزْرَ أُخْرَى"
  },
  {
    "id": 35,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 266.52,
    "end": 272.3,
    "text": "وَمَا كُنَّا مُعَذِّبِينَ حَتَّى نَبْعَثَ رَسُولًا"
  },
  {
    "id": 36,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 273.15,
    "end": 283.59,
    "text": "وَإِذَا أَرَدْنَا أَنْ نُهْلِكَ قَرْيَةً أَمَرْنَا مُتْرَفِيهَا فَفَسَقُوا فِيهَا"
  },
  {
    "id": 37,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 283.92,
    "end": 289.13,
    "text": "وَعَلَيْهَا الْقَوْلُ فَدَمَّرْنَاهَا تَدْمِيرًا"
  },
  {
    "id": 38,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 290.05,
    "end": 295.68,
    "text": "وَكَمْ أَهْلَكْنَا مِنَ الْقُرُونِ مِنْ بَعْدِ نُوحٍ"
  },
  {
    "id": 39,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 296.38,
    "end": 303.43,
    "text": "وَكَفَى بِرَبِّكَ بِذُنُوبِ عِبَادِهِ خَبِيرًا بَصِيرًا"
  },
  {
    "id": 40,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 304.64,
    "end": 318.33,
    "text": "مَنْ كَانَ يُرِيدُ الْعَاجِلَةَ عَجَّلْنَا لَهُ فِيهَا مَا نَشَاءُ لِمَنْ نُرِيدُ ثُمَّ جَعَلْنَا لَهُ جَهَنَّمَ"
  },
  {
    "id": 41,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 319.48,
    "end": 327.3,
    "text": "ثُمَّ جَعَلْنَا لَهُ جَهَنَّمَ يَصْلَاهَا مَذْمُومًا مَدْحُورًا"
  },
  {
    "id": 42,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 328.5,
    "end": 340.67,
    "text": "وَمَنْ أَرَادَ الْآخِرَةَ وَسَعَى لَهَا سَعْيَهَا وَهُوَ مُؤْمِنٌ فَأُولَئِكَ كَانَ سَعْيُهُمْ مَشْكُورًا"
  },
  {
    "id": 43,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 341.64,
    "end": 352.57,
    "text": "كُلًّا نُمِدُّ هَؤُلَاءِ وَهَؤُلَاءِ مِنْ عَطَاءِ رَبِّي"
  },
  {
    "id": 44,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 353.39,
    "end": 357.31,
    "text": "وَمَا كَانَ عَطَاءُ رَبِّكَ"
  },
  {
    "id": 45,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 357.68,
    "end": 358.43,
    "text": "وَأُورًا"
  },
  {
    "id": 46,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 359.37,
    "end": 364.35,
    "text": "انْظُرْ كَيْفَ فَضَّلْنَا بَعْضَهُمْ عَلَى بَعْضٍ"
  },
  {
    "id": 47,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 365.47,
    "end": 371.82,
    "text": "وَلَلْآخِرَةُ أَكْبَرُ دَرَجَاتٍ وَأَكْبَرُ تَفْضِيلًا"
  },
  {
    "id": 48,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 373.01,
    "end": 381.11,
    "text": "لَا تَجْعَلْ مَعَ اللَّهِ إِلَهًا آخَرَ فَتَقْعُدَ مَذْمُومًا مَخْذُولًا"
  },
  {
    "id": 49,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 382.23,
    "end": 391.69,
    "text": "وَقَضَى رَبُّكَ أَلَّا تَعْبُدُوا إِلَّا إِيَّاهُ وَبِالْوَالِدِينِ إِحْسَانًا"
  },
  {
    "id": 50,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 392.79,
    "end": 403.93,
    "text": "إِمَّا يَبْلُغَنَّ عِنْدَكَ الْكِبَرَ أَحَدُهُمَا أَوْ كِلَاهُمَا فَلَا تَقُلْ لَهُمَا"
  },
  {
    "id": 51,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 404.88,
    "end": 414.65,
    "text": "فَلَا تَقُلْ لَهُمَا أُفٍّ وَلَا تَنْهَرْهُمَا وَقُلْ لَهُمَا قَوْلًا كَرِيمًا"
  },
  {
    "id": 52,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 415.75,
    "end": 425.64,
    "text": "وَاخْفِضْ لَهُمَا جَنَاحَ الذُّلِّ مِنَ الرَّحْمَةِ وَقُلْ رَبِّ ارْحَمْهُمَا كَمَا رَبَّيَانِي صَغِيرًا"
  },
  {
    "id": 53,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 426.74,
    "end": 430.51,
    "text": "رَبُّكُمْ أَعْلَمُ بِمَا فِي نُفُوسِكُمْ"
  },
  {
    "id": 54,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 431.37,
    "end": 439.24,
    "text": "إِنْ تَكُونُوا صَالِحِينَ فَإِنَّهُ كَانَ لِلْأَوَّابِينَ غَفُورًا"
  },
  {
    "id": 55,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 440.2,
    "end": 448.63,
    "text": "وَآتِ ذَا الْقُرْبَى حَقَّهُ وَالْمِسْكِينَ وَابْنَ السَّبِيلِ وَلَا تُبَذِّرْ تَبْذِيرًا"
  },
  {
    "id": 56,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 449.63,
    "end": 456.49,
    "text": "إِنَّ الْمُبَذِّرِينَ كَانُوا إِخْوَانَ الشَّيَاطِينِ"
  },
  {
    "id": 57,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 457.27,
    "end": 461.61,
    "text": "وَكَانَ الشَّيْطَانُ لِرَبِّهِ كَفُورًا"
  },
  {
    "id": 58,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 462.65,
    "end": 474.91,
    "text": "وَإِمَّا تُعْرِضَنَّ عَنْهُمُ ابْتِغَاءَ رَحْمَةٍ مِنْ رَبِّكَ تَرْجُوهَا فَقُلْ لَهُمْ قَوْلًا مَيْسُورًا"
  },
  {
    "id": 59,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 476.13,
    "end": 486.16,
    "text": "وَلَا تَجْعَلْ يَدَكَ مَغْلُولَةً إِلَى عُنُقِكَ وَلَا تَبْسُطْهَا كُلَّ الْبَسْطِ فَتَقْعُدَ مَلُومًا"
  },
  {
    "id": 60,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 486.52,
    "end": 487.21,
    "text": "وَالطُّورَى"
  },
  {
    "id": 61,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 488.33,
    "end": 495.05,
    "text": "إِنَّ رَبَّكَ يَبْسُطُ الرِّزْقَ لِمَنْ يَشَاءُ وَيَقْدِرُ"
  },
  {
    "id": 62,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 495.97,
    "end": 502.03,
    "text": "إِنَّهُ كَانَ بِعِبَادِهِ خَبِيرًا بَصِيرًا"
  },
  {
    "id": 63,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 503.23,
    "end": 508.78,
    "text": "وَلَا تَقْتُلُوا أَوْلَادَكُمْ خَشْيَةَ إِمْلَاقٍ"
  },
  {
    "id": 64,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 509.55,
    "end": 512.37,
    "text": "نَحْنُ نَرْزُقُهُمْ وَإِيَّاكُمْ"
  },
  {
    "id": 65,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 513.13,
    "end": 518.0,
    "text": "إِنَّ قَتْلَهُمْ كَانَ خِطْئًا كَبِيرًا"
  },
  {
    "id": 66,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 518.9,
    "end": 520.94,
    "text": "وَلَا تَقْرَبُوا الزِّنَا"
  },
  {
    "id": 67,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 521.76,
    "end": 528.33,
    "text": "إِنَّهُ كَانَ فَاحِشَةً وَسَاءَ سَبِيلًا"
  },
  {
    "id": 68,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 529.27,
    "end": 534.98,
    "text": "وَلَا تَقْتُلُوا النَّفْسَ الَّتِي حَرَّمَ اللَّهُ إِلَّا بِالْحَقِّ"
  },
  {
    "id": 69,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 535.49,
    "end": 535.57,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 70,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 536.48,
    "end": 545.16,
    "text": "وَمَنْ قُتِلَ مَظْلُومًا فَقَدْ جَعَلْنَا لِوَلِيِّهِ سُلْطَانًا فَلَا يُسْرِ"
  },
  {
    "id": 71,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 545.47,
    "end": 546.16,
    "text": "وَبِرْقَتْ"
  },
  {
    "id": 72,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 546.99,
    "end": 551.31,
    "text": "إِنَّهُ كَانَ مَنْصُورًا"
  },
  {
    "id": 73,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 552.16,
    "end": 560.2,
    "text": "وَلَا تَقْرَبُوا مَالَ الْيَتِيمِ إِلَّا بِالَّتِي هِيَ أَحْسَنُ حَتَّى يَبْلُغَ أَشُدَّهُ"
  },
  {
    "id": 74,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 561.1,
    "end": 562.88,
    "text": "وَأَوْفُوا بِالْعَهْدِ"
  },
  {
    "id": 75,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 563.69,
    "end": 567.25,
    "text": "إِنَّ الْعَهْدَ كَانَ مَسْئُولًا"
  },
  {
    "id": 76,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 568.42,
    "end": 575.24,
    "text": "وَأَوْفُوا الْكَيْلَ إِذَا كِلْتُمْ وَزِنُوا بِالْقِسْطَاسِ الْمُسْتَقِيمِ"
  },
  {
    "id": 77,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 576.2,
    "end": 580.47,
    "text": "ذَلِكَ خَيْرٌ وَأَحْسَنُ تَأْوِيلًا"
  },
  {
    "id": 78,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 581.35,
    "end": 584.68,
    "text": "وَلَا تَقْفُ مَا لَيْسَ لَكَ بِهِ عِلْمٌ"
  },
  {
    "id": 79,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 585.66,
    "end": 593.18,
    "text": "إِنَّ السَّمْعَ وَالْبَصَرَ وَالْفُؤَادَ كُلُّ أُولَئِكَ كَانَ عَنْهُمْ"
  },
  {
    "id": 80,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 593.54,
    "end": 594.33,
    "text": "وَأُولَى"
  },
  {
    "id": 81,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 595.59,
    "end": 598.45,
    "text": "وَلَا تَمْشِ فِي الْأَرْضِ مَرَحًا"
  },
  {
    "id": 82,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 599.27,
    "end": 606.54,
    "text": "إِنَّكَ لَنْ تَخْرِقَ الْأَرْضَ وَلَنْ تَبْلُغَ الْجِبَالَ طُولًا"
  },
  {
    "id": 83,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 607.37,
    "end": 613.63,
    "text": "كُلُّ ذَلِكَ كَانَ سَيِّئُهُ عِنْدَ رَبِّكَ مَكْرُوهًا"
  },
  {
    "id": 84,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 615.03,
    "end": 621.85,
    "text": "ذَلِكَ مِمَّا أَوْحَى إِلَيْكَ رَبُّكَ مِنَ الْحِكْمَةِ"
  },
  {
    "id": 85,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 622.9,
    "end": 632.76,
    "text": "وَلَا تَجْعَلْ مَعَ اللَّهِ إِلَهًا آخَرَ فَتُلْقَى فِي جَهَنَّمَ مَلُومًا مَدْحُورًا"
  },
  {
    "id": 86,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 634.03,
    "end": 642.33,
    "text": "أَفَأَصْفَاكُمْ رَبُّكُمْ بِالْبَنِينَ وَاتَّخَذَ مِنَ الْمَلَائِكَةِ إِنَاثًا"
  },
  {
    "id": 87,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 643.28,
    "end": 647.7,
    "text": "إِنَّكُمْ لَتَقُولُونَ قَوْلًا عَظِيمًا"
  },
  {
    "id": 88,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 648.77,
    "end": 657.75,
    "text": "وَلَقَدْ صَرَّفْنَا فِي هَذَا الْقُرْآنِ لِيَذَّكَّرُوا وَمَا يَزِيدُهُمْ إِلَّا نُفُورًا"
  },
  {
    "id": 89,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 658.7,
    "end": 669.12,
    "text": "قُلْ لَوْ كَانَ مَعَهُ آلِهَةٌ كَمَا يَقُولُونَ إِذًا لَابْتَغَوْا إِلَى ذِي الْعَرْشِ سَبِيلًا"
  },
  {
    "id": 90,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 670.08,
    "end": 677.67,
    "text": "سُبْحَانَهُ وَتَعَالَى عَمَّا يَقُولُونَ عُلُوًّا كَبِيرًا"
  },
  {
    "id": 91,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 678.69,
    "end": 685.64,
    "text": "تُسَبِّحُ لَهُ السَّمَاوَاتُ السَّبْعُ وَالْأَرْضُ وَمَنْ فِيهِنَّ"
  },
  {
    "id": 92,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 686.91,
    "end": 696.39,
    "text": "وَإِنْ مِنْ شَيْءٍ إِلَّا يُسَبِّحُ بِحَمْدِهِ وَلَكِنْ لَا تَفْقَهُونَ تَسْبِيحَهُمْ"
  },
  {
    "id": 93,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 697.27,
    "end": 701.49,
    "text": "إِنَّهُ كَانَ حَلِيمًا غَفُورًا"
  },
  {
    "id": 94,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 702.48,
    "end": 712.7,
    "text": "وَإِذَا قَرَأْتَ الْقُرْآنَ جَعَلْنَا بَيْنَكَ وَبَيْنَ الَّذِينَ لَا يُؤْمِنُونَ بِالْآخِرَةِ حِجَابًا"
  },
  {
    "id": 95,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 713.02,
    "end": 713.72,
    "text": "سُورًا"
  },
  {
    "id": 96,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 714.85,
    "end": 724.08,
    "text": "وَجَعَلْنَا عَلَى قُلُوبِهِمْ أَكِنَّةً أَنْ يَفْقَهُوهُ وَفِي آذَانِهِمْ وَقْرًا"
  },
  {
    "id": 97,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 725.07,
    "end": 734.26,
    "text": "وَإِذَا ذَكَرْتَ رَبَّكَ فِي الْقُرْآنِ وَحْدَهُ وَلَّوْا عَلَى أَدْبَارِهِمْ نُفُورًا"
  },
  {
    "id": 98,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 735.38,
    "end": 751.35,
    "text": "نَحْنُ أَعْلَمُ بِمَا يَسْتَمِعُونَ بِهِ إِذْ يَسْتَمِعُونَ إِلَيْكَ وَإِذْ هُمْ نَجْوَى إِذْ يَقُولُ الظَّالِمُونَ إِنْ تَتَّبِعُونَ إِلَّا رَجُلًا"
  },
  {
    "id": 99,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 751.68,
    "end": 752.42,
    "text": "عُورًا"
  },
  {
    "id": 100,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 753.56,
    "end": 762.38,
    "text": "انْظُرْ كَيْفَ ضَرَبُوا لَكَ الْأَمْثَالَ فَضَلُّوا فَلَا يَسْتَطِيعُونَ سَبِيلًا"
  },
  {
    "id": 101,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 763.48,
    "end": 774.53,
    "text": "وَقَالُوا أَإِذَا كُنَّا عِظَامًا وَرُفَاتًا أَإِنَّا لَمَبْعُوثُونَ خَلْقًا جَدِيدًا"
  },
  {
    "id": 102,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 775.26,
    "end": 778.85,
    "text": "قُلْ كُونُوا حِجَارَةً أَوْ حَدِيدًا"
  },
  {
    "id": 103,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 779.73,
    "end": 784.75,
    "text": "لَوْ خَلْقًا مِمَّا يَكْبُرُ فِي صُدُورِكُمْ"
  },
  {
    "id": 104,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 785.6,
    "end": 789.12,
    "text": "فَسَيَقُولُونَ مَنْ يُعِيدُنَا"
  },
  {
    "id": 105,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 789.88,
    "end": 793.13,
    "text": "قُلِ الَّذِي فَطَرَكُمْ أَوَّلَ مَرَّةٍ"
  },
  {
    "id": 106,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 794.1,
    "end": 799.78,
    "text": "فَسَيُنْغِضُونَ إِلَيْكَ رُءُوسَهُمْ وَيَقُولُونَ مَتَاهُونَ"
  },
  {
    "id": 107,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 800.71,
    "end": 805.17,
    "text": "قُلْ عَسَى أَنْ يَكُونَ قَرِيبًا"
  },
  {
    "id": 108,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 806.01,
    "end": 815.6,
    "text": "يَوْمَ يَدْعُوكُمْ فَتَسْتَجِيبُونَ بِحَمْدِهِ وَتَظُنُّونَ إِنْ لَبِثْتُمْ إِلَّا قَلِيلًا"
  },
  {
    "id": 109,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 816.52,
    "end": 820.71,
    "text": "وَقُلْ لِعِبَادِي يَقُولُوا الَّتِي هِيَ أَحْسَنُ"
  },
  {
    "id": 110,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 821.47,
    "end": 825.23,
    "text": "إِنَّ الشَّيْطَانَ يَنْزَغُ بَيْنَهُمْ"
  },
  {
    "id": 111,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 826.05,
    "end": 832.67,
    "text": "إِنَّ الشَّيْطَانَ كَانَ لِلْإِنْسَانِ عَدُوًّا مُبِينًا"
  },
  {
    "id": 112,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 833.71,
    "end": 835.85,
    "text": "رَبُّكُمْ أَعْلَمُ بِكُمْ"
  },
  {
    "id": 113,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 836.69,
    "end": 841.66,
    "text": "إِنْ يَشَأْ يَرْحَمْكُمْ أَوْ إِنْ يَشَأْ يُعَذِّبْكُمْ"
  },
  {
    "id": 114,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 842.55,
    "end": 847.32,
    "text": "وَمَا أَرْسَلْنَاكَ عَلَيْهِمْ وَكِيلًا"
  },
  {
    "id": 115,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 848.44,
    "end": 853.2,
    "text": "وَرَبُّكَ أَعْلَمُ بِمَنْ فِي السَّمَاوَاتِ وَالْأَرْضِ"
  },
  {
    "id": 116,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 854.02,
    "end": 859.13,
    "text": "وَلَقَدْ فَضَّلْنَا بَعْضَ النَّبِيِّينَ عَلَى بَعْضٍ"
  },
  {
    "id": 117,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 860.03,
    "end": 863.41,
    "text": "وَآتَيْنَا دَاوُودَ زَبُورًا"
  },
  {
    "id": 118,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 864.52,
    "end": 875.4,
    "text": "قُلِ ادْعُوا الَّذِينَ زَعَمْتُمْ مِنْ دُونِهِ فَلَا يَمْلِكُونَ كَشْفَ الضُّرِّ عَنْكُمْ وَلَا تَحْوِيلًا"
  },
  {
    "id": 119,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 876.61,
    "end": 889.79,
    "text": "أُولَئِكَ الَّذِينَ يَدْعُونَ يَبْتَغُونَ إِلَى رَبِّهِمُ الْوَسِيلَةَ أَيُّهُمْ أَقْرَبُ وَيَرْجُونَ رَحْمَتَهُ وَيَخَافُونَ عَذَابًا"
  },
  {
    "id": 120,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 891.13,
    "end": 895.47,
    "text": "إِنَّ عَذَابَ رَبِّكَ كَانَ مَحْذُورًا"
  },
  {
    "id": 121,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 896.67,
    "end": 908.46,
    "text": "وَإِنْ مِنْ قَرْيَةٍ إِلَّا نَحْنُ مُهْلِكُوهَا قَبْلَ يَوْمِ الْقِيَامَةِ أَوْ مُعَذِّبُوهَا عَذَابًا شَدِيدًا"
  },
  {
    "id": 122,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 909.26,
    "end": 911.97,
    "text": "كَانَ ذَلِكَ فِي الْكِتَابِ"
  },
  {
    "id": 123,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 912.32,
    "end": 913.09,
    "text": "وَأُورًا"
  },
  {
    "id": 124,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 914.31,
    "end": 924.71,
    "text": "وَمَا مَنَعَنَا أَنْ نُرْسِلَ بِالْآيَاتِ إِلَّا أَنْ كَذَّبَ بِهَا الْأَوَّلُونَ"
  },
  {
    "id": 125,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 925.89,
    "end": 932.4,
    "text": "وَآتَيْنَا ثَمُودَ النَّاقَةَ مُبْصِرَةً فَظَلَمُوا بِهَا"
  },
  {
    "id": 126,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 933.15,
    "end": 937.87,
    "text": "وَمَا نُرْسِلُ بِالْآيَاتِ إِلَّا تَخْوِيفًا"
  },
  {
    "id": 127,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 938.88,
    "end": 944.68,
    "text": "وَإِذْ قُلْنَا لَكَ إِنَّ رَبَّكَ أَحَاطَ بِالنَّاسِ"
  },
  {
    "id": 128,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 945.51,
    "end": 957.17,
    "text": "وَمَا جَعَلْنَا الرُّؤْيَا الَّتِي أَرَيْنَاكَ إِلَّا فِتْنَةً لِلنَّاسِ وَالشَّجَرَةَ الْمَلْعُونَةَ فِي الْقُرْآنِ"
  },
  {
    "id": 129,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 958.23,
    "end": 965.05,
    "text": "وَنُخَوِّفُهُمْ فَمَا يَزِيدُهُمْ إِلَّا طُغْيَانًا كَبِيرًا"
  },
  {
    "id": 130,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 966.5,
    "end": 979.14,
    "text": "وَإِذْ قُلْنَا لِلْمَلَائِكَةِ اسْجُدُوا لِآدَمَ فَسَجَدُوا إِلَّا إِبْلِيسَ قَالَ أَأَسْجُدُ لِمَنْ خَلَقْتَ طِينًا"
  },
  {
    "id": 131,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 980.29,
    "end": 988.97,
    "text": "قَالَ أَرَأَيْتَكَ هَذَا الَّذِي كَرَّمْتَ عَلَيَّ لَئِنْ أَخَّرْتَنِ إِلَى يَوْمِ الْقِيَامَةِ"
  },
  {
    "id": 132,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 989.33,
    "end": 994.88,
    "text": "ذَلِكَنَّ ذُرِّيَّتَهُ إِلَّا قَلِيلًا"
  },
  {
    "id": 133,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 995.79,
    "end": 1007.47,
    "text": "قَالَ اذْهَبْ فَمَنْ تَبِعَكَ مِنْهُمْ فَإِنَّ جَهَنَّمَ جَزَاؤُكُمْ جَزَاءً مَوْفُورًا"
  },
  {
    "id": 134,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1008.53,
    "end": 1022.08,
    "text": "وَاسْتَفْزِزْ مَنِ اسْتَطَعْتَ مِنْهُمْ بِصَوْتِكَ وَأَجْلِبْ عَلَيْهِمْ بِخَيْلِكَ وَرَجِلِكَ وَشَارِكْهُمْ فِي الْأَمْوَالِ وَالْأَوْلَادِ وَعِدْهُمْ"
  },
  {
    "id": 135,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1023.23,
    "end": 1027.65,
    "text": "وَمَا يَعِدُهُمُ الشَّيْطَانُ إِلَّا غُرُورًا"
  },
  {
    "id": 136,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1028.68,
    "end": 1033.9,
    "text": "إِنَّ عِبَادِي لَيْسَ لَكَ عَلَيْهِمْ سُلْطَانٌ"
  },
  {
    "id": 137,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1034.69,
    "end": 1037.62,
    "text": "وَكَفَى بِرَبِّكَ وَكِيلًا"
  },
  {
    "id": 138,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1038.48,
    "end": 1045.98,
    "text": "رَبُّكُمُ الَّذِي يُزْجِي لَكُمُ الْفُلْكَ فِي الْبَحْرِ لِتَبْتَغُوا مِنْ فَضْلِهِ"
  },
  {
    "id": 139,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1046.86,
    "end": 1050.33,
    "text": "إِنَّهُ كَانَ بِكُمْ رَحِيمًا"
  },
  {
    "id": 140,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1051.42,
    "end": 1059.99,
    "text": "وَإِذَا مَسَّكُمُ الضُّرُّ فِي الْبَحْرِ ضَلَّ مَنْ تَدْعُونَ إِلَّا إِيَّاهُ"
  },
  {
    "id": 141,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1060.91,
    "end": 1065.4,
    "text": "فَلَمَّا نَجَّاكُمْ إِلَى الْبَرِّ أَعْرَضْتُمْ"
  },
  {
    "id": 142,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1066.26,
    "end": 1069.73,
    "text": "وَكَانَ الْإِنْسَانُ كَفُورًا"
  },
  {
    "id": 143,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1070.82,
    "end": 1083.08,
    "text": "أَفَأَمِنْتُمْ أَنْ يَخْسِفَ بِكُمْ جَانِبَ الْبَرِّ أَوْ يُرْسِلَ عَلَيْكُمْ حَاصِبًا ثُمَّ لَا تَجِدُوا لَكُمْ وَكِيلًا"
  },
  {
    "id": 144,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1084.14,
    "end": 1094.52,
    "text": "أَمْ أَمِنْتُمْ أَنْ يُعِيدَكُمْ فِيهِ تَارَةً أُخْرَى فَيُرْسِلَ عَلَيْكُمْ قَاصِفًا مِنَ الرِّيحِ"
  },
  {
    "id": 145,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1095.71,
    "end": 1108.74,
    "text": "وَيُرْسِلْ عَلَيْكُمْ قَاصِفًا مِنَ الرِّيحِ فَيُغْرِقَكُمْ بِمَا كَفَرْتُمْ ثُمَّ لَا تَجِدُوا لَكُمْ عَلَيْنَا بِهِ تَبِيعًا"
  },
  {
    "id": 146,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1109.7,
    "end": 1120.53,
    "text": "وَلَقَدْ كَرَّمْنَا بَنِي آدَمَ وَحَمَلْنَاهُمْ فِي الْبَرِّ وَالْبَحْرِ وَرَزَقْنَاهُمْ مِنَ الطَّيِّبَاتِ"
  },
  {
    "id": 147,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1121.64,
    "end": 1131.06,
    "text": "وَرَزَقْنَاهُمْ مِنَ الطَّيِّبَاتِ وَفَضَّلْنَاهُمْ عَلَى كَثِيرٍ مِمَّنْ خَلَقْنَا"
  },
  {
    "id": 148,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1131.43,
    "end": 1132.2,
    "text": "وَالْمُبْتَرِينَ"
  },
  {
    "id": 149,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1133.09,
    "end": 1137.68,
    "text": "يَوْمَ نَدْعُو كُلَّ أُنَاسٍ بِإِمَامِهِمْ"
  },
  {
    "id": 150,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1138.58,
    "end": 1149.48,
    "text": "فَمَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ فَأُولَئِكَ يَقْرَءُونَ كِتَابَهُمْ وَلَا يُظْلَمُونَ فَتِيلًا"
  },
  {
    "id": 151,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1150.44,
    "end": 1159.54,
    "text": "وَمَنْ كَانَ فِي هَذِهِ أَعْمَى فَهُوَ فِي الْآخِرَةِ أَعْمَى وَأَضَلُّ سَبِيلًا"
  },
  {
    "id": 152,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1160.5,
    "end": 1170.52,
    "text": "وَإِنْ كَادُوا لَيَفْتِنُونَكَ عَنِ الَّذِي أَوْحَيْنَا إِلَيْكَ لِتَفْتَرِيَ عَلَيْنَا غَيْرًا"
  },
  {
    "id": 153,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1171.46,
    "end": 1174.5,
    "text": "وَإِذًا لَاتَّخَذُوكَ خَلِيلًا"
  },
  {
    "id": 154,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1175.33,
    "end": 1184.29,
    "text": "وَلَوْلَا أَنْ ثَبَّتْنَاكَ لَقَدْ كِدْتَ تَرْكَنُ إِلَيْهِمْ شَيْئًا قَلِيلًا"
  },
  {
    "id": 155,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1185.31,
    "end": 1194.84,
    "text": "إِذًا لَأَذَقْنَاكَ ضِعْفَ الْحَيَاةِ وَضِعْفَ الْمَمَاتِ ثُمَّ لَا تَجِدُ لَكَ عَلَيْنَا نَصِيرًا"
  },
  {
    "id": 156,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1195.66,
    "end": 1202.39,
    "text": "وَإِنْ كَادُوا لَيَسْتَفِزُّونَكَ مِنَ الْأَرْضِ لِيُخْرِجُوكَ مِنْهَا"
  },
  {
    "id": 157,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1203.22,
    "end": 1208.22,
    "text": "وَإِذًا لَا يَلْبَسُونَ خِلَافَكَ إِلَّا قَلِيلًا"
  },
  {
    "id": 158,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1209.32,
    "end": 1214.8,
    "text": "جُنَّةَ مَنْ قَدْ أَرْسَلْنَا قَبْلَكَ مِنْ رُسُلِنَا"
  },
  {
    "id": 159,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1215.86,
    "end": 1220.17,
    "text": "وَلَا تَجِدُ لِسُنَّتِنَا تَحْوِيلًا"
  },
  {
    "id": 160,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1221.12,
    "end": 1227.82,
    "text": "ذَقِ مِصْصَلَاتَ لِدُلُوكِ الشَّمْسِ إِلَى غَسَقِ اللَّيْلِ وَقُرْآنَ الْفَجْرِ"
  },
  {
    "id": 161,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1228.73,
    "end": 1232.33,
    "text": "إِنَّ قُرْآنَ الْفَجْرِ كَانَ"
  },
  {
    "id": 162,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1232.67,
    "end": 1233.43,
    "text": "وَعُودًا"
  },
  {
    "id": 163,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1234.71,
    "end": 1245.62,
    "text": "وَمِنَ اللَّيْلِ فَتَهَجَّدْ بِهِ نَافِلَةً لَكَ عَسَى أَنْ يَبْعَثَكَ رَبُّكَ مَقَامًا مَحْمُودًا"
  },
  {
    "id": 164,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1246.79,
    "end": 1253.41,
    "text": "وَقُلْ رَبِّ أَدْخِلْنِي مُدْخَلَ صِدْقٍ وَأَخْرِجْنِي مُخْرَجَ صِدْقٍ"
  },
  {
    "id": 165,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1254.49,
    "end": 1263.17,
    "text": "وَأَخْرِجْنِي مُخْرَجَ صِدْقٍ وَاجْعَلْ لِي مِنْ لَدُنْكَ سُلْطَانًا نَصِيرًا"
  },
  {
    "id": 166,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1264.02,
    "end": 1268.11,
    "text": "وَقُلْ جَاءَ الْحَقُّ وَزَهَقَ الْبَاطِلُ"
  },
  {
    "id": 167,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1268.75,
    "end": 1272.62,
    "text": "إِنَّ الْبَاطِلَ كَانَ زَهُوقًا"
  },
  {
    "id": 168,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1273.71,
    "end": 1286.33,
    "text": "وَنُنَزِّلُ مِنَ الْقُرْآنِ مَا هُوَ شِفَاءٌ وَرَحْمَةٌ لِلْمُؤْمِنِينَ وَلَا يَزِيدُ الظَّالِمِينَ إِلَّا خَسَارًا"
  },
  {
    "id": 169,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1287.45,
    "end": 1298.07,
    "text": "وَإِذَا أَنْعَمْنَا عَلَى الْإِنْسَانِ أَعْرَضَ وَنَأَى بِجَانِبِهِ وَإِذَا مَسَّهُ الشَّرُّ كَانَ يَئُوسًا"
  },
  {
    "id": 170,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1299.29,
    "end": 1307.82,
    "text": "قُلْ كُلٌّ يَعْمَلُ عَلَى شَاكِلَتِهِ فَرَبُّكُمْ أَعْلَمُ بِمَنْ هُوَ أَهْدَى سَبِيلًا"
  },
  {
    "id": 171,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1308.86,
    "end": 1311.68,
    "text": "وَيَسْأَلُونَكَ عَنِ الرُّوحِ"
  },
  {
    "id": 172,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1312.61,
    "end": 1321.57,
    "text": "قُلِ الرُّوحُ مِنْ أَمْرِ رَبِّي وَمَا أُوتِيتُمْ مِنَ الْعِلْمِ إِلَّا قَلِيلًا"
  },
  {
    "id": 173,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1322.71,
    "end": 1335.41,
    "text": "وَلَئِنْ شِئْنَا لَنَذْهَبَنَّ بِالَّذِي أَوْحَيْنَا إِلَيْكَ ثُمَّ لَا تَجِدُ لَكَ بِهِ عَلَيْنَا وَكِيلًا"
  },
  {
    "id": 174,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1336.31,
    "end": 1339.38,
    "text": "إِلَّا رَحْمَةً مِنْ رَبِّي"
  },
  {
    "id": 175,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1340.23,
    "end": 1344.45,
    "text": "إِنَّ فَضْلَهُ كَانَ عَلَيْكَ كَبِيرًا"
  },
  {
    "id": 176,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1345.44,
    "end": 1356.89,
    "text": "قُلْ لَئِنِ اجْتَمَعَتِ الْإِنْسُ وَالْجِنُّ عَلَى أَنْ يَأْتُوا بِمِثْلِ هَذَا الْقُرْآنِ لَا يَأْتُونَ بِمِثْلِهِ"
  },
  {
    "id": 177,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1357.94,
    "end": 1364.72,
    "text": "لَا يَأْتُونَ بِمِثْلِهِ وَلَوْ كَانَ بَعْضُهُمْ لِبَعْضٍ ظَهِيرًا"
  },
  {
    "id": 178,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1365.79,
    "end": 1378.44,
    "text": "وَلَقَدْ صَرَّفْنَا لِلنَّاسِ فِي هَذَا الْقُرْآنِ مِنْ كُلِّ مَثَلٍ فَأَبَى أَكْثَرُ النَّاسِ إِلَّا كُفُورًا"
  },
  {
    "id": 179,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1379.47,
    "end": 1386.64,
    "text": "وَقَالُوا لَنْ نُؤْمِنَ لَكَ حَتَّى تَفْجُرَ لَنَا مِنَ الْأَرْضِ يَنْبُوعًا"
  },
  {
    "id": 180,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1387.65,
    "end": 1397.29,
    "text": "أَوْ تَكُونَ لَكَ جَنَّةٌ مِنْ نَخِيلٍ وَعِنَبٍ فَتُفَجِّرَ الْأَنْهَارَ خِلَالَهَا"
  },
  {
    "id": 181,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1397.63,
    "end": 1398.36,
    "text": "ذِيرًا"
  },
  {
    "id": 182,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1399.61,
    "end": 1410.89,
    "text": "أَوْ تُسْقِطَ السَّمَاءَ كَمَا زَعَمْتَ عَلَيْنَا كِسَفًا أَوْ تَأْتِيَ بِاللَّهِ وَالْمَلَائِكَةِ قَبِيلًا"
  },
  {
    "id": 183,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1412.02,
    "end": 1426.71,
    "text": "لَوْ يَكُونَ لَكَ بَيْتٌ مِنْ زُخْرُفٍ أَوْ تَرْقَى فِي السَّمَاءِ وَلَنْ نُؤْمِنَ لِرُقِيِّكَ حَتَّى تُنَزِّلَ عَلَيْنَا كِتَابًا نَقْرَؤُونَ"
  },
  {
    "id": 184,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1427.89,
    "end": 1434.26,
    "text": "قُلْ سُبْحَانَ رَبِّي هَلْ كُنْتُ إِلَّا بَشَرًا رَسُولًا"
  },
  {
    "id": 185,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1435.43,
    "end": 1449.27,
    "text": "وَمَا مَنَعَ النَّاسَ أَنْ يُؤْمِنُوا إِذْ جَاءَهُمُ الْهُدَى إِلَّا أَنْ قَالُوا أَبَعَثَ اللَّهُ بَشَرًا رَسُولًا"
  },
  {
    "id": 186,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1450.14,
    "end": 1463.66,
    "text": "قُلْ لَوْ كَانَ فِي الْأَرْضِ مَلَائِكَةٌ يَمْشُونَ مُطْمَئِنِّينَ لَنَزَّلْنَا عَلَيْهِمْ مِنَ السَّمَاءِ مَلَكًا رَسُولًا"
  },
  {
    "id": 187,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1464.76,
    "end": 1469.73,
    "text": "قُلْ كَفَى بِاللَّهِ شَهِيدًا بَيْنِي وَبَيْنَكُمْ"
  },
  {
    "id": 188,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1470.63,
    "end": 1476.34,
    "text": "إِنَّهُ كَانَ بِعِبَادِهِ خَبِيرًا بَصِيرًا"
  },
  {
    "id": 189,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1477.29,
    "end": 1480.74,
    "text": "وَمَنْ يَهْدِ اللَّهُ فَهُوَ الْمُهْتَدِ"
  },
  {
    "id": 190,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1481.97,
    "end": 1488.58,
    "text": "وَمَنْ يُضْلِلْ فَلَنْ تَجِدَ لَهُمْ أَوْلِيَاءَ مِنْ دُونِهِ"
  },
  {
    "id": 191,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1489.52,
    "end": 1498.13,
    "text": "وَنَحْشُرُهُمْ يَوْمَ الْقِيَامَةِ عَلَى وُجُوهِهِمْ عُمْيًا وَبُكْمًا وَصُمًّا"
  },
  {
    "id": 192,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1499.15,
    "end": 1501.75,
    "text": "مَأْوَاهُمْ جَهَنَّمُ"
  },
  {
    "id": 193,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1502.61,
    "end": 1506.29,
    "text": "ثُلَّمَا خَبَتْ زِدْنَاهُمْ سَعِيرًا"
  },
  {
    "id": 194,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1507.24,
    "end": 1515.03,
    "text": "ذَلِكَ جَزَاؤُهُمْ بِأَنَّهُمْ كَفَرُوا بِآيَاتِنَا وَقَالُوا"
  },
  {
    "id": 195,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1516.11,
    "end": 1526.76,
    "text": "وَقَالُوا أَإِذَا كُنَّا عِظَامًا وَرُفَاتًا أَإِنَّا لَمَبْعُوثُونَ خَلْقًا جَدِيدًا"
  },
  {
    "id": 196,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1527.81,
    "end": 1539.26,
    "text": "أَوَلَمْ يَرَوْا أَنَّ اللَّهَ الَّذِي خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ قَادِرٌ عَلَى أَنْ يَخْلُقَ مِثْلَهُمْ"
  },
  {
    "id": 197,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1540.45,
    "end": 1552.73,
    "text": "غَادِرٌ عَلَى أَنْ يَخْلُقَ مِثْلَهُمْ وَجَعَلَ لَهُمْ أَجَلَ اللَّهَ رَيْبَ فِيهِ فَأَبَى الظَّالِمُونَ إِلَّا كُفُورًا"
  },
  {
    "id": 198,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1554.12,
    "end": 1566.04,
    "text": "قُلْ لَوْ أَنْتُمْ تَمْلِكُونَ خَزَائِنَ رَحْمَةِ رَبِّي إِذًا لَأَمْسَكْتُمْ خَشْيَةَ الْإِنْفَاقِ"
  },
  {
    "id": 199,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1566.93,
    "end": 1570.5,
    "text": "وَكَانَ الْإِنْسَانُ قَتُورًا"
  },
  {
    "id": 200,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1571.45,
    "end": 1577.5,
    "text": "وَلَقَدْ آتَيْنَا مُوسَى تِسْعَ آيَاتٍ بَيِّنَاتٍ"
  },
  {
    "id": 201,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1578.43,
    "end": 1591.66,
    "text": "يَسْأَلْ بَنِي إِسْرَائِيلَ إِذْ جَاءَهُمْ فَقَالَ لَهُ فِرْعَوْنُ إِنِّي لَأَظُنُّكَ يَا مُوسَى مَسْحُورًا"
  },
  {
    "id": 202,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1592.72,
    "end": 1605.58,
    "text": "قَالَ لَقَدْ عَلِمْتَ مَا أَنْزَلَ هَؤُلَاءِ إِلَّا رَبُّ السَّمَاوَاتِ وَالْأَرْضِ بَصَائِرَ"
  },
  {
    "id": 203,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1606.52,
    "end": 1613.01,
    "text": "يَصَائِرَ وَإِنِّي لَأَظُنُّكَ يَا فِرْعَوْنُ"
  },
  {
    "id": 204,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1613.35,
    "end": 1614.03,
    "text": "وُرَارًا"
  },
  {
    "id": 205,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1615.16,
    "end": 1623.44,
    "text": "فَأَرَادَ أَنْ يَسْتَفِزَّهُمْ مِنَ الْأَرْضِ فَأَغْرَقْنَاهُ وَمَنْ مَعَهُ جَمِيعًا"
  },
  {
    "id": 206,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1624.44,
    "end": 1637.85,
    "text": "وَقُلْنَا مِنْ بَعْدِهِ لِبَنِي إِسْرَائِيلَ اسْكُنُوا الْأَرْضَ فَإِذَا جَاءَ وَعْدُ الْآخِرَةِ جِئْنَا بِكُمْ لَفِيفًا"
  },
  {
    "id": 207,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1638.83,
    "end": 1643.26,
    "text": "وَبِالْحَقِّ أَنْزَلْنَاهُ وَبِالْحَقِّ نَزَلَ"
  },
  {
    "id": 208,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1644.15,
    "end": 1650.14,
    "text": "وَمَا أَرْسَلْنَاكَ إِلَّا مُبَشِّرًا وَنَذِيرًا"
  },
  {
    "id": 209,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1651.15,
    "end": 1662.22,
    "text": "وَقُرْآنًا فَرَقْنَاهُ لِتَقْرَأَهُ عَلَى النَّاسِ عَلَى مُكْثٍ وَنَزَّلْنَاهُ تَنْزِيلًا"
  },
  {
    "id": 210,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1662.99,
    "end": 1667.43,
    "text": "قُلْ آمِنُوا بِهِ أَوْ لَا تُؤْمِنُوا"
  },
  {
    "id": 211,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1668.45,
    "end": 1679.53,
    "text": "إِنَّ الَّذِينَ أُوتُوا الْعِلْمَ مِنْ قَبْلِهِ إِذَا يُتْلَى عَلَيْهِمْ يَخِرُّونَ لِلْأَذْقَانِ سُجَّدًا"
  },
  {
    "id": 212,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1680.34,
    "end": 1689.17,
    "text": "وَيَقُولُونَ سُبْحَانَ رَبِّنَا إِنْ كَانَ وَعْدُ رَبِّنَا لَمَفْعُولًا"
  },
  {
    "id": 213,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1690.09,
    "end": 1696.22,
    "text": "وَيَخِرُّونَ لِلْأَذْقَانِ يَبْكُونَ وَيَزِيدُهُمْ خُشُوعًا"
  },
  {
    "id": 214,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1697.21,
    "end": 1701.11,
    "text": "قُلِ ادْعُوا اللَّهَ أَوِ ادْعُوا الرَّحْمَنَ"
  },
  {
    "id": 215,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1701.97,
    "end": 1707.64,
    "text": "فَلَيَّمَّا تَدْعُوا فَلَهُ الْأَسْمَاءُ الْحُسْنَى"
  },
  {
    "id": 216,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1708.64,
    "end": 1716.37,
    "text": "وَلَا تَجْهَرْ بِصَلَاتِكَ وَلَا تُخَافِتْ بِهَا وَابْتَغِ بَيْنَ ذَلِكَ سَبِيلًا"
  },
  {
    "id": 217,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1717.15,
    "end": 1730.83,
    "text": "وَقُلِ الْحَمْدُ لِلَّهِ الَّذِي لَمْ يَتَّخِذْ وَلَدًا وَلَمْ يَكُنْ لَهُ شَرِيكٌ فِي الْمُلْكِ وَلَمْ يَكُنْ لَهُ وَلِيٌّ مِنَ الذُّلِّينَ"
  },
  {
    "id": 218,
    "sura_id": 17,
    "recitation_uuid": "db143268-bdf6-462b-84c7-bf417efc939c",
    "start": 1731.71,
    "end": 1734.22,
    "text": "وَكَبِّرْهُ تَكْبِيرًا"
  }
]