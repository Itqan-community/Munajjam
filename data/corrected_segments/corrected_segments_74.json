        "uuid": "c3f834dc-4b24-4851-ac0e-bfc45b22af3f"
    },
    {
        "id": 27,
        "sura_id": 74,
        "ayah_index": 26,
        "start": 104.54,
        "end": 107.54,
        "transcribed_text": "وَمَا أَدْرَاكَ مَا سَقَرُ",
