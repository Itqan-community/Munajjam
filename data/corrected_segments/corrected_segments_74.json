    },
    {
        "id": 3,
        "sura_id": 74,
        "ayah_index": 2,
        "start": 11.94,
        "end": 13.71,
        "transcribed_text": "وَرَبَّكَ سَكَبِّرَ",
        "corrected_text": "وَرَبَّكَ فَكَبِّرۡ",
        "start_id": 4,
