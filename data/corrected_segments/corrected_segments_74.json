        "uuid": "c3f834dc-4b24-4851-ac0e-bfc45b22af3f"
    },
    {
        "id": 6,
        "sura_id": 74,
        "ayah_index": 5,
        "start": 20.08,
        "end": 22.51,
        "transcribed_text": "وَلَا تَمْنُنْ تَسْتَ",
        "corrected_text": "وَلَا تَمۡنُن تَسۡتَكۡثِرُ",
        "start_id": 7,
        "end_id": 7,
        "uuid": "c3f834dc-4b24-4851-ac0e-bfc45b22af3f"
    },
    {
        "id": 7,
        "sura_id": 74,
        "ayah_index": 6,
        "start": 22.82,
        "end": 25.62,
        "transcribed_text": "وَالْمُؤْمِنِينَ وَلِرَبِّكَ فَاصْبِرْ",
        "corrected_text": "وَلِرَبِّكَ فَٱصۡبِرۡ",
        "start_id": 9,
        "end_id": 9,
