    },
    {
        "id": 8,
        "sura_id": 74,
        "ayah_index": 7,
        "start": 26.63,
        "end": 30.43,
        "transcribed_text": "فَإِذَا نُقِرَ فِي النَّاقُورِ",
        "corrected_text": "فَإِذَا نُقِرَ فِي ٱلنَّاقُورِ",
        "start_id": 10,
        "end_id": 10,
        "uuid": "c3f834dc-4b24-4851-ac0e-bfc45b22af3f"
    },
    {
        "id": 9,
        "sura_id": 74,
        "ayah_index": 8,
        "start": 31.22,
        "end": 35.72,
        "transcribed_text": "فَذَلِكَ يَوْمَئِذٍ يَوْمٌ عَسِيرٌ",
        "corrected_text": "فَذَٰلِكَ يَوۡمَئِذٖ يَوۡمٌ عَسِيرٌ",
        "start_id": 11,
        "end_id": 11,
        "uuid": "c3f834dc-4b24-4851-ac0e-bfc45b22af3f"
    },
    {
        "id": 10,
        "sura_id": 74,
        "ayah_index": 9,
        "start": 36.54,
        "end": 40.36,
        "transcribed_text": "عَلَى الْكَافِرِينَ غَيْرُ يَسِيرٍ",
        "corrected_text": "عَلَى ٱلۡكَٰفِرِينَ غَيۡرُ يَسِيرٖ",
