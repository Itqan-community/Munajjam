    },
    {
        "id": 17,
        "sura_id": 74,
        "ayah_index": 16,
        "start": 68.04,
        "end": 70.39,
        "transcribed_text": "سَأُرْهِقُهُ صَعُودًا",
        "corrected_text": "سَأُرۡهِقُهُۥ صَعُودًا",
        "start_id": 21,
