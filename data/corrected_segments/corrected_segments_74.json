    },
    {
        "id": 14,
        "sura_id": 74,
        "ayah_index": 13,
        "start": 52.74,
        "end": 53.41,
        "transcribed_text": "وَمَهَّدَ",
        "corrected_text": "وَمَهَّدتُّ لَهُۥ تَمۡهِيدٗا",
        "start_id": 16,
        "end_id": 16,
