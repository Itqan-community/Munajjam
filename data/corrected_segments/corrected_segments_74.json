    },
    {
        "id": 2,
        "sura_id": 74,
        "ayah_index": 1,
        "start": 9.46,
        "end": 11.07,
        "transcribed_text": "قُمْ فَأَنْذِرْ",
        "corrected_text": "قُمۡ فَأَنذِرۡ",
        "start_id": 3,
        "end_id": 3,
