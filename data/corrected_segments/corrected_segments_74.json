    },
    {
        "id": 11,
        "sura_id": 74,
        "ayah_index": 10,
        "start": 41.41,
        "end": 44.67,
        "transcribed_text": "ذَرْنِي وَمَنْ خَلَقْتُ وَحِيدًا",
        "corrected_text": "ذَرۡنِي وَمَنۡ خَلَقۡتُ وَحِيدٗا",
        "start_id": 13,
        "end_id": 13,
