    },
    {
        "id": 18,
        "sura_id": 74,
        "ayah_index": 17,
        "start": 71.2,
        "end": 74.13,
        "transcribed_text": "إِنَّهُ فَكَّرَ وَقَدَّرَ",
        "corrected_text": "إِنَّهُۥ فَكَّرَ وَقَدَّرَ",
        "start_id": 22,
