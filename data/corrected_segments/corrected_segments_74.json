    },
    {
        "id": 4,
        "sura_id": 74,
        "ayah_index": 3,
        "start": 14.56,
        "end": 16.56,
        "transcribed_text": "وَثِيَابَكَ فَطَهِّي",
        "corrected_text": "وَثِيَابَكَ فَطَهِّرۡ",
        "start_id": 5,
        "end_id": 5,
        "uuid": "c3f834dc-4b24-4851-ac0e-bfc45b22af3f"
    },
    {
        "id": 5,
        "sura_id": 74,
        "ayah_index": 4,
        "start": 17.51,
        "end": 19.13,
        "transcribed_text": "وَالرُّجْزَ فَاهْجُرْ",
        "corrected_text": "وَٱلرُّجۡزَ فَٱهۡجُرۡ",
        "start_id": 6,
