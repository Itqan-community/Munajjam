        "uuid": "c3f834dc-4b24-4851-ac0e-bfc45b22af3f"
    },
    {
        "id": 26,
        "sura_id": 74,
        "ayah_index": 25,
        "start": 101.95,
        "end": 103.59,
        "transcribed_text": "سَأُصْلِيهِ سَقَرَ",
        "corrected_text": "سَأُصۡلِيهِ سَقَرَ",
        "start_id": 30,
