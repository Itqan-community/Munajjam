        "uuid": "c3f834dc-4b24-4851-ac0e-bfc45b22af3f"
    },
    {
        "id": 25,
        "sura_id": 74,
        "ayah_index": 24,
        "start": 97.36,
        "end": 101.19,
        "transcribed_text": "إِنْ هَذَا إِلَّا قَوْلُ الْبَشَرِ",
