    },
    {
        "id": 13,
        "sura_id": 74,
        "ayah_index": 12,
        "start": 49.97,
        "end": 52.0,
        "transcribed_text": "وَبَنِينَ شُهُودًا",
        "corrected_text": "وَبَنِينَ شُهُودٗا",
        "start_id": 15,
        "end_id": 15,
