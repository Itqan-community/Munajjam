    },
    {
        "id": 16,
        "sura_id": 74,
        "ayah_index": 15,
        "start": 61.01,
        "end": 67.02,
        "transcribed_text": "كَلَّا إِنَّهُ كَانَ لِآيَاتِنَا عَنِيدًا",
        "corrected_text": "كـَلَّآۖ إِنَّهُۥ كَانَ لِأٓيَٰتِنَا عَنِيدٗا",
