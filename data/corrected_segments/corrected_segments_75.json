    },
    {
        "id": 14,
        "sura_id": 75,
        "ayah_index": 13,
        "start": 69.97,
        "end": 74.31,
        "transcribed_text": "بَلِ الْإِنْسَانُ عَلَى نَفْسِهِ بَصِيرَةٌ",
