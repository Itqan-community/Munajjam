    },
    {
        "id": 7,
        "sura_id": 75,
        "ayah_index": 6,
        "start": 39.74,
        "end": 41.85,
        "transcribed_text": "فَإِذَا بَرِقَ الْبَصَرُ",
        "corrected_text": "فَإِذَا بَرِقَ ٱلۡبَصَرُ",
