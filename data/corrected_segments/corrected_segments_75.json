    },
    {
        "id": 6,
        "sura_id": 75,
        "ayah_index": 5,
        "start": 35.65,
        "end": 38.96,
        "transcribed_text": "يَسْأَلُ أَيَّانَ يَوْمُ الْقِيَامَةِ",
