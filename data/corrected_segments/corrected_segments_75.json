    },
    {
        "id": 9,
        "sura_id": 75,
        "ayah_index": 8,
        "start": 45.33,
        "end": 47.72,
        "transcribed_text": "وَجُمِعَ الشَّمْسُ وَالْقَمَرُ",
        "corrected_text": "وَجُمِعَ ٱلشَّمۡسُ وَٱلۡقَمَرُ",
