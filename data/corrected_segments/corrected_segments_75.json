        "end_id": 13,
        "uuid": "ea4e1af2-0265-4acc-ad2f-2834f54e96cf"
    },
    {
        "id": 13,
        "sura_id": 75,
        "ayah_index": 12,
        "start": 62.38,
        "end": 68.81,
        "transcribed_text": "يُنَبَّأُ الْإِنْسَانُ يَوْمَئِذٍ بِمَا قَدَّمَ وَأَخَّرَ",
