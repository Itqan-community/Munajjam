    },
    {
        "id": 8,
        "sura_id": 75,
        "ayah_index": 7,
        "start": 42.83,
        "end": 44.38,
        "transcribed_text": "وَخَسَفَ الْقَمَرُ",
        "corrected_text": "وَخَسَفَ ٱلۡقَمَرُ",
        "start_id": 9,
