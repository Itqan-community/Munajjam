        "uuid": "ea4e1af2-0265-4acc-ad2f-2834f54e96cf"
    },
    {
        "id": 18,
        "sura_id": 75,
        "ayah_index": 17,
        "start": 89.45,
        "end": 93.51,
        "transcribed_text": "فَإِذَا قَرَأْنَاهُ فَاتَّبِعْ قُرْآنَهُ",
