    },
    {
        "id": 21,
        "sura_id": 68,
        "ayah_index": 20,
        "start": 132.27,
        "end": 135.24,
        "transcribed_text": "فَتَنَادَوْا مُصْبِحِينَ",
        "corrected_text": "فَتَنَادَوۡاْ مُصۡبِحِينَ",
        "start_id": 23,
        "end_id": 23,
