    },
    {
        "id": 3,
        "sura_id": 68,
        "ayah_index": 2,
        "start": 20.6,
        "end": 25.11,
        "transcribed_text": "وَإِنَّ لَكَ لَأَجْرًا غَيْرَ مَمْنُونٍ",
