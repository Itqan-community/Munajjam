    },
    {
        "id": 22,
        "sura_id": 68,
        "ayah_index": 21,
        "start": 136.04,
        "end": 142.56,
        "transcribed_text": "أَنِ اغْدُوا عَلَى حَرْثِكُمْ إِنْ كُنْتُمْ صَارِمِينَ",
        "corrected_text": "أَنِ ٱغۡدُواْ عَلَىٰ حَرۡثِكُمۡ إِن كُنتُمۡ صَٰرِمِينَ",
        "start_id": 24,
        "end_id": 24,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 23,
        "sura_id": 68,
        "ayah_index": 22,
        "start": 143.36,
        "end": 147.98,
        "transcribed_text": "فَانْطَلَقُوا وَهُمْ يَتَخَافَتُونَ",
        "corrected_text": "فَٱنطَلَقُواْ وَهُمۡ يَتَخَٰفَتُونَ",
        "start_id": 25,
