        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 30,
        "sura_id": 68,
        "ayah_index": 29,
        "start": 190.75,
        "end": 196.52,
        "transcribed_text": "فَأَقْبَلَ بَعْضُهُمْ عَلَى بَعْضٍ يَتَلَاوَمُونَ",
        "corrected_text": "فَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضٖ يَتَلَٰوَمُونَ",
        "start_id": 32,
        "end_id": 32,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 31,
        "sura_id": 68,
        "ayah_index": 30,
        "start": 197.23,
        "end": 203.93,
        "transcribed_text": "قَالُوا يَا وَيْلَنَا إِنَّا كُنَّا طَاغِينَ",
        "corrected_text": "قَالُواْ يَٰوَيۡلَنَآ إِنَّا كُنَّا طَٰغِينَ",
        "start_id": 33,
        "end_id": 33,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 32,
        "sura_id": 68,
        "ayah_index": 31,
        "start": 204.72,
        "end": 216.96,
        "transcribed_text": "عَسَى رَبُّنَا أَنْ يُبْدِلَنَا خَيْرًا مِنْهَا إِنَّا إِلَى رَبِّنَا رَاغِبُونَ",
        "corrected_text": "عَسَىٰ رَبُّنَآ أَن يُبۡدِلَنَا خَيۡرٗا مِّنۡهَآ إِنَّآ إِلَىٰ رَبِّنَا رَٰغِبُونَ",
        "start_id": 34,
        "end_id": 34,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 33,
        "sura_id": 68,
        "ayah_index": 32,
        "start": 218.41,
        "end": 228.53,
        "transcribed_text": "كَذَلِكَ الْعَذَابُ وَلَعَذَابُ الْآخِرَةِ أَكْبَرُ لَوْ كَانُوا يَعْلَمُونَ",
        "corrected_text": "كَذَٰلِكَ ٱلۡعَذَابُۖ وَلَعَذَابُ ٱلۡأٓخِرَةِ أَكۡبَرُۚ لَوۡ كَانُواْ يَعۡلَمُونَ",
        "start_id": 37,
        "end_id": 37,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 34,
        "sura_id": 68,
        "ayah_index": 33,
        "start": 229.57,
        "end": 237.02,
        "transcribed_text": "إِنَّ لِلْمُتَّقِينَ عِنْدَ رَبِّهِمْ جَنَّاتِ النَّعِيمِ",
        "corrected_text": "إِنَّ لِلۡمُتَّقِينَ عِندَ رَبِّهِمۡ جَنَّٰتِ ٱلنَّعِيمِ",
        "start_id": 38,
        "end_id": 38,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 35,
        "sura_id": 68,
        "ayah_index": 34,
        "start": 237.91,
        "end": 242.36,
        "transcribed_text": "أَفَنَجْعَلُ الْمُسْلِمِينَ كَالْمُجْرِمِينَ",
        "corrected_text": "أَفَنَجۡعَلُ ٱلۡمُسۡلِمِينَ كَٱلۡمُجۡرِمِينَ",
