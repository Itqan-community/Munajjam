        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 10,
        "sura_id": 68,
        "ayah_index": 9,
        "start": 59.74,
        "end": 64.71,
        "transcribed_text": "وَلَا تُطِعْ كُلَّ حَلَّافٍ مَهِينٍ",
        "corrected_text": "وَلَا تُطِعۡ كُلَّ حَلَّافٖ مَّهِينٍ",
