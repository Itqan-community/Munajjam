    },
    {
        "id": 4,
        "sura_id": 68,
        "ayah_index": 3,
        "start": 25.99,
        "end": 30.44,
        "transcribed_text": "وَإِنَّكَ لَعَلَى خُلُقٍ عَظِيمٍ",
