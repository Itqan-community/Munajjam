    },
    {
        "id": 11,
        "sura_id": 68,
        "ayah_index": 10,
        "start": 65.58,
        "end": 71.47,
        "transcribed_text": "هَمَّازٍ مَشَّاءٍ بِنَمِيمٍ",
        "corrected_text": "هَمَّازٖ مَّشَّآءِۭ بِنَمِيمٖ",
