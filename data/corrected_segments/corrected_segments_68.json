    },
    {
        "id": 5,
        "sura_id": 68,
        "ayah_index": 4,
        "start": 31.37,
        "end": 34.65,
        "transcribed_text": "فَسَتُبْصِرُ وَيُبْصِرُونَ",
        "corrected_text": "فَسَتُبۡصِرُ وَيُبۡصِرُونَ",
