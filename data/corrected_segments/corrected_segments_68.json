    },
    {
        "id": 26,
        "sura_id": 68,
        "ayah_index": 25,
        "start": 161.18,
        "end": 169.67,
        "transcribed_text": "فَلَمَّا رَأَوْهَا قَالُوا إِنَّا لَضَالُّونَ",
        "corrected_text": "فَلَمَّا رَأَوۡهَا قَالُوٓاْ إِنَّا لَضَآلُّونَ",
        "start_id": 28,
        "end_id": 28,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 27,
        "sura_id": 68,
        "ayah_index": 26,
        "start": 170.52,
        "end": 173.73,
        "transcribed_text": "بَلْ نَحْنُ مَحْرُومُونَ",
        "corrected_text": "بَلۡ نَحۡنُ مَحۡرُومُونَ",
        "start_id": 29,
        "end_id": 29,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 28,
        "sura_id": 68,
        "ayah_index": 27,
        "start": 174.73,
        "end": 181.16,
        "transcribed_text": "قَالَ أَوْسَطُهُمْ أَلَمْ أَقُلْ لَكُمْ لَوْلَا تُسَبِّحُونَ",
        "corrected_text": "قَالَ أَوۡسَطُهُمۡ أَلَمۡ أَقُل لَّكُمۡ لَوۡلَا تُسَبِّحُونَ",
        "start_id": 30,
        "end_id": 30,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 29,
        "sura_id": 68,
        "ayah_index": 28,
        "start": 181.99,
        "end": 189.6,
        "transcribed_text": "قَالُوا سُبْحَانَ رَبِّنَا إِنَّا كُنَّا ظَالِمِينَ",
        "corrected_text": "قَالُواْ سُبۡحَٰنَ رَبِّنَآ إِنَّا كُنَّا ظَٰلِمِينَ",
