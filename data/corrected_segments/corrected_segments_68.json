        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 24,
        "sura_id": 68,
        "ayah_index": 23,
        "start": 148.79,
        "end": 155.07,
        "transcribed_text": "أَنْ لَا يَدْخُلَنَّهَا الْيَوْمَ عَلَيْكُمْ مِسْكِينٌ",
        "corrected_text": "أَن لَّا يَدۡخُلَنَّهَا ٱلۡيَوۡمَ عَلَيۡكُم مِّسۡكِينٞ",
        "start_id": 26,
        "end_id": 26,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 25,
        "sura_id": 68,
        "ayah_index": 24,
        "start": 155.9,
        "end": 160.39,
        "transcribed_text": "وَغَدَوْا عَلَى حَرْدٍ قَادِرِينَ",
        "corrected_text": "وَغَدَوۡاْ عَلَىٰ حَرۡدٖ قَٰدِرِينَ",
        "start_id": 27,
        "end_id": 27,
