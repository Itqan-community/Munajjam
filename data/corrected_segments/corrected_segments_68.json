    },
    {
        "id": 9,
        "sura_id": 68,
        "ayah_index": 8,
        "start": 54.76,
        "end": 58.9,
        "transcribed_text": "وَدُّوا لَوْ تُدْهِنُ فَيُدْهِنُونَ",
        "corrected_text": "وَدُّواْ لَوۡ تُدۡهِنُ فَيُدۡهِنُونَ",
        "start_id": 11,
