    },
    {
        "id": 19,
        "sura_id": 68,
        "ayah_index": 18,
        "start": 119.33,
        "end": 127.74,
        "transcribed_text": "فَطَافَ عَلَيْهَا طَائِفٌ مِنْ رَبِّكَ وَهُمْ نَائِمُونَ",
        "corrected_text": "فَطَافَ عَلَيۡهَا طَآئِفٞ مِّن رَّبِّكَ وَهُمۡ نَآئِمُونَ",
        "start_id": 21,
        "end_id": 21,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 20,
        "sura_id": 68,
        "ayah_index": 19,
        "start": 128.52,
        "end": 131.43,
        "transcribed_text": "فَأَصْبَحَتْ كَالصَّرِيمِ",
        "corrected_text": "فَأَصۡبَحَتۡ كَٱلصَّرِيمِ",
