    },
    {
        "id": 7,
        "sura_id": 68,
        "ayah_index": 6,
        "start": 39.73,
        "end": 49.47,
        "transcribed_text": "إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَنْ ضَلَّ عَنْ سَبِيلِهِ وَهُوَ أَعْلَمُ بِالْمُهْتَدِينَ",
        "corrected_text": "إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعۡلَمُ بِٱلۡمُهۡتَدِينَ",
        "start_id": 9,
        "end_id": 9,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 8,
        "sura_id": 68,
        "ayah_index": 7,
        "start": 50.53,
        "end": 53.97,
        "transcribed_text": "فَلَا تُطِعِ الْمُكَذِّبِينَ",
