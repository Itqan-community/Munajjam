    },
    {
        "id": 15,
        "sura_id": 68,
        "ayah_index": 14,
        "start": 88.86,
        "end": 96.35,
        "transcribed_text": "إِذَا تُتْلَى عَلَيْهِ آيَاتُنَا قَالَ أَسَاطِيرُ الْأَوَّلِينَ",
        "corrected_text": "إِذَا تُتۡلَىٰ عَلَيۡهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلۡأَوَّلِينَ",
        "start_id": 17,
        "end_id": 17,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 16,
        "sura_id": 68,
        "ayah_index": 15,
        "start": 97.41,
        "end": 101.01,
        "transcribed_text": "سَنَسِمُهُ عَلَى الْخُرْطُومِ",
        "corrected_text": "سَنَسِمُهُۥ عَلَى ٱلۡخُرۡطُومِ",
        "start_id": 18,
        "end_id": 18,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 17,
        "sura_id": 68,
        "ayah_index": 16,
        "start": 102.17,
        "end": 114.53,
        "transcribed_text": "إِنَّا بَلَوْنَاهُمْ كَمَا بَلَوْنَا أَصْحَابَ الْجَنَّةِ إِذْ أَقْسَمُوا لَيَصْرِمُنَّهَا مُصْبِحِينَ",
        "corrected_text": "إِنَّا بَلَوۡنَٰهُمۡ كَمَا بَلَوۡنَآ أَصۡحَٰبَ ٱلۡجَنَّةِ إِذۡ أَقۡسَمُواْ لَيَصۡرِمُنَّهَا مُصۡبِحِينَ",
        "start_id": 19,
        "end_id": 19,
        "uuid": "ad680f99-9792-4406-81c2-374642816dc6"
    },
    {
        "id": 18,
        "sura_id": 68,
        "ayah_index": 17,
        "start": 115.46,
        "end": 118.14,
        "transcribed_text": "وَلَا يَسْتَثْنُونَ",
        "corrected_text": "وَلَا يَسۡتَثۡنُونَ",
        "start_id": 20,
        "end_id": 20,
