    },
    {
        "id": 6,
        "sura_id": 68,
        "ayah_index": 5,
        "start": 35.44,
        "end": 38.65,
        "transcribed_text": "بِأَيْيِكُمُ الْمَفْتُونُ",
        "corrected_text": "بِأَييِّكُمُ ٱلۡمَفۡتُونُ",
