    },
    {
        "id": 20,
        "sura_id": 70,
        "ayah_index": 19,
        "start": 102.69,
        "end": 105.86,
        "transcribed_text": "إِذَا مَسَّهُ الشَّرُّ جَزُوعًا",
        "corrected_text": "إِذَا مَسَّهُ ٱلشَّرُّ جَزُوعٗا",
