    {
        "id": 1,
        "sura_id": 70,
        "ayah_index": 0,
        "start": 6.22,
        "end": 11.5,
        "transcribed_text": "سَأَلَ سَائِلٌ بِعَذَابٍ وَاقِعٍ",
        "corrected_text": "سَأَلَ سَآئِلُۢ بِعَذَابٖ وَاقِعٖ",
        "start_id": 2,
        "end_id": 2,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 2,
        "sura_id": 70,
        "ayah_index": 1,
        "start": 12.47,
        "end": 15.92,
        "transcribed_text": "لِلْكَافِرِينَ لَيْسَ لَهُ دَافِعٌ",
        "corrected_text": "لِّلۡكَٰفِرِينَ لَيۡسَ لَهُۥ دَافِعٞ",
        "start_id": 3,
        "end_id": 3,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 3,
        "sura_id": 70,
        "ayah_index": 2,
        "start": 16.93,
        "end": 19.41,
        "transcribed_text": "وَمِنَ اللَّهِ ذِي الْمَعَارِجِ",
        "corrected_text": "مِّنَ ٱللَّهِ ذِي ٱلۡمَعَارِجِ",
        "start_id": 4,
        "end_id": 4,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 4,
        "sura_id": 70,
        "ayah_index": 3,
        "start": 20.51,
        "end": 30.56,
        "transcribed_text": "تِعْرُجُ الْمَلَائِكَةُ وَالرُّوحُ إِلَيْهِ فِي يَوْمٍ كَانَ مِقْدَارُهُ خَمْسِينَ أَلْفَ سَنَةٍ",
        "corrected_text": "تَعۡرُجُ ٱلۡمَلَٰٓئِكَةُ وَٱلرُّوحُ إِلَيۡهِ فِي يَوۡمٖ كَانَ مِقۡدَارُهُۥ خَمۡسِينَ أَلۡفَ سَنَةٖ",
        "start_id": 5,
        "end_id": 5,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 5,
        "sura_id": 70,
        "ayah_index": 4,
        "start": 31.61,
        "end": 34.99,
        "transcribed_text": "فَاصْبِرْ صَبْرًا جَمِيلًا",
        "corrected_text": "فَٱصۡبِرۡ صَبۡرٗا جَمِيلًا",
        "start_id": 6,
        "end_id": 6,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 6,
        "sura_id": 70,
        "ayah_index": 5,
        "start": 35.72,
        "end": 39.38,
        "transcribed_text": "إِنَّهُمْ يَرَوْنَهُ بَعِيدًا",
        "corrected_text": "إِنَّهُمۡ يَرَوۡنَهُۥ بَعِيدٗا",
        "start_id": 7,
        "end_id": 7,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 7,
        "sura_id": 70,
        "ayah_index": 6,
        "start": 40.16,
        "end": 42.28,
        "transcribed_text": "وَنَرَاهُ قَرِيبًا",
        "corrected_text": "وَنَرَىٰهُ قَرِيبٗا",
        "start_id": 8,
        "end_id": 8,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 8,
        "sura_id": 70,
        "ayah_index": 7,
        "start": 43.33,
        "end": 47.19,
        "transcribed_text": "يَوْمَ تَكُونُ السَّمَاءُ كَالْمُهْلِ",
        "corrected_text": "يَوۡمَ تَكُونُ ٱلسَّمَآءُ كَٱلۡمُهۡلِ",
        "start_id": 9,
        "end_id": 9,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 9,
        "sura_id": 70,
        "ayah_index": 8,
        "start": 47.93,
        "end": 50.82,
        "transcribed_text": "وَتَكُونُ الْجِبَالُ كَالْعِهْنِ",
        "corrected_text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ",
        "start_id": 10,
        "end_id": 10,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 10,
        "sura_id": 70,
        "ayah_index": 9,
        "start": 51.72,
        "end": 55.27,
        "transcribed_text": "وَلَا يَسْأَلُ حَمِيمٌ حَمِيمًا",
        "corrected_text": "وَلَا يَسۡـَٔلُ حَمِيمٌ حَمِيمٗا",
        "start_id": 11,
        "end_id": 11,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 11,
        "sura_id": 70,
        "ayah_index": 10,
        "start": 56.43,
        "end": 66.47,
        "transcribed_text": "يُبَصَّرُونَهُمْ يَوَدُّ الْمُجْرِمُ لَوْ يَفْتَدِي مِنْ عَذَابِ يَوْمِئِذٍ بِبَنِيهِ",
        "corrected_text": "يُبَصَّرُونَهُمۡۚ يَوَدُّ ٱلۡمُجۡرِمُ لَوۡ يَفۡتَدِي مِنۡ عَذَابِ يَوۡمِئِذِۭ بِبَنِيهِ",
        "start_id": 13,
        "end_id": 13,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 12,
        "sura_id": 70,
        "ayah_index": 11,
        "start": 67.22,
        "end": 70.47,
        "transcribed_text": "وَصَاحِبَتِهِ وَأَخِيهِ",
        "corrected_text": "وَصَٰحِبَتِهِۦ وَأَخِيهِ",
        "start_id": 14,
        "end_id": 14,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 13,
        "sura_id": 70,
        "ayah_index": 12,
        "start": 71.33,
        "end": 75.06,
        "transcribed_text": "وَفَصِيلَتِهِ الَّتِي تُؤْوِيهِ",
        "corrected_text": "وَفَصِيلَتِهِ ٱلَّتِي تُـٔۡوِيهِ",
