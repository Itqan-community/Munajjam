        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 28,
        "sura_id": 70,
        "ayah_index": 27,
        "start": 146.09,
        "end": 150.89,
        "transcribed_text": "إِنَّ عَذَابَ رَبِّهِمْ غَيْرُ مَأْمُونٍ",
        "corrected_text": "إِنَّ عَذَابَ رَبِّهِمۡ غَيۡرُ مَأۡمُونٖ",
        "start_id": 31,
