        "end_id": 16,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 15,
        "sura_id": 70,
        "ayah_index": 14,
        "start": 83.82,
        "end": 87.23,
        "transcribed_text": "كَلَّا إِنَّهَا لَغَى",
        "corrected_text": "كـَلَّآۖ إِنَّهَا لَظَىٰ",
        "start_id": 18,
        "end_id": 18,
