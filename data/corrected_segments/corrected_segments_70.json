    },
    {
        "id": 36,
        "sura_id": 70,
        "ayah_index": 35,
        "start": 206.43,
        "end": 211.59,
        "transcribed_text": "فَمَالِ الَّذِينَ كَفَرُوا قِبَلَكَ مُهْطِعِينَ",
