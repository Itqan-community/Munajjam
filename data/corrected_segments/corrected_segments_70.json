    },
    {
        "id": 16,
        "sura_id": 70,
        "ayah_index": 15,
        "start": 87.93,
        "end": 90.36,
        "transcribed_text": "نَزَّاعَةً لِلشَّوَى",
        "corrected_text": "نَزَّاعَةٗ لِّلشَّوَىٰ",
        "start_id": 19,
        "end_id": 19,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 17,
        "sura_id": 70,
        "ayah_index": 16,
        "start": 91.03,
        "end": 94.24,
        "transcribed_text": "وَيَدْعُو مَنْ أَدْبَرَ وَتَوَلَّى",
        "corrected_text": "تَدۡعُواْ مَنۡ أَدۡبَرَ وَتَوَلَّىٰ",
        "start_id": 20,
        "end_id": 20,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 18,
        "sura_id": 70,
        "ayah_index": 17,
        "start": 95.01,
        "end": 96.87,
        "transcribed_text": "وَجَمَعَ فَأَوْعَى",
        "corrected_text": "وَجَمَعَ فَأَوۡعَىٰٓ",
        "start_id": 21,
        "end_id": 21,
