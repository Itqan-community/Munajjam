        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 21,
        "sura_id": 70,
        "ayah_index": 20,
        "start": 106.71,
        "end": 110.15,
        "transcribed_text": "وَإِذَا مَسَّهُ الْخَيْرُ مَنُوعًا",
        "corrected_text": "وَإِذَا مَسَّهُ ٱلۡخَيۡرُ مَنُوعًا",
        "start_id": 24,
        "end_id": 24,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 22,
        "sura_id": 70,
        "ayah_index": 21,
        "start": 111.06,
        "end": 113.58,
        "transcribed_text": "إِلَّا الْمُصَلِّينَ",
        "corrected_text": "إِلَّا ٱلۡمُصَلِّينَ",
        "start_id": 25,
        "end_id": 25,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 23,
        "sura_id": 70,
        "ayah_index": 22,
        "start": 114.33,
        "end": 120.66,
        "transcribed_text": "الَّذِينَ هُمْ عَلَى صَلَاتِهِمْ دَائِمُونَ",
        "corrected_text": "ٱلَّذِينَ هُمۡ عَلَىٰ صَلَاتِهِمۡ دَآئِمُونَ",
        "start_id": 26,
        "end_id": 26,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 24,
        "sura_id": 70,
        "ayah_index": 23,
        "start": 121.73,
        "end": 128.0,
        "transcribed_text": "وَالَّذِينَ فِي أَمْوَالِهِمْ حَقٌّ مَعْلُومٌ",
        "corrected_text": "وَٱلَّذِينَ فِيٓ أَمۡوَٰلِهِمۡ حَقّٞ مَّعۡلُومٞ",
        "start_id": 27,
        "end_id": 27,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 25,
        "sura_id": 70,
        "ayah_index": 24,
        "start": 128.74,
        "end": 132.95,
        "transcribed_text": "لِلسَّائِلِ وَالْمَحْرُومِ",
        "corrected_text": "لِّلسَّآئِلِ وَٱلۡمَحۡرُومِ",
        "start_id": 28,
        "end_id": 28,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 26,
        "sura_id": 70,
        "ayah_index": 25,
        "start": 133.81,
        "end": 138.42,
        "transcribed_text": "وَالَّذِينَ يُصَدِّقُونَ بِيَوْمِ الدِّينِ",
        "corrected_text": "وَٱلَّذِينَ يُصَدِّقُونَ بِيَوۡمِ ٱلدِّينِ",
        "start_id": 29,
        "end_id": 29,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 27,
        "sura_id": 70,
        "ayah_index": 26,
        "start": 139.22,
        "end": 145.36,
        "transcribed_text": "وَالَّذِينَ هُمْ مِنْ عَذَابِ رَبِّهِمْ مُشْفِقُونَ",
        "corrected_text": "وَٱلَّذِينَ هُم مِّنۡ عَذَابِ رَبِّهِم مُّشۡفِقُونَ",
        "start_id": 30,
