        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 29,
        "sura_id": 70,
        "ayah_index": 28,
        "start": 152.1,
        "end": 156.8,
        "transcribed_text": "وَالَّذِينَ هُمْ لِفُرُوجِهِمْ آفِضُونَ",
        "corrected_text": "وَٱلَّذِينَ هُمۡ لِفُرُوجِهِمۡ حَٰفِظُونَ",
        "start_id": 32,
        "end_id": 32,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 30,
        "sura_id": 70,
        "ayah_index": 29,
        "start": 157.51,
        "end": 167.69,
        "transcribed_text": "إِلَّا عَلَى أَزْوَاجِهِمْ أَوْ مَا مَلَكَتْ أَيْمَانُهُمْ فَإِنَّهُمْ غَيْرُ مَلُومِينَ",
        "corrected_text": "إِلَّا عَلَىٰٓ أَزۡوَٰجِهِمۡ أَوۡ مَا مَلَكَتۡ أَيۡمَٰنُهُمۡ فَإِنَّهُمۡ غَيۡرُ مَلُومِينَ",
        "start_id": 33,
        "end_id": 33,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 31,
        "sura_id": 70,
        "ayah_index": 30,
        "start": 168.91,
        "end": 176.92,
        "transcribed_text": "فَمَنِ ابْتَغَى وَرَاءَ ذَلِكَ فَأُولَئِكَ هُمُ الْعَادُونَ",
        "corrected_text": "فَمَنِ ٱبۡتَغَىٰ وَرَآءَ ذَٰلِكَ فَأُوْلَٰٓئِكَ هُمُ ٱلۡعَادُونَ",
        "start_id": 34,
        "end_id": 34,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 32,
        "sura_id": 70,
        "ayah_index": 31,
        "start": 177.73,
        "end": 183.54,
        "transcribed_text": "وَالَّذِينَ هُمْ لِأَمَانَاتِهِمْ وَعَهْدِهِمْ رَاعُونَ",
        "corrected_text": "وَٱلَّذِينَ هُمۡ لِأَمَٰنَٰتِهِمۡ وَعَهۡدِهِمۡ رَٰعُونَ",
        "start_id": 35,
        "end_id": 35,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 33,
        "sura_id": 70,
        "ayah_index": 32,
        "start": 184.53,
        "end": 191.52,
        "transcribed_text": "وَالَّذِينَ هُمْ بِشَهَادَاتِهِمْ قَائِمُونَ",
        "corrected_text": "وَٱلَّذِينَ هُم بِشَهَٰدَٰتِهِمۡ قَآئِمُونَ",
        "start_id": 36,
        "end_id": 36,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 34,
        "sura_id": 70,
        "ayah_index": 33,
        "start": 192.6,
        "end": 197.95,
        "transcribed_text": "وَالَّذِينَ هُمْ عَلَى صَلَاتِهِمْ يُحَافِظُونَ",
        "corrected_text": "وَٱلَّذِينَ هُمۡ عَلَىٰ صَلَاتِهِمۡ يُحَافِظُونَ",
        "start_id": 37,
        "end_id": 37,
        "uuid": "cfb0b8c0-f992-4062-adb3-f7101df8f9c6"
    },
    {
        "id": 35,
        "sura_id": 70,
        "ayah_index": 34,
        "start": 198.62,
        "end": 205.18,
        "transcribed_text": "أُولَئِكَ فِي جَنَّاتٍ مُكْرَمُونَ",
        "corrected_text": "أُوْلَٰٓئِكَ فِي جَنَّٰتٖ مُّكۡرَمُونَ",
        "start_id": 38,
        "end_id": 38,
