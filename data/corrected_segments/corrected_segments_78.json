[
    {
        "id": 1,
        "sura_id": 78,
        "ayah_index": 0,
        "start": 5.73,
        "end": 10.1,
        "transcribed_text": "عَمَّ يَتَسَاءَلُونَ",
        "corrected_text": "عَمَّ يَتَسَآءَلُونَ",
        "start_id": 2,
        "end_id": 2,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 2,
        "sura_id": 78,
        "ayah_index": 1,
        "start": 11.17,
        "end": 14.34,
        "transcribed_text": "عَنِ النَّبَإِ الْعَظِيمِ",
        "corrected_text": "عَنِ ٱلنَّبَإِ ٱلۡعَظِيمِ",
        "start_id": 3,
        "end_id": 3,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 3,
        "sura_id": 78,
        "ayah_index": 2,
        "start": 15.26,
        "end": 19.25,
        "transcribed_text": "الَّذِي هُمْ فِيهِ مُخْتَلِفُونَ",
        "corrected_text": "ٱلَّذِي هُمۡ فِيهِ مُخۡتَلِفُونَ",
        "start_id": 4,
        "end_id": 4,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 4,
        "sura_id": 78,
        "ayah_index": 3,
        "start": 20.16,
        "end": 23.02,
        "transcribed_text": "كَلَّا سَيَعْلَمُونَ",
        "corrected_text": "كـَلَّا سَيَعۡلَمُونَ",
        "start_id": 5,
        "end_id": 5,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 5,
        "sura_id": 78,
        "ayah_index": 4,
        "start": 23.89,
        "end": 27.73,
        "transcribed_text": "ثُمَّ كَلَّا سَيَعْلَمُونَ",
        "corrected_text": "ثُمَّ كـَلَّا سَيَعۡلَمُونَ",
        "start_id": 6,
        "end_id": 6,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 6,
        "sura_id": 78,
        "ayah_index": 5,
        "start": 28.86,
        "end": 31.96,
        "transcribed_text": "أَلَمْ نَجْعَلِ الْأَرْضَ مِهَادًا",
        "corrected_text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ مِهَٰدٗا",
        "start_id": 7,
        "end_id": 7,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 7,
        "sura_id": 78,
        "ayah_index": 6,
        "start": 32.86,
        "end": 35.41,
        "transcribed_text": "وَالْجِبَالَ أَوْتَادًا",
        "corrected_text": "وَٱلۡجِبَالَ أَوۡتَادٗا",
        "start_id": 8,
        "end_id": 8,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 8,
        "sura_id": 78,
        "ayah_index": 7,
        "start": 36.34,
        "end": 39.28,
        "transcribed_text": "وَخَلَقْنَاكُمْ أَزْوَاجًا",
        "corrected_text": "وَخَلَقۡنَٰكُمۡ أَزۡوَٰجٗا",
        "start_id": 9,
        "end_id": 9,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 9,
        "sura_id": 78,
        "ayah_index": 8,
        "start": 40.25,
        "end": 43.53,
        "transcribed_text": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًا",
        "corrected_text": "وَجَعَلۡنَا نَوۡمَكُمۡ سُبَاتٗا",
        "start_id": 10,
        "end_id": 10,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 10,
        "sura_id": 78,
        "ayah_index": 9,
        "start": 44.66,
        "end": 47.6,
        "transcribed_text": "وَجَعَلْنَا اللَّيْلَ لِبَاسًا",
        "corrected_text": "وَجَعَلۡنَا ٱلَّيۡلَ لِبَاسٗا",
        "start_id": 11,
        "end_id": 11,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 11,
        "sura_id": 78,
        "ayah_index": 10,
        "start": 48.4,
        "end": 51.77,
        "transcribed_text": "وَجَعَلْنَا النَّهَارَ مَعَاشًا",
        "corrected_text": "وَجَعَلۡنَا ٱلنَّهَارَ مَعَاشٗا",
        "start_id": 12,
        "end_id": 12,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 12,
        "sura_id": 78,
        "ayah_index": 11,
        "start": 52.7,
        "end": 57.35,
        "transcribed_text": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًا",
        "corrected_text": "وَبَنَيۡنَا فَوۡقَكُمۡ سَبۡعٗا شِدَادٗا",
        "start_id": 13,
        "end_id": 13,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 13,
        "sura_id": 78,
        "ayah_index": 12,
        "start": 58.35,
        "end": 62.21,
        "transcribed_text": "وَجَعَلْنَا سِرَاجًا وَهَّاجًا",
        "corrected_text": "وَجَعَلۡنَا سِرَاجٗا وَهَّاجٗا",
        "start_id": 14,
        "end_id": 14,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 14,
        "sura_id": 78,
        "ayah_index": 13,
        "start": 63.27,
        "end": 70.44,
        "transcribed_text": "وَأَنْزَلْنَا مِنَ الْمُعْصِرَاتِ مَاءً ثَجَّاجًا",
        "corrected_text": "وَأَنزَلۡنَا مِنَ ٱلۡمُعۡصِرَٰتِ مَآءٗ ثَجَّاجٗا",
        "start_id": 15,
        "end_id": 15,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 15,
        "sura_id": 78,
        "ayah_index": 14,
        "start": 71.75,
        "end": 76.1,
        "transcribed_text": "لِنُخْرِجَ بِهِ حَبًّا وَنَبَاتًا",
        "corrected_text": "لِّنُخۡرِجَ بِهِۦ حَبّٗا وَنَبَاتٗا",
        "start_id": 16,
        "end_id": 16,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 16,
        "sura_id": 78,
        "ayah_index": 15,
        "start": 76.92,
        "end": 79.93,
        "transcribed_text": "وَجَنَّاتٍ أَلْفَافًا",
        "corrected_text": "وَجَنَّٰتٍ أَلۡفَافًا",
        "start_id": 17,
        "end_id": 17,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 17,
        "sura_id": 78,
        "ayah_index": 16,
        "start": 80.84,
        "end": 85.14,
        "transcribed_text": "إِنَّ يَوْمَ الْفَصْلِ كَانَ مِيقَاتًا",
        "corrected_text": "إِنَّ يَوۡمَ ٱلۡفَصۡلِ كَانَ مِيقَٰتٗا",
        "start_id": 18,
        "end_id": 18,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 18,
        "sura_id": 78,
        "ayah_index": 17,
        "start": 86.25,
        "end": 91.53,
        "transcribed_text": "يَوْمَ يُنْفَخُ فِي الصُّورِ فَتَأْتُونَ أَفْوَاجًا",
        "corrected_text": "يَوۡمَ يُنفَخُ فِي ٱلصُّورِ فَتَأۡتُونَ أَفۡوَاجٗا",
        "start_id": 19,
        "end_id": 19,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 19,
        "sura_id": 78,
        "ayah_index": 18,
        "start": 92.66,
        "end": 98.13,
        "transcribed_text": "وَفُتِحَتِ السَّمَاءُ فَكَانَتْ أَبْوَابًا",
        "corrected_text": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتۡ أَبۡوَٰبٗا",
        "start_id": 20,
        "end_id": 20,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 20,
        "sura_id": 78,
        "ayah_index": 19,
        "start": 99.13,
        "end": 103.69,
        "transcribed_text": "وَسُيِّرَتِ الْجِبَالُ فَكَانَتْ سَرَابًا",
        "corrected_text": "وَسُيِّرَتِ ٱلۡجِبَالُ فَكَانَتۡ سَرَابًا",
        "start_id": 21,
        "end_id": 21,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 21,
        "sura_id": 78,
        "ayah_index": 20,
        "start": 104.78,
        "end": 109.31,
        "transcribed_text": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا",
        "corrected_text": "إِنَّ جَهَنَّمَ كَانَتۡ مِرۡصَادٗا",
        "start_id": 22,
        "end_id": 22,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 22,
        "sura_id": 78,
        "ayah_index": 21,
        "start": 110.36,
        "end": 112.87,
        "transcribed_text": "لِلطَّاغِينَ مَآبًا",
        "corrected_text": "لِّلطَّٰغِينَ مَـَٔابٗا",
        "start_id": 23,
        "end_id": 23,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 23,
        "sura_id": 78,
        "ayah_index": 22,
        "start": 113.87,
        "end": 118.13,
        "transcribed_text": "لَابِثِينَ فِيهَا طه",
        "corrected_text": "لَّٰبِثِينَ فِيهَآ أَحۡقَابٗا",
        "start_id": 25,
        "end_id": 25,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 24,
        "sura_id": 78,
        "ayah_index": 23,
        "start": 119.24,
        "end": 124.71,
        "transcribed_text": "لَا يَذُوقُونَ فِيهَا بَرْدًا وَلَا شَرَابًا",
        "corrected_text": "لَّا يَذُوقُونَ فِيهَا بَرۡدٗا وَلَا شَرَابًا",
        "start_id": 26,
        "end_id": 26,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 25,
        "sura_id": 78,
        "ayah_index": 24,
        "start": 125.61,
        "end": 129.29,
        "transcribed_text": "إِلَّا حَمِيمًا وَغَسَّاقًا",
        "corrected_text": "إِلَّا حَمِيمٗا وَغَسَّاقٗا",
        "start_id": 27,
        "end_id": 27,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 26,
        "sura_id": 78,
        "ayah_index": 25,
        "start": 130.35,
        "end": 133.94,
        "transcribed_text": "جَزَاءً وِفَاقًا",
        "corrected_text": "جَزَآءٗ وِفَاقًا",
        "start_id": 28,
        "end_id": 28,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 27,
        "sura_id": 78,
        "ayah_index": 26,
        "start": 134.97,
        "end": 139.76,
        "transcribed_text": "إِنَّهُمْ كَانُوا لَا يَرْجُونَ حِسَابًا",
        "corrected_text": "إِنَّهُمۡ كَانُواْ لَا يَرۡجُونَ حِسَابٗا",
        "start_id": 29,
        "end_id": 29,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 28,
        "sura_id": 78,
        "ayah_index": 27,
        "start": 140.65,
        "end": 144.88,
        "transcribed_text": "وَكَذَّبُوا بِآيَاتِنَا كِذَّابًا",
        "corrected_text": "وَكَذَّبُواْ بِـَٔايَٰتِنَا كِذَّابٗا",
        "start_id": 30,
        "end_id": 30,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 29,
        "sura_id": 78,
        "ayah_index": 28,
        "start": 145.75,
        "end": 150.04,
        "transcribed_text": "وَكُلَّ شَيْءٍ أَحْصَيْنَاهُ كِتَابًا",
        "corrected_text": "وَكُلَّ شَيۡءٍ أَحۡصَيۡنَٰهُ كِتَٰبٗا",
        "start_id": 31,
        "end_id": 31,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 30,
        "sura_id": 78,
        "ayah_index": 29,
        "start": 151.02,
        "end": 156.86,
        "transcribed_text": "فَذُوقُوا فَلَنْ نَزِيدَكُمْ إِلَّا عَذَابًا",
        "corrected_text": "فَذُوقُواْ فَلَن نَّزِيدَكُمۡ إِلَّا عَذَابًا",
        "start_id": 32,
        "end_id": 32,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 31,
        "sura_id": 78,
        "ayah_index": 30,
        "start": 157.81,
        "end": 161.63,
        "transcribed_text": "إِنَّ لِلْمُتَّقِينَ مَفَازًا",
        "corrected_text": "إِنَّ لِلۡمُتَّقِينَ مَفَازًا",
        "start_id": 33,
        "end_id": 33,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 32,
        "sura_id": 78,
        "ayah_index": 31,
        "start": 162.38,
        "end": 166.07,
        "transcribed_text": "حَدَائِقَ وَأَعْنَابًا",
        "corrected_text": "حَدَآئِقَ وَأَعۡنَٰبٗا",
        "start_id": 34,
        "end_id": 34,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 33,
        "sura_id": 78,
        "ayah_index": 32,
        "start": 166.98,
        "end": 169.57,
        "transcribed_text": "وَكَوَاعِبَ أَتْرَابًا",
        "corrected_text": "وَكَوَاعِبَ أَتۡرَابٗا",
        "start_id": 35,
        "end_id": 35,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 34,
        "sura_id": 78,
        "ayah_index": 33,
        "start": 170.28,
        "end": 173.32,
        "transcribed_text": "وَكَأْسًا دِهَاقًا",
        "corrected_text": "وَكَأۡسٗا دِهَاقٗا",
        "start_id": 36,
        "end_id": 36,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 35,
        "sura_id": 78,
        "ayah_index": 34,
        "start": 174.15,
        "end": 179.71,
        "transcribed_text": "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا كِذَّابًا",
        "corrected_text": "لَّا يَسۡمَعُونَ فِيهَا لَغۡوٗا وَلَا كِذَّٰبٗا",
        "start_id": 37,
        "end_id": 37,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 36,
        "sura_id": 78,
        "ayah_index": 35,
        "start": 180.73,
        "end": 187.16,
        "transcribed_text": "جَزَاءً مِنْ رَبِّكَ عَطَاءً حِسَابًا",
        "corrected_text": "جَزَآءٗ مِّن رَّبِّكَ عَطَآءً حِسَابٗا",
        "start_id": 38,
        "end_id": 38,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 37,
        "sura_id": 78,
        "ayah_index": 36,
        "start": 188.4,
        "end": 199.4,
        "transcribed_text": "رَبِّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا الرَّحْمَنِ لَا يَمْلِكُونَ مِنْهُ خِطَابًا",
        "corrected_text": "رَّبِّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَمَا بَيۡنَهُمَا ٱلرَّحۡمَٰنِۖ لَا يَمۡلِكُونَ مِنۡهُ خِطَابٗا",
        "start_id": 40,
        "end_id": 40,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 38,
        "sura_id": 78,
        "ayah_index": 37,
        "start": 200.44,
        "end": 214.81,
        "transcribed_text": "يَوْمَ يَقُومُ الرُّوحُ وَالْمَلَائِكَةُ صَفًّا لَا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ الرَّحْمَنُ وَقَالَ صَوَابًا",
        "corrected_text": "يَوۡمَ يَقُومُ ٱلرُّوحُ وَٱلۡمَلَٰٓئِكَةُ صَفّٗاۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنۡ أَذِنَ لَهُ ٱلرَّحۡمَٰنُ وَقَالَ صَوَابٗا",
        "start_id": 42,
        "end_id": 42,
        "uuid": "edb1c831-7d83-4a10-8c55-6c53cc4058ed"
    },
    {
        "id": 39,
        "sura_id": 78,
        "ayah_index": 38,
