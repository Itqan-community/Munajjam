[
    {
        "id": 1,
        "sura_id": 79,
        "ayah_index": 0,
        "start": 5.47,
        "end": 8.25,
        "transcribed_text": "وَالنَّازِعَاتِ غَرْقًا",
        "corrected_text": "وَٱلنَّٰزِعَٰتِ غَرۡقٗا",
        "start_id": 2,
        "end_id": 2,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 2,
        "sura_id": 79,
        "ayah_index": 1,
        "start": 9.13,
        "end": 11.87,
        "transcribed_text": "وَالنَّاشِطَاتِ نَشْطًا",
        "corrected_text": "وَٱلنَّٰشِطَٰتِ نَشۡطٗا",
        "start_id": 3,
        "end_id": 3,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 3,
        "sura_id": 79,
        "ayah_index": 2,
        "start": 12.88,
        "end": 15.38,
        "transcribed_text": "وَالسَّابِحَاتِ سَبْحًا",
        "corrected_text": "وَٱلسَّٰبِحَٰتِ سَبۡحٗا",
        "start_id": 4,
        "end_id": 4,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 4,
        "sura_id": 79,
        "ayah_index": 3,
        "start": 16.37,
        "end": 18.92,
        "transcribed_text": "فَالسَّابِقَاتِ سَبْقًا",
        "corrected_text": "فَٱلسَّٰبِقَٰتِ سَبۡقٗا",
        "start_id": 5,
        "end_id": 5,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 5,
        "sura_id": 79,
        "ayah_index": 4,
        "start": 19.78,
        "end": 22.34,
        "transcribed_text": "فَالْمُدَبِّرَاتِ أَمْرًا",
        "corrected_text": "فَٱلۡمُدَبِّرَٰتِ أَمۡرٗا",
        "start_id": 6,
        "end_id": 6,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 6,
        "sura_id": 79,
        "ayah_index": 5,
        "start": 23.44,
        "end": 25.97,
        "transcribed_text": "يَوْمَ تَرْجُفُ الرَّاجِفَةُ",
        "corrected_text": "يَوۡمَ تَرۡجُفُ ٱلرَّاجِفَةُ",
        "start_id": 7,
        "end_id": 7,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 7,
        "sura_id": 79,
        "ayah_index": 6,
        "start": 26.98,
        "end": 29.06,
        "transcribed_text": "تَتْبَعُهَا الرَّادِفَةُ",
        "corrected_text": "تَتۡبَعُهَا ٱلرَّادِفَةُ",
        "start_id": 8,
        "end_id": 8,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 8,
        "sura_id": 79,
        "ayah_index": 7,
        "start": 29.96,
        "end": 34.02,
        "transcribed_text": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ",
        "corrected_text": "قُلُوبٞ يَوۡمَئِذٖ وَاجِفَةٌ",
        "start_id": 9,
        "end_id": 9,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 9,
        "sura_id": 79,
        "ayah_index": 8,
        "start": 34.89,
        "end": 37.23,
        "transcribed_text": "أَبْصَارُهَا خَاشِعَةٌ",
        "corrected_text": "أَبۡصَٰرُهَا خَٰشِعَةٞ",
        "start_id": 10,
        "end_id": 10,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 10,
        "sura_id": 79,
        "ayah_index": 9,
        "start": 38.31,
        "end": 43.59,
        "transcribed_text": "يَقُولُونَ أَإِنَّا لَمَرْدُودُونَ فِي الْحَافِرَةِ",
        "corrected_text": "يَقُولُونَ أَءِنَّا لَمَرۡدُودُونَ فِي ٱلۡحَافِرَةِ",
        "start_id": 11,
        "end_id": 11,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 11,
        "sura_id": 79,
        "ayah_index": 10,
        "start": 44.66,
        "end": 48.59,
        "transcribed_text": "أَإِذَا كُنَّا عِظَامًا نَخِرَةً",
        "corrected_text": "أَءِذَا كُنَّا عِظَٰمٗا نَّخِرَةٗ",
        "start_id": 12,
        "end_id": 12,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 12,
        "sura_id": 79,
        "ayah_index": 11,
        "start": 49.48,
        "end": 53.77,
        "transcribed_text": "قَالُوا تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ",
        "corrected_text": "قَالُواْ تِلۡكَ إِذٗا كَرَّةٌ خَاسِرَةٞ",
        "start_id": 13,
        "end_id": 13,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 13,
        "sura_id": 79,
        "ayah_index": 12,
        "start": 54.91,
        "end": 58.95,
        "transcribed_text": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ",
        "corrected_text": "فَإِنَّمَا هِيَ زَجۡرَةٞ وَٰحِدَةٞ",
        "start_id": 14,
        "end_id": 14,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 14,
        "sura_id": 79,
        "ayah_index": 13,
        "start": 59.78,
        "end": 62.52,
        "transcribed_text": "فَإِذَا هُمْ بِالسَّاهِرَةِ",
        "corrected_text": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
        "start_id": 15,
        "end_id": 15,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 15,
        "sura_id": 79,
        "ayah_index": 14,
        "start": 63.77,
        "end": 66.65,
        "transcribed_text": "هَلْ أَتَاكَ حَدِيثُ مُوسَى",
        "corrected_text": "هَلۡ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
        "start_id": 16,
        "end_id": 16,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 16,
        "sura_id": 79,
        "ayah_index": 15,
        "start": 67.69,
        "end": 72.81,
        "transcribed_text": "إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوًى",
        "corrected_text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى",
        "start_id": 17,
        "end_id": 17,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 17,
        "sura_id": 79,
        "ayah_index": 16,
        "start": 73.78,
        "end": 77.97,
        "transcribed_text": "اذْهَبْ إِلَى فِرْعَوْنَ إِنَّهُ طَغَى",
        "corrected_text": "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ",
        "start_id": 18,
        "end_id": 18,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 18,
        "sura_id": 79,
        "ayah_index": 17,
        "start": 78.96,
        "end": 83.77,
        "transcribed_text": "فَقُلْ هَلْ لَكَ إِلَى أَنْ تَزَكَّى",
        "corrected_text": "فَقُلۡ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
        "start_id": 19,
        "end_id": 19,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 19,
        "sura_id": 79,
        "ayah_index": 18,
        "start": 84.61,
        "end": 88.31,
        "transcribed_text": "وَأَهْدِيَكَ إِلَى رَبِّكَ فَتَخْشَى",
        "corrected_text": "وَأَهۡدِيَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ",
        "start_id": 20,
        "end_id": 20,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 20,
        "sura_id": 79,
        "ayah_index": 19,
        "start": 89.27,
        "end": 92.25,
        "transcribed_text": "فَأَرَاهُ الْآيَةَ الْكُبْرَى",
        "corrected_text": "فَأَرَىٰهُ ٱلۡأٓيَةَ ٱلۡكُبۡرَىٰ",
        "start_id": 21,
        "end_id": 21,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 21,
        "sura_id": 79,
        "ayah_index": 20,
        "start": 93.21,
        "end": 95.1,
        "transcribed_text": "فَكَذَّبَ وَعَصَى",
        "corrected_text": "فَكَذَّبَ وَعَصَىٰ",
        "start_id": 22,
        "end_id": 22,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 22,
        "sura_id": 79,
        "ayah_index": 21,
        "start": 95.94,
        "end": 98.58,
        "transcribed_text": "ثُمَّ أَدْبَرَ يَسْعَى",
        "corrected_text": "ثُمَّ أَدۡبَرَ يَسۡعَىٰ",
        "start_id": 23,
        "end_id": 23,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 23,
        "sura_id": 79,
        "ayah_index": 22,
        "start": 99.51,
        "end": 101.42,
        "transcribed_text": "فَحَشَرَ فَنَادَى",
        "corrected_text": "فَحَشَرَ فَنَادَىٰ",
        "start_id": 24,
        "end_id": 24,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 24,
        "sura_id": 79,
        "ayah_index": 23,
        "start": 102.18,
        "end": 105.48,
        "transcribed_text": "فَقَالَ أَنَا رَبُّكُمُ الْأَعْلَى",
        "corrected_text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلۡأَعۡلَىٰ",
        "start_id": 25,
        "end_id": 25,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 25,
        "sura_id": 79,
        "ayah_index": 24,
        "start": 106.43,
        "end": 111.7,
        "transcribed_text": "فَأَخَذَهُ اللَّهُ نَكَالَ الْآخِرَةِ وَالْأُولَى",
        "corrected_text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡأٓخِرَةِ وَٱلۡأُولَىٰٓ",
        "start_id": 26,
        "end_id": 26,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 26,
        "sura_id": 79,
        "ayah_index": 25,
        "start": 112.59,
        "end": 117.85,
        "transcribed_text": "إِنَّ فِي ذَلِكَ لَعِبْرَةً لِمَنْ يَغْفَى وَالْمُؤْمِنِينَ",
        "corrected_text": "إِنَّ فِي ذَٰلِكَ لَعِبۡرَةٗ لِّمَن يَخۡشَىٰٓ",
        "start_id": 28,
        "end_id": 28,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 27,
        "sura_id": 79,
        "ayah_index": 26,
        "start": 119.02,
        "end": 125.94,
        "transcribed_text": "أَأَنْتُمْ أَشَدُّ خَلْقًا أَمِ السَّمَاءُ جَنَاهَا",
        "corrected_text": "ءَأَنتُمۡ أَشَدُّ خَلۡقًا أَمِ ٱلسَّمَآءُۚ بَنَىٰهَا",
        "start_id": 30,
        "end_id": 30,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 28,
        "sura_id": 79,
        "ayah_index": 27,
        "start": 126.89,
        "end": 130.18,
        "transcribed_text": "رَفَعَ سَمْكَهَا فَسَوَّاهَا",
        "corrected_text": "رَفَعَ سَمۡكَهَا فَسَوَّىٰهَا",
        "start_id": 31,
        "end_id": 31,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 29,
        "sura_id": 79,
        "ayah_index": 28,
        "start": 131.02,
        "end": 135.14,
        "transcribed_text": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَاهَا",
        "corrected_text": "وَأَغۡطَشَ لَيۡلَهَا وَأَخۡرَجَ ضُحَىٰهَا",
        "start_id": 32,
        "end_id": 32,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 30,
        "sura_id": 79,
        "ayah_index": 29,
        "start": 136.24,
        "end": 139.74,
        "transcribed_text": "وَالْأَرْضَ بَعْدَ ذَلِكَ دَحَاهَا",
        "corrected_text": "وَٱلۡأَرۡضَ بَعۡدَ ذَٰلِكَ دَحَىٰهَآ",
        "start_id": 33,
        "end_id": 33,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 31,
        "sura_id": 79,
        "ayah_index": 30,
        "start": 140.59,
        "end": 145.71,
        "transcribed_text": "أَخْرَجَ مِنْهَا مَاءَهَا وَمَرْعَاهَا",
        "corrected_text": "أَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعَىٰهَا",
        "start_id": 34,
        "end_id": 34,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 32,
        "sura_id": 79,
        "ayah_index": 31,
        "start": 146.58,
        "end": 149.13,
        "transcribed_text": "وَالْجِبَالَ أَرْسَاهَا",
        "corrected_text": "وَٱلۡجِبَالَ أَرۡسَىٰهَا",
        "start_id": 35,
        "end_id": 35,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 33,
        "sura_id": 79,
        "ayah_index": 32,
        "start": 150.02,
        "end": 153.61,
        "transcribed_text": "مَتَاعًا لَكُمْ وَلِأَنْعَامِكُمْ",
        "corrected_text": "مَتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ",
        "start_id": 36,
        "end_id": 36,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 34,
        "sura_id": 79,
        "ayah_index": 33,
        "start": 154.8,
        "end": 161.71,
        "transcribed_text": "فَإِذَا جَاءَتِ الطَّامَّةُ الْكُبْرَى",
        "corrected_text": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلۡكُبۡرَىٰ",
        "start_id": 37,
        "end_id": 37,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 35,
        "sura_id": 79,
        "ayah_index": 34,
        "start": 162.62,
        "end": 167.31,
        "transcribed_text": "يَوْمَ يَتَذَكَّرُ الْإِنْسَانُ مَا سَعَى",
        "corrected_text": "يَوۡمَ يَتَذَكَّرُ ٱلۡإِنسَٰنُ مَا سَعَىٰ",
        "start_id": 38,
        "end_id": 38,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 36,
        "sura_id": 79,
        "ayah_index": 35,
        "start": 168.3,
        "end": 172.39,
        "transcribed_text": "وَبُرِّزَتِ الْجَحِيمُ لِمَنْ يَرَى",
        "corrected_text": "وَبُرِّزَتِ ٱلۡجَحِيمُ لِمَن يَرَىٰ",
        "start_id": 39,
        "end_id": 39,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 37,
        "sura_id": 79,
        "ayah_index": 36,
        "start": 173.24,
        "end": 176.22,
        "transcribed_text": "فَأَمَّا مَنْ طَغَى",
        "corrected_text": "فَأَمَّا مَن طَغَىٰ",
        "start_id": 40,
        "end_id": 40,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 38,
        "sura_id": 79,
        "ayah_index": 37,
        "start": 177.02,
        "end": 180.07,
        "transcribed_text": "وَآثَرَ الْحَيَاةَ الدُّنْيَا",
        "corrected_text": "وَءَاثَرَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
        "start_id": 41,
        "end_id": 41,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 39,
        "sura_id": 79,
        "ayah_index": 38,
        "start": 180.83,
        "end": 184.43,
        "transcribed_text": "فَإِنَّ الْجَحِيمَ هِيَ الْمَأْوَى",
        "corrected_text": "فَإِنَّ ٱلۡجَحِيمَ هِيَ ٱلۡمَأۡوَىٰ",
        "start_id": 42,
        "end_id": 42,
        "uuid": "b8ed2f39-f324-433a-a68d-c82d872b417f"
    },
    {
        "id": 40,
        "sura_id": 79,
        "ayah_index": 39,
        "start": 185.5,
        "end": 192.49,
        "transcribed_text": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِ وَنَهَى النَّفْسَ عَنِ الْهَوَى",
