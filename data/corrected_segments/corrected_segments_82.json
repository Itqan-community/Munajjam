    {
        "id": 8,
        "sura_id": 82,
        "ayah_index": 7,
        "start": 41.76,
        "end": 47.58,
        "transcribed_text": "فِي أَيِّ صُورَةٍ مَا شَاءَ رَكَّبَةٍ",
