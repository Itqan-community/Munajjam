    },
    {
        "id": 7,
        "sura_id": 82,
        "ayah_index": 6,
        "start": 36.91,
        "end": 40.79,
        "transcribed_text": "الَّذِي خَلَقَكَ فَسَوَّاكَ فَعَدَلَ",
