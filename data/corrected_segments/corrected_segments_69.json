    },
    {
        "id": 2,
        "sura_id": 69,
        "ayah_index": 1,
        "start": 10.84,
        "end": 14.07,
        "transcribed_text": "مَا الْحَاقُ",
        "corrected_text": "مَا ٱلۡحَآقَّةُ",
        "start_id": 4,
        "end_id": 4,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 3,
        "sura_id": 69,
        "ayah_index": 2,
        "start": 14.53,
        "end": 21.75,
        "transcribed_text": "وَالْمُؤْمِنِينَ وَمَا أَدْرَاكَ مَا الْحَاقُ",
        "corrected_text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡحَآقَّةُ",
        "start_id": 6,
        "end_id": 6,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 4,
        "sura_id": 69,
        "ayah_index": 3,
        "start": 22.3,
        "end": 28.06,
        "transcribed_text": "وَالْمُؤْمِنِينَ كَذَّبَتْ ثَمُودُ وَعَادٌ بِالْقَارِعَةِ",
        "corrected_text": "كَذَّبَتۡ ثَمُودُ وَعَادُۢ بِٱلۡقَارِعَةِ",
        "start_id": 8,
        "end_id": 8,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 5,
        "sura_id": 69,
        "ayah_index": 4,
        "start": 29.21,
        "end": 33.69,
        "transcribed_text": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا بِالطَّاغِيَةِ",
        "corrected_text": "فَأَمَّا ثَمُودُ فَأُهۡلِكُواْ بِٱلطَّاغِيَةِ",
        "start_id": 9,
        "end_id": 9,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 6,
        "sura_id": 69,
        "ayah_index": 5,
        "start": 34.61,
        "end": 41.74,
        "transcribed_text": "وَأَمَّا عَادٌ فَأُهْلِكُوا بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ",
        "corrected_text": "وَأَمَّا عَادٞ فَأُهۡلِكُواْ بِرِيحٖ صَرۡصَرٍ عَاتِيَةٖ",
        "start_id": 10,
        "end_id": 10,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 7,
        "sura_id": 69,
        "ayah_index": 6,
        "start": 42.94,
        "end": 58.98,
        "transcribed_text": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَانِيَةَ أَيَّامٍ حُسُومًا فَتَرَى الْقَوْمَ فِيهَا صَرْعَى كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ",
        "corrected_text": "سَخَّرَهَا عَلَيۡهِمۡ سَبۡعَ لَيَالٖ وَثَمَٰنِيَةَ أَيَّامٍ حُسُومٗاۖ فَتَرَى ٱلۡقَوۡمَ فِيهَا صَرۡعَىٰ كَأَنَّهُمۡ أَعۡجَازُ نَخۡلٍ خَاوِيَةٖ",
        "start_id": 12,
        "end_id": 12,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 8,
        "sura_id": 69,
        "ayah_index": 7,
        "start": 60.14,
        "end": 64.16,
        "transcribed_text": "فَهَلْ تَرَى لَهُمْ مِنْ بَاقِيَةٍ",
        "corrected_text": "فَهَلۡ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٖ",
        "start_id": 13,
        "end_id": 13,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 9,
        "sura_id": 69,
        "ayah_index": 8,
        "start": 65.14,
        "end": 72.95,
        "transcribed_text": "وَجَاءَ فِرْعَوْنُ وَمَنْ قَبْلَهُ وَالْمُؤْتَفِكَاتُ بِالْخَاطِئَةِ",
        "corrected_text": "وَجَآءَ فِرۡعَوۡنُ وَمَن قَبۡلَهُۥ وَٱلۡمُؤۡتَفِكَٰتُ بِٱلۡخَاطِئَةِ",
        "start_id": 14,
        "end_id": 14,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 10,
        "sura_id": 69,
        "ayah_index": 9,
        "start": 74.11,
        "end": 79.71,
        "transcribed_text": "فَعَصَوْا رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَابِيَةً",
        "corrected_text": "فَعَصَوۡاْ رَسُولَ رَبِّهِمۡ فَأَخَذَهُمۡ أَخۡذَةٗ رَّابِيَةً",
        "start_id": 15,
        "end_id": 15,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 11,
        "sura_id": 69,
        "ayah_index": 10,
        "start": 80.85,
        "end": 88.13,
        "transcribed_text": "إِنَّا لَمَّا طَغَى الْمَاءُ حَمَلْنَاكُمْ فِي الْجَارِيَةِ",
        "corrected_text": "إِنَّا لَمَّا طَغَا ٱلۡمَآءُ حَمَلۡنَٰكُمۡ فِي ٱلۡجَارِيَةِ",
        "start_id": 16,
        "end_id": 16,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 12,
        "sura_id": 69,
        "ayah_index": 11,
        "start": 89.1,
        "end": 97.11,
        "transcribed_text": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَا أُذُنٌ وَاعِيَةٌ",
        "corrected_text": "لِنَجۡعَلَهَا لَكُمۡ تَذۡكِرَةٗ وَتَعِيَهَآ أُذُنٞ وَٰعِيَةٞ",
        "start_id": 17,
        "end_id": 17,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 13,
        "sura_id": 69,
        "ayah_index": 12,
        "start": 98.61,
        "end": 103.34,
        "transcribed_text": "فَإِذَا نُفِخَ فِي الصُّورِ نَفْخَةٌ وَاحِدَةٌ",
        "corrected_text": "فَإِذَا نُفِخَ فِي ٱلصُّورِ نَفۡخَةٞ وَٰحِدَةٞ",
        "start_id": 18,
        "end_id": 18,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 14,
        "sura_id": 69,
        "ayah_index": 13,
        "start": 104.44,
        "end": 111.05,
        "transcribed_text": "وَحُمِلَتِ الْأَرْضُ وَالْجِبَالُ فَدُكَّتَا دَكَّةً وَاحِدَةً",
        "corrected_text": "وَحُمِلَتِ ٱلۡأَرۡضُ وَٱلۡجِبَالُ فَدُكَّتَا دَكَّةٗ وَٰحِدَةٗ",
        "start_id": 19,
        "end_id": 19,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 15,
        "sura_id": 69,
        "ayah_index": 14,
        "start": 111.87,
        "end": 115.46,
        "transcribed_text": "فَيَوْمَئِذٍ وَقَعَتِ الْوَاقِعَةُ",
        "corrected_text": "فَيَوۡمَئِذٖ وَقَعَتِ ٱلۡوَاقِعَةُ",
        "start_id": 20,
        "end_id": 20,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 16,
        "sura_id": 69,
        "ayah_index": 15,
        "start": 116.42,
        "end": 123.05,
        "transcribed_text": "وَانْشَقَّتِ السَّمَاءُ فَهِيَ يَوْمَئِذٍ وَاهِيَةٌ",
        "corrected_text": "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِيَ يَوۡمَئِذٖ وَاهِيَةٞ",
        "start_id": 21,
        "end_id": 21,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 17,
        "sura_id": 69,
        "ayah_index": 16,
        "start": 124.13,
        "end": 135.85,
        "transcribed_text": "وَالْمَلَكُ عَلَى أَرْجَائِهَا وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَانِيَةٌ",
        "corrected_text": "وَٱلۡمَلَكُ عَلَىٰٓ أَرۡجَآئِهَاۚ وَيَحۡمِلُ عَرۡشَ رَبِّكَ فَوۡقَهُمۡ يَوۡمَئِذٖ ثَمَٰنِيَةٞ",
        "start_id": 23,
        "end_id": 23,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 18,
        "sura_id": 69,
        "ayah_index": 17,
        "start": 137.03,
        "end": 143.57,
        "transcribed_text": "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَى مِنْكُمْ خَافِيَةٌ",
        "corrected_text": "يَوۡمَئِذٖ تُعۡرَضُونَ لَا تَخۡفَىٰ مِنكُمۡ خَافِيَةٞ",
        "start_id": 24,
        "end_id": 24,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 19,
        "sura_id": 69,
        "ayah_index": 18,
        "start": 144.68,
        "end": 154.43,
        "transcribed_text": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ فَيَقُولُ هَاؤُمُ اقْرَءُوا كِتَابِيَهْ",
        "corrected_text": "فَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقۡرَءُواْ كِتَٰبِيَهۡ",
        "start_id": 25,
        "end_id": 25,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 20,
        "sura_id": 69,
        "ayah_index": 19,
        "start": 155.43,
        "end": 161.09,
        "transcribed_text": "إِنِّي ظَنَنْتُ أَنِّي مُلَاقٍ حِسَابِيَهْ",
        "corrected_text": "إِنِّي ظَنَنتُ أَنِّي مُلَٰقٍ حِسَابِيَهۡ",
        "start_id": 26,
        "end_id": 26,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 21,
        "sura_id": 69,
        "ayah_index": 20,
        "start": 161.84,
        "end": 164.83,
        "transcribed_text": "فَهُوَ فِي عِيشَةٍ رَاضِيَةٍ",
        "corrected_text": "فَهُوَ فِي عِيشَةٖ رَّاضِيَةٖ",
        "start_id": 27,
        "end_id": 27,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 22,
        "sura_id": 69,
        "ayah_index": 21,
        "start": 165.68,
        "end": 168.4,
        "transcribed_text": "فِي جَنَّةٍ عَالِيَةٍ",
        "corrected_text": "فِي جَنَّةٍ عَالِيَةٖ",
        "start_id": 28,
        "end_id": 28,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 23,
        "sura_id": 69,
        "ayah_index": 22,
        "start": 169.03,
        "end": 171.27,
        "transcribed_text": "قُطُوفُهَا دَانِيَةٌ",
        "corrected_text": "قُطُوفُهَا دَانِيَةٞ",
        "start_id": 29,
        "end_id": 29,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 24,
        "sura_id": 69,
        "ayah_index": 23,
        "start": 172.03,
        "end": 180.77,
        "transcribed_text": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا أَسْلَفْتُمْ فِي الْأَيَّامِ الْخَالِيَةِ",
        "corrected_text": "كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَآ أَسۡلَفۡتُمۡ فِي ٱلۡأَيَّامِ ٱلۡخَالِيَةِ",
        "start_id": 30,
        "end_id": 30,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 25,
        "sura_id": 69,
        "ayah_index": 24,
        "start": 181.84,
        "end": 191.35,
        "transcribed_text": "وَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِشِمَالِهِ فَيَقُولُ يَا لَيْتَنِي لَمْ أُوتَ كِتَابِيَهْ",
        "corrected_text": "وَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَٰلَيۡتَنِي لَمۡ أُوتَ كِتَٰبِيَهۡ",
        "start_id": 31,
        "end_id": 31,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 26,
        "sura_id": 69,
        "ayah_index": 25,
        "start": 192.23,
        "end": 194.99,
        "transcribed_text": "وَلَمْ أَدْرِ مَا حِسَابِيَهْ",
        "corrected_text": "وَلَمۡ أَدۡرِ مَا حِسَابِيَهۡ",
        "start_id": 32,
        "end_id": 32,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 27,
        "sura_id": 69,
        "ayah_index": 26,
        "start": 195.84,
        "end": 199.41,
        "transcribed_text": "يَا لَيْتَهَا كَانَتِ الْقَاضِيَةَ",
        "corrected_text": "يَٰلَيۡتَهَا كَانَتِ ٱلۡقَاضِيَةَ",
        "start_id": 33,
        "end_id": 33,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 28,
        "sura_id": 69,
        "ayah_index": 27,
        "start": 200.18,
        "end": 204.43,
        "transcribed_text": "وَمَا أَغْنَى عَنِّي مَالِيَهْ",
        "corrected_text": "مَآ أَغۡنَىٰ عَنِّي مَالِيَهۡۜ",
        "start_id": 34,
        "end_id": 34,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 29,
        "sura_id": 69,
        "ayah_index": 28,
        "start": 205.61,
        "end": 208.96,
        "transcribed_text": "هَلَكَ عَنِّي سُلْطَانِيَهْ",
        "corrected_text": "هَلَكَ عَنِّي سُلۡطَٰنِيَهۡ",
        "start_id": 35,
        "end_id": 35,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 30,
        "sura_id": 69,
        "ayah_index": 29,
        "start": 209.94,
        "end": 212.54,
        "transcribed_text": "خُذُوهُ فَغُلُّوهُ",
        "corrected_text": "خُذُوهُ فَغُلُّوهُ",
        "start_id": 36,
        "end_id": 36,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 31,
        "sura_id": 69,
        "ayah_index": 30,
        "start": 213.16,
        "end": 216.74,
        "transcribed_text": "ثُمَّ الْجَحِيمَ صَلُّوهُ",
        "corrected_text": "ثُمَّ ٱلۡجَحِيمَ صَلُّوهُ",
        "start_id": 37,
        "end_id": 37,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 32,
        "sura_id": 69,
        "ayah_index": 31,
        "start": 217.54,
        "end": 225.48,
        "transcribed_text": "ثُمَّ فِي سِلْسِلَةٍ ذَرْعُهَا سَبْعُونَ ذِرَاعًا فَاسْلُكُوهُ",
        "corrected_text": "ثُمَّ فِي سِلۡسِلَةٖ ذَرۡعُهَا سَبۡعُونَ ذِرَاعٗا فَٱسۡلُكُوهُ",
        "start_id": 38,
        "end_id": 38,
        "uuid": "a76c36f4-5692-4b59-ad5b-627f6f986cd6"
    },
    {
        "id": 33,
        "sura_id": 69,
        "ayah_index": 32,
        "start": 226.63,
        "end": 232.41,
        "transcribed_text": "إِنَّهُ كَانَ لَا يُؤْمِنُ بِاللَّهِ الْعَظِيمِ",
        "corrected_text": "إِنَّهُۥ كَانَ لَا يُؤۡمِنُ بِٱللَّهِ ٱلۡعَظِيمِ",
        "start_id": 39,
        "end_id": 39,
